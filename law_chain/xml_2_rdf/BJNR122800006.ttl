dtd:entry-id237 a dtd:entry ;
    dtd:has_BR dtd:BR-id129 ;
    dtd:has_Value "Projekte(§ 4 Nr. 7)"@de .

dtd:entry-id238 a dtd:entry ;
    dtd:has_Value " "@de .

dtd:entry-id239 a dtd:entry ;
    dtd:has_Value "7.1"@de .

dtd:entry-id240 a dtd:entry ;
    dtd:has_BR dtd:BR-id130 ;
    dtd:has_Value "Projektvorbereitung(§ 4 Nr. 7.1)"@de .

dtd:entry-id241 a dtd:entry ;
    dtd:has_DL dtd:DL-id285 ;
    dtd:has_Value "a)Projektpläne aus Projektzielen ableitenb)Teilaufgaben entwickelnc)Informations- und Kommunikationsstrukturen für die Projektarbeit einrichten und nutzend)Gesprächsleitfäden für Projekte erarbeiten"@de .

dtd:entry-id242 a dtd:entry ;
    dtd:has_Value "7.2"@de .

dtd:entry-id243 a dtd:entry ;
    dtd:has_BR dtd:BR-id131 ;
    dtd:has_Value "Projektdurchführung(§ 4 Nr. 7.2)"@de .

dtd:entry-id244 a dtd:entry ;
    dtd:has_DL dtd:DL-id286 ;
    dtd:has_Value "a)Ablaufpläne umsetzenb)Projektaufgaben mit Beteiligten koordinierenc)betriebsbedingte Abweichungen im Projektablauf melden, an der Entwicklung von Lösungsalternativen mitwirkend)Projektabläufe an veränderte Anforderungen von Auftraggebern anpassene)Arbeitszeitkonten führen"@de .

dtd:entry-id245 a dtd:entry ;
    dtd:has_Value "7.3"@de .

dtd:entry-id246 a dtd:entry ;
    dtd:has_BR dtd:BR-id132 ;
    dtd:has_Value "Projektcontrolling(§ 4 Nr. 7.3)"@de .

dtd:entry-id247 a dtd:entry ;
    dtd:has_DL dtd:DL-id287 ;
    dtd:has_Value "a)Projektabläufe und -ergebnisse dokumentierenb)Kennzahlen von Projekten erfassen, für die Auftragsabrechnung aufbereiten und weiterleitenc)Projektstatistiken erstellend)Kosten ermitteln und erfassene)an der Erstellung des Abschlussberichtes mitwirken"@de .

dtd:entry-id248 a dtd:entry ;
    dtd:has_Value "8"@de .

dtd:entry-id249 a dtd:entry ;
    dtd:has_BR dtd:BR-id133 ;
    dtd:has_Value "Personal(§ 4 Nr. 8)"@de .

dtd:entry-id250 a dtd:entry ;
    dtd:has_DL dtd:DL-id288 ;
    dtd:has_Value "a)an der Personalplanung und Teambildung mitwirkenb)bei Maßnahmen der Personalbeschaffung mitwirken, Bewerbervorauswahl durchführenc)neue Mitarbeiter einführend)Personaleinsatz auftragsorientiert planen, dabei insbesondere arbeitszeitrechtliche Vorschriften und Mitarbeiterbedürfnisse berücksichtigen, Schichtpläne erstellene)Motivationsinstrumente einsetzen, Vorschläge zu Konzepten für Anreizmodelle entwickelnf)Vorschläge für Schulungs- und Fortbildungskonzepte entwickeln, Mitarbeiterschulungen organisieren, Mitarbeiter-Briefing durchführen"@de .

dtd:entry-id251 a dtd:entry ;
    dtd:has_Value "9"@de .

dtd:entry-id252 a dtd:entry ;
    dtd:has_BR dtd:BR-id134 ;
    dtd:has_Value "Kaufmännische Steuerung und Kontrolle(§ 4 Nr. 9)"@de .

dtd:entry-id253 a dtd:entry ;
    dtd:has_Value " "@de .

dtd:entry-id254 a dtd:entry ;
    dtd:has_Value "9.1"@de .

dtd:entry-id255 a dtd:entry ;
    dtd:has_BR dtd:BR-id135 ;
    dtd:has_Value "Kosten- und Leistungsrechnung(§ 4 Nr. 9.1)"@de .

dtd:entry-id256 a dtd:entry ;
    dtd:has_DL dtd:DL-id289 ;
    dtd:has_Value "a)Rechnungswesen als Instrument kaufmännischer Steuerung und Kontrolle begründenb)Kosten überwachenc)Leistungen kalkulierend)Kosten und Erlöse erbrachter Leistungen errechnen und bewertene)Leistungsabrechnungen erstellen"@de .

dtd:entry-id257 a dtd:entry ;
    dtd:has_Value "9.2"@de .

dtd:entry-id258 a dtd:entry ;
    dtd:has_BR dtd:BR-id136 ;
    dtd:has_Value "Controlling(§ 4 Nr. 9.2)"@de .

dtd:entry-id259 a dtd:entry ;
    dtd:has_DL dtd:DL-id290 ;
    dtd:has_Value "a)betriebliche Planungs-, Steuerungs- und Kontrollinstrumente anwenden, dabei branchenspezifische Kennzahlen ermitteln und aufbereitenb)Statistiken auswerten, Entscheidungen vorbereiten, Berichte erstellen"@de .

dtd:entry-id260 a dtd:entry ;
    dtd:has_Value "10"@de .

dtd:entry-id261 a dtd:entry ;
    dtd:has_BR dtd:BR-id137 ;
    dtd:has_Value "Qualitätssicherung der Auftragsdurchführung(§ 4 Nr. 10)"@de .

dtd:entry-id262 a dtd:entry ;
    dtd:has_DL dtd:DL-id291 ;
    dtd:has_Value "a)qualitätssichernde Maßnahmen organisieren, durchführen und auswerten sowie für den Auftraggeber dokumentierenb)Projektabläufe überwachen und mit Mitarbeitern sowie Auftraggebern rückkoppeln, Beschwerden berücksichtigen, Projektabläufe optimieren, Schlussfolgerungen für neue Projekte ziehen"@de .

dtd:entry-id263 a dtd:entry ;
    dtd:has_Value "11"@de .

dtd:entry-id264 a dtd:entry ;
    dtd:has_BR dtd:BR-id138 ;
    dtd:has_Value "Vertrieb und Marketing(§ 4 Nr. 11)"@de .

dtd:entry-id265 a dtd:entry ;
    dtd:has_Value " "@de .

dtd:entry-id266 a dtd:entry ;
    dtd:has_Value "11.1"@de .

dtd:entry-id267 a dtd:entry ;
    dtd:has_BR dtd:BR-id139 ;
    dtd:has_Value "Angebotserstellung und Verkauf(§ 4 Nr. 11.1)"@de .

dtd:entry-id268 a dtd:entry ;
    dtd:has_DL dtd:DL-id292 ;
    dtd:has_Value "a)Dialogmarketing-Markt beobachten, Informationen auswerten und nutzenb)Dienstleistungen und Konditionen von Wettbewerbern vergleichenc)Vorschläge zur Ausgestaltung und Verbesserung des Dienstleistungsangebotes entwickelnd)Auftraggeberwünsche ermittelne)Angebote erstellenf)Verträge anbahnen und Vertragsabschlüsse vorbereiteng)rechtliche Vorschriften beachten und allgemeine Geschäftsbedingungen anwenden"@de .

dtd:entry-id269 a dtd:entry ;
    dtd:has_Value "11.2"@de .

dtd:entry-id270 a dtd:entry ;
    dtd:has_BR dtd:BR-id140 ;
    dtd:has_Value "Vermarktung von Dienstleistungen(§ 4 Nr. 11.2)"@de .

dtd:entry-id271 a dtd:entry ;
    dtd:has_DL dtd:DL-id293 ;
    dtd:has_Value "a)über Dienstleistungen informieren, Dienstleistungen anbieten und präsentieren, auch in einer Fremdspracheb)Projektkonzepte erarbeitenc)Verkaufsargumente für Dienstleistungsangebote entwickelnd)Auftraggeber-Briefing beachtene)bei der Planung verkaufsfördernder Maßnahmen mitwirken, verkaufsfördernde Maßnahmen umsetzen"@de .


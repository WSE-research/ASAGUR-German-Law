dtd:LA-id2943 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Betrieb verbleiben, bis die Negativbefunde der nach Nummer 5 durchgefhrten Untersuchung vorliegen."@de .


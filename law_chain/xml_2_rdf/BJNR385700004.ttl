dtd:LA-id2479 a dtd:LA ;
    dtd:has_DL dtd:DL-id1296 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verdacht auf Maul- und Klauenseuche, wenn das Ergebnis a)der klinischen,b)der pathologisch-anatomischen oderc)der labordiagnostischenUntersuchung den Ausbruch der Maul- und Klauenseuche befürchten lässt."@de .

dtd:LA-id2480 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der klinischen,"@de .

dtd:LA-id2481 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der pathologisch-anatomischen oder"@de .

dtd:LA-id2482 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der labordiagnostischen"@de .

dtd:LA-id2483 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betrieb:"@de .

dtd:LA-id2484 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle Ställe mit Tieren empfänglicher Arten oder sonstigen Standorte zur ständigen oder vorübergehenden Haltung dieser Tiere einschließlich der dazugehörigen Nebengebäude und des dazugehörigen Geländes, die hinsichtlich der tatsächlichen Nutzung und der räumlichen Anordnung, insbesondere der Ver- und Entsorgung, eine Einheit bilden, mit Ausnahme von Schlachtstätten, Transportmitteln und Grenzkontrollstellen sowie Wildgehegen, die größer als 25 Hektar sind;"@de .

dtd:LA-id2485 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tiere empfänglicher Arten:"@de .

dtd:LA-id2486 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tiere der Unterordnung Wiederkäuer (Ruminantia), Schweine (Suina) und Schwielensohler (Tylopoda) der Ordnung Paarhufer (Artiodactyla);"@de .

dtd:LA-id2487 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fleisch:"@de .

dtd:LA-id2488 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle Teile von Tieren empfänglicher Arten, frisch oder in Form von Hackfleisch, Fleischerzeugnissen oder Fleischzubereitungen, die zum Genuss für Menschen oder zur Verfütterung an Tiere geeignet sind;"@de .

dtd:LA-id2489 a dtd:LA ;
    dtd:has_DL dtd:DL-id1324 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Milch: a)nicht über 40 Grad Celsius erhitzte Milch (Rohmilch),b)über 40 Grad Celsius erhitzte oder einer Behandlung mit ähnlicher Wirkung unterzogene Milch oderc)Milcherzeugnissevon Tieren empfänglicher Arten;"@de .

dtd:LA-id2490 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht über 40 Grad Celsius erhitzte Milch (Rohmilch),"@de .

dtd:LA-id2491 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über 40 Grad Celsius erhitzte oder einer Behandlung mit ähnlicher Wirkung unterzogene Milch oder"@de .

dtd:LA-id2492 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Milcherzeugnisse"@de .

dtd:LA-id2493 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Häute:"@de .

dtd:LA-id2494 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Häute, Felle, Wolle, Haare oder Borsten von Tieren empfänglicher Arten;"@de .

dtd:LA-id2495 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Futtermittel:"@de .

dtd:LA-id2496 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einzel- oder Mischfuttermittel einschließlich Heu und Stroh;"@de .

dtd:LA-id2497 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dung:"@de .

dtd:LA-id2498 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ausscheidungen von Tieren empfänglicher Arten, auch in Mischung mit Einstreu, insbesondere Mist, Jauche oder Gülle;"@de .

dtd:LA-id2499 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Notimpfung:"@de .

dtd:LA-id2500 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schutzimpfung oder Suppressivimpfung;"@de .

dtd:LA-id2501 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schutzimpfung:"@de .

dtd:LA-id2502 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Impfung von Tieren empfänglicher Arten zum Schutz der Tiere vor der Ansteckung mit dem Virus der Maul- und Klauenseuche;"@de .

dtd:LA-id2503 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Suppressivimpfung:"@de .

dtd:LA-id2504 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Impfung von Tieren empfänglicher Arten zur Verhinderung der Verschleppung des Virus der Maul- und Klauenseuche in einen Betrieb oder in ein bestimmtes oder innerhalb eines bestimmten Gebiets."@de .

dtd:LA-id2505 a dtd:LA ;
    dtd:has_Value "gehäuft fieberhafte Erkrankungen,"@de .

dtd:LA-id2506 a dtd:LA ;
    dtd:has_Value "eine erhebliche Verminderung der Milchleistung oder"@de .

dtd:LA-id2507 a dtd:LA ;
    dtd:has_Value "gehäufte Todesfälle bei Jungtieren"@de .

dtd:LA-id2508 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die virologische Untersuchung der seuchenverdächtigen Tiere empfänglicher Arten entsprechend Anhang I Nummer 4 und 5 Buchstabe b der Richtlinie 2003/85/EG des Rates vom 29. September 2003 über Maßnahmen der Gemeinschaft zur Bekämpfung der Maul- und Klauenseuche, zur Aufhebung der Richtlinie 85/511/EWG sowie der Entscheidungen 89/531/EWG und 91/665/EWG und zur Änderung der Richtlinie 92/46/EWG (ABl. EU Nr. L 306 S. 1) in der jeweils geltenden Fassung und die klinische und serologische Untersuchung nach Anhang III der Richtlinie 2003/85/EG und"@de .

dtd:LA-id2509 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Zählung der im Betrieb vorhandenen Tiere empfänglicher Arten und die Aufzeichnung des Ergebnisses der Zählung"@de .

dtd:LA-id2510 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die serologische und virologische Untersuchung weiterer Tiere empfänglicher Arten des Verdachtsbetriebs, die nicht bereits nach Satz 1 untersucht worden sind, und"@de .

dtd:LA-id2511 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Tötung und unschädliche Beseitigung aller Tiere empfänglicher Arten des Verdachtsbetriebs"@de .

dtd:LA-id2512 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Zeitraum, in dem das Virus der Maul- und Klauenseuche bereits im Betrieb vorhanden gewesen sein kann, bevor der Verdacht angezeigt worden ist,"@de .

dtd:LA-id2513 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die mögliche Ursache der Maul- und Klauenseuche,"@de .

dtd:LA-id2514 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ermittlung anderer Betriebe, aus denen Tiere empfänglicher Arten in den Verdachtsbetrieb oder in die Tiere empfänglicher Arten aus dem Verdachtsbetrieb verbracht worden sind,"@de .

dtd:LA-id2515 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, Fahrzeuge, Fleisch, Milch, Tierkörper, Häute, Samen, Eizellen, Embryonen, Futtermittel, Dung und alle sonstigen Gegenstände, mit denen das Virus in den oder aus dem Verdachtsbetrieb verschleppt worden sein kann."@de .

dtd:LA-id2516 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an den Zufahrten und Eingängen des Betriebs Schilder mit der deutlichen und haltbaren Aufschrift „Maul- und Klauenseuche-Verdacht – Unbefugter Zutritt verboten“ gut sichtbar anzubringen,"@de .

dtd:LA-id2517 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sämtliche Tiere empfänglicher Arten des Betriebs abzusondern,"@de .

dtd:LA-id2518 a dtd:LA ;
    dtd:has_DL dtd:DL-id1376 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "täglich Aufzeichnungen über a)die Besuche betriebsfremder Personen unter Angabe von Namen, Anschrift und Besuchsdatum sowieb)bereits erkrankte, verendete und ansteckungsverdächtige Tiere empfänglicher Arten, getrennt nach Zucht- und Masttieren,zu machen,"@de .

dtd:LA-id2519 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Besuche betriebsfremder Personen unter Angabe von Namen, Anschrift und Besuchsdatum sowie"@de .

dtd:LA-id2520 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bereits erkrankte, verendete und ansteckungsverdächtige Tiere empfänglicher Arten, getrennt nach Zucht- und Masttieren,"@de .

dtd:LA-id2521 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle im Betrieb vorhandenen Vorräte an Fleisch, Milch, Häuten, Samen, Eizellen, Embryonen und Futtermitteln sowie die dort vorhandenen Tierkörper, die dort vorhandene Einstreu und den dort vorhandenen Dung ihrer Art nach zu erfassen und hierüber Aufzeichnungen zu machen,"@de .

dtd:LA-id2522 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "verendete oder getötete Tiere empfänglicher Arten so aufzubewahren, dass sie Witterungseinflüssen nicht ausgesetzt sind und Menschen oder Tiere nicht mit ihnen in Berührung kommen können,"@de .

dtd:LA-id2523 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für das Verbringen verendeter oder getöteter Tiere empfänglicher Arten aus dem Betrieb die Genehmigung der zuständigen Behörde einzuholen, die nur zu diagnostischen Zwecken oder zur unschädlichen Beseitigung erteilt werden darf,"@de .

dtd:LA-id2524 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an den Ein- und Ausgängen der Ställe oder sonstigen Standorte Matten oder sonstige saugfähige Bodenauflagen auszulegen und sie mit einem wirksamen Desinfektionsmittel zu tränken und feucht zu halten,"@de .

dtd:LA-id2525 a dtd:LA ;
    dtd:has_DL dtd:DL-id1381 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sicherzustellen, dass a)der jeweilige Stall oder sonstige Standort nur mit Schutzkleidung betreten wird und diese unverzüglich nach Verlassen des Stalls oder sonstigen Standorts abgelegt, gereinigt und desinfiziert oder, im Falle von Einwegschutzkleidung, unverzüglich nach Gebrauch so beseitigt wird, dass eine Seuchenverbreitung vermieden wird,b)Schuhwerk vor dem Betreten und Verlassen des Betriebs sowie nach Verlassen eines Stalls oder sonstigen Standorts gereinigt und desinfiziert wird,c)Tiere weder in den noch aus dem Betrieb verbracht werden,"@de .

dtd:LA-id2526 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der jeweilige Stall oder sonstige Standort nur mit Schutzkleidung betreten wird und diese unverzüglich nach Verlassen des Stalls oder sonstigen Standorts abgelegt, gereinigt und desinfiziert oder, im Falle von Einwegschutzkleidung, unverzüglich nach Gebrauch so beseitigt wird, dass eine Seuchenverbreitung vermieden wird,"@de .

dtd:LA-id2527 a dtd:LA ;

dtd:entry-id6657 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Bei einem verbundenen Vertrag nach § 358 BGB, der nicht den Erwerb von Finanzinstrumenten zum Gegenstand hat, ist hier Folgendes einzufügen:"@de ;
    dtd:has_nameend "col4"@de ;

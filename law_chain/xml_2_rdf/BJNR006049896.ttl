dtd:entry-id6662 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Dieser Hinweis entfällt, wenn der Darlehensgeber zugleich Vertragspartner des Darlehensnehmers aus dem weiteren Vertrag ist."@de ;
    dtd:has_nameend "col4"@de ;

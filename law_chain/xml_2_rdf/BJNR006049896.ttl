    dtd:has_VJ "1"@de ;
    dtd:has_Value "Ist Vertragsgegenstand die Lieferung von nicht auf einem körperlichen Datenträger befindlichen digitalen Inhalten, so kann hier folgender Hinweis gegeben werden:"@de ;

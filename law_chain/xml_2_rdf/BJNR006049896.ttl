dtd:entry-id6652 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Bei einem angegebenen Geschäft nach § 360 Absatz 2 Satz 2 BGB ist hier Folgendes einzufügen:"@de ;

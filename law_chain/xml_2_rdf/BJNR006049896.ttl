dtd:entry-id6620 a dtd:entry ;
    dtd:has_F dtd:F-id4520 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Bei einem verbundenen Vertrag nach § 358 BGB über die Überlassung einer Sache oder bei einem zusammenhängenden Vertrag, gerichtet auf die Überlassung einer Sache, wenn von Gestaltungshinweis   Gebrauch gemacht wurde, ist hier nachstehender Unterabsatz einzufügen:"@de ;
    dtd:has_nameend "col4"@de ;
    dtd:has_namest "col2"@de .


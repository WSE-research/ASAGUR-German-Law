dtd:entry-id6163 a dtd:entry ;
    dtd:has_B dtd:B-id4195,
        dtd:B-id4196 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Die unter den Nummern 4 bis 8, Nummer 9 Buchstabe b, Nummer 10 Buchstabe c, Nummer 10 Buchstabe d, Nummer 10 Buchstabe e, Nummer 10 Buchstabe g, Nummer 10 Buchstabe h und Nummer 11 Buchstabe b bis Nummer 15 aufgelisteten und kursiv gedruckten Informationen sind nur dann in die Widerrufsbelehrung aufzunehmen, wenn sie für den vorliegenden Vertrag einschlägig sind. Der Kursivdruck ist dabei zu entfernen."@de ;
    dtd:has_nameend "col3"@de ;
    dtd:has_namest "col2"@de .


    dtd:has_VJ "1"@de ;
    dtd:has_Value "Wenn ein zusammenhängender Vertrag (§ 360 BGB) vorliegt, ist der nachfolgende Hinweis einzufügen:"@de ;
    dtd:has_nameend "col3"@de ;
    dtd:has_namest "col2"@de .


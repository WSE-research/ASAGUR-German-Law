dtd:entry-id5665 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Sie haben das Recht, binnen vierzehn Tagen ohne Angabe von Gründen diesen Vertrag zu widerrufen."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id5666 a dtd:entry ;
    dtd:has_F dtd:F-id4379 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Die Widerrufsfrist beträgt vierzehn Tage ab dem Tag  ."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id5667 a dtd:entry ;
    dtd:has_F dtd:F-id4380,
        dtd:F-id4381 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Um Ihr Widerrufsrecht auszuüben, müssen Sie uns (   ) mittels einer eindeutigen Erklärung (z. B. ein mit der Post versandter Brief oder eine E-Mail) über Ihren Entschluss, diesen Vertrag zu widerrufen, informieren. Sie können dafür das beigefügte Muster-Widerrufsformular verwenden, das jedoch nicht vorgeschrieben ist."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id5668 a dtd:entry ;

dtd:LA-id266 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Ergebnis von Teil 2 der Abschlussprüfung mit mindestens „ausreichend“,"@de .

dtd:LA-id267 a dtd:LA ;

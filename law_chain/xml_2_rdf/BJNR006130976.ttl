dtd:LA-id9959 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "natürliche Personen, die nach bürgerlichem Recht in der Geschäftsfähigkeit beschränkt sind, soweit sie für den Gegenstand des Verfahrens durch Vorschriften des bürgerlichen Rechts als geschäftsfähig oder durch Vorschriften des öffentlichen Rechts als handlungsfähig anerkannt sind,"@de .

dtd:LA-id9960 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "juristische Personen sowie Personenvereinigungen oder Vermögensmassen durch die in § 34 bezeichneten Personen oder durch besonders Beauftragte,"@de .

dtd:LA-id9961 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Behörden durch ihre Leiter, deren Vertreter oder Beauftragte."@de .

dtd:LA-id9962 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für einen Beteiligten, dessen Person unbekannt ist,"@de .

dtd:LA-id9963 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für einen abwesenden Beteiligten, dessen Aufenthalt unbekannt ist oder der an der Besorgung seiner Angelegenheiten verhindert ist,"@de .

dtd:LA-id9964 a dtd:LA ;
    dtd:has_DL dtd:DL-id7690 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für einen Beteiligten ohne Aufenthalt a)im Inland,b)in einem anderen Mitgliedstaat der Europäischen Union oderc)in einem anderen Staat, auf den das Abkommen über den Europäischen Wirtschaftsraum anzuwenden ist,wenn er der Aufforderung der Finanzbehörde, einen Vertreter zu bestellen, innerhalb der ihm gesetzten Frist nicht nachgekommen ist,"@de .

dtd:LA-id9965 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Inland,"@de .

dtd:LA-id9966 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in einem anderen Mitgliedstaat der Europäischen Union oder"@de .

dtd:LA-id9967 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in einem anderen Staat, auf den das Abkommen über den Europäischen Wirtschaftsraum anzuwenden ist,"@de .

dtd:LA-id9968 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für einen Beteiligten, der infolge einer psychischen Krankheit oder körperlichen, geistigen oder seelischen Behinderung nicht in der Lage ist, in dem Verwaltungsverfahren selbst tätig zu werden,"@de .

dtd:LA-id9969 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei herrenlosen Sachen, auf die sich das Verfahren bezieht, zur Wahrung der sich in bezug auf die Sache ergebenden Rechte und Pflichten."@de .

dtd:LA-id9970 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer selbst Beteiligter ist,"@de .

dtd:LA-id9971 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer Angehöriger (§ 15) eines Beteiligten ist,"@de .

dtd:LA-id9972 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer einen Beteiligten kraft Gesetzes oder Vollmacht allgemein oder in diesem Verfahren vertritt,"@de .

dtd:LA-id9973 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer Angehöriger (§ 15) einer Person ist, die für einen Beteiligten in diesem Verfahren Hilfe in Steuersachen leistet,"@de .

dtd:LA-id9974 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer bei einem Beteiligten gegen Entgelt beschäftigt ist oder bei ihm als Mitglied des Vorstands, des Aufsichtsrats oder eines gleichartigen Organs tätig ist; dies gilt nicht für den, dessen Anstellungskörperschaft Beteiligte ist,"@de .

dtd:LA-id9975 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer außerhalb seiner amtlichen Eigenschaft in der Angelegenheit ein Gutachten abgegeben hat oder sonst tätig geworden ist."@de .

dtd:LA-id9976 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Amts wegen oder auf Antrag tätig werden muss,"@de .

dtd:LA-id9977 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nur auf Antrag tätig werden darf und ein Antrag nicht vorliegt."@de .

dtd:LA-id9978 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch unmittelbare Abgabe der Erklärung in einem elektronischen Formular, das von der Behörde in einem Eingabegerät oder über öffentlich zugängliche Netze zur Verfügung gestellt wird;"@de .

dtd:LA-id9979 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Versendung eines elektronischen Dokuments an die Behörde mit der Versandart nach § 5 Absatz 5 des De-Mail-Gesetzes."@de .

dtd:LA-id9980 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit einer qualifizierten elektronischen Signatur versehen und mit einem geeigneten Verfahren verschlüsselt ist oder"@de .

dtd:LA-id9981 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit einer De-Mail-Nachricht nach § 5 Absatz 5 des De-Mail-Gesetzes versandt wird, bei der die Bestätigung des akkreditierten Diensteanbieters die erlassende Finanzbehörde als Nutzer des De-Mail-Kontos erkennen lässt."@de .

dtd:LA-id9982 a dtd:LA ;
    dtd:has_Value "das Verfahren zur Identifikation der am Verfahren Beteiligten,"@de .

dtd:LA-id9983 a dtd:LA ;
    dtd:has_Value "das Nähere über Form, Inhalt, Verarbeitung und Sicherung der zu übermittelnden Daten,"@de .

dtd:LA-id9984 a dtd:LA ;
    dtd:has_Value "die Art und Weise der Übermittlung der Daten,"@de .

dtd:LA-id9985 a dtd:LA ;
    dtd:has_Value "die Mitwirkungspflichten Dritter und"@de .

dtd:LA-id9986 a dtd:LA ;
    dtd:has_Value "die Erprobung der Verfahren."@de .

dtd:LA-id9987 a dtd:LA ;
    dtd:has_Value "die Gewährleistung, dass durch Zufallsauswahl eine hinreichende Anzahl von Fällen zur umfassenden Prüfung durch Amtsträger ausgewählt wird,"@de .

dtd:LA-id9988 a dtd:LA ;
    dtd:has_Value "die Prüfung der als prüfungsbedürftig ausgesteuerten Sachverhalte durch Amtsträger,"@de .

dtd:LA-id9989 a dtd:LA ;
    dtd:has_Value "die Gewährleistung, dass Amtsträger Fälle für eine umfassende Prüfung auswählen können,"@de .

dtd:LA-id9990 a dtd:LA ;
    dtd:has_Value "die regelmäßige Überprüfung der Risikomanagementsysteme auf ihre Zielerfüllung."@de .

dtd:LA-id9991 a dtd:LA ;
    dtd:has_Value "länderübergreifenden Steuerverkürzungen,"@de .

dtd:LA-id9992 a dtd:LA ;
    dtd:has_Value "Steuerverkürzungen von internationaler Bedeutung oder"@de .

dtd:LA-id9993 a dtd:LA ;
    dtd:has_Value "Steuerverkürzungen von erheblicher Bedeutung"@de .

dtd:LA-id9994 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gefahr einer Doppelbesteuerung bezüglich des bestimmten Sachverhalts besteht und"@de .

dtd:LA-id9995 a dtd:LA ;
    dtd:has_DL dtd:DL-id7757 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "es wahrscheinlich ist, a)die Doppelbesteuerung durch das Vorabverständigungsverfahren zu vermeiden undb)eine übereinstimmende Abkommensauslegung mit der zuständigen Behörde des anderen Vertragsstaates zu erreichen."@de .

dtd:LA-id9996 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Doppelbesteuerung durch das Vorabverständigungsverfahren zu vermeiden und"@de .

dtd:LA-id9997 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine übereinstimmende Abkommensauslegung mit der zuständigen Behörde des anderen Vertragsstaates zu erreichen."@de .

dtd:LA-id9998 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die genaue Bezeichnung des Antragstellers und aller anderen Beteiligten,"@de .

dtd:LA-id9999 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bezeichnung der örtlich zuständigen Finanzbehörde sowie die maßgebliche Steuernummer,"@de .

dtd:NB-id4988 a dtd:NB ;
    dtd:has_Value "§ 139c;"@de .


dtd:LA-id2311 a dtd:LA ;
    dtd:has_Value "die Art des Flugs,"@de .

dtd:LA-id2312 a dtd:LA ;
    dtd:has_Value "bei einem Flug, der über die Umgebung des Startflugplatzes hinaus führt (Überlandflug), den Zielflugplatz;"@de .

dtd:LA-id2313 a dtd:LA ;
    dtd:has_DL dtd:DL-id1291 ;
    dtd:has_Value "nach der Landung: aa)das Kennzeichen,bb)das Luftfahrzeugmuster,cc)bei einem Überlandflug den Startflugplatz;"@de .

dtd:LA-id2314 a dtd:LA ;
    dtd:has_Value "das Kennzeichen,"@de .

dtd:LA-id2315 a dtd:LA ;
    dtd:has_Value "das Luftfahrzeugmuster,"@de .

dtd:LA-id2316 a dtd:LA ;
    dtd:has_Value "bei einem Überlandflug den Startflugplatz;"@de .

dtd:LA-id2317 a dtd:LA ;
    dtd:has_Value "beim Rollen Start- und Landebahnen möglichst rechtwinklig und nur dann zu kreuzen, wenn sich dort kein anderes Luftfahrzeug im Landeanflug oder im Start befindet,"@de .

dtd:LA-id2318 a dtd:LA ;
    dtd:has_Value "nach dem Start unter Beachtung der flugtechnischen Sicherheit so schnell wie möglich Höhe zu gewinnen,"@de .

dtd:LA-id2319 a dtd:LA ;
    dtd:has_Value "nach dem Durchstarten entsprechend Nummer 5 zu verfahren,"@de .

dtd:LA-id2320 a dtd:LA ;
    dtd:has_Value "eine Flugplatzverkehrszone zu meiden, wenn nicht beabsichtigt ist, innerhalb der Flugplatzverkehrszone zu landen,"@de .

dtd:LA-id2321 a dtd:LA ;
    dtd:has_Value "rechts neben dem Landezeichen aufzusetzen, sofern nicht eine andere Regelung getroffen ist,"@de .

dtd:LA-id2322 a dtd:LA ;
    dtd:has_Value "nach der Landung die Landebahn unverzüglich freizumachen."@de .

dtd:LA-id2323 a dtd:LA ;
    dtd:has_Value "sich im Sitz des verantwortlichen Luftfahrzeugführers eine Person befindet, die in der Bedienung sachkundig ist, und"@de .

dtd:LA-id2324 a dtd:LA ;
    dtd:has_Value "Personen nicht gefährdet werden können."@de .

dtd:LA-id2325 a dtd:LA ;
    dtd:has_Value "für den Start Instrumentenabflugverfahren und für die Landung Instrumentenanflugverfahren festgelegt sind und"@de .


dtd:LA-id373 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Brunsbüttel"@de .

dtd:LA-id374 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "landseitig durch Umzäunungen oder Hinweisschilder,"@de .

dtd:LA-id375 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wasserseitig östlich durch die Verbindungslinie vom Anleger Kreystraße über die Köpfe der Schleusenleitwerke bis zur Hafengrenze des Südkais, westlich durch die Verbindungslinie der Einfahrtsfeuer,"@de .

dtd:LA-id376 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kiel-Holtenau"@de .

dtd:LA-id377 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "landseitig durch Umzäunungen oder Hinweisschilder,"@de .

dtd:LA-id378 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wasserseitig östlich durch die Verbindungslinie der Einfahrtsfeuer und die Grenzen des Schutz- und Sicherheitshafens Kiel-Holtenau, westlich durch die Verbindungslinie von der westlichen Ecke des Entwässerungskanals über den Kopf des südlichen Leitwerkes der Neuen Schleusen zum Südufer,"@de .

dtd:LA-id379 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Strohbrück"@de .

dtd:LA-id380 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "landseitig durch Umzäunungen oder Hinweisschilder,"@de .

dtd:LA-id381 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wasserseitig südlich durch eine Verbindungslinie vom Schreibpegel rechtwinklig zum gegenüberliegenden Ufer,"@de .

dtd:LA-id382 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nördlich durch eine Linie in Fortsetzung der geraden Uferlinie des Nord-Ostsee-Kanals,"@de .

dtd:LA-id383 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gieselau"@de .

dtd:LA-id384 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "landseitig durch Umzäunungen oder Hinweisschilder,"@de .

dtd:LA-id385 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wasserseitig nördlich und südlich durch Verbindungslinien von Ufer zu Ufer in Höhe der Köpfe der Leitwerke; ausgenommen ist die Straßenbrücke,"@de .

dtd:LA-id386 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Lexfähr"@de .

dtd:LA-id387 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "landseitig durch Umzäunungen oder Hinweisschilder,"@de .

dtd:LA-id388 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wasserseitig östlich und westlich durch Verbindungslinien von Ufer zu Ufer in Höhe der Köpfe der Leitwerke. Ausgenommen ist die Straßenbrücke,"@de .

dtd:LA-id389 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nordfeld"@de .

dtd:LA-id390 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "landseitig durch Umzäunungen oder Hinweisschilder,"@de .

dtd:LA-id391 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wasserseitig östlich und westlich durch Verbindungslinien geradlinig vom Nordufer über die Köpfe der Leitwerke bis zum Ende der Steinböschungen,"@de .

dtd:LA-id392 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Eider-Sperrwerk"@de .

dtd:LA-id393 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "landseitig durch Umzäunungen oder Hinweisschilder,"@de .

dtd:LA-id394 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wasserseitig östlich durch die Verbindungslinie von der am Nordufer gekennzeichneten Schleusengrenze bis zum Kopf des Leitdammes Süd,"@de .

dtd:LA-id395 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "westlich durch die Verbindungslinie vom Kopfdalben des Leitdammes Nord bis zum Kopf des Leitdammes Süd; ausgenommen ist die Landesstraße."@de .

dtd:LA-id396 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Brunsbüttel, Kiel-Holtenau und Eider-Sperrwerk täglich von 00.00 bis 24.00 Uhr,"@de .

dtd:LA-id397 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Auslaufen aus den Schleusen zu behindern;"@de .

dtd:LA-id398 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vermeidbare Geräusche zu verursachen, insbesondere Schallsignale außer in den vorgeschriebenen Fällen abzugeben;"@de .

dtd:LA-id399 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Scheinwerfer außer in notwendigen Fällen zu gebrauchen;"@de .

dtd:LA-id400 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "schädliche oder störende Stoffe einzubringen, einzuleiten oder abzulagern oder das Schleusengelände zu verunreinigen;"@de .

dtd:LA-id401 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rettungsgeräte missbräuchlich zu benutzen;"@de .

dtd:LA-id402 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anlagen zu beschädigen oder unbefugt zu benutzen."@de .

dtd:LA-id403 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von seinem in der Schleusenkammer festgemachten Fahrzeug, dessen Heck in einem Abstand von weniger als 25 Meter von der dahinter liegenden Torbahn entfernt ist, kein nach achtern gerichteter Schraubenstrom auf das Schleusentor einwirken kann,"@de .

dtd:LA-id404 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Falle eines Fahrzeuges mit Verstellpropeller der Propeller während der Schleusenliegezeit in Nullstellung eine geringst mögliche Strömung in der Schleuse verursacht."@de .

dtd:LA-id405 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "geeignete und ausreichende Feuerlöschgeräte betriebsklar bereitzuhalten und ausreichende Feuerwachen einzuteilen,"@de .

dtd:LA-id406 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schornsteine und Tankräume nicht zu reinigen,"@de .

dtd:LA-id407 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tankräume geschlossen zu halten,"@de .

dtd:LA-id408 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schornsteine und Auspuffrohre mit Funkenfängern zu versehen."@de .

dtd:LA-id409 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu rauchen oder glühende Stoffe bei sich zu führen oder wegzuwerfen,"@de .


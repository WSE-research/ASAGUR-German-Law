    dtd:has_Value "Ausweich-Liegestellen im Binnenhafen Brunsbüttel (bei Kanal-km 2,7)"@de .

dtd:LA-id415 a dtd:LA ;
    dtd:has_BR dtd:BR-id135,
        dtd:BR-id136,
        dtd:BR-id137 ;
    dtd:has_Size dtd:normal ;

dtd:LA-id413 a dtd:LA ;
    dtd:has_BR dtd:BR-id133,
        dtd:BR-id134 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Norden und Westen durch das Ufer, im Osten durch die Verbindungslinie vom Enddalben des nördlichen Leitwerkes der Neuen Schleuse bis zum östlichen Endpunkt des Brückensteges und von dort rechtwinklig zum Ufer, im Süden durch das nördliche Leitwerk der Neuen Schleuse."@de .


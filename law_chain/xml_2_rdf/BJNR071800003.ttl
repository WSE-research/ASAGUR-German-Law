dtd:LA-id800 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Im Prüfungsbereich Depotgeschäft und Marketing:"@de .

dtd:LA-id801 a dtd:LA ;
    dtd:has_DL dtd:DL-id632 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "In höchstens 90 Minuten soll der Prüfling praxisbezogene Aufgaben und Fälle insbesondere aus den Gebietena)Depotführung, b)Marketinginstrumente und Vertriebskanäle "@de .

dtd:LA-id802 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bearbeiten. Dabei soll er zeigen, dass er Geschäftsvorfälle der Investmentdepots bearbeiten sowie zielgruppenorientierte Marketingstrategien für den Vertriebserfolg darstellen kann."@de .

dtd:LA-id803 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Depotführung,"@de .

dtd:LA-id804 a dtd:LA ;

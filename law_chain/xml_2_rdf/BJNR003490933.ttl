dtd:I-id60 a dtd:I ;
    dtd:has_Value "Reichsregierung"@de .

dtd:P-id62 a dtd:P ;
    dtd:has_Value "(+++ Textnachweis Geltung ab: 21.3.1975 +++)"@de .

dtd:P-id64 a dtd:P ;
    dtd:has_Value "Die Reichsregierung hat das folgende Gesetz beschlossen, das hiermit verkündet wird:"@de .

dtd:P-id66 a dtd:P ;
    dtd:has_Value "-"@de .

dtd:P-id68 a dtd:P ;
    dtd:has_Value "(1) Es wird eine Konversionskasse für deutsche Auslandsschulden errichtet. Die Konversionskasse ist eine Körperschaft des öffentlichen Rechts; sie steht unter Aufsicht des Bundesministers der Finanzen ... . Der Bundesminister der Finanzen bestellt die verantwortlichen Organe."@de .

dtd:P-id69 a dtd:P ;
    dtd:has_Value "(2) Die übrigen Rechtsverhältnisse der Konversionskasse regelt die Satzung, die der Bundesminister der Finanzen feststellt."@de .

dtd:P-id70 a dtd:P ;
    dtd:has_I dtd:I-id59 ;
    dtd:has_Value "(3) Von den Steuern, die das Reich, die Länder und die Gemeinden (Gemeindeverbände) vom Einkommen, vom Vermögen sowie vom Gewerbebetrieb erheben, ist die Konversionskasse befreit."@de .

dtd:P-id72 a dtd:P ;
    dtd:has_Value "Die eingezahlten Beträge ... werden den ausländischen Gläubigern gutgeschrieben. Die Ansprüche der Gläubiger aus der Gutschrift bestimmen sich nach Grundsätzen, die in der Satzung der Konversionskasse festgelegt werden. ..."@de .

dtd:P-id74 a dtd:P ;
    dtd:has_Value ""@de .

dtd:P-id76 a dtd:P ;
    dtd:has_I dtd:I-id60 ;
    dtd:has_Value "(1) Die Reichsregierung erläßt die zur Durchführung dieses Gesetzes erforderlichen Rechtsverordnungen und Verwaltungsvorschriften. ..."@de .

dtd:P-id77 a dtd:P ;
    dtd:has_Value "(2)"@de .

dtd:P-id79 a dtd:P ;
    dtd:has_Value "Das Gesetz tritt am 1. Juli 1933 in Kraft; ... ."@de .


    dtd:has_Value "um eine Schule, die nicht oder nicht überwiegend aus öffentlichen Mitteln finanziert wird und die Schüler auf internationale Abschlüsse, Abschlüsse anderer Staaten oder staatlich anerkannte Abschlüsse vorbereitet."@de .

dtd:LA-id5467 a dtd:LA ;
    dtd:has_DL dtd:DL-id3067 ;
    dtd:has_Size dtd:normal ;

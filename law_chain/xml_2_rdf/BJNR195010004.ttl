dtd:LA-id6447 a dtd:LA ;
    dtd:has_Value "es am 1. Juli 2007 das 14. Lebensjahr vollendet hat,"@de .

dtd:LA-id6448 a dtd:LA ;
    dtd:has_Value "es sich seit mindestens sechs Jahren rechtmäßig oder geduldet in Deutschland aufhält,"@de .

dtd:LA-id6449 a dtd:LA ;

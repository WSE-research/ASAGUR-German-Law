dtd:LA-id598 a dtd:LA ;
    dtd:has_Value "die Kopie eines an die Dienststelle gerichteten Antrags der Gewählten, mit Wirkung ihrer Bestellung von der Befassung mit Personalangelegenheiten entbunden zu werden."@de .

dtd:LA-id599 a dtd:LA ;
    dtd:has_Value "innerhalb der Nachfrist nach § 12 Absatz 1 Satz 1 keine gültige Bewerbung eingegangen ist oder"@de .

dtd:LA-id600 a dtd:LA ;
    dtd:has_Value "keine Nachrückerin zur Verfügung steht."@de .

dtd:LA-id601 a dtd:LA ;
    dtd:has_Value "mit Ablauf der Anfechtungsfrist nach § 21 Absatz 3 des Bundesgleichstellungsgesetzes,"@de .

dtd:LA-id602 a dtd:LA ;
    dtd:has_Value "im Fall einer Anfechtung mit dem bestands- oder rechtskräftigen Abschluss des Anfechtungsverfahrens oder"@de .


    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Die Verordnung dient der Umsetzung der Richtlinie 2009/13/EG des Rates vom 16. Februar 2009 zur Durchführung der Vereinbarung zwischen dem Verband der Reeder in der Europäischen Gemeinschaft (ECSA) und der Europäischen Transportarbeiter-Föderation (ETF) über das Seearbeitsübereinkommen 2006 und zur Änderung der Richtlinie 1999/63/EG (ABl. L 124 vom 20.5.2009, S. 30)."@de ;
    dtd:has_noindex dtd:noindex-id89 .

dtd:Footnotes-id214 a dtd:Footnotes ;

dtd:LA-id557 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Antarktis:"@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gebiet südlich von 60 Grad südlicher Breite;"@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tätigkeit:"@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Expeditionen, Reisen, Versorgungsfahrten und -flüge, Inspektionen und sonstige Unternehmungen in die oder in der Antarktis sowie der Bau, Umbau, Abbau oder Betrieb wissenschaftlicher Stationen und sonstiger Anlagen und Einrichtungen, die in der Bundesrepublik Deutschland organisiert werden oder von ihrem Hoheitsgebiet ausgehen. Der Begriff der Tätigkeit schließt jede Veränderung einer Tätigkeit ein;"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Umwelterheblichkeitsprüfung:"@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die vorläufige Bewertung der Umweltauswirkungen nach Artikel 2 der Anlage I des Umweltschutzprotokolls zum Antarktis-Vertrag;"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Umweltverträglichkeitsprüfung:"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die umfassende Bewertung der Umweltauswirkungen nach Artikel 3 der Anlage I des Umweltschutzprotokolls zum Antarktis-Vertrag;"@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Abfälle:"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bewegliche Sachen, auch flüssige und gasförmige, derer sich der Besitzer entledigen will, oder deren geordnete Entsorgung zur Wahrung des Wohles der Allgemeinheit, insbesondere des Schutzes der Umwelt geboten ist, oder radioaktive Reststoffe sowie ausgebaute oder abgebaute radioaktive Anlagenteile, die nach § 9a Abs. 1 Nr. 2 des Atomgesetzes geordnet zu beseitigen sind."@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "deutschen Staatsangehörigen,"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "anderen natürlichen Personen mit Wohnsitz oder gewöhnlichem Aufenthalt im Gebiet der Bundesrepublik Deutschland,"@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "juristischen Personen oder sonstigen Personenvereinigungen mit Sitz im Gebiet der Bundesrepublik Deutschland,"@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ausländischen juristischen Personen oder sonstigen Personenvereinigungen"@de .

dtd:LA-id571 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tätigkeiten, die von einer anderen Vertragspartei des Umweltschutzprotokolls zum Antarktis-Vertrag genehmigt wurden;"@de .

dtd:LA-id572 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schiffsbewegungen, die nur zum Zweck der Durchfahrt durch die Antarktis erfolgen, ohne daß Ziele in der Antarktis angesteuert werden;"@de .

dtd:LA-id573 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tätigkeiten, die sich unmittelbar auf die Erforschung oder Nutzung antarktischer Robben nach dem Gesetz zu dem Übereinkommen vom 1. Juni 1972 zur Erhaltung der antarktischen Robben (BGBl. 1987 II S. 90) beziehen;"@de .

dtd:LA-id574 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tätigkeiten, die sich unmittelbar auf die Erforschung oder Nutzung lebender Meeresschätze nach dem Gesetz zu dem Übereinkommen vom 20. Mai 1980 über die Erhaltung der lebenden Meeresschätze der Antarktis (BGBl. 1982 II S. 420) beziehen."@de .

dtd:LA-id575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nachteiligen Wirkungen auf Klima- oder Wetterverhältnisse,"@de .

dtd:LA-id576 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erheblichen nachteiligen Wirkungen auf die Luft- oder Wasserqualität,"@de .

dtd:LA-id577 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erheblichen Veränderungen der atmosphärischen, Land-, Wasser-, Gletscher- oder Meeresumwelt,"@de .

dtd:LA-id578 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "schädlichen Veränderungen in der Verbreitung, Häufigkeit oder Produktivität von Tier- oder Pflanzenarten oder deren Populationen,"@de .

dtd:LA-id579 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zusätzlichen Gefahren für gefährdete oder bedrohte Arten oder deren Populationen,"@de .

dtd:LA-id580 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schädigung oder erhebliche Gefährdung der Gebiete von biologischer, wissenschaftlicher, historischer, ästhetischer Bedeutung oder der Gebiete mit ursprünglichem Charakter,"@de .

dtd:LA-id581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstigen erheblichen Beeinträchtigungen der Umwelt und der abhängigen und verbundenen Ökosysteme"@de .

dtd:LA-id582 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "weniger als geringfügige oder vorübergehende Auswirkungen,"@de .

dtd:LA-id583 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "geringfügige oder vorübergehende Auswirkungen,"@de .

dtd:LA-id584 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mehr als nur geringfügige oder vorübergehende Auswirkungen"@de .

dtd:LA-id585 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Seeaufgabengesetz,"@de .

dtd:LA-id586 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Seemannsgesetz,"@de .

dtd:LA-id587 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Gesetz über die Beförderung gefährlicher Güter,"@de .


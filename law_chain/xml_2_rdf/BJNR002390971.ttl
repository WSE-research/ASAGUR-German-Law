    dtd:has_DL dtd:DL-id153 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bau oder Ausbau von Verkehrswegen der a)Straßenbahnen, Hoch- und Untergrundbahnen sowie Bahnen besonderer Bauart,b)nichtbundeseigenen Eisenbahnen,c)Seilbahnsysteme, sofern die nach dem Beihilferecht der Europäischen Union zu beachtenden Voraussetzungen vorliegen,"@de .

dtd:LA-id227 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Straßenbahnen, Hoch- und Untergrundbahnen sowie Bahnen besonderer Bauart,"@de .


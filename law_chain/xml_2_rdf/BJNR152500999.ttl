    dtd:has_Value "einer deutschen Außenhandelskammer im Aufenthaltsstaat,"@de .

dtd:LA-id304 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines ausländischen Unternehmens oder"@de .

dtd:LA-id305 a dtd:LA ;

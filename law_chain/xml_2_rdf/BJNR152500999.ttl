dtd:LA-id311 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einer Zuverlässigkeitsüberprüfung nach § 3 Absatz 1 innerhalb der letzten zehn Jahre und"@de .

dtd:LA-id312 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einer Zuverlässigkeitsüberprüfung nach § 3 Absatz 2 oder Absatz 3 innerhalb der letzten fünf Jahre."@de .

dtd:LA-id313 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mitgliedschaft in Organisationen, Aktionsbündnissen oder Gruppierungen im Sinne von § 3 Absatz 1 Nummer 1, 3 oder 4 des Bundesverfassungsschutzgesetzes oder deren Unterstützung,"@de .

dtd:LA-id314 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Umständen, die die Annahme rechtfertigen, daß der Betroffene zu politisch motivierter Gewaltanwendung neigt,"@de .

dtd:LA-id315 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geisteskrankheit oder Geistesschwäche,"@de .

dtd:LA-id316 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verdacht auf Alkohol- oder auf Betäubungsmittelabhängigkeit,"@de .

dtd:LA-id317 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Umständen, die auf eine derartige Überschuldung des Betroffenen hindeuten, daß er seinen laufenden finanziellen Verpflichtungen nicht mehr nachkommen kann oder"@de .

dtd:LA-id318 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verhängung einer Geldbuße wegen Verletzung der Vorschriften des Atomgesetzes, des Strahlenschutzgesetzes, des Gesetzes über die Kontrolle von Kriegswaffen, des Gesetzes über die Beförderung gefährlicher Güter, des Waffengesetzes, des Beschussgesetzes, des Sprengstoffgesetzes oder einer auf Grund dieser Gesetze erlassenen Rechtsverordnung."@de .

dtd:LA-id319 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Verurteilung wegen einer einzelnen Straftat, für die eine Geldstrafe von weniger als neunzig Tagessätzen oder eine Freiheitsstrafe von weniger als drei Monaten verhängt worden ist, es sei denn, daß es sich um eine rechtskräftige Verurteilung wegen Verletzung von Vorschriften des Atomgesetzes, des Gesetzes über die Kontrolle von Kriegswaffen, des Gesetzes über die Beförderung gefährlicher Güter, des Waffengesetzes, des Beschussgesetzes, des Sprengstoffgesetzes oder einer auf Grund dieser Gesetze erlassenen Rechtsverordnung handelt,"@de .

dtd:LA-id320 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "laufende oder eingestellte Ermittlungsverfahren oder Strafverfahren ohne gerichtliche Verurteilung,"@de .

dtd:LA-id321 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erkenntnisse aus der Anfrage nach § 5 Absatz 1 Nummer 6,"@de .

dtd:LA-id322 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kontakte zu fremden Nachrichtendiensten,"@de .

dtd:LA-id323 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sachverhalte, die zu einer Erpressung durch Dritte verwendet werden können, oder"@de .

dtd:LA-id324 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wiederholte Verstöße gegen Betriebsvorschriften."@de .

dtd:LA-id325 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Begründung dafür, dass die Tätigkeit sofort aufgenommen oder der Zutritt sofort gewährt werden muss,"@de .

dtd:LA-id326 a dtd:LA ;

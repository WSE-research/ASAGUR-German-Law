dtd:P-id228 a dtd:P ;
    dtd:has_BR dtd:BR-id217 ;
    dtd:has_Value " (+++ Textnachweis Geltung ab: 14.4.1980 +++)Überschrift: Buchstabenabkürzung eingef. durch Art. 7 Nr. 1 G v. 20.12.2001 I 4013 mWv 1.1.2002"@de ;

    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit anderen Bietern Absprachen über die Gebotswerte der in dieser oder einer vorangegangenen Ausschreibung abgegebenen Gebote getroffen hat oder"@de .

dtd:LA-id1027 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gebotsmengen mehrerer Zuschläge eines Bieters aus mindestens zwei vorangegangenen Ausschreibungen nach § 16 Absatz 1 Nummer 1 vollständig entwertet worden sind."@de .

dtd:LA-id1028 a dtd:LA ;
    dtd:has_Value "der Bundesrepublik Deutschland zugeordnet worden sind, gelten zugunsten der nach § 21 anspruchsberechtigten Übertragungsnetzbetreiber,"@de .

dtd:LA-id1029 a dtd:LA ;
    dtd:has_Value "dem Kooperationsstaat zugeordnet worden sind, gelten zugunsten der vom Kooperationsstaat benannten zuständigen ausländischen Stelle."@de .

dtd:LA-id1030 a dtd:LA ;
    dtd:has_Value "dem Gebotstermin der Ausschreibung und die bezuschlagten Gebotsmengen,"@de .

dtd:LA-id1031 a dtd:LA ;
    dtd:has_DL dtd:DL-id648 ;
    dtd:has_Value "den Namen der Bieter, die einen Zuschlag erhalten haben, mit a)dem jeweils in dem Gebot angegebenen Standort,b)den jeweils in dem Gebot angegebenen Nummern, unter denen das Projekt, die KWK-Anlage, das innovative KWK-System sowie die jeweiligen Einheiten im Marktstammdatenregister registriert sind,c)der Nummer des Gebots, sofern ein Bieter mehrere Gebote abgegeben hat, undd)einer eindeutigen Zuschlagsnummer,"@de .

dtd:LA-id1032 a dtd:LA ;
    dtd:has_Value "dem jeweils in dem Gebot angegebenen Standort,"@de .

dtd:LA-id1033 a dtd:LA ;
    dtd:has_Value "den jeweils in dem Gebot angegebenen Nummern, unter denen das Projekt, die KWK-Anlage, das innovative KWK-System sowie die jeweiligen Einheiten im Marktstammdatenregister registriert sind,"@de .

dtd:LA-id1034 a dtd:LA ;
    dtd:has_Value "der Nummer des Gebots, sofern ein Bieter mehrere Gebote abgegeben hat, und"@de .

dtd:LA-id1035 a dtd:LA ;
    dtd:has_Value "einer eindeutigen Zuschlagsnummer,"@de .

dtd:LA-id1036 a dtd:LA ;
    dtd:has_Value "dem niedrigsten und höchsten Gebotswert, die einen Zuschlag erhalten haben,"@de .

dtd:LA-id1037 a dtd:LA ;
    dtd:has_Value "dem mengengewichteten durchschnittlichen Zuschlagswert und"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "(nicht darstellbares Tafelzeichen E.17 Wasserskistrecke, BGBl. I 1990, 107)"@de .

dtd:LA-id149 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hierfür freigegebenen Strecken und Wasserflächen, 0 2."@de .

dtd:LA-id150 a dtd:LA ;

dtd:LA-id1000 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Recht, Mitglieder des Aufsichts- oder Verwaltungsorgans der juristischen Person zu wählen, zu bestellen, zu empfehlen oder abzulehnen, beseitigt oder eingeschränkt wird."@de .

dtd:LA-id1001 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Beschluss über den Abschluss einer Vereinbarung nach § 13 Abs. 1,"@de .

dtd:LA-id1002 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Beschluss über die Nichtaufnahme oder den Abbruch der Verhandlungen nach § 16 Abs. 1 und"@de .

dtd:LA-id1003 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die jeweiligen Mehrheiten, mit denen die Beschlüsse gefasst worden sind."@de .

dtd:LA-id1004 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Geltungsbereich der Vereinbarung, einschließlich der außerhalb des Hoheitsgebiets der Mitgliedstaaten liegenden juristischen Personen und Betriebe, sofern diese in den Geltungsbereich einbezogen werden,"@de .

dtd:LA-id1005 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zusammensetzung des SCE-Betriebsrats, die Anzahl seiner Mitglieder und die Sitzverteilung, einschließlich der Auswirkungen wesentlicher Änderungen der Zahl der in der Europäischen Genossenschaft beschäftigten Arbeitnehmer,"@de .

dtd:LA-id1006 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Befugnisse und das Verfahren zur Unterrichtung und Anhörung des SCE-Betriebsrats,"@de .

dtd:LA-id1007 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Häufigkeit der Sitzungen des SCE-Betriebsrats,"@de .

dtd:LA-id1008 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für den SCE-Betriebsrat bereitzustellenden finanziellen und materiellen Mittel,"@de .

dtd:LA-id1009 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Zeitpunkt des Inkrafttretens der Vereinbarung und ihre Laufzeit sowie die Fälle, in denen die Vereinbarung neu ausgehandelt werden soll und das dabei anzuwendende Verfahren."@de .

dtd:LA-id1010 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der Mitglieder des Aufsichts- oder Verwaltungsorgans der Europäischen Genossenschaft, welche die Arbeitnehmer wählen oder bestellen können oder deren Bestellung sie empfehlen oder ablehnen können,"@de .

dtd:LA-id1011 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Verfahren, nach dem die Arbeitnehmer diese Mitglieder wählen oder bestellen können oder deren Bestellung empfehlen oder ablehnen können,"@de .

dtd:LA-id1012 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Rechte dieser Mitglieder,"@de .

dtd:LA-id1013 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass auch vor strukturellen Änderungen der Europäischen Genossenschaft, ihrer Tochtergesellschaften oder ihrer Betriebe, die nach Gründung der Europäischen Genossenschaft eintreten, Verhandlungen über die Beteiligung der Arbeitnehmer in der Europäischen Genossenschaft aufgenommen werden und welches Verfahren dabei anzuwenden ist."@de .

dtd:LA-id1014 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Parteien dies vereinbaren oder"@de .

dtd:LA-id1015 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bis zum Ende des in § 20 angegebenen Zeitraums keine Vereinbarung zustande gekommen ist und das besondere Verhandlungsgremium keinen Beschluss nach § 16 gefasst hat."@de .

dtd:LA-id1016 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Geschäftsberichte,"@de .

dtd:LA-id1017 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Tagesordnung aller Sitzungen des Leitungsorgans und des Aufsichts- oder Verwaltungsorgans und"@de .

dtd:LA-id1018 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kopien aller Unterlagen, die der Generalversammlung vorgelegt werden."@de .

dtd:LA-id1019 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Struktur der Europäischen Genossenschaft sowie die wirtschaftliche und finanzielle Lage,"@de .

dtd:LA-id1020 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die voraussichtliche Entwicklung der Geschäfts-, Produktions- und Absatzlage,"@de .

dtd:LA-id1021 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beschäftigungslage und ihre voraussichtliche Entwicklung,"@de .

dtd:LA-id1022 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Investitionen (Investitionsprogramme),"@de .

dtd:LA-id1023 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "grundlegende Änderungen der Organisation,"@de .

dtd:LA-id1024 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einführung neuer Arbeits- und Fertigungsverfahren,"@de .


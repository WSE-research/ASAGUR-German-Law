    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben zur Laufzeit des Versorgungsverhältnisses,"@de .

dtd:LA-id9600 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "allgemeine Angaben über die für das Versorgungsverhältnis geltenden Steuerregeln,"@de .

dtd:LA-id9601 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die mit dem Altersversorgungssystem verbundenen finanziellen, versicherungstechnischen und sonstigen Risiken sowie die Art und Aufteilung der Risiken,"@de .


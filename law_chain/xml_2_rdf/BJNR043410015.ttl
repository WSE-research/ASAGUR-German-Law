    dtd:has_Size dtd:normal ;
    dtd:has_Value "unverzüglich nach einer wesentlichen Änderung der Anlagepolitik."@de .

dtd:LA-id9590 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Anlageformen, die in § 215 Absatz 2 Satz 1 Nummer 1 bis 7 genannt sind, und"@de .

dtd:LA-id9591 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstigen Anlagen, die nach der Rechtsverordnung zu § 235 Absatz 1 Nummer 10 zugelassen sind."@de .

dtd:LA-id9592 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in deutscher Sprache gefasst sein;"@de .

dtd:LA-id9593 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "klar, prägnant und verständlich formuliert sein, wobei fachsprachliche Begriffe oder Wendungen nicht verwendet werden, wenn der Sachverhalt auch in Allgemeinsprache dargestellt werden kann;"@de .

dtd:LA-id9594 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "schlüssig sein, wobei Begriffe und Bezeichnungen einheitlich verwendet und beibehalten werden;"@de .


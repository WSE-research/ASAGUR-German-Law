dtd:LA-id9611 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über die Berechnung und die Höhe der Solvabilitätskapitalanforderung;"@de .

dtd:LA-id9612 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über den maßgebenden Mindestbetrag der Mindestkapitalanforderung sowie über seine Berechnung;"@de .

dtd:LA-id9613 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "darüber, wie nicht in der Bilanz ausgewiesene Eigenmittel errechnet werden und in welchem Umfang sie auf die Solvabilitätskapitalanforderung und die Mindestkapitalanforderung angerechnet werden dürfen;"@de .

dtd:LA-id9614 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über einen oder mehrere Höchstwerte für den Rechnungszins bei Versicherungsverträgen mit Zinsgarantie;"@de .


dtd:LA-id9183 a dtd:LA ;
    dtd:has_Value "die die Niederlassung betreffenden Geschäftsunterlagen dort zur Verfügung zu halten sind und"@de .

dtd:LA-id9184 a dtd:LA ;
    dtd:has_Value "§ 13 Absatz 2 nicht anzuwenden ist."@de .

dtd:LA-id9185 a dtd:LA ;

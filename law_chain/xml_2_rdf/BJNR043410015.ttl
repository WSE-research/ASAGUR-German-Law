dtd:LA-id9227 a dtd:LA ;
    dtd:has_Value "sämtliche Zahlungen an Versicherungsnehmer und Anspruchsberechtigte, einschließlich künftiger Überschussbeteiligungen, die die Versicherungsunternehmen erwarten vorzunehmen, unabhängig davon, ob sie vertraglich garantiert sind oder nicht."@de .

dtd:LA-id9228 a dtd:LA ;
    dtd:has_Value "die Angemessenheit der Höhe ihrer versicherungstechnischen Rückstellungen,"@de .

dtd:LA-id9229 a dtd:LA ;
    dtd:has_Value "die Eignung und die Erheblichkeit der verwendeten Methoden sowie"@de .

dtd:LA-id9230 a dtd:LA ;

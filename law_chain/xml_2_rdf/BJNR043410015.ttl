dtd:LA-id9235 a dtd:LA ;
    dtd:has_Value "der Überschuss der Vermögenswerte über die Verbindlichkeiten abzüglich des Betrags der eigenen Aktien in der Solvabilitätsübersicht und"@de .

dtd:LA-id9236 a dtd:LA ;
    dtd:has_Value "die nachrangigen Verbindlichkeiten."@de .

dtd:LA-id9237 a dtd:LA ;
    dtd:has_Value "denjenigen Teil des nicht eingezahlten Grundkapitals, des Gründungsstocks oder des bei öffentlich-rechtlichen Versicherungsunternehmen dem Grundkapital bei Aktiengesellschaften entsprechenden Postens, der nicht eingefordert wurde,"@de .

dtd:LA-id9238 a dtd:LA ;
    dtd:has_Value "bei Versicherungsvereinen auf Gegenseitigkeit mit variabler Nachschussverpflichtung die künftigen Forderungen, die der Verein gegenüber seinen Mitgliedern hat, wenn er innerhalb der folgenden zwölf Monate Nachschüsse einfordert,"@de .

dtd:LA-id9239 a dtd:LA ;
    dtd:has_Value "Kreditbriefe und Garantien sowie"@de .

dtd:LA-id9240 a dtd:LA ;
    dtd:has_Value "alle sonstigen rechtsverbindlichen Zahlungsverpflichtungen Dritter gegenüber dem Versicherungsunternehmen."@de .

dtd:LA-id9241 a dtd:LA ;
    dtd:has_Value "die Fähigkeit und Bereitschaft der Gegenparteien zur Zahlung,"@de .

dtd:LA-id9242 a dtd:LA ;
    dtd:has_Value "die Einforderbarkeit der Mittel unter Berücksichtigung der rechtlichen Ausgestaltung des Bestandteils und etwaiger sonstiger Umstände, die die erfolgreiche Einzahlung oder Einforderung dieses Bestandteils verhindern können, und"@de .

dtd:LA-id9243 a dtd:LA ;
    dtd:has_Value "etwaige Informationen über das Ergebnis bisheriger Einforderungen des Versicherungsunternehmens für derartige ergänzende Eigenmittel, soweit diese Informationen auf verlässliche Weise verwendet werden können, um das erwartete Ergebnis künftiger Einforderungen zu bewerten."@de .

dtd:LA-id9244 a dtd:LA ;
    dtd:has_Value "verfügbar oder einforderbar sind, um Verluste bei Unternehmensfortführung und im Fall der Liquidation vollständig aufzufangen und"@de .

dtd:LA-id9245 a dtd:LA ;
    dtd:has_Value "im Fall der Liquidation nachrangig gegenüber allen anderen Verbindlichkeiten sind."@de .

dtd:LA-id9246 a dtd:LA ;
    dtd:has_Value "Verpflichtungen oder Anreizen zur Rückzahlung des Nominalbetrags,"@de .

dtd:LA-id9247 a dtd:LA ;
    dtd:has_Value "obligatorischen festen Kosten und"@de .

dtd:LA-id9248 a dtd:LA ;
    dtd:has_Value "sonstigen Belastungen."@de .


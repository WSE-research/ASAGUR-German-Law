dtd:LA-id134 a dtd:LA ;
    dtd:has_Value "Unfallfürsorge nach dem Beamtenversorgungsgesetz einschließlich Prävention,"@de .

dtd:LA-id135 a dtd:LA ;
    dtd:has_Value "Sachschadensersatz nach § 78 des Bundesbeamtengesetzes und"@de .

dtd:LA-id136 a dtd:LA ;
    dtd:has_Value "Geltendmachung von Schadensersatzansprüchen gegenüber Dritten nach § 76 Satz 2 des Bundesbeamtengesetzes."@de .

dtd:LA-id137 a dtd:LA ;
    dtd:has_Value "jeden Unfall, der mutmaßlich in Ausübung oder infolge des Dienstes eingetreten ist und"@de .

dtd:LA-id138 a dtd:LA ;
    dtd:has_Value "jede mutmaßlich dienstlich verursachte Erkrankung."@de .

dtd:LA-id139 a dtd:LA ;
    dtd:has_Value "den Unfallhergang zu untersuchen und bei der Aufklärung der Entstehung von Berufskrankheiten und dienstbedingten Erkrankungen mitzuwirken,"@de .

dtd:LA-id140 a dtd:LA ;

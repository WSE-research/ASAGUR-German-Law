dtd:LA-id2853 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch eine Erwerbstätigkeit Hilfebedürftigkeit vermieden oder beseitigt, die Dauer der Hilfebedürftigkeit verkürzt oder der Umfang der Hilfebedürftigkeit verringert wird,"@de .

dtd:LA-id2854 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erwerbsfähigkeit einer leistungsberechtigten Person erhalten, verbessert oder wieder hergestellt wird,"@de .

dtd:LA-id2855 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nachteile, die erwerbsfähigen Leistungsberechtigten aus einem der in § 1 des Allgemeinen Gleichbehandlungsgesetzes genannten Gründe entstehen können, überwunden werden,"@de .

dtd:LA-id2856 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die familienspezifischen Lebensverhältnisse von erwerbsfähigen Leistungsberechtigten, die Kinder erziehen oder pflegebedürftige Angehörige betreuen, berücksichtigt werden,"@de .

dtd:LA-id2857 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anreize zur Aufnahme und Ausübung einer Erwerbstätigkeit geschaffen und aufrechterhalten werden."@de .

dtd:LA-id2858 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beratung,"@de .

dtd:LA-id2859 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beendigung oder Verringerung der Hilfebedürftigkeit insbesondere durch Eingliederung in Ausbildung oder Arbeit und"@de .

dtd:LA-id2860 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sicherung des Lebensunterhalts."@de .

dtd:LA-id2861 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Eignung der erwerbsfähigen Leistungsberechtigten,"@de .

dtd:LA-id2862 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die individuelle Lebenssituation der erwerbsfähigen Leistungsberechtigten, insbesondere ihre familiäre Situation,"@de .

dtd:LA-id2863 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die voraussichtliche Dauer der Hilfebedürftigkeit der erwerbsfähigen Leistungsberechtigten und"@de .

dtd:LA-id2864 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Dauerhaftigkeit der Eingliederung der erwerbsfähigen Leistungsberechtigten."@de .

dtd:LA-id2865 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht über ausreichende deutsche Sprachkenntnisse verfügen, vorrangig an einem Integrationskurs nach § 43 des Aufenthaltsgesetzes teilnehmen, oder"@de .

dtd:LA-id2866 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "darüber hinaus notwendige berufsbezogene Sprachkenntnisse benötigen, vorrangig an der berufsbezogenen Deutschsprachförderung nach § 45a Absatz 1 des Aufenthaltsgesetzes teilnehmen."@de .

dtd:LA-id2867 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dienstleistungen,"@de .

dtd:LA-id2868 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geldleistungen und"@de .

dtd:LA-id2869 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sachleistungen."@de .

dtd:LA-id2870 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bundesagentur für Arbeit (Bundesagentur), soweit Nummer 2 nichts Anderes bestimmt,"@de .

dtd:LA-id2871 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die kreisfreien Städte und Kreise für die Leistungen nach § 16a, für das Bürgergeld nach § 19 Absatz 1 Satz 1 und 2 und die Leistungen nach § 27 Absatz 3, soweit diese Leistungen für den Bedarf für Unterkunft und Heizung geleistet werden, für die Leistungen nach § 24 Absatz 3 Satz 1 Nummer 1 und 2 sowie für die Leistungen nach § 28, soweit durch Landesrecht nicht andere Träger bestimmt sind (kommunale Träger)."@de .

dtd:LA-id2872 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "geeignet sind, die Aufgaben zu erfüllen,"@de .

dtd:LA-id2873 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich verpflichten, eine besondere Einrichtung nach Absatz 5 zu schaffen,"@de .

dtd:LA-id2874 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich verpflichten, mindestens 90 Prozent der Beamtinnen und Beamten, Arbeitnehmerinnen und Arbeitnehmer der Bundesagentur, die zum Zeitpunkt der Zulassung mindestens seit 24 Monaten in der im Gebiet des kommunalen Trägers gelegenen Arbeitsgemeinschaft oder Agentur für Arbeit in getrennter Aufgabenwahrnehmung im Aufgabenbereich nach § 6 Absatz 1 Satz 1 tätig waren, vom Zeitpunkt der Zulassung an, dauerhaft zu beschäftigen,"@de .

dtd:LA-id2875 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich verpflichten, mit der zuständigen Landesbehörde eine Zielvereinbarung über die Leistungen nach diesem Buch abzuschließen, und"@de .

dtd:LA-id2876 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich verpflichten, die in der Rechtsverordnung nach § 51b Absatz 1 Satz 2 festgelegten Daten zu erheben und gemäß den Regelungen nach § 51b Absatz 4 an die Bundesagentur zu übermitteln, um bundeseinheitliche Datenerfassung, Ergebnisberichterstattung, Wirkungsforschung und Leistungsvergleiche zu ermöglichen."@de .

dtd:LA-id2877 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das 15. Lebensjahr vollendet und die Altersgrenze nach § 7a noch nicht erreicht haben,"@de .

dtd:LA-id2878 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erwerbsfähig sind,"@de .

dtd:LA-id2879 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hilfebedürftig sind und"@de .

dtd:LA-id2880 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihren gewöhnlichen Aufenthalt in der Bundesrepublik Deutschland haben (erwerbsfähige Leistungsberechtigte)."@de .

dtd:LA-id2881 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ausländerinnen und Ausländer, die weder in der Bundesrepublik Deutschland Arbeitnehmerinnen, Arbeitnehmer oder Selbständige noch aufgrund des § 2 Absatz 3 des Freizügigkeitsgesetzes/EU freizügigkeitsberechtigt sind, und ihre Familienangehörigen für die ersten drei Monate ihres Aufenthalts,"@de .

dtd:LA-id2882 a dtd:LA ;
    dtd:has_DL dtd:DL-id2097 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ausländerinnen und Ausländer, a)die kein Aufenthaltsrecht haben oderb)deren Aufenthaltsrecht sich allein aus dem Zweck der Arbeitsuche ergibt,und ihre Familienangehörigen,"@de .

dtd:LA-id2883 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die kein Aufenthaltsrecht haben oder"@de .

dtd:LA-id2884 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "deren Aufenthaltsrecht sich allein aus dem Zweck der Arbeitsuche ergibt,"@de .

dtd:LA-id2885 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leistungsberechtigte nach § 1 des Asylbewerberleistungsgesetzes."@de .

dtd:LA-id2886 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die erwerbsfähigen Leistungsberechtigten,"@de .

dtd:LA-id2887 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die im Haushalt lebenden Eltern oder der im Haushalt lebende Elternteil eines unverheirateten erwerbsfähigen Kindes, welches das 25. Lebensjahr noch nicht vollendet hat, und die im Haushalt lebende Partnerin oder der im Haushalt lebende Partner dieses Elternteils,"@de .

dtd:LA-id2888 a dtd:LA ;
    dtd:has_DL dtd:DL-id2111 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Partnerin oder Partner der erwerbsfähigen Leistungsberechtigten a)die nicht dauernd getrennt lebende Ehegattin oder der nicht dauernd getrennt lebende Ehegatte,b)die nicht dauernd getrennt lebende Lebenspartnerin oder der nicht dauernd getrennt lebende Lebenspartner,c)eine Person, die mit der erwerbsfähigen leistungsberechtigten Person in einem gemeinsamen Haushalt so zusammenlebt, dass nach verständiger Würdigung der wechselseitige Wille anzunehmen ist, Verantwortung füreinander zu tragen und füreinander einzustehen."@de .

dtd:LA-id2889 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nicht dauernd getrennt lebende Ehegattin oder der nicht dauernd getrennt lebende Ehegatte,"@de .

dtd:LA-id2890 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nicht dauernd getrennt lebende Lebenspartnerin oder der nicht dauernd getrennt lebende Lebenspartner,"@de .

dtd:LA-id2891 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Person, die mit der erwerbsfähigen leistungsberechtigten Person in einem gemeinsamen Haushalt so zusammenlebt, dass nach verständiger Würdigung der wechselseitige Wille anzunehmen ist, Verantwortung füreinander zu tragen und füreinander einzustehen."@de .

dtd:LA-id2892 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die dem Haushalt angehörenden unverheirateten Kinder der in den Nummern 1 bis 3 genannten Personen, wenn sie das 25. Lebensjahr noch nicht vollendet haben, soweit sie die Leistungen zur Sicherung ihres Lebensunterhalts nicht aus eigenem Einkommen oder Vermögen beschaffen können."@de .

dtd:LA-id2893 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "länger als ein Jahr zusammenleben,"@de .

dtd:LA-id2894 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit einem gemeinsamen Kind zusammenleben,"@de .

dtd:LA-id2895 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kinder oder Angehörige im Haushalt versorgen oder"@de .

dtd:LA-id2896 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "befugt sind, über Einkommen oder Vermögen des anderen zu verfügen."@de .

dtd:LA-id2897 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer voraussichtlich für weniger als sechs Monate in einem Krankenhaus (§ 107 des Fünften Buches) untergebracht ist oder"@de .

dtd:LA-id2898 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer in einer stationären Einrichtung nach Satz 1 untergebracht und unter den üblichen Bedingungen des allgemeinen Arbeitsmarktes mindestens 15 Stunden wöchentlich erwerbstätig ist."@de .

dtd:LA-id2899 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die aufgrund von § 2 Absatz 1a des Bundesausbildungsförderungsgesetzes keinen Anspruch auf Ausbildungsförderung haben,"@de .

dtd:LA-id2900 a dtd:LA ;
    dtd:has_DL dtd:DL-id2139 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "deren Bedarf sich nach den §§ 12, 13 Absatz 1 in Verbindung mit Absatz 2 Nummer 1 oder nach § 13 Absatz 1 Nummer 1 in Verbindung mit Absatz 2 Nummer 2 des Bundesausbildungsförderungsgesetzes bemisst und die Leistungen nach dem Bundesausbildungsförderungsgesetz a)erhalten oder nur wegen der Vorschriften zur Berücksichtigung von Einkommen und Vermögen nicht erhalten oderb)beantragt haben und über deren Antrag das zuständige Amt für Ausbildungsförderung noch nicht entschieden hat; lehnt das zuständige Amt für Ausbildungsförderung die Leistungen ab, findet Absatz 5 mit Beginn des folgenden Monats Anwendung, oder"@de .

dtd:LA-id2901 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erhalten oder nur wegen der Vorschriften zur Berücksichtigung von Einkommen und Vermögen nicht erhalten oder"@de .

dtd:LA-id2902 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "beantragt haben und über deren Antrag das zuständige Amt für Ausbildungsförderung noch nicht entschieden hat; lehnt das zuständige Amt für Ausbildungsförderung die Leistungen ab, findet Absatz 5 mit Beginn des folgenden Monats Anwendung, oder"@de .

dtd:LA-id2903 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die eine Abendhauptschule, eine Abendrealschule oder ein Abendgymnasium besuchen, sofern sie aufgrund des § 10 Absatz 3 des Bundesausbildungsförderungsgesetzes keinen Anspruch auf Ausbildungsförderung haben."@de .

dtd:LA-id2904 a dtd:LA ;
    dtd:has_Value "Teilnahme an einer ärztlich verordneten Maßnahme der medizinischen Vorsorge oder Rehabilitation,"@de .

dtd:LA-id2905 a dtd:LA ;
    dtd:has_Value "Teilnahme an einer Veranstaltung, die kirchlichen oder gewerkschaftlichen Zwecken dient oder im öffentlichen Interesse liegt,"@de .

dtd:LA-id2906 a dtd:LA ;
    dtd:has_Value "Aufenthalten außerhalb des näheren Bereichs, die überwiegend der Eingliederung in Ausbildung oder Arbeit dienen, oder"@de .

dtd:LA-id2907 a dtd:LA ;
    dtd:has_Value "Ausübung einer ehrenamtlichen Tätigkeit, wenn die Eingliederung in Ausbildung oder Arbeit nicht wesentlich beeinträchtigt wird."@de .

dtd:LA-id2908 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie zu der bestimmten Arbeit körperlich, geistig oder seelisch nicht in der Lage ist,"@de .

dtd:LA-id2909 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ausübung der Arbeit die künftige Ausübung der bisherigen überwiegenden Arbeit wesentlich erschweren würde, weil die bisherige Tätigkeit besondere körperliche Anforderungen stellt,"@de .

dtd:LA-id2910 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ausübung der Arbeit die Erziehung ihres Kindes oder des Kindes ihrer Partnerin oder ihres Partners gefährden würde; die Erziehung eines Kindes, das das dritte Lebensjahr vollendet hat, ist in der Regel nicht gefährdet, soweit die Betreuung in einer Tageseinrichtung oder in Tagespflege im Sinne der Vorschriften des Achten Buches oder auf sonstige Weise sichergestellt ist; die zuständigen kommunalen Träger sollen darauf hinwirken, dass erwerbsfähigen Erziehenden vorrangig ein Platz zur Tagesbetreuung des Kindes angeboten wird,"@de .

dtd:LA-id2911 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ausübung der Arbeit mit der Pflege einer oder eines Angehörigen nicht vereinbar wäre und die Pflege nicht auf andere Weise sichergestellt werden kann,"@de .

dtd:LA-id2912 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Ausübung der Arbeit ein sonstiger wichtiger Grund entgegensteht."@de .

dtd:LA-id2913 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie nicht einer früheren beruflichen Tätigkeit entspricht, für die die erwerbsfähige leistungsberechtigte Person ausgebildet ist oder die früher ausgeübt wurde,"@de .

dtd:LA-id2914 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie im Hinblick auf die Ausbildung der erwerbsfähigen leistungsberechtigten Person als geringerwertig anzusehen ist,"@de .

dtd:LA-id2915 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Beschäftigungsort vom Wohnort der erwerbsfähigen leistungsberechtigten Person weiter entfernt ist als ein früherer Beschäftigungs- oder Ausbildungsort,"@de .

dtd:LA-id2916 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Arbeitsbedingungen ungünstiger sind als bei den bisherigen Beschäftigungen der erwerbsfähigen leistungsberechtigten Person,"@de .

dtd:LA-id2917 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie mit der Beendigung einer Erwerbstätigkeit verbunden ist, es sei denn, es liegen begründete Anhaltspunkte vor, dass durch die bisherige Tätigkeit künftig die Hilfebedürftigkeit beendet werden kann."@de .

dtd:LA-id2918 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leistungen nach diesem Buch,"@de .

dtd:LA-id2919 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id2920 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Renten oder Beihilfen, die nach dem Bundesentschädigungsgesetz für Schaden an Leben sowie an Körper oder Gesundheit erbracht werden, bis zur Höhe der vergleichbaren Entschädigungszahlungen nach Kapitel 9 des Vierzehnten Buches,"@de .

dtd:LA-id2921 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufwandspauschalen nach § 1878 des Bürgerlichen Gesetzbuchs kalenderjährlich bis zu dem in § 3 Nummer 26 Satz 1 des Einkommensteuergesetzes genannten Betrag,"@de .

dtd:LA-id2922 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufwandsentschädigungen oder Einnahmen aus nebenberuflichen Tätigkeiten, die nach § 3 Nummer 12, Nummer 26 oder Nummer 26a des Einkommensteuergesetzes steuerfrei sind, soweit diese Einnahmen einen Betrag in Höhe von 3 000 Euro im Kalenderjahr nicht überschreiten,"@de .

dtd:LA-id2923 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mutterschaftsgeld nach § 19 des Mutterschutzgesetzes"@de .

dtd:LA-id2924 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einmalige Einnahmen aus Erbschaften, Vermächtnissen und Pflichtteilszuwendungen."@de .

dtd:LA-id2925 a dtd:LA ;
    dtd:has_DL dtd:DL-id2201 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Leistungen nach § 39 des Achten Buches, die für den erzieherischen Einsatz erbracht werden, a)für das dritte Pflegekind zu 75 Prozent,b)für das vierte und jedes weitere Pflegekind vollständig,"@de .

dtd:LA-id2926 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für das dritte Pflegekind zu 75 Prozent,"@de .

dtd:LA-id2927 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:LA-id2949 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den Teil des monatlichen Erwerbseinkommens, der 520 Euro übersteigt und nicht mehr als 1 000 Euro beträgt, auf 30 Prozent und"@de .


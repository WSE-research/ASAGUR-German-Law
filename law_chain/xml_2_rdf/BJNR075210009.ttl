dtd:LA-id975 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Betrieb, in dem mindestens 350 Junghennen erwerbsmäßig zum Zwecke der Zucht von Hühnern für die Konsumeierproduktion gehalten werden;"@de .

dtd:LA-id976 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Legehennenbetrieb:"@de .

dtd:LA-id977 a dtd:LA ;

dtd:LA-id202 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 60 Abs. 1 des Bundesbeamtengesetzes Beamtinnen und Beamten des einfachen, mittleren oder gehobenen Dienstes die Führung der Dienstgeschäfte zu verbieten,"@de .

dtd:LA-id203 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 64 Satz 1 des Bundesbeamtengesetzes die Übernahme oder Fortführung einer Nebentätigkeit im öffentlichen Dienst zu verlangen,"@de .

dtd:LA-id204 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 65 Abs. 4 Satz 1 des Bundesbeamtengesetzes Nebentätigkeiten zu genehmigen oder zu versagen sowie Genehmigungen zu widerrufen,"@de .

dtd:LA-id205 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 69a Abs. 1 und 3 des Bundesbeamtengesetzes die Anzeige ihrer Ruhestandsbeamtinnen und Ruhestandsbeamten oder früherer Beamtinnen und früherer Beamten mit Versorgungsbezügen über eine Beschäftigung oder Erwerbstätigkeit nach Beendigung des Beamtenverhältnisses entgegenzunehmen und gegebenenfalls eine solche zu untersagen,"@de .

dtd:LA-id206 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 70 Satz 1 des Bundesbeamtengesetzes der Annahme von Belohnungen und Geschenken zuzustimmen,"@de .

dtd:LA-id207 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 87 Abs. 2 Satz 3 des Bundesbeamtengesetzes von der Rückforderung aus Billigkeitsgründen bei Beträgen bis 1.000 Euro ganz oder teilweise abzusehen; insoweit erteile ich allgemein meine Zustimmung,"@de .

dtd:LA-id208 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 9 Abs. 1 der Bundesnebentätigkeitsverordnung Genehmigungen für die Inanspruchnahme von Einrichtungen, Personal oder Material des Dienstherrn zu erteilen."@de .

dtd:LA-id209 a dtd:LA ;
    dtd:has_DL dtd:DL-id112 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegenüber den Beamtinnen und Beamten des einfachen, mittleren und gehobenen Dienstes,a)nach § 33 Abs. 3 Nr. 1 des Bundesdisziplinargesetzes die Befugnis, die Kürzung der Dienstbezüge bis zum Höchstmaß festzusetzen, b)nach § 34 Abs. 2 des Bundesdisziplinargesetzes die Befugnis, Disziplinarklage zu erheben, c)nach § 42 Abs. 1 des Bundesdisziplinargesetzes die Zuständigkeit zum Erlass von Widerspruchsbescheiden, "@de .

dtd:LA-id210 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 33 Abs. 3 Nr. 1 des Bundesdisziplinargesetzes die Befugnis, die Kürzung der Dienstbezüge bis zum Höchstmaß festzusetzen,"@de .

dtd:LA-id211 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 34 Abs. 2 des Bundesdisziplinargesetzes die Befugnis, Disziplinarklage zu erheben,"@de .

dtd:LA-id212 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 42 Abs. 1 des Bundesdisziplinargesetzes die Zuständigkeit zum Erlass von Widerspruchsbescheiden,"@de .

dtd:LA-id213 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ausübung der Disziplinarbefugnisse gegenüber den Ruhestandsbeamtinnen und Ruhestandsbeamten des einfachen, mittleren und gehobenen Dienstes nach § 84 Satz 1 des Bundesdisziplinargesetzes."@de .

dtd:LA-id214 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 27 Abs. 4 Satz 2 des Bundesbesoldungsgesetzes über die Gewährung einer Leistungsstufe oder über die Hemmung des Aufstiegs zu entscheiden,"@de .

dtd:LA-id215 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 42a Abs. 2 Satz 7 des Bundesbesoldungsgesetzes über die Bewilligung einer Leistungsprämie oder Leistungszulage zu entscheiden."@de .

dtd:LA-id216 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 12 Abs. 2 Satz 3 des Bundesbesoldungsgesetzes von der Rückforderung aus Billigkeitsgründen bei Beträgen bis 1.000 Euro ganz oder teilweise abzusehen; insoweit erteile ich allgemein meine Zustimmung,"@de .

dtd:LA-id217 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 15 Abs. 2 Satz 1 des Bundesbesoldungsgesetzes den dienstlichen Wohnsitz anzuweisen,"@de .

dtd:LA-id218 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 28 Abs. 3 Nr. 3 des Bundesbesoldungsgesetzes spätestens bei Beendigung des Urlaubs schriftlich anzuerkennen, dass dieser dienstlichen Interessen oder öffentlichen Belangen dient, soweit die anerkennende Stelle für die Beurlaubung zuständig ist."@de .

dtd:LA-id219 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 11 Abs. 2 des Bundesreisekostengesetzes das Tage- und Übernachtungsgeld (§§ 9 und 10 des Bundesreisekostengesetzes) in besonderen Fällen bis zu weiteren 28 Tagen zu bewilligen,"@de .

dtd:LA-id220 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 18 des Bundesreisekostengesetzes nach Maßgabe der hierzu erlassenen allgemeinen Bestimmungen eine Pauschvergütung als pauschalierte Aufwandsvergütung zu gewähren,"@de .

dtd:LA-id221 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 4 Abs. 2 Satz 1 Nr. 2 des Bundesumzugskostengesetzes die Umzugskostenvergütung aus Anlass der Räumung einer bundeseigenen oder im Besetzungsrecht des Bundes stehenden Mietwohnung zuzusagen,"@de .

dtd:LA-id222 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 1 Abs. 2 der Auslandsreisekostenverordnung nach Maßgabe der hierzu erlassenen Bestimmungen Auslandsdienstreisen anzuordnen oder zu genehmigen."@de .


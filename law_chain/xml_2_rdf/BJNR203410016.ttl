dtd:LA-id2085 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Ausstattung von Messstellen der leitungsgebundenen Energieversorgung mit modernen Messeinrichtungen und intelligenten Messsystemen,"@de .

dtd:LA-id2086 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Ausgestaltung des Messstellenbetriebs und zur freien Wahl eines Messstellenbetreibers,"@de .

dtd:LA-id2087 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Aufgabentrennung von Messstellenbetrieb und Netzbetrieb,"@de .

dtd:LA-id2088 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu technischen Mindestanforderungen an Smart-Meter-Gateways und den Einsatz von intelligenten Messsystemen,"@de .

dtd:LA-id2089 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur energiewirtschaftlichen Datenkommunikation und zur allgemeinen Datenkommunikation mit Smart-Meter-Gateways,"@de .

dtd:LA-id2090 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Verarbeitung von Messwerten und weiteren personenbezogenen Daten zur Erfllung von vorvertraglichen Verpflichtungen, von Vertrgen, rechtlichen Verpflichtungen und zur Erfllung von Aufgaben im ffentlichen Interesse."@de .

dtd:LA-id2091 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anlagenbetreiber: der Betreiber von Anlagen nach dem Erneuerbare-Energien-Gesetz vom 21. Juli 2014 (BGBl. I S. 1066) oder dem Kraft-Wrme-Kopplungsgesetz vom 21. Dezember 2015 (BGBl. I S. 2498) in der jeweils geltenden Fassung,"@de .

dtd:LA-id2092 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anschlussnehmer der Eigentmer oder Erbbauberechtigte eines Grundstcks oder Gebudes, das an das Energieversorgungsnetz angeschlossen ist oder die natrliche oder juristische Person, in deren Auftrag ein Grundstck oder Gebude an das Energieversorgungsnetz angeschlossen wird,"@de .

dtd:LA-id2093 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anschlussnutzer: der zur Nutzung des Netzanschlusses berechtigte Letztverbraucher oder Betreiber von Erzeugungsanlagen nach dem Erneuerbare-Energien-Gesetz oder dem Kraft-Wrme-Kopplungsgesetz,"@de .

dtd:LA-id2094 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "grundzustndiger Messstellenbetreiber: der Betreiber von Energieversorgungsnetzen, der Auffangmessstellenbetreiber nach  11 Absatz 2 Satz 1 ab dem in  11 Absatz 3 Satz 1 genannten Zeitpunkt oder ein Dritter, dem aufgrund eines Verfahrens nach den  41 und 43 die Grundzustndigkeit fr den Messstellenbetrieb erfolgreich bertragen worden ist,"@de .

dtd:LA-id2095 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundzustndigkeit fr den Messstellenbetrieb: die Verpflichtung zur Wahrnehmung des Messstellenbetriebs fr alle Messstellen des jeweiligen Netzgebiets solange und soweit kein Dritter nach den  5 und 6 den Messstellenbetrieb durchfhrt,"@de .

dtd:LA-id2096 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundzustndigkeit fr den Messstellenbetrieb fr moderne Messeinrichtungen und intelligente Messsysteme: die Verpflichtung zur Wahrnehmung des Messstellenbetriebs mit modernen Messeinrichtungen und intelligenten Messsystemen im jeweiligen Netzgebiet fr diejenigen Messstellen, die nach Magabe der  29 bis 32 mit modernen Messeinrichtungen und intelligenten Messsystemen auszustatten sind und fr die kein Dritter nach den  5 und 6 den Messstellenbetrieb durchfhrt,"@de .

dtd:LA-id2097 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "intelligentes Messsystem"@de .

dtd:LA-id2098 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine ber ein Smart-Meter-Gateway in ein Kommunikationsnetz eingebundene moderne Messeinrichtung oder Messeinrichtung zur registrierenden Leistungsmessung zur Erfassung elektrischer Energie, die in tatschlicher Hinsicht mindestens Stromverbrauch, -erzeugung und Nutzungszeit widerspiegelt und ber den Smart-Meter-Gateway-Administrator im Zusammenwirken mit den informationstechnischen Systemen weiterer Berechtigter aus  49 Absatz 2 den besonderen Anforderungen nach den  21 und 22 in Verbindung mit  31 Absatz 1 gengt, die zur Gewhrleistung des Datenschutzes, der Datensicherheit und Interoperabilitt in Schutzprofilen und Technischen Richtlinien festgelegt werden knnen,"@de .

dtd:LA-id2099 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Letztverbraucher: natrliche oder juristische Personen, die Energie fr den eigenen Verbrauch oder fr den Betrieb von Ladepunkten zur Versorgung von Elektromobilnutzern beziehen,"@de .

dtd:LA-id2100 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mehrwertdienst: eine energieversorgungsfremde Dienstleistung, die als Kommunikationsinfrastruktur das Smart-Meter-Gateway benutzt,"@de .

dtd:LA-id2101 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Messeinrichtung: ein Messgert, das allein oder in Verbindung mit anderen Messgerten fr die Gewinnung eines oder mehrerer Messwerte eingesetzt wird,"@de .

dtd:LA-id2102 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Messstelle: die Gesamtheit aller Mess-, Steuerungs- und Kommunikationseinrichtungen zur sicheren Verarbeitung von Messdaten und Steuerungsinformationen und zur sicheren Anbindung von Erzeugungsanlagen und steuerbaren Lasten an Zhlpunkten eines Anschlussnutzers,"@de .

dtd:LA-id2103 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Messstellenbetreiber: der grundzustndige Messstellenbetreiber oder ein Dritter, der die Aufgabe des Messstellenbetriebs durch Vertrag nach  9 wahrnimmt,"@de .

dtd:LA-id2104 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Messsystem: eine in ein Kommunikationsnetz eingebundene Messeinrichtung,"@de .

dtd:LA-id2105 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Messwerte: Angaben ber vom Anschlussnutzer ber einen bestimmten Zeitraum entnommene, erzeugte oder eingespeiste Energiemengen,"@de .

dtd:LA-id2106 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "moderne Messeinrichtung"@de .

dtd:LA-id2107 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Messeinrichtung, die in tatschlicher Hinsicht mindestens Elektrizittsverbrauch und Nutzungszeit sowie sptestens, wenn eine Messung der eingespeisten Strommengen erforderlich ist, auch die Elektrizittserzeugung widerspiegelt und ber ein Smart-Meter-Gateway sicher in ein Kommunikationsnetz eingebunden werden kann,"@de .

dtd:LA-id2108 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Netzzustandsdaten: Spannungs- und Stromwerte und Phasenwinkel sowie daraus errechenbare oder herleitbare Werte, die zur Ermittlung des Netzzustandes verarbeitet werden knnen,"@de .

dtd:LA-id2109 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Plausibilisierung und Ersatzwertbildung: im Rahmen der Aufbereitung von Messwerten rechnerische Vorgnge, die ausgefallene Messwerte oder Messwertreihen berbrcken oder unplausible Messwerte korrigieren,"@de .

dtd:LA-id2110 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schaltprofil: eine Einstellung zum Stromeinspeise- oder -entnahmeverhalten an Erzeugungs- und Verbrauchsanlagen,"@de .

dtd:LA-id2111 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Smart-Meter-Gateway: die Kommunikationseinheit eines intelligenten Messsystems, die ein oder mehrere moderne Messeinrichtungen und weitere technische Einrichtungen wie insbesondere Erzeugungsanlagen nach dem Erneuerbare-Energien-Gesetz und dem Kraft-Wrme-Kopplungsgesetz zur Gewhrleistung des Datenschutzes, der Datensicherheit und Interoperabilitt unter Beachtung der besonderen Anforderungen von Schutzprofilen und Technischen Richtlinien nach  22 Absatz 1 und 2 sicher in ein Kommunikationsnetz einbinden kann und ber Funktionalitten zur Erfassung, Verarbeitung und Versendung von Daten verfgt,"@de .

dtd:LA-id2112 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Smart-Meter-Gateway-Administrator: eine natrliche oder juristische Person, die als Messstellenbetreiber oder in dessen Auftrag fr den technischen Betrieb des intelligenten Messsystems verantwortlich ist,"@de .

dtd:LA-id2113 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Smart-Metering-Public-Key-Infrastruktur: ein System zur Ausstellung, Verteilung und Prfung von digitalen Zertifikaten, welches die Authentizitt und die Vertraulichkeit bei der Kommunikation und den gesicherten Datenaustausch der verschiedenen Marktteilnehmer mit den Smart-Meter-Gateways sicherstellt,"@de .

dtd:LA-id2114 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Stammdaten: Informationen ber Art und technische Ausstattung, Ort und Spannungsebene sowie Art der kommunikativen Anbindung von an das Smart-Meter-Gateway angeschlossenen Anlagen,"@de .

dtd:LA-id2115 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tarifierung: die Zuordnung der gemessenen elektrischen Energie oder Volumenmengen zu verschiedenen Tarifstufen,"@de .

dtd:LA-id2116 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "technischer Betrieb des intelligenten Messsystems: die Installation, Inbetriebnahme, Konfiguration, Administration, berwachung und Wartung des Smart-Meter-Gateways und der informationstechnischen Anbindung von modernen Messeinrichtungen und anderen an das Smart-Meter-Gateway angebundenen technischen Einrichtungen sowie Ermglichung weiterer energiewirtschaftlicher und sonstiger Dienste,"@de .

dtd:LA-id2117 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "technischer Betrieb der Messstelle: die Installation, Inbetriebnahme, Konfiguration, Administration, berwachung und Wartung der modernen Messeinrichtungen und intelligenten Messsysteme einschlielich der informationstechnischen Anbindung von Messeinrichtungen und anderen an das Smart-Meter-Gateway angebundenen technischen Einrichtungen einschlielich Steuerungseinrichtungen sowie Ermglichung weiterer energiewirtschaftlicher und sonstiger Dienste,"@de .

dtd:LA-id2118 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wurzelzertifikat: ein auf dem Gebiet der Kryptografie und Informationssicherheit selbst signiertes Zertifikat der obersten Zertifizierungsinstanz, welches dazu dient, die Gltigkeit aller untergeordneten Zertifikate zu besttigen,"@de .

dtd:LA-id2119 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zhlerstandsgangmessung: die Messung einer Reihe viertelstndig ermittelter Zhlerstnde von elektrischer Arbeit und stndlich ermittelter Zhlerstnde von Gasmengen,"@de .

dtd:LA-id2120 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zhlpunkt: der Punkt, an dem der Energiefluss messtechnisch erfasst wird."@de .

dtd:LA-id2121 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einbau, Betrieb und Wartung der Messstelle und ihrer Messeinrichtungen und Messsysteme, Gewhrleistung einer mess- und eichrechtskonformen Messung entnommener, verbrauchter und eingespeister Energie einschlielich der Messwertaufbereitung und form- und fristgerechten Datenbertragung nach Magabe dieses Gesetzes sowie Standard- und Zusatzleistungen nach  34 einschlielich Einbau, Betrieb und Wartung von beauftragten technischen Einrichtungen einschlielich Steuerungseinrichtungen,"@de .

dtd:LA-id2122 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "technischer Betrieb der Messstelle nach den Magaben dieses Gesetzes einschlielich der form- und fristgerechten Datenbertragung nach Magabe dieses Gesetzes,"@de .

dtd:LA-id2123 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erfllung weiterer Anforderungen, die sich aus diesem Gesetz, aus den auf Grund dieses Gesetzes erlassenen Rechtsverordnungen oder aus den Festlegungen der Bundesnetzagentur ergeben."@de .

dtd:LA-id2124 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dadurch alle Zhlpunkte der Liegenschaft fr Strom mit intelligenten Messsystemen auszustatten,"@de .

dtd:LA-id2125 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "neben dem Messstellenbetrieb der Sparte Strom mindestens einen zustzlichen Messstellenbetrieb der Sparten Gas, Fernwrme oder Heizwrme ber das Smart-Meter-Gateway zu bndeln (Bndelangebot) und"@de .

dtd:LA-id2126 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den gebndelten Messstellenbetrieb fr jeden betroffenen Anschlussnutzer der Liegenschaft ohne Mehrkosten im Vergleich zur Summe der Kosten fr den bisherigen getrennten Messstellenbetrieb durchzufhren einschlielich der Abrechnungsdienstleistungen nach der Verordnung ber Heizkostenabrechnung, soweit das Bndelangebot den Messstellenbetrieb fr die Sparte Heizwrme erfasst."@de .

dtd:LA-id2127 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Vergleichsberechnung zum Nachweis der Erfllung der Anforderung aus Absatz 1 Nummer 3,"@de .

dtd:LA-id2128 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angabe des Zeitpunkts des Messstellenbetreiberwechsels und Erluterungen zur Durchfhrung der Liegenschaftsmodernisierung sowie"@de .

dtd:LA-id2129 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben zum Messstellenvertrag des Anschlussnehmers, zu Entgelten fr den Messstellenbetrieb und deren knftiger Abrechnung."@de .

dtd:LA-id2130 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit dem Anschlussnutzer oder dem Anschlussnehmer,"@de .

dtd:LA-id2131 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit dem Energielieferanten auf dessen Verlangen,"@de .

dtd:LA-id2132 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit dem Netzbetreiber fr jede Messstelle,"@de .

dtd:LA-id2133 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit dem Nachfrager einer Zusatzleistung im Sinne des  34 Absatz 2 oder Absatz 3."@de .

dtd:LA-id2134 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bedingungen des Messstellenbetriebs und Regelungen zur Messstellennutzung,"@de .

dtd:LA-id2135 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Standard- und Zusatzleistungen nach  34 einschlielich deren Entgelte und deren Abrechnung,"@de .

dtd:LA-id2136 a dtd:LA ;
    dtd:has_Size dtd:normal ;

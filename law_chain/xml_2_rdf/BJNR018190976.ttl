dtd:LA-id924 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ablauf der Amtszeit,"@de .

dtd:LA-id925 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Niederlegung des Amtes,"@de .

dtd:LA-id926 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ausscheiden aus dem Heim,"@de .

dtd:LA-id927 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verlust der Wählbarkeit,"@de .

dtd:LA-id928 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Feststellung der zuständigen Behörde auf Antrag von zwei Drittel der Mitglieder des Heimbeirates, dass das Heimbeiratsmitglied seinen Pflichten nicht mehr nachkommt oder nicht mehr nachkommen kann."@de .

dtd:LA-id929 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Heimfürsprecher die Voraussetzungen für das Amt nicht mehr erfüllt,"@de .

dtd:LA-id930 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Heimfürsprecher gegen seine Amtspflichten verstößt,"@de .

dtd:LA-id931 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Heimfürsprecher sein Amt niederlegt oder"@de .

dtd:LA-id932 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Heimbeirat gebildet worden ist."@de .

dtd:LA-id933 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ablauf seiner Amtszeit,"@de .

dtd:LA-id934 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufhebung seiner Bestellung durch die zuständige Behörde nach § 26."@de .

dtd:LA-id935 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen des Heimbetriebes, die den Bewohnerinnen oder Bewohnern des Heims dienen, bei der Leitung oder dem Träger zu beantragen,"@de .

dtd:LA-id936 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anregungen und Beschwerden von Bewohnerinnen und Bewohnern entgegenzunehmen und erforderlichenfalls durch Verhandlungen mit der Leitung oder in besonderen Fällen mit dem Träger auf ihre Erledigung hinzuwirken,"@de .

dtd:LA-id937 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Eingliederung der Bewohnerinnen und Bewohner in dem Heim zu fördern,"@de .

dtd:LA-id938 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Entscheidungen in Angelegenheiten nach den §§ 30, 31 mitzuwirken,"@de .

dtd:LA-id939 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vor Ablauf der Amtszeit einen Wahlausschuss zu bestellen (§ 6),"@de .

dtd:LA-id940 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Bewohnerversammlung durchzuführen und den Bewohnerinnen und Bewohnern einen Tätigkeitsbericht zu erstatten (§ 20),"@de .

dtd:LA-id941 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mitwirkung bei Maßnahmen zur Förderung einer angemessenen Qualität der Betreuung,"@de .


dtd:LA-id946 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Planung oder Durchführung von Veranstaltungen,"@de .

dtd:LA-id947 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Alltags- und Freizeitgestaltung,"@de .

dtd:LA-id948 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unterkunft, Betreuung und Verpflegung,"@de .

dtd:LA-id949 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erweiterung, Einschränkung oder Einstellung des Heimbetriebes,"@de .

dtd:LA-id950 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:LA-id944 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Verhütung von Unfällen,"@de .

dtd:LA-id945 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:LA-id203 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Eigentümer der Anlage auf Aufforderung des Grundstückseigentümers die Bestellung der Dienstbarkeit abgelehnt oder sich in einem Zeitraum von sechs Monaten nach Zugang der Aufforderung nicht erklärt hat."@de .

dtd:LA-id204 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Eigentümer der Anlage Gelegenheit gegeben hat, die Anlage zu beseitigen, und"@de .

dtd:LA-id205 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine hierfür gesetzte angemessene Frist fruchtlos verstrichen ist."@de .

dtd:LA-id206 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie den Grundstückseigentümer von Besitz und Nutzung seines Grundstücks ausschließen oder"@de .

dtd:LA-id207 a dtd:LA ;

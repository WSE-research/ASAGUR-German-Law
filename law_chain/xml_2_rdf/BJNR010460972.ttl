dtd:LA-id552 a dtd:LA ;
    dtd:has_DL dtd:DL-id323 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Brucellose a)im Falle von Rindern, wenn Brucella abortus durch aa)bakteriologische oder molekularbiologische Untersuchung oderbb)mindestens zwei unterschiedliche serologische Untersuchungsverfahren in Verbindung mit klinischen oder pathologisch-anatomischen Untersuchungen oder epidemiologischen Anhaltspunkten,b)im Falle von Hausschweinen, wenn Brucella suis durch eine in Buchstabe a genannte Untersuchung,c)im Falle von Schafen und Ziegen, wenn Brucella melitensis durch eine in Buchstabe a genannte Untersuchung"@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "festgestellt ist;"@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_DL dtd:DL-id330 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Falle von Rindern, wenn Brucella abortus durch aa)bakteriologische oder molekularbiologische Untersuchung oderbb)mindestens zwei unterschiedliche serologische Untersuchungsverfahren in Verbindung mit klinischen oder pathologisch-anatomischen Untersuchungen oder epidemiologischen Anhaltspunkten,"@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bakteriologische oder molekularbiologische Untersuchung oder"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens zwei unterschiedliche serologische Untersuchungsverfahren in Verbindung mit klinischen oder pathologisch-anatomischen Untersuchungen oder epidemiologischen Anhaltspunkten,"@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Falle von Hausschweinen, wenn Brucella suis durch eine in Buchstabe a genannte Untersuchung,"@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Falle von Schafen und Ziegen, wenn Brucella melitensis durch eine in Buchstabe a genannte Untersuchung"@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verdacht auf Brucellose, wenn das Ergebnis einer klinischen, pathologisch-anatomischen, bakteriologischen, molekularbiologischen oder serologischen Untersuchung in Verbindung mit epidemiologischen Anhaltspunkten den Ausbruch der Brucellose befürchten lässt."@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Absonderung,"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine amtliche Beobachtung"@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Untersuchung eines für die Seuche empfänglichen Tieres oder empfänglicher Tiere eines Bestandes oder innerhalb eines bestimmten Gebietes anordnen,"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einrichtung bestimmen, in der die jeweilige Untersuchung durchzuführen ist,"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Untersuchung eine in der vom Friedrich-Loeffler-Institut nach § 27 Absatz 5 Nummer 1 des Tiergesundheitsgesetzes herausgegebenen amtlichen Methodensammlung beschriebene Methode vorschreiben und"@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Alter festlegen, in dem die Rinder zu untersuchen sind."@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Veränderungen in dem Bestand dürfen nicht vorgenommen werden."@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Abgestoßene Früchte oder Nachgeburten sind so aufzubewahren, dass Ansteckungsstoff nicht verschleppt werden kann."@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seuchenverdächtige Rinder von den übrigen Rindern des Bestandes sowie von anderen für die Seuche empfänglichen Tieren unverzüglich abzusondern,"@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Milch der Kühe des Bestandes entweder vor Abgabe oder Verfütterung aufzukochen oder an Sammelmolkereien abzugeben, in denen eine ausreichende Erhitzung sichergestellt ist,"@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die mit abgestoßenen oder abgestorbenen Früchten, totgeborenen Kälbern oder Nachgeburten in Berührung gekommene Streu unverzüglich unschädlich zu beseitigen oder nach näherer Anweisung der zuständigen Behörde zu desinfizieren,"@de .

dtd:LA-id571 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Behälter, Gerätschaften und sonstige Gegenstände, die an Standorten, in oder auf denen sich seuchenverdächtige Tiere befinden, benutzt worden sind, nach näherer Anweisung der zuständigen Behörde zu reinigen und zu desinfizieren,"@de .

dtd:LA-id572 a dtd:LA ;
    dtd:has_DL dtd:DL-id365 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sicherzustellen, dass die Rinder des Bestandes a)nicht aus dem Bestand verbracht werden undb)nicht gedeckt oder künstlich besamt werden."@de .

dtd:LA-id573 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht aus dem Bestand verbracht werden und"@de .

dtd:LA-id574 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht gedeckt oder künstlich besamt werden."@de .

dtd:LA-id575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seuchenverdächtige Rinder ohne Blutentzug zu töten und unschädlich zu beseitigen sind,"@de .

dtd:LA-id576 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Rinder des Bestandes aufzustallen sind,"@de .

dtd:LA-id577 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ställe, Weideflächen und sonstige Standorte, in oder auf denen sich seuchenverdächtige Rinder befinden, nur vom Tierhalter, seinem Vertreter, den mit der Beaufsichtigung, Wartung und Pflege der Tiere betrauten Personen, von Tierärzten und von Personen im amtlichen Auftrag betreten werden dürfen, und diese Personen sich nach Verlassen des Stalles nach näherer Anweisung der zuständigen Behörde reinigen und desinfizieren müssen,"@de .

dtd:LA-id578 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Weiden und Ausläufe, auf denen seuchenverdächtige Rinder gehalten worden sind, für die Dauer von vier Monaten, gerechnet von dem Tag der amtlichen Feststellung des Verdachts auf Brucellose, nicht mit Huf- und Klauentieren beschickt werden dürfen."@de .

dtd:LA-id579 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Feststellung des Verseuchungsgrades des Schweinebestandes und"@de .

dtd:LA-id580 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Pferde, Hunde und andere für die Seuche empfängliche Tiere, die mit Schweinen des Bestandes in demselben Stall oder an demselben Standort untergebracht sind oder waren,"@de .

dtd:LA-id581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seuchenverdächtige Schweine von den übrigen Schweinen des Bestandes sowie von anderen für die Seuche empfänglichen Tieren unverzüglich abzusondern,"@de .

dtd:LA-id582 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die mit abgestoßenen oder abgestorbenen Früchten, totgeborenen Ferkeln oder Nachgeburten in Berührung gekommene Streu unverzüglich unschädlich zu beseitigen oder nach näherer Anweisung der zuständigen Behörde zu desinfizieren,"@de .

dtd:LA-id583 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Behälter, Gerätschaften und sonstige Gegenstände, die an Standorten, in oder auf denen sich seuchenverdächtige Tiere befinden, benutzt worden sind, nach näherer Anweisung der zuständigen Behörde zu reinigen und zu desinfizieren,"@de .

dtd:LA-id584 a dtd:LA ;
    dtd:has_DL dtd:DL-id393 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sicherzustellen, dass die Schweine des Bestandes a)nicht aus dem Bestand verbracht werden undb)nicht gedeckt oder künstlich besamt werden."@de .

dtd:LA-id585 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht aus dem Bestand verbracht werden und"@de .

dtd:LA-id586 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht gedeckt oder künstlich besamt werden."@de .

dtd:LA-id587 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die seuchenverdächtigen Schweine ohne Blutentzug zu töten und unschädlich zu beseitigen sind,"@de .

dtd:LA-id588 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Schweine des Bestandes aufzustallen sind,"@de .

dtd:LA-id589 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ställe, Weideflächen und sonstige Standorte, in oder auf denen sich seuchenverdächtige Schweine befinden, nur vom Tierhalter, seinem Vertreter, den mit der Beaufsichtigung, Wartung und Pflege der Tiere betrauten Personen, von Tierärzten und von Personen im amtlichen Auftrag betreten werden dürfen, und diese Personen sich nach Verlassen des Stalles reinigen und desinfizieren müssen,"@de .

dtd:LA-id590 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Weiden und Ausläufe, auf denen seuchenverdächtige Schweine gehalten worden sind, für die Dauer von vier Monaten, gerechnet von dem Tag der amtlichen Feststellung des Verdachts auf Brucellose, nicht mit Huf- und Klauentieren beschickt werden dürfen."@de .

dtd:LA-id591 a dtd:LA ;
    dtd:has_DL dtd:DL-id412 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Absatz 1 Satz 1 Nummer 4 Buchstabe a a)für über vier Monate alte Schweine, bei denen zwei nach Feststellung des Verdachts im Abstand von 28 Tagen entnommene Blutproben vor dem Verbringen im Rahmen einer Untersuchung im Sinne des § 1 Nummer 1 Buchstabe b mit negativem Ergebnis auf Brucellose untersucht worden sind,b)für Schweine, die unmittelbar zur Schlachtung verbracht werden,"@de .

dtd:LA-id592 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für über vier Monate alte Schweine, bei denen zwei nach Feststellung des Verdachts im Abstand von 28 Tagen entnommene Blutproben vor dem Verbringen im Rahmen einer Untersuchung im Sinne des § 1 Nummer 1 Buchstabe b mit negativem Ergebnis auf Brucellose untersucht worden sind,"@de .

dtd:LA-id593 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Schweine, die unmittelbar zur Schlachtung verbracht werden,"@de .

dtd:LA-id594 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Absatz 1 Satz 1 Nummer 4 Buchstabe b für die künstliche Besamung,"@de .

dtd:LA-id595 a dtd:LA ;
    dtd:has_DL dtd:DL-id422 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in dem gefährdeten Gebiet a)das Decken der Schweine anderer Tierhalter,b)den gemeinschaftlichen Weidegang der Schweine aus verschiedenen Beständen,c)Körveranstaltungen, Versteigerungen und Märkte von Schweinen sowie ähnliche Veranstaltungen;"@de .

dtd:LA-id596 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Decken der Schweine anderer Tierhalter,"@de .

dtd:LA-id597 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den gemeinschaftlichen Weidegang der Schweine aus verschiedenen Beständen,"@de .

dtd:LA-id598 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Körveranstaltungen, Versteigerungen und Märkte von Schweinen sowie ähnliche Veranstaltungen;"@de .

dtd:LA-id599 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Verbringen von Schweinen aus dem gefährdeten Gebiet, außer zur unverzüglichen Tötung,"@de .


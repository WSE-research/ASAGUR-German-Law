dtd:LA-id603 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Milch von Schafen und Ziegen des Bestandes vor der Abgabe oder Verfütterung aufzukochen oder an Sammelmolkereien abzugeben, in denen eine ausreichende Erhitzung sichergestellt ist,"@de .

dtd:LA-id604 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die mit den abgestoßenen oder abgestorbenen Früchten, totgeborenen Lämmern oder Nachgeburten in Berührung gekommene Streu unverzüglich unschädlich zu beseitigen oder nach näherer Anweisung der zuständigen Behörde zu desinfizieren,"@de .

dtd:LA-id605 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Behälter, Gerätschaften und sonstige Gegenstände, die in Ställen oder an sonstigen Standorten, in oder auf denen sich seuchenverdächtige Schafe oder Ziegen befinden, benutzt worden sind, nach näherer Anweisung der zuständigen Behörde zu reinigen und zu desinfizieren,"@de .

dtd:LA-id606 a dtd:LA ;
    dtd:has_DL dtd:DL-id445 ;

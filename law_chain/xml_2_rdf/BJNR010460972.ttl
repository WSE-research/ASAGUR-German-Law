dtd:LA-id630 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Rinderbestand in demselben Gehöft ein anerkannter Bestand ist."@de .

dtd:LA-id631 a dtd:LA ;

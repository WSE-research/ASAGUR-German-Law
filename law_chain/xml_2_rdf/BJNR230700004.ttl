    dtd:has_Value "Konstruktionen für Bauteile und Bauwerke von Feuerungsanlagen und Industrieöfen sowie Schornsteine von Industrieanlagen entwerfen und berechnen,"@de .

dtd:LA-id357 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Konstruktionen für den Ausbau auswählen und bewerten,"@de .

dtd:LA-id358 a dtd:LA ;

dtd:LA-id302 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einkünfte aus Vermietung oder Verpachtung, Einkünfte aus Kapitalvermögen,"@de .

dtd:LA-id303 a dtd:LA ;

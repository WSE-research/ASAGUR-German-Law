    dtd:has_Value "Alleinerziehende Personen, die mit einem oder mehreren minderjährigen Kindern zusammenleben"@de .

dtd:LA-id326 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Behinderte Personen, denen bestimmte Leistungen gem. SGB XII zuerkannt werden"@de .

dtd:LA-id327 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, die medizinisch bedingt einer kostenaufwändigen Ernährung bedürfen"@de .

dtd:LA-id328 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dezentrale Warmwasserversorgung"@de .

dtd:LA-id329 a dtd:LA ;

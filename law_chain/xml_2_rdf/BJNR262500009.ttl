dtd:LA-id209 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "allgemeine oder sektorale Regeln zu ermitteln, welche die Anforderungen, insbesondere aus Rechtsvorschriften, für Akkreditierungstätigkeiten konkretisieren oder ergänzen,"@de .

dtd:LA-id210 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Nutzung der Akkreditierung als vertrauensbildendes Element der Konformitätsbewertung zu fördern,"@de .

dtd:LA-id211 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die deutsche Vertretung und Haltung für die Sitzungen der Europäischen Kooperation für Akkreditierung zu koordinieren."@de .


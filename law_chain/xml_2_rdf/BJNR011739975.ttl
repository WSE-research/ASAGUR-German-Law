    dtd:has_Size dtd:normal ;
    dtd:has_Value "die folgenden Beamten auf Lebenszeit:"@de .

dtd:LA-id745 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Abteilungsdirektoren (und Professoren),"@de .

dtd:LA-id746 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Abteilungsvorsteher (und Professoren), soweit sie sich in Besoldungsgruppen befinden, deren Grundgehälter mindestens denen der Besoldungsgruppe A 15 entsprechen,"@de .

dtd:LA-id747 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wissenschaftliche Räte (und Professoren), soweit sie sich in Besoldungsgruppen befinden, deren Grundgehälter mindestens denen der Besoldungsgruppe A 15 entsprechen,"@de .

dtd:LA-id748 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Professoren an Hochschulen (außer Fachhochschulen), soweit sie sich in Besoldungsgruppen befinden, deren Grundgehälter mindestens denen der Besoldungsgruppe A 15 entsprechen, und nicht unter Buchstabe a fallen;"@de .

dtd:LA-id749 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Professor in die Besoldungsgruppe C 2"@de .

dtd:LA-id750 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die folgenden Beamten auf Lebenszeit, soweit sie nicht unter Buchstaben a oder b fallen:"@de .

dtd:LA-id751 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Abteilungsvorsteher (und Professoren),"@de .

dtd:LA-id752 a dtd:LA ;

dtd:LA-id555 a dtd:LA ;
    dtd:has_Value "„Unternehmen der Außer-Haus-Verpflegung“: Anbieter von Gemeinschaftsverpflegung im Sinne des Artikels 2 Absatz 2 Buchstabe d der Verordnung (EU) 1169/2011 des Europäischen Parlamentes und des Rates vom 25. Oktober 2011 betreffend die Information der Verbraucher über Lebensmittel und zur Änderung der Verordnungen (EG) Nr. 1924/2006 und (EG) Nr. 1925/2006 des Europäischen Parlaments und des Rates und zur Aufhebung der Richtlinie 87/250/EWG der Kommission, der Richtlinie 90/496/EWG des Rates, der Richtlinie 1999/10/EG der Kommission, der Richtlinie 2000/13/EG des Europäischen Parlaments und des Rates, der Richtlinien 2002/67/EG und 2008/5/EG der Kommission und der Verordnung (EG) Nr. 608/2004 der Kommission (ABl. L 304 vom 22.11.2011, S.18),"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Value "„Unternehmer“: jede natürliche oder juristische Person, die dafür verantwortlich ist, dass die Anforderungen dieser Verordnung in dem ihrer Aufsicht unterstehenden Unternehmen der Außer-Haus-Verpflegung erfüllt werden,"@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Value "„Lebensmittel“: Lebensmittel im Sinne des Artikels 2 der Verordnung (EG) 178/2002 des Europäischen Parlaments und des Rates vom 28. Januar 2002 zur Festlegung der allgemeinen Grundsätze und Anforderungen des Lebensmittelrechts, zur Errichtung der Europäischen Behörde für Lebensmittelsicherheit und zur Festlegung von Verfahren zur Lebensmittelsicherheit (ABl. L 31 vom 1.2.2002, S.1),"@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Value "„Zutat“: Zutat im Sinne des Artikels 2 Absatz 2 Buchstabe f der VO (EU) 1169/2011 von Erzeugnissen der Außer-Haus-Verpflegung,"@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Value "„Erzeugnis der Außer-Haus-Verpflegung“: Lebensmittel, das in einem Unternehmen der Außer-Haus-Verpflegung hergestellt oder zubereitet und dort an Verbraucher abgegeben wird,"@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Value "„ökologische/biologische Zutat“: Zutat, welche aus ökologischer/biologischer Produktion gemäß Artikel 3 Nummer 1 der Verordnung (EU) 2018/848 des Europäischen Parlaments und des Rates vom 30. Mai 2018 über die ökologische/biologische Produktion und die Kennzeichnung von ökologischen/biologischen Erzeugnissen sowie zur Aufhebung der Verordnung (EG) Nr. 834/2007 des Rates (ABl. L 150 vom 14.6.2018, S. 1) stammt, ausgenommen eine solche, die während des Umstellungszeitraums gemäß Artikel 10 der Verordnung (EU) 2018/848 hergestellt wird,"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Value "„ökologisches/biologisches Erzeugnis“: Erzeugnis, welches aus ökologischer/biologischer Produktion gemäß Artikel 3 Nummer 1 der Verordnung (EU) 2018/848 stammt, ausgenommen ein solches, das während des Umstellungszeitraums gemäß Artikel 10 der Verordnung (EU) 2018/848 hergestellt wird,"@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Value "„Umstellungsprodukt“: Zutat oder Erzeugnis, das während des Umstellungszeitraums nach Artikel 10 der Verordnung (EU) 2018/848 hergestellt wird,"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Value "„ökologische/biologische Produktion“: ökologische/biologische Produktion im Sinne des Artikels 3 Nummer 1 der Verordnung (EU) 2018/848,"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Value "„Bio-Anteil“: der in Prozent ausgewiesene Anteil der ökologischen/biologischen Zutaten und Erzeugnisse am Geldwert des Gesamtwareneinkaufs der von einer Betriebseinheit bezogenen Zutaten und Erzeugnisse,"@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Value "„zuständige Behörde“: die nach § 2 Absatz 1 des Öko-Landbaugesetzes zuständige Behörde,"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Value "„Betriebseinheit“: Unternehmen oder Teil eines Unternehmens der Außer-Haus-Verpflegung mit eigenständiger Organisation und Abrechnung,"@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Value "„Bio-Zutatenübersicht“: Übersicht aller Zutaten und Erzeugnisse, die mit Bezug auf die ökologische/biologische Produktion gekennzeichnet sind,"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Value "„Nicht-Bio-Zutatenübersicht“: Übersicht aller nichtökologischen/nichtbiologischen Zutaten und Erzeugnisse einschließlich der Umstellungsprodukte,"@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Value "„Veranstaltungszertifikat“: Zertifikat, das von einer Kontrollstelle für einen Anlass, der bis auf eine Dauer von bis zu zwei Monaten befristet ist, ausgestellt wird."@de .

dtd:LA-id570 a dtd:LA ;

    dtd:has_Value "Zeitpunkt und Ort des Losverkaufes und der Ziehung,"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Value "Spielplan und"@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Value "geplante Höhe und Verwendung des Reinertrags, soweit eine Steuerbefreiung nach § 28 Rennwett- und Lotteriegesetz geltend gemacht werden soll."@de .


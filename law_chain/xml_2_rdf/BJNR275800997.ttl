    dtd:has_FnZ "1"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F789649_13_BJNR275800997BJNE002011118"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Bei mehreren Beständen sind die Bestände einzeln aufzuführen."@de .


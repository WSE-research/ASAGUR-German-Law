dtd:LA-id2479 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 2 Abs. 1 Satz 1 nicht das vorgeschriebene Formblatt anwendet,"@de .

dtd:LA-id2480 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen §§ 3 bis 5, 6 Abs. 1 Satz 1 oder 2, Abs. 2 oder 4 die dort genannten Posten nicht, nicht in der vorgeschriebenen Weise oder nicht mit dem vorgeschriebenen Inhalt ausweist,"@de .

dtd:LA-id2481 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 6 Abs. 3 dort genannte Vermögensgegenstände oder Schulden in seine Bilanz aufnimmt,"@de .

dtd:LA-id2482 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Vorschrift des § 9 über die Fristengliederung zuwiderhandelt,"@de .

dtd:LA-id2483 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 10 Abs. 1 dort genannte Verbindlichkeiten nicht verrechnet,"@de .

dtd:LA-id2484 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 10 Abs. 2 Forderungen oder Verbindlichkeiten verrechnet,"@de .

dtd:LA-id2485 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Vorschrift der §§ 12 bis 33 über die in einzelne Posten der Bilanz oder der Gewinn- und Verlustrechnung aufzunehmenden Angaben zuwiderhandelt,"@de .

dtd:LA-id2486 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Vorschrift des § 34 oder 35 über zusätzliche Erläuterungen oder Pflichtangaben zuwiderhandelt oder"@de .

dtd:LA-id2487 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Vorschrift des § 36 über Termingeschäfte zuwiderhandelt."@de .

dtd:P-id2489 a dtd:P ;
    dtd:has_Value "Diese Verordnung dient der Umsetzung der Richtlinie 86/635/EWG des Rates vom 8. Dezember 1986 über den Jahresabschluß und den konsolidierten Abschluß von Banken und anderen Finanzinstituten (ABl. EG Nr. L 372 S. 1) und der Richtlinie 89/117/EWG des Rates vom 13. Februar 1989 über die Pflichten der in einem Mitgliedstaat eingerichteten Zweigniederlassungen von Kreditinstituten und Finanzinstituten mit Sitz außerhalb dieses Mitgliedstaats zur Offenlegung von Jahresabschlußunterlagen (ABl. EG Nr. L 44 S. 40)."@de ;
    dtd:has_noindex dtd:noindex-id450 .

dtd:P-id2491 a dtd:P ;
    dtd:has_BR dtd:BR-id406 ;
    dtd:has_Value " (+++ Textnachweis ab: 15.2.1992 +++)(+++ Zur Anwendung vgl. § 39 +++)(+++ Amtliche Hinweise des Normgebers auf EG-Recht:     Umsetzung der            EWGRL 635/86 (CELEX Nr: 386L0635)            EWGRL 117/89 (CELEX Nr: 389L0117) +++)Überschrift: Langüberschrift idF d. Art. 7 Abs. 3 Nr. 1 G v. 12.5.2021 I 990 mWv 26.6.2021; Buchstabenabkürzung idF d. Art. 1 Nr. 1 V v. 11.12.1998 I 3654 mWv 19.12.1998; Kurzüberschrift eingef. durch Art. 1 Nr. 1 V v. 11.12.1998 I 3654 mWv 19.12.1998"@de ;
    dtd:has_pre dtd:pre-id2652 .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "andere Verbindlichkeiten gegenüber Kunden mit vereinbarter Laufzeit oder Kündigungsfrist (Passivposten Nr. 2 Buchstabe b Doppelbuchstabe bb),"@de .


    dtd:has_Value "der Verwendung neuer Luftfahrzeugmuster,"@de .

dtd:LA-id486 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Anzahl von Zwischenlandungen,"@de .

dtd:NB-id269 a dtd:NB ;

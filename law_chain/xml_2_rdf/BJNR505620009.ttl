dtd:LA-id457 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zeit für die Hin- und Rückfahrt zwischen dem Wohnort und der Heimatbasis;"@de .

dtd:LA-id458 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zeit für den örtlichen Transfer vom Ruheort zum Ort des Dienstbeginns und für die Rückfahrt."@de .

dtd:LA-id459 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zeiten für Vorarbeiten vom angeordneten Antritt des Flugdienstes bis zum Beginn der Blockzeit, mindestens jedoch eine halbe Stunde,"@de .

dtd:LA-id460 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Blockzeit,"@de .

dtd:LA-id461 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens 15 Minuten für Abschlussarbeiten nach dem Ende der Blockzeit,"@de .

dtd:LA-id462 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die auf Anordnung im Flugübungsgerät verbrachte Zeit einschließlich der Zeiten für Vor- und Abschlussarbeiten nach den Nummern 1 und 3,"@de .

dtd:LA-id463 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zeit, die nach § 13 und § 14 als Flugdienstzeit anzurechnen ist."@de .

dtd:LA-id464 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei mehr als drei, jedoch weniger als sechs Landungen um eine weitere Stunde,"@de .

dtd:LA-id465 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei mehr als fünf Landungen um weitere zwei Stunden"@de .

dtd:LA-id466 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Jedes Flugbesatzungsmitglied darf nicht länger als zehn Stunden ein Luftfahrzeug führen und bedienen."@de .

dtd:LA-id467 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Innerhalb des Flugdienstes dürfen nicht mehr als zwei Landungen nach der Pause geplant werden."@de .

dtd:LA-id468 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Innerhalb jeweils sieben aufeinanderfolgender Tage dürfen nicht mehr als zwei Flugdienste nach Absatz 2 geleistet werden."@de .

dtd:LA-id469 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Flugdienste nach Absatz 2 und § 9 dürfen nicht innerhalb von sieben aufeinanderfolgenden Tagen geleistet werden."@de .

dtd:LA-id470 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entweder dem Besatzungsmitglied während der Bereitschaftszeit kein ruhig gelegener Raum mit Schlafgelegenheit zur Verfügung steht,"@de .

dtd:LA-id471 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "oder dem Besatzungsmitglied während der Bereitschaftszeit ein ruhig gelegener Raum mit Schlafgelegenheit zur Verfügung steht, die Bereitschaftszeit jedoch weniger als zwei Stunden beträgt, es sei denn, die Bereitschaftszeit wird im Anschluss an eine Ruhezeit abgeleistet."@de .

dtd:LA-id472 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Undurchführbarkeit eines Fluges auf Grund der vorgeschriebenen Flugdienst- und Ruhezeiten und mangels geeigneter Flugplätze für Zwischenlandungen,"@de .

dtd:LA-id473 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nachteilige Auswirkungen auf die Gesamtbelastung der Besatzungsmitglieder bei Einhaltung der vorgeschriebenen Flugdienst- und Ruhezeiten,"@de .

dtd:LA-id474 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "unverhältnismäßig hoher Mehraufwand für bestimmte Flüge bei Einhaltung der vorgeschriebenen Flugdienst- und Ruhezeiten."@de .

dtd:LA-id475 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Betriebsausrüstung und deren Zustand der verwendeten Luftfahrzeuge,"@de .

dtd:LA-id476 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zusammensetzung der Besatzung und deren Flug-, Strecken- und Luftfahrzeugmustererfahrung,"@de .

dtd:LA-id477 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anzahl von Zwischenlandungen,"@de .

dtd:LA-id478 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstige die Belastung der Besatzung beeinflussende Faktoren."@de .

dtd:LA-id479 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Einhaltung der Vorschriften der Zweck der Forschungs-, Arbeits- oder Wettbewerbsflüge gefährdet ist,"@de .

dtd:LA-id480 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ausnahmen auf Flüge ohne die Beförderung von Personen, bei Arbeits- und Wettbewerbsflügen zusätzlich auf Flüge nach Sichtflugregeln beschränkt bleiben,"@de .

dtd:LA-id481 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Gefahr für die öffentliche Sicherheit und Ordnung ausgeschlossen ist."@de .

dtd:LA-id482 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Betriebsausrüstung und deren Zustand oder der Betriebseigenschaften der verwendeten Luftfahrzeuge,"@de .

dtd:LA-id483 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Einsatzes von Besatzungsmitgliedern mit geringer Flug-, Strecken- oder Luftfahrzeugmustererfahrung,"@de .

dtd:LA-id484 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erschwerter Flugdurchführung insbesondere auf Strecken mit fehlenden oder unzureichenden Navigationshilfen, hoher Luftverkehrsdichte oder häufigem Schlechtwetter,"@de .


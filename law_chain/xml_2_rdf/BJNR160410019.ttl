dtd:LA-id795 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein besonderer Härtefall vorliegt und"@de .

dtd:LA-id796 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "davon auszugehen ist, dass die Ausbildung spätestens am 31. August 2035 erfolgreich abgeschlossen sein wird."@de .


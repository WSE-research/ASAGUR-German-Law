dtd:LA-id1301 a dtd:LA ;
    dtd:has_Value "entgegen § 17 Absatz 6 finanzielle Mittel entgegennimmt oder hält."@de .

dtd:LA-id1302 a dtd:LA ;
    dtd:has_Value "entgegen § 6 Absatz 1 Satz 1 eine Information nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig zur Verfügung stellt,"@de .

dtd:LA-id1303 a dtd:LA ;
    dtd:has_Value "entgegen § 6 Absatz 3 Satz 1 oder 2, § 8 Absatz 1 oder Absatz 3 Satz 1, § 10 Absatz 6, § 24 Absatz 1 oder Absatz 4 Satz 1 oder § 30 Absatz 1 Satz 1 eine Mitteilung nicht, nicht richtig oder nicht rechtzeitig macht,"@de .

dtd:LA-id1304 a dtd:LA ;
    dtd:has_Value "entgegen § 7 Absatz 1 oder Absatz 2 einen Kreditdienstleister nicht oder nicht rechtzeitig beauftragt,"@de .


dtd:LA-id1231 a dtd:LA ;
    dtd:has_Value "berufsübergreifende berufsprofilgebende Fertigkeiten, Kenntnisse und Fähigkeiten,"@de .

dtd:LA-id1232 a dtd:LA ;
    dtd:has_Value "berufsprofilgebende Fertigkeiten, Kenntnisse und Fähigkeiten sowie"@de .

dtd:LA-id1233 a dtd:LA ;
    dtd:has_Value "berufsübergreifende, integrativ zu vermittelnde Fertigkeiten, Kenntnisse und Fähigkeiten."@de .

dtd:LA-id1234 a dtd:LA ;
    dtd:has_Value "die Sicherheitsrichtlinien für den Eisenbahnbetrieb anwenden,"@de .

dtd:LA-id1235 a dtd:LA ;
    dtd:has_Value "rechtliche Regelungen einhalten; die Rollen der Beteiligten im Eisenbahnbetrieb und ihre Aufgaben im Eisenbahnsystem verstehen und unterscheiden,"@de .

dtd:LA-id1236 a dtd:LA ;
    dtd:has_Value "Fahrzeuge sowie Bahn- und Gleisanlagen einschließlich technischer Serviceeinrichtungen nach ihren Zwecken unterscheiden,"@de .

dtd:LA-id1237 a dtd:LA ;
    dtd:has_Value "Steuerung und Sicherung der Zugfolge, Fahrwegelemente und Fahrstraßen in ihrer Funktion beschreiben und unterscheiden,"@de .

dtd:LA-id1238 a dtd:LA ;
    dtd:has_Value "Zugbeeinflussungssysteme beschreiben und unterscheiden, Zugbeeinflussungsanlagen bedienen und"@de .

dtd:LA-id1239 a dtd:LA ;
    dtd:has_Value "am Notfallmanagement mitwirken."@de .

dtd:LA-id1240 a dtd:LA ;
    dtd:has_Value "Aufträge entgegennehmen und die für die Ausführung notwendigen Arbeitsmittel auf ihre Einsatzfähigkeit prüfen,"@de .

dtd:LA-id1241 a dtd:LA ;
    dtd:has_Value "Fahrzeuge vor und nach der Fahrt prüfen,"@de .

dtd:LA-id1242 a dtd:LA ;
    dtd:has_Value "Bremsen prüfen und bedienen,"@de .

dtd:LA-id1243 a dtd:LA ;
    dtd:has_Value "Zug- und Rangierfahrten im Regelfall durchführen,"@de .

dtd:LA-id1244 a dtd:LA ;
    dtd:has_Value "Zug- und Rangierfahrten bei Abweichungen und Störungen durchführen,"@de .

dtd:LA-id1245 a dtd:LA ;
    dtd:has_Value "Verkehrs-, Personal- und Fahrzeugdispositionen sowie Planung innerhalb des Aufgabengebietes beschreiben und"@de .

dtd:LA-id1246 a dtd:LA ;
    dtd:has_Value "Güter transportieren und Personen befördern."@de .

dtd:LA-id1247 a dtd:LA ;
    dtd:has_Value "Organisation des Ausbildungsbetriebes, Berufsbildung sowie Arbeits- und Tarifrecht,"@de .

dtd:LA-id1248 a dtd:LA ;
    dtd:has_Value "Sicherheit und Gesundheit bei der Arbeit,"@de .

dtd:LA-id1249 a dtd:LA ;
    dtd:has_Value "Umweltschutz und Nachhaltigkeit,"@de .

dtd:LA-id1250 a dtd:LA ;
    dtd:has_Value "digitalisierte Arbeitswelt,"@de .

dtd:LA-id1251 a dtd:LA ;
    dtd:has_Value "Mitwirken an logistischen und betrieblichen Prozessen sowie an Qualitäts- und Sicherheitsmanagementprozessen und"@de .

dtd:LA-id1252 a dtd:LA ;
    dtd:has_Value "Durchführen von betrieblicher und technischer Kommunikation sowie von Kundenkommunikation."@de .

dtd:LA-id1253 a dtd:LA ;
    dtd:has_Value "Güterverkehr und"@de .

dtd:LA-id1254 a dtd:LA ;
    dtd:has_Value "Personenverkehr."@de .

dtd:LA-id1255 a dtd:LA ;
    dtd:has_Value "die im Ausbildungsrahmenplan für die ersten 18 Monate genannten Fertigkeiten, Kenntnisse und Fähigkeiten sowie"@de .

dtd:LA-id1256 a dtd:LA ;
    dtd:has_Value "den im Berufsschulunterricht zu vermittelnden Lehrstoff, soweit er den im Ausbildungsrahmenplan genannten Fertigkeiten, Kenntnissen und Fähigkeiten entspricht."@de .

dtd:LA-id1257 a dtd:LA ;
    dtd:has_Value "mit den am Eisenbahnbetrieb Beteiligten zu kommunizieren und sich mit ihnen zu verständigen,"@de .

dtd:LA-id1258 a dtd:LA ;
    dtd:has_Value "die eigene Sicherheit im Eisenbahnbetrieb zu gewährleisten,"@de .

dtd:LA-id1259 a dtd:LA ;
    dtd:has_Value "Zweck und Aufbau von Bahnanlagen zu beschreiben,"@de .

dtd:LA-id1260 a dtd:LA ;
    dtd:has_Value "Zugbeeinflussungssysteme sowie Kommunikationssysteme zu unterscheiden,"@de .

dtd:LA-id1261 a dtd:LA ;
    dtd:has_Value "die rechtlichen Vorschriften für den Eisenbahnbetrieb einzuhalten und"@de .

dtd:LA-id1262 a dtd:LA ;
    dtd:has_Value "die Funktion und Bedeutung von Signalen, von Fahrstraßen und von Rangierstraßen sowie die Grundlagen des Rad-Schiene-Systems zu beschreiben."@de .

dtd:LA-id1263 a dtd:LA ;
    dtd:has_Value "eine fahrzeugspezifische Bremsprobe durchzuführen,"@de .

dtd:LA-id1264 a dtd:LA ;
    dtd:has_Value "Zugdaten zu erfassen und die dazugehörigen betrieblichen Dokumente zu erstellen,"@de .

dtd:LA-id1265 a dtd:LA ;
    dtd:has_Value "eine fahrzeugspezifische, wagentechnische Behandlung durchzuführen sowie"@de .

dtd:LA-id1266 a dtd:LA ;
    dtd:has_Value "Arbeitsschutzbestimmungen bei Aufenthalten und Arbeiten im Gleisbereich einzuhalten."@de .

dtd:LA-id1267 a dtd:LA ;
    dtd:has_Value "die Bewertung für den ersten Teil mit 50 Prozent und"@de .

dtd:LA-id1268 a dtd:LA ;
    dtd:has_Value "die Bewertung für den zweiten Teil mit 50 Prozent."@de .

dtd:LA-id1269 a dtd:LA ;
    dtd:has_Value "die im Ausbildungsrahmenplan genannten Fertigkeiten, Kenntnisse und Fähigkeiten sowie"@de .

dtd:LA-id1270 a dtd:LA ;
    dtd:has_Value "den im Berufsschulunterricht zu vermittelnden Lehrstoff, soweit er den im Ausbildungsrahmenplan genannten Fertigkeiten, Kenntnissen und Fähigkeiten entspricht."@de .

dtd:LA-id1271 a dtd:LA ;
    dtd:has_Value "„Prüfen von Triebfahrzeugen“,"@de .

dtd:LA-id1272 a dtd:LA ;
    dtd:has_Value "„Zug- und Rangierfahrten durchführen“,"@de .

dtd:LA-id1273 a dtd:LA ;
    dtd:has_Value "„Eisenbahnbetrieb im Regelbetrieb sowie bei Abweichungen und Störungen“ sowie"@de .

dtd:LA-id1274 a dtd:LA ;

dtd:LA-id207 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bundesknappschaft 32 Vertreterinnen und Vertreter der Versicherten und 16 Vertreterinnen und Vertreter der Arbeitgeber,"@de .

dtd:LA-id208 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bahnversicherungsanstalt zwölf Vertreterinnen und Vertreter der Versicherten und eine Vertreterin oder ein Vertreter der Arbeitgeber und"@de .

dtd:LA-id209 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Seekasse vier Vertreterinnen und Vertreter der Versicherten und vier Vertreterinnen und Vertreter der Arbeitgeber."@de .

dtd:LA-id210 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Wahl der Mitglieder der Geschäftsführung und die Wahl der oder des Vorsitzenden der Geschäftsführung,"@de .

dtd:LA-id211 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die personelle Besetzung von Ausschüssen,"@de .

dtd:LA-id212 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einstellung von Bewerberinnen und Bewerbern für die Laufbahn des höheren Dienstes sowie die Anstellung, die Beförderung und die Entlassung,"@de .

dtd:LA-id213 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einstellung, Höhergruppierung und Entlassung von Angestellten, mit Ausnahme der Assistenzärztinnen und Assistenzärzte, in Vergütungsgruppen, deren Tätigkeit nach den Tätigkeitsmerkmalen mindestens den Tätigkeiten im Eingangsamt der Laufbahn des höheren Dienstes vergleichbar ist,"@de .

dtd:LA-id214 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Festsetzung von Beiträgen zur Krankenversicherung über 11 vom Hundert der beitragspflichtigen Einnahmen."@de .

dtd:P-id216 a dtd:P ;
    dtd:has_BR dtd:BR-id186 ;
    dtd:has_Value " (+++ Textnachweis ab: 1.1.2005 +++)Das G wurde als Artikel 83 des G v. 9.12.2004 I 3242 (RVOrgG) vom Bundestag mit Zustimmung des Bundesrates beschlossen. Es tritt gem. Art. 86 Abs. 1 dieses G am 1.1.2005 in Kraft. Die §§ 19 und 21 treten gem. Abs. 3 am 15.12.2004 in Kraft."@de ;

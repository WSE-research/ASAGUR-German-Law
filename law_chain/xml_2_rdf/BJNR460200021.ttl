dtd:P-id62 a dtd:P ;
    dtd:has_Value "Der Bundestag hat mit Zustimmung des Bundesrates das folgende Gesetz beschlossen:"@de .

dtd:P-id67 a dtd:P ;
    dtd:has_Value "Die Bundesregierung evaluiert unter Beteiligung der Länder zum 31. Dezember 2027 und zum 31. Dezember 2030 die durch dieses Gesetz verursachten Investitionskosten und die Betriebskosten. Im Lichte der Ergebnisse der Evaluation werden Bund und Länder unter Beachtung der Aufgabenverantwortung Mehrbelastungen und Minderbelastungen der Länder auf Grundlage der in diesem Gesetz geregelten wechselseitigen Finanzbeiträge angemessen ausgleichen."@de .

dtd:P-id69 a dtd:P ;
    dtd:has_Value "(1) Dieses Gesetz tritt vorbehaltlich der Absätze 2 bis 6 am Tag nach der Verkündung in Kraft."@de .

dtd:P-id70 a dtd:P ;
    dtd:has_Value "(2) Artikel 1 Nummer 1 und 4 tritt am 1. Januar 2023 in Kraft."@de .

dtd:P-id71 a dtd:P ;
    dtd:has_Value "(3) Artikel 1 Nummer 5 bis 8 tritt am 1. Juli 2022 in Kraft."@de .

dtd:P-id72 a dtd:P ;
    dtd:has_Value "(4) Artikel 1 Nummer 2 und 3 tritt am 1. August 2026 in Kraft."@de .

dtd:P-id73 a dtd:P ;
    dtd:has_Value "(5) Artikel 2 tritt am 1. August 2029 in Kraft."@de .

dtd:P-id74 a dtd:P ;
    dtd:has_Value "(6) Artikel 4 tritt am 1. Januar 2026 in Kraft."@de .


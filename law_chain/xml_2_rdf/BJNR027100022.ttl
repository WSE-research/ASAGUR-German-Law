    dtd:has_Value "Rechte und Pflichten aus den berufsbezogenen Arbeitsschutz- und Unfallverhtungsvorschriften kennen und diese Vorschriften anwenden"@de .

dtd:LA-id1624 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gefhrdungen von Sicherheit und Gesundheit am Arbeitsplatz und auf dem Arbeitsweg prfen und beurteilen"@de .

dtd:LA-id1625 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sicheres und gesundheitsgerechtes Arbeiten erlutern"@de .

dtd:LA-id1626 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "technische und organisatorische Manahmen zur Vermeidung von Gefhrdungen sowie von psychischen und physischen Belastungen fr sich und andere, auch prventiv, ergreifen"@de .

dtd:LA-id1627 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ergonomische Arbeitsweisen beachten und anwenden"@de .

dtd:LA-id1628 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verhaltensweisen bei Unfllen beschreiben und erste Manahmen bei Unfllen einleiten"@de .

dtd:LA-id1629 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "betriebsbezogene Vorschriften des vorbeugenden Brandschutzes anwenden, Verhaltensweisen bei Brnden beschreiben und erste Manahmen zur Brandbekmpfung ergreifen"@de .

dtd:LA-id1630 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mglichkeiten zur Vermeidung betriebsbedingter Belastungen fr Umwelt und Gesellschaft im eigenen Aufgabenbereich erkennen und zu deren Weiterentwicklung beitragen"@de .

dtd:LA-id1631 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Arbeitsprozessen und im Hinblick auf Produkte, Waren oder Dienstleistungen Materialien und Energie unter wirtschaftlichen, umweltvertrglichen und sozialen Gesichtspunkten der Nachhaltigkeit nutzen"@de .

dtd:LA-id1632 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fr den Ausbildungsbetrieb geltende Regelungen des Umweltschutzes einhalten"@de .

dtd:LA-id1633 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Abflle vermeiden sowie Stoffe und Materialien einer umweltschonenden Wiederverwertung oder Entsorgung zufhren"@de .

dtd:LA-id1634 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorschlge fr nachhaltiges Handeln fr den eigenen Arbeitsbereich entwickeln"@de .

dtd:LA-id1635 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "unter Einhaltung betrieblicher Regelungen im Sinne einer konomischen, kologischen und sozial nachhaltigen Entwicklung zusammenarbeiten und adressatengerecht kommunizieren"@de .

dtd:LA-id1636 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit eigenen und betriebsbezogenen Daten sowie mit Daten Dritter umgehen und dabei die Vorschriften zum Datenschutz und zur Datensicherheit einhalten"@de .

dtd:LA-id1637 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Risiken bei der Nutzung von digitalen Medien und informationstechnischen Systemen einschtzen und bei deren Nutzung betriebliche Regelungen einhalten"@de .

dtd:LA-id1638 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ressourcenschonend, adressatengerecht und effizient kommunizieren sowie Kommunikationsergebnisse dokumentieren"@de .

dtd:LA-id1639 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Strungen in Kommunikationsprozessen erkennen und zu ihrer Lsung beitragen"@de .

dtd:LA-id1640 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Informationen in digitalen Netzen recherchieren und aus digitalen Netzen beschaffen sowie Informationen, auch fremde, prfen, bewerten und auswhlen"@de .

dtd:LA-id1641 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Lern- und Arbeitstechniken sowie Methoden des selbstgesteuerten Lernens anwenden, digitale Lernmedien nutzen und Erfordernisse des lebensbegleitenden Lernens erkennen und ableiten"@de .

dtd:LA-id1642 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufgaben zusammen mit Beteiligten, einschlielich der Beteiligten anderer Arbeits- und Geschftsbereiche, auch unter Nutzung digitaler Medien, planen, bearbeiten und gestalten"@de .

dtd:LA-id1643 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wertschtzung anderer unter Bercksichtigung gesellschaftlicher Vielfalt praktizieren"@de .

dtd:LA-id1644 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Informations- und Kommunikationsmglichkeiten aufgabenbezogen auswhlen und nutzen"@de .

dtd:LA-id1645 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nautische und technische Informationen zur Wahrung der Sicherheit des Schiffsverkehrs einholen, insbesondere ber den Binnenschifffahrtsinformationsdienst"@de .

dtd:LA-id1646 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Funkverkehr aufgaben- und situationsorientiert einsetzen"@de .

dtd:LA-id1647 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fremdsprachliche Fachbegriffe anwenden"@de .


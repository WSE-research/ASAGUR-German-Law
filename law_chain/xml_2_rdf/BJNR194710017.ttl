    dtd:has_Group dtd:column ;
    dtd:has_ID "F798863_01_BJNR194710017"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Dieses Gesetz dient der Umsetzung der Richtlinie 2014/53/EU des Europäischen Parlaments und des Rates vom 16. April 2014 über die Harmonisierung der Rechtsvorschriften der Mitgliedstaaten über die Bereitstellung von Funkanlagen auf dem Markt und zur Aufhebung der Richtlinie 1999/5/EG (ABl. L 153 vom 22.5.2014, S. 62)."@de ;
    dtd:has_noindex dtd:noindex-id381 .


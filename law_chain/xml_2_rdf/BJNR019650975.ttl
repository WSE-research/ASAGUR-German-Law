dtd:LA-id542 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Datum seines Erlasses,"@de .

dtd:LA-id543 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Namen der Mitglieder des Wahlvorstandes,"@de .

dtd:LA-id544 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Voraussetzungen der Wählbarkeit zur Schwerbehindertenvertretung"@de .

dtd:LA-id545 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Hinweis, wo und wann die Liste der Wahlberechtigten und diese Verordnung zur Einsicht ausliegen,"@de .

dtd:LA-id546 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Hinweis, dass nur wählen kann, wer in die Liste der Wahlberechtigten eingetragen ist und dass Einsprüche gegen die Richtigkeit der Liste der Wahlberechtigten nur vor Ablauf von zwei Wochen seit dem Erlaß des Wahlausschreibens beim Wahlvorstand schriftlich eingelegt werden können; der letzte Tag der Frist ist anzugeben,"@de .

dtd:LA-id547 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der zu wählenden stellvertretenden Mitglieder,"@de .

dtd:LA-id548 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Hinweis, daß Schwerbehindertenvertretung und stellvertretende Mitglieder in zwei getrennten Wahlgängen gewählt werden und daß sich aus den Wahlvorschlägen ergeben muß, wer als Schwerbehindertenvertretung und wer als stellvertretende Mitglieder vorgeschlagen wird,"@de .

dtd:LA-id549 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Hinweis, daß Wahlberechtigte sowohl einen Wahlvorschlag für die Wahl der Schwerbehindertenvertretung als auch für die Wahl des stellvertretenden Mitglieds unterzeichnen können und daß ein Bewerber oder eine Bewerberin sowohl als Schwerbehindertenvertretung als auch als stellvertretendes Mitglied vorgeschlagen werden kann,"@de .

dtd:LA-id550 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Aufforderung, Wahlvorschläge innerhalb von zwei Wochen nach Erlaß des Wahlausschreibens beim Wahlvorstand einzureichen; der letzte Tag der Frist ist anzugeben,"@de .

dtd:LA-id551 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mindestzahl von Wahlberechtigten, von denen ein Wahlvorschlag unterzeichnet sein muß (§ 6 Abs. 2 Satz 1),"@de .

dtd:LA-id552 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Hinweis, daß die Stimmabgabe an die Wahlvorschläge gebunden ist und daß nur solche Wahlvorschläge berücksichtigt werden dürfen, die fristgerecht (Nummer 9) eingereicht sind,"@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bestimmung des Ortes, an dem die Wahlvorschläge bis zum Abschluß der Stimmabgabe durch Aushang oder in sonst geeigneter Weise bekanntgegeben werden,"@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ort, Tag und Zeit der Stimmabgabe,"@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Hinweis auf die Möglichkeit der schriftlichen Stimmabgabe (§ 11 Abs. 1), falls der Wahlvorstand nicht die schriftliche Stimmabgabe beschlossen hat (§ 11 Abs. 2),"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Ort und die Zeit der Stimmauszählung und der Sitzung des Wahlvorstandes, in der das Wahlergebnis abschließend festgestellt wird,"@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Ort, an dem Einsprüche, Wahlvorschläge und sonstige Erklärungen gegenüber dem Wahlvorstand abzugeben sind (Anschrift des Wahlvorstandes)."@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Wahlausschreiben,"@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Stimmzettel und den Wahlumschlag,"@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine vorgedruckte Erklärung, die der Wähler oder die Wählerin abgibt,"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen größeren Freiumschlag, der die Anschrift des Wahlvorstandes und als Absender Namen und Anschrift der wahlberechtigten Person sowie den Vermerk \"Schriftliche Stimmabgabe\" trägt."@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Stimmzettel unbeobachtet persönlich kennzeichnet und in den Wahlumschlag einlegt,"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die vorgedruckte Erklärung unter Angabe des Ortes und des Datums unterschreibt und"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Wahlumschlag und die unterschriebene, vorgedruckte Erklärung in dem Freiumschlag verschließt und diesen so rechtzeitig an den Wahlvorstand absendet oder übergibt, daß er vor Abschluß der Wahl vorliegt."@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Voraussetzungen der Wählbarkeit zur Schwerbehindertenvertretung"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Hinweis über eine für Zwecke der Wahl erfolgte Zusammenfassung von Gerichten,"@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Hinweis, wo und wann die Liste der Wahlberechtigten und diese Verordnung zur Einsicht ausliegen,"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ort, Tag und Zeit der Wahlversammlung."@de .


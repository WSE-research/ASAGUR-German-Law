dtd:I-id3521 a dtd:I ;
    dtd:has_Value "Meinungsaustausch"@de .

dtd:LA-id5149 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gesellschaft börsennotiert ist oder"@de .

dtd:LA-id5150 a dtd:LA ;
    dtd:has_DL dtd:DL-id3995 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Anspruch auf Einzelverbriefung ausgeschlossen ist und die Sammelurkunde bei einer der folgenden Stellen hinterlegt wird: a)einer Wertpapiersammelbank im Sinne des § 1 Absatz 3 Satz 1 des Depotgesetzes,b)einem zugelassenen Zentralverwahrer oder einem anerkannten Drittland-Zentralverwahrer gemäß der Verordnung (EU) Nr. 909/2014 des Europäischen Parlaments und des Rates vom 23. Juli 2014 zur Verbesserung der Wertpapierlieferungen und -abrechnungen in der Europäischen Union und über Zentralverwahrer sowie zur Änderung der Richtlinien 98/26/EG und 2014/65/EU und der Verordnung (EU) Nr. 236/2012 (ABl. L 257 vom 28.8.2014, S. 1) oderc)einem sonstigen ausländischen Verwahrer, der die Voraussetzungen des § 5 Absatz 4 Satz 1 des Depotgesetzes erfüllt, oder"@de .

dtd:LA-id5151 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Wertpapiersammelbank im Sinne des § 1 Absatz 3 Satz 1 des Depotgesetzes,"@de .

dtd:LA-id5152 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem zugelassenen Zentralverwahrer oder einem anerkannten Drittland-Zentralverwahrer gemäß der Verordnung (EU) Nr. 909/2014 des Europäischen Parlaments und des Rates vom 23. Juli 2014 zur Verbesserung der Wertpapierlieferungen und -abrechnungen in der Europäischen Union und über Zentralverwahrer sowie zur Änderung der Richtlinien 98/26/EG und 2014/65/EU und der Verordnung (EU) Nr. 236/2012 (ABl. L 257 vom 28.8.2014, S. 1) oder"@de .

dtd:LA-id5153 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem sonstigen ausländischen Verwahrer, der die Voraussetzungen des § 5 Absatz 4 Satz 1 des Depotgesetzes erfüllt, oder"@de .

dtd:LA-id5154 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verbriefung ausgeschlossen ist und die Aktie in einem zentralen Wertpapierregister gemäß § 12 des Gesetzes über elektronische Wertpapiere eingetragen wird."@de .

dtd:LA-id5155 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "deren Übereignung das Unternehmen, ein von ihm abhängiges Unternehmen oder ein anderer für Rechnung des Unternehmens oder eines von diesem abhängigen Unternehmens verlangen kann;"@de .

dtd:LA-id5156 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu deren Abnahme das Unternehmen, ein von ihm abhängiges Unternehmen oder ein anderer für Rechnung des Unternehmens oder eines von diesem abhängigen Unternehmens verpflichtet ist."@de .

dtd:LA-id5157 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gründer;"@de .

dtd:LA-id5158 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Nennbetragsaktien der Nennbetrag, bei Stückaktien die Zahl, der Ausgabebetrag und, wenn mehrere Gattungen bestehen, die Gattung der Aktien, die jeder Gründer übernimmt;"@de .

dtd:LA-id5159 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der eingezahlte Betrag des Grundkapitals."@de .

dtd:LA-id5160 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Firma und den Sitz der Gesellschaft;"@de .

dtd:LA-id5161 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Gegenstand des Unternehmens; namentlich ist bei Industrie- und Handelsunternehmen die Art der Erzeugnisse und Waren, die hergestellt und gehandelt werden sollen, näher anzugeben;"@de .

dtd:LA-id5162 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Höhe des Grundkapitals;"@de .

dtd:LA-id5163 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zerlegung des Grundkapitals entweder in Nennbetragsaktien oder in Stückaktien, bei Nennbetragsaktien deren Nennbeträge und die Zahl der Aktien jeden Nennbetrags, bei Stückaktien deren Zahl, außerdem, wenn mehrere Gattungen bestehen, die Gattung der Aktien und die Zahl der Aktien jeder Gattung;"@de .

dtd:LA-id5164 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ob die Aktien auf den Inhaber oder auf den Namen ausgestellt werden;"@de .

dtd:LA-id5165 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der Mitglieder des Vorstands oder die Regeln, nach denen diese Zahl festgelegt wird."@de .

dtd:LA-id5166 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die vorausgegangenen Rechtsgeschäfte, die auf den Erwerb durch die Gesellschaft hingezielt haben;"@de .

dtd:LA-id5167 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anschaffungs- und Herstellungskosten aus den letzten beiden Jahren;"@de .

dtd:LA-id5168 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "beim Übergang eines Unternehmens auf die Gesellschaft die Betriebserträge aus den letzten beiden Geschäftsjahren."@de .

dtd:LA-id5169 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Mitglied des Vorstands oder des Aufsichtsrats zu den Gründern gehört oder"@de .

dtd:LA-id5170 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Gründung für Rechnung eines Mitglieds des Vorstands oder des Aufsichtsrats Aktien übernommen worden sind oder"@de .

dtd:LA-id5171 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Mitglied des Vorstands oder des Aufsichtsrats sich einen besonderen Vorteil oder für die Gründung oder ihre Vorbereitung eine Entschädigung oder Belohnung ausbedungen hat oder"@de .

dtd:LA-id5172 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Gründung mit Sacheinlagen oder Sachübernahmen vorliegt."@de .

dtd:LA-id5173 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, die in der Buchführung ausreichend vorgebildet und erfahren sind;"@de .

dtd:LA-id5174 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Prüfungsgesellschaften, von deren gesetzlichen Vertretern mindestens einer in der Buchführung ausreichend vorgebildet und erfahren ist."@de .

dtd:LA-id5175 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "übertragbare Wertpapiere oder Geldmarktinstrumente im Sinne des § 2 Absatz 1 und 2 des Wertpapierhandelsgesetzes, wenn sie mit dem gewichteten Durchschnittspreis bewertet werden, zu dem sie während der letzten drei Monate vor dem Tag ihrer tatsächlichen Einbringung auf einem oder mehreren organisierten Märkten im Sinne von § 2 Absatz 11 des Wertpapierhandelsgesetzes gehandelt worden sind,"@de .

dtd:LA-id5176 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "andere als die in Nummer 1 genannten Vermögensgegenstände, wenn eine Bewertung zu Grunde gelegt wird, die ein unabhängiger, ausreichend vorgebildeter und erfahrener Sachverständiger nach den allgemein anerkannten Bewertungsgrundsätzen mit dem beizulegenden Zeitwert ermittelt hat und wenn der Bewertungsstichtag nicht mehr als sechs Monate vor dem Tag der tatsächlichen Einbringung liegt."@de .

dtd:LA-id5177 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ob die Angaben der Gründer über die Übernahme der Aktien, über die Einlagen auf das Grundkapital und über die Festsetzungen nach §§ 26 und 27 richtig und vollständig sind;"@de .

dtd:LA-id5178 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ob der Wert der Sacheinlagen oder Sachübernahmen den geringsten Ausgabebetrag der dafür zu gewährenden Aktien oder den Wert der dafür zu gewährenden Leistungen erreicht."@de .

dtd:LA-id5179 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine inländische Geschäftsanschrift,"@de .

dtd:LA-id5180 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Art und Umfang der Vertretungsbefugnis der Vorstandsmitglieder."@de .

dtd:LA-id5181 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Satzung und die Urkunden, in denen die Satzung festgestellt worden ist und die Aktien von den Gründern übernommen worden sind;"@de .

dtd:LA-id5182 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall der §§ 26 und 27 die Verträge, die den Festsetzungen zugrunde liegen oder zu ihrer Ausführung geschlossen worden sind, und eine Berechnung des der Gesellschaft zur Last fallenden Gründungsaufwands; in der Berechnung sind die Vergütungen nach Art und Höhe und die Empfänger einzeln anzuführen;"@de .

dtd:LA-id5183 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Urkunden über die Bestellung des Vorstands und des Aufsichtsrats;"@de .

dtd:LA-id5184 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Liste der Mitglieder des Aufsichtsrats, aus welcher Name, Vorname, ausgeübter Beruf und Wohnort der Mitglieder ersichtlich ist;"@de .

dtd:LA-id5185 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gründungsbericht und die Prüfungsberichte der Mitglieder des Vorstands und des Aufsichtsrats sowie der Gründungsprüfer nebst ihren urkundlichen Unterlagen."@de .

dtd:LA-id5186 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id5187 a dtd:LA ;
    dtd:has_Value "Unterlagen über die Ermittlung des gewichteten Durchschnittspreises, zu dem die einzubringenden Wertpapiere oder Geldmarktinstrumente während der letzten drei Monate vor dem Tag ihrer tatsächlichen Einbringung auf einem organisierten Markt gehandelt worden sind,"@de .

dtd:LA-id5188 a dtd:LA ;
    dtd:has_Value "jedes Sachverständigengutachten, auf das sich die Bewertung in den Fällen des § 33a Abs. 1 Nr. 2 stützt."@de .

dtd:LA-id5189 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tatsachen oder Rechtsverhältnisse betrifft, die nach § 23 Abs. 3 oder auf Grund anderer zwingender gesetzlicher Vorschriften in der Satzung bestimmt sein müssen oder die in das Handelsregister einzutragen oder von dem Gericht bekanntzumachen sind,"@de .

dtd:LA-id5190 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorschriften verletzt, die ausschließlich oder überwiegend zum Schutze der Gläubiger der Gesellschaft oder sonst im öffentlichen Interesse gegeben sind, oder"@de .

dtd:LA-id5191 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Nichtigkeit der Satzung zur Folge hat."@de .

dtd:LA-id5192 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer bei Empfang einer Vergütung, die entgegen den Vorschriften nicht in den Gründungsaufwand aufgenommen ist, wußte oder nach den Umständen annehmen mußte, daß die Verheimlichung beabsichtigt oder erfolgt war, oder wer zur Verheimlichung wissentlich mitgewirkt hat;"@de .

dtd:LA-id5193 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer im Fall einer vorsätzlichen oder grobfahrlässigen Schädigung der Gesellschaft durch Einlagen oder Sachübernahmen an der Schädigung wissentlich mitgewirkt hat;"@de .

dtd:LA-id5194 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer vor Eintragung der Gesellschaft in das Handelsregister oder in den ersten zwei Jahren nach der Eintragung die Aktien öffentlich ankündigt, um sie in den Verkehr einzuführen, wenn er die Unrichtigkeit oder Unvollständigkeit der Angaben, die zum Zwecke der Gründung der Gesellschaft gemacht worden sind (§ 46 Abs. 1), oder die Schädigung der Gesellschaft durch Einlagen oder Sachübernahmen kannte oder bei Anwendung der Sorgfalt eines ordentlichen Geschäftsmanns kennen mußte."@de .

dtd:LA-id5195 a dtd:LA ;
    dtd:has_Value "an die im Aktienregister Eingetragenen, soweit die Gesellschaft Namensaktien ausgegeben hat,"@de .

dtd:LA-id5196 a dtd:LA ;
    dtd:has_Value "im Übrigen an die Intermediäre, die Aktien der Gesellschaft verwahren."@de .

dtd:LA-id5197 a dtd:LA ;
    dtd:has_Value "die Kosten für die notwendigen Aufwendungen der Letztintermediäre für die nichtelektronische Übermittlung von Informationen an den Aktionär gemäß § 67b Absatz 1 Satz 1 und"@de .

dtd:LA-id5198 a dtd:LA ;
    dtd:has_Value "bei der Gesellschaft, die Namensaktien ausgegeben hat, die Kosten für die notwendigen Aufwendungen der Intermediäre für die Übermittlung und Weiterleitung von Informationen vom im Aktienregister eingetragenen Intermediär an den Aktionär nach § 125 Absatz 2 und 5 in Verbindung mit den §§ 67a und 67b."@de .

dtd:LA-id5199 a dtd:LA ;
    dtd:has_Value "die Übermittlung der Angaben gemäß § 67 Absatz 4,"@de .

dtd:LA-id5200 a dtd:LA ;
    dtd:has_Value "die Übermittlung und Weiterleitung von Informationen und Mitteilungen gemäß den §§ 67a bis 67d, 118 Absatz 1 Satz 3 bis 5 sowie Absatz 2 Satz 2, § 118a Absatz 1 Satz 4 und § 129 Absatz 5 und"@de .

dtd:LA-id5201 a dtd:LA ;
    dtd:has_Value "die Vervielfältigung, Übermittlung und Weiterleitung der Mitteilungen gemäß § 125 Absatz 1, 2 und 5 in Verbindung mit den §§ 67a und 67b."@de .

dtd:LA-id5202 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn der Erwerb notwendig ist, um einen schweren, unmittelbar bevorstehenden Schaden von der Gesellschaft abzuwenden,"@de .

dtd:LA-id5203 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn die Aktien Personen, die im Arbeitsverhältnis zu der Gesellschaft oder einem mit ihr verbundenen Unternehmen stehen oder standen, zum Erwerb angeboten werden sollen,"@de .

dtd:LA-id5204 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn der Erwerb geschieht, um Aktionäre nach § 305 Abs. 2, § 320b oder nach § 29 Abs. 1, § 125 Satz 1 in Verbindung mit § 29 Abs. 1, § 207 Abs. 1 Satz 1, § 313 Absatz 1, auch in Verbindung mit § 327, oder § 340 Absatz 1 des Umwandlungsgesetzes abzufinden,"@de .

dtd:LA-id5205 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn der Erwerb unentgeltlich geschieht oder ein Kreditinstitut oder Wertpapierinstitut mit dem Erwerb eine Einkaufskommission ausführt,"@de .

dtd:LA-id5206 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Gesamtrechtsnachfolge,"@de .

dtd:LA-id5207 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Grund eines Beschlusses der Hauptversammlung zur Einziehung nach den Vorschriften über die Herabsetzung des Grundkapitals,"@de .

dtd:LA-id5208 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn sie ein Kreditinstitut, ein Finanzdienstleistungsinstitut, ein Wertpapierinstitut oder ein Finanzunternehmen ist, aufgrund eines Beschlusses der Hauptversammlung zum Zwecke des Wertpapierhandels. Der Beschluß muß bestimmen, daß der Handelsbestand der zu diesem Zweck zu erwerbenden Aktien fünf vom Hundert des Grundkapitals am Ende jeden Tages nicht übersteigen darf; er muß den niedrigsten und höchsten Gegenwert festlegen. Die Ermächtigung darf höchstens fünf Jahre gelten; oder"@de .

dtd:LA-id5209 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aufgrund einer höchstens fünf Jahre geltenden Ermächtigung der Hauptversammlung, die den niedrigsten und höchsten Gegenwert sowie den Anteil am Grundkapital, der zehn vom Hundert nicht übersteigen darf, festlegt. Als Zweck ist der Handel in eigenen Aktien ausgeschlossen. § 53a ist auf Erwerb und Veräußerung anzuwenden. Erwerb und Veräußerung über die Börse genügen dem. Eine andere Veräußerung kann die Hauptversammlung beschließen; § 186 Abs. 3, 4 und § 193 Abs. 2 Nr. 4 sind in diesem Fall entsprechend anzuwenden. Die Hauptversammlung kann den Vorstand ermächtigen, die eigenen Aktien ohne weiteren Hauptversammlungsbeschluß einzuziehen."@de .

dtd:LA-id5210 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Betreuter bei der Besorgung seiner Vermögensangelegenheiten ganz oder teilweise einem Einwilligungsvorbehalt (§ 1825 des Bürgerlichen Gesetzbuchs) unterliegt,"@de .

dtd:LA-id5211 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aufgrund eines gerichtlichen Urteils oder einer vollziehbaren Entscheidung einer Verwaltungsbehörde einen Beruf, einen Berufszweig, ein Gewerbe oder einen Gewerbezweig nicht ausüben darf, sofern der Unternehmensgegenstand ganz oder teilweise mit dem Gegenstand des Verbots übereinstimmt,"@de .

dtd:LA-id5212 a dtd:LA ;
    dtd:has_DL dtd:DL-id4136 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wegen einer oder mehrerer vorsätzlich begangener Straftaten a)des Unterlassens der Stellung des Antrags auf Eröffnung des Insolvenzverfahrens (Insolvenzverschleppung),b)nach den §§ 283 bis 283d des Strafgesetzbuchs (Insolvenzstraftaten),c)der falschen Angaben nach § 399 dieses Gesetzes oder § 82 des Gesetzes betreffend die Gesellschaften mit beschränkter Haftung,d)der unrichtigen Darstellung nach § 400 dieses Gesetzes, § 331 des Handelsgesetzbuchs, § 346 des Umwandlungsgesetzes oder § 17 des Publizitätsgesetzes,e)nach den §§ 263 bis 264a oder den §§ 265b bis 266a des Strafgesetzbuchs zu einer Freiheitsstrafe von mindestens einem Jahrverurteilt worden ist; dieser Ausschluss gilt für die Dauer von fünf Jahren seit der Rechtskraft des Urteils, wobei die Zeit nicht eingerechnet wird, in welcher der Täter auf behördliche Anordnung in einer Anstalt verwahrt worden ist."@de .

dtd:LA-id5213 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Unterlassens der Stellung des Antrags auf Eröffnung des Insolvenzverfahrens (Insolvenzverschleppung),"@de .

dtd:LA-id5214 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach den §§ 283 bis 283d des Strafgesetzbuchs (Insolvenzstraftaten),"@de .

dtd:LA-id5215 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der falschen Angaben nach § 399 dieses Gesetzes oder § 82 des Gesetzes betreffend die Gesellschaften mit beschränkter Haftung,"@de .

dtd:LA-id5216 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der unrichtigen Darstellung nach § 400 dieses Gesetzes, § 331 des Handelsgesetzbuchs, § 346 des Umwandlungsgesetzes oder § 17 des Publizitätsgesetzes,"@de .

dtd:LA-id5217 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach den §§ 263 bis 264a oder den §§ 265b bis 266a des Strafgesetzbuchs zu einer Freiheitsstrafe von mindestens einem Jahr"@de .

dtd:LA-id5218 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall des Mutterschutzes widerrufen und dabei die Wiederbestellung nach Ablauf des Zeitraums der in § 3 Absatz 1 und 2 des Mutterschutzgesetzes genannten Schutzfristen zusichern,"@de .

dtd:LA-id5219 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Fällen der Elternzeit, der Pflege eines Familienangehörigen oder der Krankheit widerrufen und dabei die Wiederbestellung nach einem Zeitraum von bis zu drei Monaten entsprechend dem Verlangen des Vorstandsmitglieds zusichern; der Aufsichtsrat kann von dem Widerruf der Bestellung absehen, wenn ein wichtiger Grund vorliegt."@de .

dtd:LA-id5220 a dtd:LA ;
    dtd:has_Value "die Festlegung einer Maximalvergütung der Vorstandsmitglieder;"@de .

dtd:LA-id5221 a dtd:LA ;
    dtd:has_Value "den Beitrag der Vergütung zur Förderung der Geschäftsstrategie und zur langfristigen Entwicklung der Gesellschaft;"@de .

dtd:LA-id5222 a dtd:LA ;
    dtd:has_Value "alle festen und variablen Vergütungsbestandteile und ihren jeweiligen relativen Anteil an der Vergütung;"@de .

dtd:LA-id5223 a dtd:LA ;
    dtd:has_DL dtd:DL-id4175 ;
    dtd:has_Value "alle finanziellen und nichtfinanziellen Leistungskriterien für die Gewährung variabler Vergütungsbestandteile einschließlich a)einer Erläuterung, wie diese Kriterien zur Förderung der Ziele gemäß Nummer 2 beitragen, undb)einer Darstellung der Methoden, mit denen die Erreichung der Leistungskriterien festgestellt wird;"@de .

dtd:LA-id5224 a dtd:LA ;
    dtd:has_Value "einer Erläuterung, wie diese Kriterien zur Förderung der Ziele gemäß Nummer 2 beitragen, und"@de .

dtd:LA-id5225 a dtd:LA ;
    dtd:has_Value "einer Darstellung der Methoden, mit denen die Erreichung der Leistungskriterien festgestellt wird;"@de .

dtd:LA-id5226 a dtd:LA ;
    dtd:has_Value "Aufschubzeiten für die Auszahlung von Vergütungsbestandteilen;"@de .

dtd:LA-id5227 a dtd:LA ;
    dtd:has_Value "Möglichkeiten der Gesellschaft, variable Vergütungsbestandteile zurückzufordern;"@de .

dtd:LA-id5228 a dtd:LA ;
    dtd:has_DL dtd:DL-id4180 ;
    dtd:has_Value "im Falle aktienbasierter Vergütung: a)Fristen,b)die Bedingungen für das Halten von Aktien nach dem Erwerb undc)eine Erläuterung, wie diese Vergütung zur Förderung der Ziele gemäß Nummer 2 beiträgt;"@de .

dtd:LA-id5229 a dtd:LA ;
    dtd:has_Value "Fristen,"@de .

dtd:LA-id5230 a dtd:LA ;
    dtd:has_Value "die Bedingungen für das Halten von Aktien nach dem Erwerb und"@de .

dtd:LA-id5231 a dtd:LA ;
    dtd:has_Value "eine Erläuterung, wie diese Vergütung zur Förderung der Ziele gemäß Nummer 2 beiträgt;"@de .

dtd:LA-id5232 a dtd:LA ;
    dtd:has_DL dtd:DL-id4187 ;
    dtd:has_Value "hinsichtlich vergütungsbezogener Rechtsgeschäfte: a)die Laufzeiten und die Voraussetzungen ihrer Beendigung, einschließlich der jeweiligen Kündigungsfristen,b)etwaige Zusagen von Entlassungsentschädigungen undc)die Hauptmerkmale der Ruhegehalts- und Vorruhestandsregelungen;"@de .

dtd:LA-id5233 a dtd:LA ;
    dtd:has_Value "die Laufzeiten und die Voraussetzungen ihrer Beendigung, einschließlich der jeweiligen Kündigungsfristen,"@de .

dtd:LA-id5234 a dtd:LA ;
    dtd:has_Value "etwaige Zusagen von Entlassungsentschädigungen und"@de .

dtd:LA-id5235 a dtd:LA ;
    dtd:has_Value "die Hauptmerkmale der Ruhegehalts- und Vorruhestandsregelungen;"@de .

dtd:LA-id5236 a dtd:LA ;
    dtd:has_Value "eine Erläuterung, wie die Vergütungs- und Beschäftigungsbedingungen der Arbeitnehmer bei der Festsetzung des Vergütungssystems berücksichtigt wurden, einschließlich einer Erläuterung, welcher Kreis von Arbeitnehmern einbezogen wurde;"@de .

dtd:LA-id5237 a dtd:LA ;
    dtd:has_Value "eine Darstellung des Verfahrens zur Fest- und zur Umsetzung sowie zur Überprüfung des Vergütungssystems, einschließlich der Rolle eventuell betroffener Ausschüsse und der Maßnahmen zur Vermeidung und zur Behandlung von Interessenkonflikten;"@de .

dtd:LA-id5238 a dtd:LA ;
    dtd:has_DL dtd:DL-id4194 ;
    dtd:has_Value "im Fall der Vorlage eines gemäß § 120a Absatz 3 überprüften Vergütungssystems: a)eine Erläuterung aller wesentlichen Änderungen undb)eine Übersicht, inwieweit Abstimmung und Äußerungen der Aktionäre in Bezug auf das Vergütungssystem und die Vergütungsberichte berücksichtigt wurden."@de .

dtd:LA-id5239 a dtd:LA ;
    dtd:has_Value "eine Erläuterung aller wesentlichen Änderungen und"@de .

dtd:LA-id5240 a dtd:LA ;
    dtd:has_Value "eine Übersicht, inwieweit Abstimmung und Äußerungen der Aktionäre in Bezug auf das Vergütungssystem und die Vergütungsberichte berücksichtigt wurden."@de .

dtd:LA-id5241 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die beabsichtigte Geschäftspolitik und andere grundsätzliche Fragen der Unternehmensplanung (insbesondere die Finanz-, Investitions- und Personalplanung), wobei auf Abweichungen der tatsächlichen Entwicklung von früher berichteten Zielen unter Angabe von Gründen einzugehen ist;"@de .

dtd:LA-id5242 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Rentabilität der Gesellschaft, insbesondere die Rentabilität des Eigenkapitals;"@de .

dtd:LA-id5243 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Gang der Geschäfte, insbesondere den Umsatz, und die Lage der Gesellschaft;"@de .

dtd:LA-id5244 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geschäfte, die für die Rentabilität oder Liquidität der Gesellschaft von erheblicher Bedeutung sein können."@de .

dtd:LA-id5245 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Berichte nach Nummer 1 mindestens einmal jährlich, wenn nicht Änderungen der Lage oder neue Fragen eine unverzügliche Berichterstattung gebieten;"@de .

dtd:LA-id5246 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Berichte nach Nummer 2 in der Sitzung des Aufsichtsrats, in der über den Jahresabschluß verhandelt wird;"@de .

dtd:LA-id5247 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Berichte nach Nummer 3 regelmäßig, mindestens vierteljährlich;"@de .

dtd:LA-id5248 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Berichte nach Nummer 4 möglichst so rechtzeitig, daß der Aufsichtsrat vor Vornahme der Geschäfte Gelegenheit hat, zu ihnen Stellung zu nehmen."@de .

dtd:LA-id5249 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einlagen an die Aktionäre zurückgewährt werden,"@de .

dtd:LA-id5250 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Aktionären Zinsen oder Gewinnanteile gezahlt werden,"@de .

dtd:LA-id5251 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eigene Aktien der Gesellschaft oder einer anderen Gesellschaft gezeichnet, erworben, als Pfand genommen oder eingezogen werden,"@de .

dtd:LA-id5252 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aktien vor der vollen Leistung des Ausgabebetrags ausgegeben werden,"@de .

dtd:LA-id5253 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gesellschaftsvermögen verteilt wird,"@de .

dtd:LA-id5254 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id5255 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vergütungen an Aufsichtsratsmitglieder gewährt werden,"@de .

dtd:LA-id5256 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kredit gewährt wird,"@de .

dtd:LA-id5257 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der bedingten Kapitalerhöhung außerhalb des festgesetzten Zwecks oder vor der vollen Leistung des Gegenwerts Bezugsaktien ausgegeben werden."@de .

dtd:LA-id5258 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Vorstand,"@de .

dtd:LA-id5259 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jedes Aufsichtsratsmitglied,"@de .

dtd:LA-id5260 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jeder Aktionär,"@de .

dtd:LA-id5261 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gesamtbetriebsrat der Gesellschaft oder, wenn in der Gesellschaft nur ein Betriebsrat besteht, der Betriebsrat,"@de .

dtd:LA-id5262 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gesamt- oder Unternehmenssprecherausschuss der Gesellschaft oder, wenn in der Gesellschaft nur ein Sprecherausschuss besteht, der Sprecherausschuss,"@de .

dtd:LA-id5263 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gesamtbetriebsrat eines anderen Unternehmens, dessen Arbeitnehmer nach den gesetzlichen Vorschriften, deren Anwendung streitig oder ungewiß ist, selbst oder durch Delegierte an der Wahl von Aufsichtsratsmitgliedern der Gesellschaft teilnehmen, oder, wenn in dem anderen Unternehmen nur ein Betriebsrat besteht, der Betriebsrat,"@de .

dtd:LA-id5264 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gesamt- oder Unternehmenssprecherausschuss eines anderen Unternehmens, dessen Arbeitnehmer nach den gesetzlichen Vorschriften, deren Anwendung streitig oder ungewiss ist, selbst oder durch Delegierte an der Wahl von Aufsichtsratsmitgliedern der Gesellschaft teilnehmen, oder, wenn in dem anderen Unternehmen nur ein Sprecherausschuss besteht, der Sprecherausschuss,"@de .

dtd:LA-id5265 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens ein Zehntel oder einhundert der Arbeitnehmer, die nach den gesetzlichen Vorschriften, deren Anwendung streitig oder ungewiß ist, selbst oder durch Delegierte an der Wahl von Aufsichtsratsmitgliedern der Gesellschaft teilnehmen,"@de .

dtd:LA-id5266 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Spitzenorganisationen der Gewerkschaften, die nach den gesetzlichen Vorschriften, deren Anwendung streitig oder ungewiß ist, ein Vorschlagsrecht hätten,"@de .

dtd:LA-id5267 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gewerkschaften, die nach den gesetzlichen Vorschriften, deren Anwendung streitig oder ungewiß ist, ein Vorschlagsrecht hätten."@de .

dtd:LA-id5268 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bereits in zehn Handelsgesellschaften, die gesetzlich einen Aufsichtsrat zu bilden haben, Aufsichtsratsmitglied ist,"@de .

dtd:LA-id5269 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gesetzlicher Vertreter eines von der Gesellschaft abhängigen Unternehmens ist,"@de .

dtd:LA-id5270 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gesetzlicher Vertreter einer anderen Kapitalgesellschaft ist, deren Aufsichtsrat ein Vorstandsmitglied der Gesellschaft angehört, oder"@de .

dtd:LA-id5271 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den letzten zwei Jahren Vorstandsmitglied derselben börsennotierten Gesellschaft war, es sei denn, seine Wahl erfolgt auf Vorschlag von Aktionären, die mehr als 25 Prozent der Stimmrechte an der Gesellschaft halten."@de .

dtd:LA-id5272 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gesamtbetriebsrat der Gesellschaft oder, wenn in der Gesellschaft nur ein Betriebsrat besteht, der Betriebsrat, sowie, wenn die Gesellschaft herrschendes Unternehmen eines Konzerns ist, der Konzernbetriebsrat,"@de .

dtd:LA-id5273 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gesamt- oder Unternehmenssprecherausschuss der Gesellschaft oder, wenn in der Gesellschaft nur ein Sprecherausschuss besteht, der Sprecherausschuss sowie, wenn die Gesellschaft herrschendes Unternehmen eines Konzerns ist, der Konzernsprecherausschuss,"@de .

dtd:LA-id5274 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gesamtbetriebsrat eines anderen Unternehmens, dessen Arbeitnehmer selbst oder durch Delegierte an der Wahl teilnehmen, oder, wenn in dem anderen Unternehmen nur ein Betriebsrat besteht, der Betriebsrat,"@de .

dtd:LA-id5275 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gesamt- oder Unternehmenssprecherausschuss eines anderen Unternehmens, dessen Arbeitnehmer selbst oder durch Delegierte an der Wahl teilnehmen, oder, wenn in dem anderen Unternehmen nur ein Sprecherausschuss besteht, der Sprecherausschuss,"@de .

dtd:LA-id5276 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens ein Zehntel oder einhundert der Arbeitnehmer, die selbst oder durch Delegierte an der Wahl teilnehmen,"@de .

dtd:LA-id5277 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Spitzenorganisationen der Gewerkschaften, die das Recht haben, Aufsichtsratsmitglieder der Arbeitnehmer vorzuschlagen,"@de .

dtd:LA-id5278 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gewerkschaften, die das Recht haben, Aufsichtsratsmitglieder der Arbeitnehmer vorzuschlagen."@de .

dtd:LA-id5279 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "daß das Gericht den Aufsichtsrat hinsichtlich des weiteren Mitglieds, das nach dem Montan-Mitbestimmungsgesetz oder dem Mitbestimmungsergänzungsgesetz auf Vorschlag der übrigen Aufsichtsratsmitglieder gewählt wird, nicht ergänzen kann,"@de .

dtd:LA-id5280 a dtd:LA ;

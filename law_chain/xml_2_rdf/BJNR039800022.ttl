dtd:LA-id1580 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ressourcenschonend, adressatengerecht und effizient kommunizieren sowie Kommunikationsergebnisse dokumentieren"@de .

dtd:LA-id1581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Störungen in Kommunikationsprozessen erkennen und zu ihrer Lösung beitragen"@de .

dtd:LA-id1582 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Informationen in digitalen Netzen recherchieren und aus digitalen Netzen beschaffen sowie Informationen, auch fremde, prüfen, bewerten und auswählen"@de .

dtd:LA-id1583 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Lern- und Arbeitstechniken sowie Methoden des selbstgesteuerten Lernens anwenden, digitale Lernmedien nutzen und Erfordernisse des lebensbegleitenden Lernens erkennen und ableiten"@de .

dtd:LA-id1584 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufgaben zusammen mit Beteiligten, einschließlich der Beteiligten anderer Arbeits- und Geschäftsbereiche, auch unter Nutzung digitaler Medien, planen, bearbeiten und gestalten"@de .

dtd:LA-id1585 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wertschätzung anderer unter Berücksichtigung gesellschaftlicher Vielfalt praktizieren"@de .

dtd:LA-id1586 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Grundsätze und die Vorschriften zur Personal-, Betriebs- und Produkthygiene sowie zum Arbeits- und Gesundheitsschutz anwenden"@de .

dtd:LA-id1587 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die rechtlichen Hygienevorschriften und das betriebliche Hygienekonzept, insbesondere das HACCP-Konzept, umsetzen"@de .

dtd:LA-id1588 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schädlingsbefall erkennen und Maßnahmen einleiten"@de .

dtd:LA-id1589 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Desinfektions- und Reinigungsmittel lagern, unter Berücksichtigung der ökologischen Auswirkungen auswählen und ökonomisch einsetzen"@de .

dtd:LA-id1590 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Meldepflichten nach dem Infektionsschutzrecht beachten und die Beschäftigungsverbote einhalten"@de .

dtd:LA-id1591 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vegetarische und vegane Zutaten und Produkte hinsichtlich ihrer Verwendung einordnen, insbesondere Getreide, Vollkornprodukte, Sojaprodukte, Algen, Hülsenfrüchte, Nüsse sowie Würz- und Süßungsmittel"@de .

dtd:LA-id1592 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Möglichkeiten für Alternativprodukte aufzeigen, insbesondere Alternativen für Fleisch sowie für Eier, Milch und Käse"@de .

dtd:LA-id1593 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "traditionelle und moderne vegetarische und vegane Speisen und Menüs, insbesondere unter Berücksichtigung des Nährstoffgehalts, zusammenstellen und zubereiten"@de .

dtd:LA-id1594 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "pflanzliche Zutaten vitamin- und geschmacksschonend einlagern, haltbar machen und garen"@de .

dtd:LA-id1595 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vegetarische und vegane Produkte und Alternativprodukte beschaffen"@de .

dtd:LA-id1596 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "beim Einkauf und bei der Lieferantenauswahl insbesondere Nachhaltigkeit, Saisonalität und Regionalität berücksichtigen und Besonderheiten von Zertifikaten und Gütesiegeln kennen"@de .

dtd:LA-id1597 a dtd:LA ;

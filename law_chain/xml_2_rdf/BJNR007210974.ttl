dtd:P-id2945 a dtd:P ;
    dtd:has_BR dtd:BR-id1062 ;
    dtd:has_Value " (+++ Textnachweis Geltung ab: 1.4.1982 +++)(+++ nderungen aufgrund EinigVtr vgl.  10, 10a, 67a u. 74 +++)(+++ Amtlicher Hinweis des Normgebers auf EG-Recht:     Umsetzung der       EWGRL 50/2008 (CELEX Nr: 32008L0050) vgl. G v. 31.7.2010 I 1059        EURL 2018/2001 (CELEX Nr: 32018L2001) vgl. G v. 24.9.2021 I 4458     Notifizierung der        EURL 2015/1535 (CELEX Nr: 32015L1535) vgl. G v. 24.9.2021 I 4458 +++)"@de ;
    dtd:has_pre dtd:pre-id3478 .


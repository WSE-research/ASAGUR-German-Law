    dtd:has_Value "Art, Auswirkungen und Menge der jeweiligen Emissionen,"@de .

dtd:LA-id2867 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zeitpunkte der Inbetriebnahme der neuen oder der bestehenden Anlagen,"@de .

dtd:LA-id2868 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fr die Einfhrung einer besseren verfgbaren Technik erforderliche Zeit,"@de .

dtd:LA-id2869 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verbrauch an Rohstoffen und Art der bei den einzelnen Verfahren verwendeten Rohstoffe (einschlielich Wasser) sowie Energieeffizienz,"@de .

dtd:LA-id2870 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Notwendigkeit, die Gesamtwirkung der Emissionen und die Gefahren fr den Menschen und die Umwelt so weit wie mglich zu vermeiden oder zu verringern,"@de .

dtd:LA-id2871 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Notwendigkeit, Unfllen vorzubeugen und deren Folgen fr den Menschen und die Umwelt zu verringern,"@de .

dtd:LA-id2872 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Informationen, die von internationalen Organisationen verffentlicht werden,"@de .


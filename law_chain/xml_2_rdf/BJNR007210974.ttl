dtd:LA-id2850 a dtd:LA ;
    dtd:has_DL dtd:DL-id2425 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer unmittelbar geltenden Vorschrift in Rechtsakten der Europischen Union zuwiderhandelt, die inhaltlich a)einem in Absatz 1 Nummer 1, 3, 4, 5, 6, 7a, 9 oder Nummer 10 oderb)einem in Absatz 2bezeichneten Gebot oder Verbot entspricht, soweit eine Rechtsverordnung nach Satz 2 fr einen bestimmten Tatbestand auf diese Bugeldvorschrift verweist, oder"@de .

dtd:LA-id2851 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem in Absatz 1 Nummer 1, 3, 4, 5, 6, 7a, 9 oder Nummer 10 oder"@de .

dtd:LA-id2852 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem in Absatz 2"@de .

dtd:LA-id2853 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer unmittelbar geltenden Vorschrift in Rechtsakten der Europischen Union zuwiderhandelt, die inhaltlich einer Regelung entspricht, zu der die in Absatz 1 Nummer 2, 7 oder Nummer 8 genannten Vorschriften ermchtigen, soweit eine Rechtsverordnung nach Satz 2 fr einen bestimmten Tatbestand auf diese Bugeldvorschrift verweist."@de .

dtd:LA-id2854 a dtd:LA ;

dtd:LA-id2855 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Genehmigung fr die Anlage erteilt wurde oder vom Vorhabentrger ein vollstndiger Genehmigungsantrag gestellt wurde."@de .

dtd:LA-id2856 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gentechnisch vernderten Mikroorganismen,"@de .

dtd:LA-id2857 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gentechnisch vernderten Zellkulturen, soweit sie nicht dazu bestimmt sind, zu Pflanzen regeneriert zu werden,"@de .

dtd:LA-id2858 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bestandteilen oder Stoffwechselprodukten von Mikroorganismen nach Nummer 1 oder Zellkulturen nach Nummer 2, soweit sie biologisch aktive, rekombinante Nukleinsure enthalten,"@de .

dtd:LA-id2859 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zusatzbelastung geringfgig ist und mit einer deutlichen Verminderung der Immissionsbelastung im Einwirkungsbereich der Anlage innerhalb von fnf Jahren ab Genehmigung zu rechnen ist oder"@de .

dtd:LA-id2860 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Zusammenhang mit dem Vorhaben Anlagen stillgelegt oder verbessert werden und dadurch eine Verminderung der Vorbelastung herbeigefhrt wird, die im Jahresmittel mindestens doppelt so gro ist wie die von der Neuanlage verursachte Zusatzbelastung."@de .

dtd:LA-id2861 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einsatz abfallarmer Technologie,"@de .

dtd:LA-id2862 a dtd:LA ;

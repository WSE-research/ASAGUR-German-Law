dtd:LA-id533 a dtd:LA ;
    dtd:has_DL dtd:DL-id436 ;
    dtd:has_Value "in den Fällen des § 45 Absatz 2 Satz 1 und 2 des Berufsbildungsgesetzes a)der Tätigkeitsnachweis,b)gegebenenfalls der Nachweis der Dauer der Berufsausbildung in dem oder in einem anderen einschlägigen Ausbildungsberuf undc)gegebenenfalls die glaubhafte Darlegung über den Erwerb der beruflichen Handlungsfähigkeit,"@de .

dtd:LA-id534 a dtd:LA ;
    dtd:has_Value "der Tätigkeitsnachweis,"@de .

dtd:LA-id535 a dtd:LA ;
    dtd:has_Value "gegebenenfalls der Nachweis der Dauer der Berufsausbildung in dem oder in einem anderen einschlägigen Ausbildungsberuf und"@de .

dtd:LA-id536 a dtd:LA ;

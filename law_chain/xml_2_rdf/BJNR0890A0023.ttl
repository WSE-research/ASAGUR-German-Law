dtd:LA-id530 a dtd:LA ;
    dtd:has_DL dtd:DL-id431 ;
    dtd:has_Value "im Fall des § 45 Absatz 1 des Berufsbildungsgesetzes zusätzlich zu den Unterlagen nach Nummer 1 a)das letzte Zeugnis oderb)eine aktuelle Leistungsbeurteilung der zuletzt besuchten berufsbildenden Schule,"@de .

dtd:LA-id531 a dtd:LA ;

dtd:LA-id559 a dtd:LA ;
    dtd:has_Value "die für die Prüfung zur Verfügung stehende Zeit,"@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Value "die für die Prüfung zugelassenen Arbeits- und Hilfsmittel,"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Value "die Folgen von Täuschungshandlungen bei der Prüfung,"@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Value "die Folgen von Ordnungsverstößen während der Prüfung,"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Value "die Folgen eines Rücktritts von der Prüfung und"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Value "die Folgen einer Nichtteilnahme an der Prüfung."@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Value "die Bezeichnung „Prüfungszeugnis nach § 37 Berufsbildungsgesetz“,"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Value "den Namen und die Vornamen des Prüflings,"@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Value "das Geburtsdatum des Prüflings,"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Value "die Bezeichnung des Ausbildungsberufs mit Fachrichtung,"@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Value "die Ergebnisse der Prüfungsbereiche und das Gesamtergebnis der Abschlussprüfung,"@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_Value "das Datum des Bestehens der Abschlussprüfung,"@de .

dtd:LA-id571 a dtd:LA ;
    dtd:has_Value "die Unterschriften des Vorsitzes des Prüfungsausschusses und einer beauftragten Person der zuständigen Stelle und"@de .


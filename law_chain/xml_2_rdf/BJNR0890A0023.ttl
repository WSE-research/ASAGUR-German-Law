dtd:LA-id537 a dtd:LA ;
    dtd:has_DL dtd:DL-id443 ;
    dtd:has_Value "in den Fällen des § 45 Absatz 2 Satz 3 und Absatz 3 des Berufsbildungsgesetzes a)die glaubhafte Darlegung des Erwerbs der beruflichen Handlungsfähigkeit oderb)die Bescheinigung über den Erwerb der beruflichen Fertigkeiten, Kenntnisse und Fähigkeiten,"@de .

dtd:LA-id538 a dtd:LA ;
    dtd:has_Value "die glaubhafte Darlegung des Erwerbs der beruflichen Handlungsfähigkeit oder"@de .

dtd:LA-id539 a dtd:LA ;

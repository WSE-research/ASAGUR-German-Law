    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Entgeltabrechnung steuer- und sozialversicherungsrechtliche Vorschriften einhalten"@de .

dtd:LA-id1003 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Entgeltabrechnung lohnsteuer- und sozialversicherungsrechtliche Aspekte beurteilen sowie Möglichkeiten zur Lohnsteuerermäßigung aufzeigen"@de .


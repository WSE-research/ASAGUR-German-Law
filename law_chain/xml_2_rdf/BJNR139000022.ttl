    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erklärungen zur gesonderten Feststellung von Besteuerungsgrundlagen erstellen"@de .

dtd:LA-id1035 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Umsatzsteuererklärungen erstellen und Umsatzsteuerverprobungen durchführen"@de .

dtd:LA-id1036 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gewerbesteuererklärungen, einschließlich Zerlegungserklärungen, erstellen"@de .


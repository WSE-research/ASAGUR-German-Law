    dtd:has_Size dtd:normal ;
    dtd:has_Value "Entgeltabrechnungen erstellen und prüfen"@de .

dtd:LA-id1005 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Entgeltabrechnungen in die Buchführung übertragen und verarbeiten"@de .

dtd:LA-id1006 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nachweise, Anträge und Meldungen im Zusammenhang mit der Entgeltabrechnung digital erstellen und übermitteln"@de .

dtd:LA-id1007 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auswertungen aus dem Entgeltabrechnungssystem situations- und mandatsbezogen erstellen"@de .

dtd:LA-id1008 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an der mandatsbezogenen Beratung mitwirken"@de .

dtd:LA-id1009 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "rechtliche Vorschriften, insbesondere handels- und steuerrechtliche Vorschriften, einhalten"@de .

dtd:LA-id1010 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Eröffnungsbilanz erstellen, Bilanz sowie Gewinn- und Verlustrechnung für den Jahresabschluss aus der Buchführung entwickeln"@de .

dtd:LA-id1011 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Auswirkungen unterschiedlicher Wertansätze in der Handels- und Steuerbilanz bei der Erstellung der Jahresabschlüsse berücksichtigen"@de .

dtd:LA-id1012 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unterschiede und Auswirkungen der Gesellschaftsformen bei der Erstellung von Jahresabschlüssen berücksichtigen"@de .

dtd:LA-id1013 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Jahresabschlüsse mit ihren jeweiligen Bestandteilen erstellen und digital übermitteln"@de .

dtd:LA-id1014 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Voraussetzungen der Einnahmenüberschussrechnung prüfen, Gewinn ermitteln und Einnahmenüberschussrechnung erstellen"@de .

dtd:LA-id1015 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unterschiede und Gemeinsamkeiten zwischen Jahresabschluss sowie Einnahmenüberschussrechnung der Mandantin oder dem Mandanten gegenüber erläutern"@de .

dtd:LA-id1016 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "betriebswirtschaftliche Auswertungen mandatsbezogen auswählen, auf Plausibilität prüfen und Positionen gegenüber den Mandantinnen und Mandanten erläutern"@de .

dtd:LA-id1017 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "betriebliche Kennzahlen ermitteln und im Rahmen innerer und äußerer Betriebsvergleiche auswerten, Mandantinnen und Mandanten informieren"@de .

dtd:LA-id1018 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vermögens- und Kapitalstrukturen ermitteln, Mandantinnen und Mandanten informieren"@de .

dtd:LA-id1019 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Möglichkeiten der Finanzierung, insbesondere der Eigen- und Fremdfinanzierung sowie der Außen- und Innenfinanzierung, gegenüber den Mandantinnen und Mandanten erläutern"@de .

dtd:LA-id1020 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kennzahlen mandatsbezogen überwachen und bei Veränderungen Mandantinnen und Mandanten informieren"@de .

dtd:LA-id1021 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "steuer- und verfahrensrechtliche Vorschriften einhalten, insbesondere Rechte und Pflichten der Beteiligten, ihrer gesetzlichen Vertreterinnen und Vertreter sowie der Finanzbehörden im Besteuerungsverfahren einordnen"@de .

dtd:LA-id1022 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mandantinnen und Mandanten über Vorschriften der Entstehung und der Festsetzung der Steuer sowie deren Fälligkeit informieren und auf Gesetzesverstöße hinweisen"@de .

dtd:LA-id1023 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einspruchsfrist und Festsetzungsverjährungsfrist berechnen und beachten"@de .

dtd:LA-id1024 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anträge auf Fristverlängerung entwerfen"@de .

dtd:LA-id1025 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anspruchsvoraussetzungen auf Stundung prüfen und Anträge vorbereiten"@de .

dtd:LA-id1026 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verwaltungsakte prüfen, insbesondere Steuerbescheid mit Steuererklärung abgleichen"@de .

dtd:LA-id1027 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zulässigkeit und Einleitung des außergerichtlichen Rechtsbehelfsverfahrens prüfen, Mandantinnen und Mandanten über Instanzen der Finanzgerichtsbarkeit informieren"@de .

dtd:LA-id1028 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einsprüche und Anträge bezüglich Aufhebung oder Änderung von Steuerbescheiden entwerfen"@de .

dtd:LA-id1029 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anträge auf Aussetzung der Vollziehung und Erlass entwerfen"@de .

dtd:LA-id1030 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "steuerrechtliche Vorschriften einhalten"@de .

dtd:LA-id1031 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "steuerrechtliche Sachverhalte und Bemessungsgrundlagen ermitteln, digitale Daten bei Finanzbehörden abrufen und überprüfen"@de .

dtd:LA-id1032 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Steuerpflicht prüfen"@de .

dtd:LA-id1033 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einkommensteuererklärungen erstellen, dabei das zu versteuernde Einkommen ermitteln"@de .


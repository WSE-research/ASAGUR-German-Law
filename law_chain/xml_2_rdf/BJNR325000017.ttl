dtd:LA-id672 a dtd:LA ;
    dtd:has_Value "dies von Ärztinnen oder Ärzten oder Zahnärztinnen oder Zahnärzten der Bundeswehr veranlasst wurde oder"@de .

dtd:LA-id673 a dtd:LA ;
    dtd:has_Value "ein Notfall (§ 30) besteht."@de .

dtd:LA-id674 a dtd:LA ;
    dtd:has_Value "ärztliche Untersuchungen zur Früherkennung von Krankheiten,"@de .

dtd:LA-id675 a dtd:LA ;
    dtd:has_Value "Schutzimpfungen im notwendigen Umfang einschließlich medikamentöser Prophylaxe, ausgenommen Schutzimpfungen und medikamentöser Prophylaxe und medikamentöser Prophylaxe anlässlich privater Auslandsreisen, und"@de .

dtd:LA-id676 a dtd:LA ;
    dtd:has_Value "sonstige medizinische Prophylaxemaßnahmen."@de .

dtd:LA-id677 a dtd:LA ;
    dtd:has_Value "eine Schwächung der Gesundheit, die in absehbarer Zeit voraussichtlich zu einer Krankheit führen würde, zu beseitigen,"@de .

dtd:LA-id678 a dtd:LA ;
    dtd:has_Value "Krankheiten zu verhüten oder deren Verschlimmerung zu vermeiden oder"@de .

dtd:LA-id679 a dtd:LA ;
    dtd:has_Value "Pflegebedürftigkeit zu vermeiden."@de .

dtd:LA-id680 a dtd:LA ;
    dtd:has_Value "ambulant in einem anerkannten Heilbad oder anerkannten Kurort,"@de .

dtd:LA-id681 a dtd:LA ;
    dtd:has_Value "als vollstationäre Behandlung in einer Vorsorgeeinrichtung, mit der ein Vertrag nach § 111 des Fünften Buches Sozialgesetzbuch besteht, oder"@de .

dtd:LA-id682 a dtd:LA ;
    dtd:has_Value "als vollstationäre Behandlung in einer anderen Vorsorgeeinrichtung, wenn besondere medizinische Gründe dies erfordern."@de .

dtd:LA-id683 a dtd:LA ;
    dtd:has_Value "medizinische Vorsorgeleistungen in Einrichtungen der Elly Heuss-Knapp-Stiftung – Deutsches Müttergenesungswerk –,"@de .

dtd:LA-id684 a dtd:LA ;
    dtd:has_Value "medizinische Vorsorgeleistungen in gleichartigen Einrichtungen, mit denen ein Versorgungsvertrag nach § 111a des Fünften Buches Sozialgesetzbuch besteht."@de .

dtd:LA-id685 a dtd:LA ;
    dtd:has_Value "von Truppenärztinnen oder Truppenärzten und"@de .

dtd:LA-id686 a dtd:LA ;
    dtd:has_Value "von anderen Ärztinnen oder Ärzten, die die Funktion der Truppenärztin oder des Truppenarztes wahrnehmen."@de .

dtd:LA-id687 a dtd:LA ;
    dtd:has_Value "einer Truppenärztin oder einem Truppenarzt bei der für den Aufenthaltsort zuständigen regionalen Sanitätseinrichtung der Bundeswehr oder"@de .

dtd:LA-id688 a dtd:LA ;
    dtd:has_Value "einer anderen Ärztin oder einem anderen Arzt, die oder der am Aufenthaltsort die Funktion der Truppenärztin oder des Truppenarztes wahrnimmt."@de .

dtd:LA-id689 a dtd:LA ;
    dtd:has_Value "am Standort der Soldatin oder des Soldaten und im Umkreis von 50 Kilometern keine Fachärztin und kein Facharzt der Bundeswehr zur Verfügung steht,"@de .

dtd:LA-id690 a dtd:LA ;
    dtd:has_Value "in den Sanitätseinrichtungen der Bundeswehr die technischen oder personellen Voraussetzungen für die erforderliche Untersuchung oder Behandlung fehlen,"@de .

dtd:LA-id691 a dtd:LA ;
    dtd:has_Value "ein Notfall besteht,"@de .

dtd:LA-id692 a dtd:LA ;
    dtd:has_Value "die Wartezeit für einen Termin in einer Sanitätseinrichtung der Bundeswehr unangemessen lang wäre oder"@de .

dtd:LA-id693 a dtd:LA ;
    dtd:has_Value "für eine behandlungsbedürftige Soldatin oder einen behandlungsbedürftigen Soldaten während ihres oder seines Urlaubs oder während einer Freistellung vom Dienst am inländischen Aufenthaltsort oder in der näheren Umgebung keine Versorgung in Sanitätseinrichtungen der Bundeswehr möglich ist."@de .

dtd:LA-id694 a dtd:LA ;
    dtd:has_Value "von Zahnärztinnen und Zahnärzten der Bundeswehr in zahnärztlichen Behandlungseinrichtungen der Bundeswehr,"@de .

dtd:LA-id695 a dtd:LA ;
    dtd:has_Value "von anderen Zahnärztinnen und Zahnärzten, die die Funktion der Truppenzahnärztin oder des Truppenzahnarztes wahrnehmen, oder"@de .

dtd:LA-id696 a dtd:LA ;
    dtd:has_Value "von zivilen Zahnärztinnen oder zivilen Zahnärzten, sofern eine Behandlung nach den Nummern 1 und 2 nicht möglich ist und eine Überweisung durch eine Zahnärztin oder einen Zahnarzt der Bundeswehr oder eine Ärztin oder einen Arzt der Bundeswehr vorliegt."@de .

dtd:LA-id697 a dtd:LA ;
    dtd:has_Value "zur Erhaltung, Wiederherstellung und Verbesserung der Funktionstüchtigkeit des Kauapparats, in den ersten vier Monaten nach Beginn und in den letzten sechs Monaten vor Ende des Wehrdienstverhältnisses jedoch nur zur Erhaltung oder Wiederherstellung der Dienstfähigkeit, oder"@de .

dtd:LA-id698 a dtd:LA ;

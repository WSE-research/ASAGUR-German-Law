dtd:LA-id290 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kriterien und Verfahren zur Auswahl von Auszubildenden auch unter Berücksichtigung ihrer Verschiedenartigkeit anwenden,"@de .


dtd:LA-id160 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Stimmzettel nebst Umschlag,"@de .

dtd:LA-id161 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine vorgedruckte, vom Stimmberechtigten zu unterzeichnende Erklärung, in der dieser versichert, daß er den Stimmzettel persönlich gekennzeichnet hat,"@de .

dtd:LA-id162 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen größeren, freigemachten Abstimmungsbriefumschlag, der die Anschrift des Abstimmungsvorstands und den Vermerk: \"Schriftliche Stimmabgabe\" trägt, sowie"@de .

dtd:LA-id163 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Merkblatt über die Art und Weise der brieflichen Stimmabgabe"@de .

dtd:LA-id164 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Stimmzettel unbeobachtet persönlich zu kennzeichnen und ihn in dem nicht bedruckten Umschlag zu verschließen,"@de .

dtd:LA-id165 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die vorgedruckte Erklärung (Absatz 1 Nr. 2) unter Angabe des Datums zu unterzeichnen und diese zusammen mit dem nicht bedruckten Umschlag in dem freigemachten Abstimmungsbriefumschlag zu verschließen,"@de .

dtd:LA-id166 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf der Rückseite des freigemachten Abstimmungsbriefumschlags seinen Namen und seine Anschrift zu vermerken und diesen so rechtzeitig an den Abstimmungsvorstand abzusenden oder ihm zu übergeben, daß er dem Abstimmungsvorstand vor Abschluß der Abstimmung vorliegt."@de .

dtd:LA-id167 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der nach der Abstimmungsliste Stimmberechtigten,"@de .

dtd:LA-id168 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der abgegebenen Stimmzettel,"@de .

dtd:LA-id169 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der gültigen Stimmzettel,"@de .

dtd:LA-id170 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der ungültigen Stimmzettel,"@de .

dtd:LA-id171 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der gültig abgegebenen bejahenden Stimmen,"@de .

dtd:LA-id172 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der gültig abgegebenen verneinenden Stimmen,"@de .


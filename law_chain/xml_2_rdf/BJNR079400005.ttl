dtd:LA-id974 a dtd:LA ;
    dtd:has_DL dtd:DL-id758 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "In höchstens 150 Minuten soll der Prüfling praxisbezogene Aufgaben oder Fälle insbesondere aus den Gebietena)Gestaltung von Produkten und Leistungen, b)Marketing, c)Organisation von Betriebsabläufen "@de .

dtd:LA-id975 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bearbeiten und dabei zeigen, dass er unter Berücksichtigung der branchenspezifischen Strukturen wirtschaftliche und organisatorische Zusammenhänge und Problemstellungen analysieren, Betriebsabläufe koordinieren sowie Lösungsmöglichkeiten kunden- und marktorientiert entwickeln und darstellen kann;"@de .

dtd:LA-id976 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gestaltung von Produkten und Leistungen,"@de .


dtd:LA-id481 a dtd:LA ;
    dtd:has_Value "Geschlecht,"@de .

dtd:LA-id482 a dtd:LA ;
    dtd:has_Value "Angaben zum Bildungsstand, zur beruflichen Qualifikation, zum Vorliegen einer Beschäftigung und Angaben dazu, ob die Voraussetzungen nach § 4 Absatz 4 Satz 2 und Absatz 6 vorliegen,"@de .

dtd:LA-id483 a dtd:LA ;
    dtd:has_Value "Angaben zum Sprachstand nach dem Gemeinsamen Europäischen Referenzrahmen für Sprachen und"@de .

dtd:LA-id484 a dtd:LA ;

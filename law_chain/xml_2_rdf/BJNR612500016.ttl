dtd:LA-id424 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leistungen nach dem Zweiten Buch Sozialgesetzbuch beziehen oder"@de .

dtd:LA-id425 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "beschäftigt sind, ohne zum Personenkreis nach den Buchstaben a oder b zu gehören,"@de .

dtd:LA-id426 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "weil sie begleitend zur Anerkennung ausländischer Berufsabschlüsse oder für den Zugang zum Beruf ein bestimmtes Sprachniveau erreichen müssen,"@de .

dtd:LA-id427 a dtd:LA ;

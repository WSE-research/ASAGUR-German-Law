dtd:LA-id434 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Sprachniveaus B 2, ausgehend vom Niveau B 1 oder"@de .

dtd:LA-id435 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:LA-id475 a dtd:LA ;
    dtd:has_Value "Namen, Vornamen,"@de .

dtd:LA-id476 a dtd:LA ;
    dtd:has_Value "Geburtsdatum,"@de .

dtd:LA-id477 a dtd:LA ;
    dtd:has_Value "Anschrift, Telefonnummer, E-Mail-Adresse,"@de .

dtd:LA-id478 a dtd:LA ;
    dtd:has_Value "Staatsangehörigkeiten,"@de .

dtd:LA-id479 a dtd:LA ;

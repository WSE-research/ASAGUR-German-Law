dtd:LA-id924 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vollziehbare Entscheidungen über die Versagung der Prüfungszulassung oder der Erteilung, Entziehung, Widerruf, Rücknahme, Aussetzung, vorläufige Sicherstellung und Ruhen von Befähigungszeugnissen oder sonstigen Befähigungsnachweisen,"@de .

dtd:LA-id925 a dtd:LA ;

    dtd:has_Size dtd:normal ;
    dtd:has_Value "Positionen der eigenen Entgeltabrechnung erläutern"@de .

dtd:LA-id1303 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wesentliche Inhalte von Arbeitsverträgen erläutern"@de .

dtd:LA-id1304 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Möglichkeiten des beruflichen Aufstiegs und der beruflichen Weiterentwicklung erläutern"@de .

dtd:LA-id1305 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rechte und Pflichten aus den berufsbezogenen Arbeitsschutz- und Unfallverhütungsvorschriften kennen und diese Vorschriften anwenden"@de .

dtd:LA-id1306 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gefährdungen von Sicherheit und Gesundheit am Arbeitsplatz und auf dem Arbeitsweg prüfen und beurteilen"@de .

dtd:LA-id1307 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sicheres und gesundheitsgerechtes Arbeiten erläutern"@de .

dtd:LA-id1308 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "technische und organisatorische Maßnahmen zur Vermeidung von Gefährdungen sowie von psychischen und physischen Belastungen für sich und andere, auch präventiv, ergreifen"@de .

dtd:LA-id1309 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ergonomische Arbeitsweisen beachten und anwenden"@de .

dtd:LA-id1310 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verhaltensweisen bei Unfällen beschreiben und erste Maßnahmen bei Unfällen einleiten"@de .

dtd:LA-id1311 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "betriebsbezogene Vorschriften des vorbeugenden Brandschutzes anwenden, Verhaltensweisen bei Bränden beschreiben und erste Maßnahmen zur Brandbekämpfung ergreifen"@de .

dtd:LA-id1312 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Möglichkeiten zur Vermeidung betriebsbedingter Belastungen für Umwelt und Gesellschaft im eigenen Aufgabenbereich erkennen und zu deren Weiterentwicklung beitragen"@de .

dtd:LA-id1313 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Arbeitsprozessen und im Hinblick auf Produkte, Waren oder Dienstleistungen, Materialien und Energie unter wirtschaftlichen, umweltverträglichen und sozialen Gesichtspunkten der Nachhaltigkeit nutzen"@de .

dtd:LA-id1314 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für den Ausbildungsbetrieb geltenden Regelungen des Umweltschutzes einhalten"@de .

dtd:LA-id1315 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Abfälle vermeiden sowie Stoffe und Materialien einer umweltschonenden Wiederverwertung oder Entsorgung zuführen"@de .

dtd:LA-id1316 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den eigenen Arbeitsbereich Vorschläge für nachhaltiges Handeln entwickeln"@de .

dtd:LA-id1317 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "unter Einhaltung betrieblicher Regelungen im Sinne einer ökonomischen, ökologischen und sozial nachhaltigen Entwicklung zusammenarbeiten und adressatengerecht kommunizieren"@de .

dtd:LA-id1318 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit eigenen und betriebsbezogenen Daten sowie mit Daten Dritter umgehen und dabei die Vorschriften zum Datenschutz und zur Datensicherheit einhalten"@de .

dtd:LA-id1319 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Risiken bei der Nutzung von digitalen Medien und informationstechnischen Systemen einschätzen und bei deren Nutzung betriebliche Regelungen einhalten"@de .

dtd:LA-id1320 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ressourcenschonend, adressatengerecht und effizient kommunizieren sowie Kommunikationsergebnisse dokumentieren"@de .

dtd:LA-id1321 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Störungen in Kommunikationsprozessen erkennen und zu ihrer Lösung beitragen"@de .

dtd:LA-id1322 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Informationen in digitalen Netzen recherchieren und aus digitalen Netzen beschaffen sowie Informationen, auch fremde, prüfen, bewerten und auswählen"@de .

dtd:LA-id1323 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Lern- und Arbeitstechniken sowie Methoden des selbstgesteuerten Lernens anwenden, digitale Lernmedien nutzen und Erfordernisse des lebensbegleitenden Lernens erkennen und ableiten"@de .

dtd:LA-id1324 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufgaben zusammen mit Beteiligten, einschließlich der Beteiligten anderer Arbeits- und Geschäftsbereiche, auch unter Nutzung digitaler Medien, planen, bearbeiten und gestalten"@de .

dtd:LA-id1325 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wertschätzung anderer unter Berücksichtigung gesellschaftlicher Vielfalt praktizieren"@de .


dtd:P-id637 a dtd:P ;
    dtd:has_Value "(1) Begonnene PrÃ¼fungsverfahren kÃ¶nnen bis zum 31. Dezember 2011 nach den bisherigen Vorschriften zu Ende gefÃ¼hrt werden."@de .

dtd:P-id638 a dtd:P ;

dtd:LA-id612 a dtd:LA ;
    dtd:has_Value "die Einführung neuer Arbeits- und Fertigungsverfahren,"@de .

dtd:LA-id613 a dtd:LA ;
    dtd:has_Value "die Verlegung von Unternehmen, Betrieben oder wesentlichen Betriebsteilen sowie Verlagerungen der Produktion,"@de .

dtd:LA-id614 a dtd:LA ;
    dtd:has_Value "Zusammenschlüsse oder Spaltungen von Unternehmen oder Betrieben,"@de .

dtd:LA-id615 a dtd:LA ;
    dtd:has_Value "die Einschränkung oder Stilllegung von Unternehmen, Betrieben oder wesentlichen Betriebsteilen,"@de .

dtd:LA-id616 a dtd:LA ;
    dtd:has_Value "Massenentlassungen."@de .

dtd:LA-id617 a dtd:LA ;
    dtd:has_Value "die Verlegung von Unternehmen, Betrieben oder wesentlichen Betriebsteilen,"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "Reagenzien und Materialien"@de .

dtd:LA-id5899 a dtd:LA ;
    dtd:has_F dtd:F-id1921 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "n-Heptan (für die Chromatographie, Reinheit ≥ 99 %,)"@de .

dtd:LA-id5900 a dtd:LA ;
    dtd:has_F dtd:F-id1922 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Toluol (für die Chromatographie, Reinheit ≥ 99,9 %)"@de .

dtd:LA-id5901 a dtd:LA ;
    dtd:has_F dtd:F-id1923 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Xylol-Isomerengemisch oder o-Xylol (für die Chromatographie, Reinheit ≥ 98 %)"@de .

dtd:LA-id5902 a dtd:LA ;

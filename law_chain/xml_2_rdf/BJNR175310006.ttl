dtd:LA-id5977 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Führen Sie die Kalibrierung regelmäßig (mindestens alle zwei Wochen) und nach jeder Änderung am Gerät (z. B. MSD-Tuning, Wechsel des Liners, Änderung des Heart-Cut-Zeitfensters) oder im Falle eines Qualitätskontrollfehlers durch."@de .

dtd:LA-id5978 a dtd:LA ;

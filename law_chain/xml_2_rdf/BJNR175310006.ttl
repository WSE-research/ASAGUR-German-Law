    dtd:has_Value "BPE (Reinheit ≥ 99 %)"@de .

dtd:LA-id5903 a dtd:LA ;
    dtd:has_F dtd:F-id1925 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "d5-BPE (Reinheit ≥ 98 %)"@de .


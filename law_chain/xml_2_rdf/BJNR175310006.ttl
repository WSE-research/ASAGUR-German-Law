dtd:LA-id5875 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "relative Messunsicherheit aufgrund der Vergleichbarkeit"@de .

dtd:LA-id5876 a dtd:LA ;

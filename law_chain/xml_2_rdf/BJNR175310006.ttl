    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mengen der eingesetzten Hilfsenergie und"@de .

dtd:LA-id5294 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mengen der genutzten erzeugten thermischen und mechanischen oder elektrischen Energie."@de .

dtd:LA-id5295 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Name und die Anschrift des Betreibers,"@de .

dtd:LA-id5296 a dtd:LA ;
    dtd:has_Size dtd:normal ;

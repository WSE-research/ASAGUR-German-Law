dtd:LA-id5966 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bei Routineanalysen wird eine lineare 7-Punkte-Kalibrierung durchgeführt (6 Punkte und erzwungener Nullpunkt, siehe auch 5.3.3.2 und 5.4.4)."@de .

dtd:LA-id5967 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "VERFAHREN A):"@de .

dtd:LA-id5968 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Kalibrierkurve wird erstellt, indem die Fläche des zu quantifizierenden Ions (m/z = 94) des BPE-Peaks in jedem Standardchromatogramm gegen die genaue Konzentration des jeweiligen Standards in mg/l aufgetragen wird. Es wird eine lineare Regression mit erzwungenem Nulldurchgang angewandt."@de .


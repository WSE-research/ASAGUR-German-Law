dtd:LA-id63 a dtd:LA ;
    dtd:has_Value "Einzelangaben aus Wirtschaftsstatistiken des Statistischen Bundesamtes,"@de .

dtd:LA-id64 a dtd:LA ;
    dtd:has_Value "den nach Artikel 16 Absatz 1 der Verordnung (EU) 2019/2152 des Europäischen Parlaments und des Rates vom 27. November 2019 über europäische Unternehmensstatistiken, zur Aufhebung von zehn Rechtsakten im Bereich Unternehmensstatistiken (ABl. L 327 vom 17.12.2019, S. 1) übermittelten Daten,"@de .


dtd:LA-id265 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zu betreuenden Personen, insbesondere ältere Menschen, bei der Haushaltsführung zu unterstützen, bei Bedarf die hauswirtschaftliche Versorgung zu übernehmen und hierbei die jeweilige Haushaltssituation, die Bedürfnisse und Wünsche der zu betreuenden Personen zu berücksichtigen;"@de .

dtd:LA-id266 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zu betreuenden Personen bei der eigenständigen Lebensführung zu unterstützen, ihnen je nach Bedarf bei personenbezogenen Alltagsverrichtungen sowie bei der Bewältigung von Problemlagen des Alltags Hilfestellung zu geben, sie bei ihren Lebensgestaltungsmöglichkeiten zu beraten und hierbei den jeweiligen Gesundheitszustand, die Bedürfnisse und Wünsche der zu betreuenden Personen zu berücksichtigen;"@de .

dtd:LA-id267 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der hauswirtschaftlichen Versorgung und den Betreuungsaufgaben die zu betreuenden Personen mit einzubeziehen, ihre Fähigkeiten zu trainieren und sie zu eigener Lebensgestaltung zu aktivieren und zu motivieren;"@de .

dtd:LA-id268 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei seiner Tätigkeit mit anderen Hilfen und Diensten zusammenzuarbeiten und unter Berücksichtigung seiner eigenen fachlichen Handlungsmöglichkeiten erforderlichenfalls auf Hinzuziehung weiterer Fachkräfte hinzuwirken."@de .

dtd:LA-id269 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine mit Erfolg abgelegte Abschlußprüfung zur Hauswirtschafterin/zum Hauswirtschafter und danach eine mindestens zweijährige Berufspraxis, davon eine mindestens halbjährige dem angestrebten Abschluß entsprechende Berufspraxis, oder"@de .

dtd:LA-id270 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine mindestens sechsjährige, dem angestrebten Abschluß entsprechende Berufspraxis"@de .

dtd:LA-id271 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Hauswirtschaftliche Leistungen;"@de .

dtd:LA-id272 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betreuung bei alltagsbezogenen Verrichtungen;"@de .

dtd:LA-id273 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kommunikation;"@de .

dtd:LA-id274 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berufliche und rechtliche Rahmenbedingungen."@de .

dtd:LA-id275 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Analysieren der Haushaltssituation unter Berücksichtigung des Leistungsvermögens der zu betreuenden Personen, der verfügbaren Mittel und Dienstleistungsangebote;"@de .

dtd:LA-id276 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Planen, Gestalten und Erfassen der Versorgungsleistungen unter Einbeziehung der Gewohnheiten der zu betreuenden Personen und Einsatz spezieller Hilfsmittel;"@de .

dtd:LA-id277 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unterstützen der zu betreuenden Personen bei der Erfassung und Bewertung des Haushaltsbudgets, bei der Prüfung von Einsparungs- und zusätzlichen Finanzierungsmöglichkeiten sowie beim Aufstellen eines Haushaltsvoranschlages;"@de .

dtd:LA-id278 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Planen und Unterstützen bei Verpflegung und Nahrungsaufnahme unter Berücksichtigung alters- und krankheitsbedingter Kostformen, spezifischer Probleme bei Ernährung, Nahrungsaufnahme und des Flüssigkeitshaushaltes, von Darreichungsformen und des Einsatzes von Hilfsmitteln;"@de .

dtd:LA-id279 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Versorgen mit bedarfsgerechter Kleidung und situationsgerechter Haushaltswäsche unter besonderer Beachtung hygienischer Anforderungen bei Wäsche und Reinigung;"@de .

dtd:LA-id280 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Analysieren der Wohnsituation, Gestalten und Pflegen einer bedarfsgerechten Wohnung unter Vermeidung von Unfallgefahren."@de .

dtd:LA-id281 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Planen und Dokumentieren von Betreuungsaufgaben;"@de .

dtd:LA-id282 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "individuelle Unterstützungsmöglichkeiten bei der Körperpflege unter Beachtung der Erhaltung der Selbständigkeit sowie hygienischer Gewohnheiten;"@de .

dtd:LA-id283 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beachten von Kleidungsgewohnheiten sowie Hilfestellung beim An- und Auskleiden, Kenntnisse über Spezialkleidung;"@de .

dtd:LA-id284 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fördern der Mobilität durch Einbeziehung in alltägliche Verrichtungen;"@de .

dtd:LA-id285 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erkennen von Ressourcen und Problemen in der alltäglichen Lebensgestaltung, Unterstützen bei der Tagesstrukturierung;"@de .

dtd:LA-id286 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einleiten von Maßnahmen der Ersten Hilfe und Grundkenntnisse zur Medikamenteneinnahme, Kenntnisse über Hilfsmittel."@de .

dtd:LA-id287 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorgänge des Alterns, insbesondere im Hinblick auf Veränderungen der Persönlichkeit, der Lebenssituation sowie der Wohn- und Haushaltssituation;"@de .

dtd:LA-id288 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auswirkungen individueller Lebensverläufe auf Verhalten und Einstellungen der Menschen, Konsequenzen für die Fachhauswirtschafterin/den Fachhauswirtschafter;"@de .

dtd:LA-id289 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundzüge der Kommunikation, insbesondere Gesprächsformen und Gesprächsinhalte, Techniken und Methoden der Gesprächsführung, Unterstützungs- und Motivierungsmöglichkeiten sowie Methoden der Konfliktlösung;"@de .

dtd:LA-id290 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verhalten bei Kommunikations- und Gesundheitsstörungen, insbesondere Auswirkungen geriatrischer Erkrankungen auf Verhalten und Kommunikation, typische körperliche und psychische Veränderungen und Krankheiten;"@de .

dtd:LA-id291 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verhalten bei Lebenskrisen, chronischen Krankheiten, Behinderungen, Sterben, Tod, Trauer;"@de .

dtd:LA-id292 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kommunikation im sozialen Umfeld zur Einbeziehung von Hilfemöglichkeiten, insbesondere von Familie und Nachbarschaft;"@de .

dtd:LA-id293 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Möglichkeiten der Bewältigung berufsbedingter Belastungen."@de .

dtd:LA-id294 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Funktionsbild der Fachhauswirtschafterin/des Fachhauswirtschafters, insbesondere Aufgaben, Anforderungen und Weiterbildungsmöglichkeiten;"@de .

dtd:LA-id295 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Versorgungs- und Dienstleistungsangebote, Kooperationsmöglichkeiten und -formen und Möglichkeiten ihrer Erschließung;"@de .


dtd:entry-id263 a dtd:entry ;
    dtd:has_DL dtd:DL-id338 ;
    dtd:has_Value "a)Daten erfassen, ordnen, pflegen und auswertenb)externe und interne Informationsquellen für betriebliche Prozesse nutzen"@de .


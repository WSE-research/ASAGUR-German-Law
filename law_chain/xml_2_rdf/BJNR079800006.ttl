dtd:entry-id251 a dtd:entry ;
    dtd:has_DL dtd:DL-id335 ;
    dtd:has_Value "a)rechtliche Bestimmungen zum Datenschutz beachtenb)Datenschutz in seiner Wirkung auf Unternehmen, Geschäftspartner und Kunden unterscheiden und im Arbeitsprozess anwenden"@de .


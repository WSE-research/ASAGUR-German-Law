    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Europäische Zentralbank, sofern sie nach Maßgabe der Verordnung (EU) Nr. 1024/2013 zuständige Behörde für ein gruppenangehöriges Unternehmen ist;"@de .

dtd:LA-id5807 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Aufsichtsbehörde;"@de .


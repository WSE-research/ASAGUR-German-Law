dtd:LA-id5800 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Festlegung von Mindestanforderungen auf Gruppenebene und Einzelinstitutsebene gemäß den §§ 49 bis 50."@de .


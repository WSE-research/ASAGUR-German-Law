    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Entscheidung über die Notwendigkeit der Ausarbeitung eines Gruppenabwicklungskonzepts gemäß den §§ 161 bis 165 oder § 166;"@de .

dtd:LA-id5797 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Einigung über ein Gruppenabwicklungskonzept, das gemäß den §§ 161 bis 165 oder § 166 vorgeschlagen wird;"@de .


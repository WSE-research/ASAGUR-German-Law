    dtd:has_Value "ein Mitglied auf dem Fachgebiet Toxikologie, das dem Bundesinstitut für Risikobewertung angehört,"@de .

dtd:LA-id80 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Mitglied auf dem Fachgebiet Ökotoxikologie,"@de .

dtd:LA-id81 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Mitglied auf dem Fachgebiet Umwelt- und Tierhygiene."@de .


    dtd:has_DL dtd:DL-id1190 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Falle einer Bevollmächtigung nach § 35 Absatz 2: a)Name, Anschrift und Kontaktdaten des Bevollmächtigten entsprechend Nummer 1 sowieb)die schriftliche Beauftragung durch den Hersteller;"@de .

dtd:LA-id1797 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name, Anschrift und Kontaktdaten des Bevollmächtigten entsprechend Nummer 1 sowie"@de .


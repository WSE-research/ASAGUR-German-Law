    dtd:has_Size dtd:normal ;
    dtd:has_Value "80 Masseprozent bei Glas; ab dem 1. Januar 2022 90 Masseprozent,"@de .

dtd:LA-id1829 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "85 Masseprozent bei Papier, Pappe und Karton; ab dem 1. Januar 2022 90 Masseprozent,"@de .

dtd:LA-id1830 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "80 Masseprozent bei Eisenmetallen; ab dem 1. Januar 2022 90 Masseprozent,"@de .

dtd:LA-id1831 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "80 Masseprozent bei Aluminium; ab dem 1. Januar 2022 90 Masseprozent,"@de .

dtd:LA-id1832 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "75 Masseprozent bei Getränkekartonverpackungen; ab dem 1. Januar 2022 80 Masseprozent,"@de .

dtd:LA-id1833 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "55 Masseprozent bei sonstigen Verbundverpackungen (ohne Getränkekartonverpackungen); ab dem 1. Januar 2022 70 Masseprozent."@de .

dtd:LA-id1834 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in dem betreffenden Land flächendeckend eingerichtet ist, insbesondere die notwendigen Sammelstrukturen vorhanden sind,"@de .

dtd:LA-id1835 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit allen öffentlich-rechtlichen Entsorgungsträgern in dem betreffenden Land Abstimmungsvereinbarungen nach § 22 Absatz 1 abgeschlossen hat oder sich bestehenden Abstimmungsvereinbarungen unterworfen hat,"@de .

dtd:LA-id1836 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über die notwendigen Sortier- und Verwertungskapazitäten verfügt,"@de .

dtd:LA-id1837 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "finanziell leistungsfähig ist und"@de .

dtd:LA-id1838 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit der Zentralen Stelle eine Finanzierungsvereinbarung nach § 25 Absatz 1 Satz 2 abgeschlossen hat."@de .

dtd:LA-id1839 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "verfügbare Finanzmittel einschließlich Bankguthaben sowie zugesagte Überziehungskredite und Darlehen,"@de .

dtd:LA-id1840 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Sicherheit verfügbare Mittel und Vermögensgegenstände,"@de .

dtd:LA-id1841 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebskapital,"@de .

dtd:LA-id1842 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Belastungen des Betriebsvermögens,"@de .

dtd:LA-id1843 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Steuern und Sozialversicherungsbeiträge."@de .

dtd:LA-id1844 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufteilung der Entsorgungskosten auf Grundlage der von der Zentralen Stelle gemäß § 26 Absatz 1 Satz 2 Nummer 14 und 15 festgestellten Marktanteile;"@de .

dtd:LA-id1845 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufteilung der gemäß § 22 Absatz 9 vereinbarten Nebenentgelte auf Grundlage der von der Zentralen Stelle gemäß § 26 Absatz 1 Satz 2 Nummer 14 und 15 festgestellten Marktanteile;"@de .

dtd:LA-id1846 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wettbewerbsneutrale Koordination der Ausschreibungen nach § 23, insbesondere Bestimmung der Ausschreibungsführer für jedes Sammelgebiet;"@de .

dtd:LA-id1847 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Festlegung der Einzelheiten zur elektronischen Ausschreibungsplattform und zum Ausschreibungsverfahren gemäß § 23 Absatz 10;"@de .

dtd:LA-id1848 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Benennung der gemeinsamen Vertreter gemäß § 22 Absatz 7 Satz 1;"@de .

dtd:LA-id1849 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Benennung der Systemprüfer gemäß § 20 Absatz 4;"@de .

dtd:LA-id1850 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wettbewerbsneutrale Koordination der Informationsmaßnahmen nach § 14 Absatz 3 und Aufteilung der Kosten dieser Maßnahmen auf Grundlage der von der Zentralen Stelle gemäß § 26 Absatz 1 Satz 2 Nummer 14 und 15 festgestellten Marktanteile."@de .

dtd:LA-id1851 a dtd:LA ;
    dtd:has_Value "bis zum 15. Kalendertag des letzten Monats des jeweils laufenden Quartals die für das folgende Quartal erwartete Masse an beteiligten Verpackungen (Zwischenmeldung);"@de .

dtd:LA-id1852 a dtd:LA ;
    dtd:has_Value "bis zum 1. Juni eines jeden Jahres die Masse der für das vorangegangene Kalenderjahr tatsächlich beteiligten Verpackungen (Jahresmeldung)."@de .

dtd:LA-id1853 a dtd:LA ;
    dtd:has_Value "die Verwendung von Materialien und Materialkombinationen zu fördern, die unter Berücksichtigung der Praxis der Sortierung und Verwertung zu einem möglichst hohen Prozentsatz recycelt werden können, und"@de .

dtd:LA-id1854 a dtd:LA ;
    dtd:has_Value "die Verwendung von Rezyklaten sowie von nachwachsenden Rohstoffen zu fördern."@de .

dtd:LA-id1855 a dtd:LA ;
    dtd:has_Value "der Art des Sammelsystems, entweder Holsystem, Bringsystem oder Kombination aus beiden Sammelsystemen,"@de .

dtd:LA-id1856 a dtd:LA ;
    dtd:has_Value "der Art und Größe der Sammelbehälter, sofern es sich um Standard-Sammelbehälter handelt, sowie"@de .

dtd:LA-id1857 a dtd:LA ;
    dtd:has_Value "der Häufigkeit und des Zeitraums der Behälterleerungen"@de .

dtd:LA-id1858 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die in § 26 genannten, von der Zentralen Stelle zu erfüllenden Aufgaben verbindlich festschreiben,"@de .

dtd:LA-id1859 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Organisation und Ausstattung der Zentralen Stelle so ausgestalten, dass eine ordnungsgemäße Erfüllung der in § 26 genannten Aufgaben sichergestellt ist,"@de .

dtd:LA-id1860 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Rahmen der Ausgestaltung und Organisation der Zentralen Stelle sicherstellen, dass die in Satz 1 genannten Hersteller und Vertreiber ihre Interessen zu gleichen Bedingungen und in angemessenem Umfang einbringen können,"@de .

dtd:LA-id1861 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sicherstellen, dass die Neutralität der Zentralen Stelle gegenüber allen Marktteilnehmern stets gewahrt bleibt,"@de .

dtd:LA-id1862 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sicherstellen, dass die Vorschriften zum Schutz personenbezogener Daten sowie von Betriebs- und Geschäftsgeheimnissen eingehalten werden, insbesondere gegenüber den Mitgliedern des Kuratoriums, des Verwaltungsrats, des Beirats Erfassung, Sortierung und Verwertung sowie gegenüber Dritten und der Öffentlichkeit."@de .

dtd:LA-id1863 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nimmt auf Antrag Registrierungen gemäß § 9 Absatz 1 vor, erteilt Bestätigungen nach § 9 Absatz 3 Satz 2 und veröffentlicht gemäß § 9 Absatz 4 eine Liste der registrierten Hersteller im Internet,"@de .

dtd:LA-id1864 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "prüft die gemäß § 10 übermittelten Datenmeldungen,"@de .

dtd:LA-id1865 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kann den Systemen gemäß § 10 Absatz 3 die Möglichkeit einräumen, die sich auf ihr System beziehenden Datenmeldungen elektronisch abzurufen,"@de .

dtd:LA-id1866 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "prüft die gemäß § 11 Absatz 3 hinterlegten Vollständigkeitserklärungen, insbesondere im Hinblick auf ihre Übereinstimmung mit den Registerangaben nach § 9, den Datenmeldungen nach § 10 und den Jahresmeldungen nach § 20 Absatz 1 Nummer 2, kann erforderlichenfalls Anordnungen nach § 11 Absatz 3 Satz 4 und 5 erteilen und informiert im Falle von nicht aufklärbaren Unregelmäßigkeiten die zuständigen Landesbehörden über das Ergebnis ihrer Prüfung,"@de .

dtd:LA-id1867 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kann gemäß § 11 Absatz 4 Satz 2 die Hinterlegung einer Vollständigkeitserklärung anordnen,"@de .

dtd:LA-id1868 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "veröffentlicht im Internet eine Liste der Hersteller, die eine Vollständigkeitserklärung gemäß § 11 Absatz 1 Satz 1 hinterlegt haben,"@de .

dtd:LA-id1869 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kann von den Systemen eine Begründung gemäß § 14 Absatz 4 Satz 3 verlangen, prüft die übermittelte Begründung und informiert im Fall fortbestehender Zweifel am Vorliegen von Geschäftsgeheimnissen unverzüglich die zuständigen Landesbehörden über das Ergebnis der Prüfung,"@de .

dtd:LA-id1870 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "prüft die von den Systemen gemäß § 17 Absatz 3 Satz 1 hinterlegten Mengenstromnachweise, kann gemäß § 17 Absatz 3 Satz 4 die Vorlage der zugehörigen Prüfdokumente verlangen und informiert die zuständigen Landesbehörden und die Systeme über das Ergebnis ihrer Prüfung,"@de .

dtd:LA-id1871 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "prüft auf Anforderung der zuständigen Landesbehörden die gemäß § 18 Absatz 1a Satz 6 übermittelten Unterlagen und teilt den zuständigen Landesbehörden ihre Einschätzung zur finanziellen Leistungsfähigkeit des Systems mit,"@de .

dtd:LA-id1872 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "prüft die gemäß § 20 Absatz 1 übermittelten Meldungen der Systeme, kann erforderlichenfalls Anordnungen nach § 20 Absatz 2 Satz 3 und 4 erteilen, nimmt erforderlichenfalls Schätzungen nach § 20 Absatz 2 Satz 5 vor und informiert im letztgenannten Falle hierüber unverzüglich die zuständigen Landesbehörden,"@de .

dtd:LA-id1873 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "prüft die gemäß § 20 Absatz 5 Satz 1 übermittelten Meldungen der Systeme, kann erforderlichenfalls Anordnungen nach § 20 Absatz 5 Satz 4 erteilen und informiert unverzüglich die zuständigen Landesbehörden, wenn ein System keine Meldung nach § 20 Absatz 5 Satz 1 übermittelt hat oder die Anhaltspunkte nach § 20 Absatz 5 Satz 4 nicht zur Überzeugung der Zentralen Stelle ausräumen kann,"@de .

dtd:LA-id1874 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "benennt erforderlichenfalls Systemprüfer gemäß § 20 Absatz 4 Satz 2,"@de .

dtd:LA-id1875 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nimmt die Berichte der Systeme nach § 21 Absatz 2 entgegen, prüft diese auf Plausibilität und erteilt, sofern sich aus der Prüfung keine Beanstandungen ergeben, im Einvernehmen mit dem Umweltbundesamt dem jeweiligen System die Erlaubnis, den Bericht zu veröffentlichen,"@de .

dtd:LA-id1876 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kann gemäß § 21 Absatz 2 Satz 3 im Einvernehmen mit dem Umweltbundesamt verbindliche Vorgaben hinsichtlich der Form der Berichte nach § 21 Absatz 2 Satz 1 beschließen und veröffentlichen,"@de .

dtd:LA-id1877 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entwickelt und veröffentlicht gemäß § 21 Absatz 3 im Einvernehmen mit dem Umweltbundesamt einen Mindeststandard für die Bemessung der Recyclingfähigkeit von systembeteiligungspflichtigen Verpackungen,"@de .

dtd:LA-id1878 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entwickelt und veröffentlicht im Einvernehmen mit dem Bundeskartellamt ein Verfahren zur Berechnung der Marktanteile der einzelnen Systeme an der Gesamtmenge der an allen Systemen beteiligten Verpackungen,"@de .

dtd:LA-id1879 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entwickelt und veröffentlicht im Einvernehmen mit dem Bundeskartellamt ein Verfahren zur Berechnung der Marktanteile der einzelnen Systeme und Branchenlösungen an der Gesamtmenge der an allen Systemen und Branchenlösungen beteiligten Verpackungen,"@de .

dtd:LA-id1880 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "berechnet gemäß dem nach Nummer 12 veröffentlichten Verfahren vierteljährlich nach Erhalt der Zwischenmeldungen nach § 20 Absatz 1 Nummer 1 die den einzelnen Systemen in diesem Zeitraum vorläufig zuzuordnenden Marktanteile, stellt diese durch Verwaltungsakt fest und veröffentlicht das Ergebnis der Feststellung im Internet,"@de .

dtd:LA-id1881 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "berechnet gemäß dem nach Nummer 12 veröffentlichten Verfahren kalenderjährlich nach Erhalt der Jahresmeldungen nach § 20 Absatz 1 Nummer 2 die den einzelnen Systemen in diesem Zeitraum zuzuordnenden Marktanteile, stellt diese durch Verwaltungsakt fest und veröffentlicht das Ergebnis der Feststellung im Internet,"@de .

dtd:LA-id1882 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "berechnet gemäß dem nach Nummer 13 veröffentlichten Verfahren kalenderjährlich nach Erhalt der Jahresmeldungen nach § 20 Absatz 1 Nummer 2 und der Vollständigkeitserklärungen nach § 11 die den einzelnen Systemen und Branchenlösungen in diesem Zeitraum zuzuordnenden Marktanteile, stellt diese durch Verwaltungsakt fest und veröffentlicht das Ergebnis der Feststellung im Internet,"@de .

dtd:LA-id1883 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kann gemäß § 7 Absatz 5 die Aufnahme einer systembeteiligungspflichtigen Verpackung in ein System untersagen,"@de .

dtd:LA-id1884 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "prüft Anzeigen nach § 8 Absatz 2 sowie Mengenstromnachweise nach § 8 Absatz 3 und trifft die zur Überwachung einer Branchenlösung im Einzelfall erforderlichen Anordnungen,"@de .

dtd:LA-id1885 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kann die Leistung von Sicherheiten nach § 8 Absatz 5 und § 25 Absatz 6 verlangen,"@de .

dtd:LA-id1886 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gewährt den zuständigen Landesbehörden auf deren Verlangen Einsicht in die bei ihr hinterlegten Datenmeldungen nach § 10, Vollständigkeitserklärungen nach § 11, Mengenstromnachweise nach § 17 und Meldungen der Systeme nach § 20 Absatz 1 und erteilt ihnen auf der Grundlage der §§ 4 bis 8 des Verwaltungsverfahrensgesetzes die zur Erfüllung ihrer Aufgaben erforderlichen Auskünfte,"@de .

dtd:LA-id1887 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "informiert die zuständigen Landesbehörden unverzüglich, wenn ihr konkrete Anhaltspunkte für die Begehung einer Ordnungswidrigkeit nach § 36 vorliegen, und fügt vorhandene Beweisdokumente bei,"@de .

dtd:LA-id1888 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kann nähere Verfahrensanweisungen für die Registrierung nach § 9 Absatz 3 Satz 3, die Datenmeldungen nach § 10 Absatz 2, die Hinterlegung der Vollständigkeitserklärungen nach § 11 Absatz 3 Satz 3, die Hinterlegung der Mengenstromnachweise nach § 17 Absatz 3 Satz 3 und die Übermittlung der Zwischen- und Jahresmeldungen nach § 20 Absatz 2 Satz 2 erteilen und veröffentlichen,"@de .

dtd:LA-id1889 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entscheidet auf Antrag durch Verwaltungsakt über die Einordnung einer Verpackung als systembeteiligungspflichtig im Sinne von § 3 Absatz 8; sie kann hierzu Verwaltungsvorschriften erlassen,"@de .

dtd:LA-id1890 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entscheidet auf Antrag durch Verwaltungsakt über die Einordnung einer Verpackung als Mehrwegverpackung im Sinne von § 3 Absatz 3,"@de .

dtd:LA-id1891 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entscheidet auf Antrag durch Verwaltungsakt über die Einordnung einer Getränkeverpackung als pfandpflichtig im Sinne von § 31,"@de .

dtd:LA-id1892 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entscheidet auf Antrag durch Verwaltungsakt über die Einordnung einer Anfallstelle von Abfällen als eine mit privaten Haushaltungen vergleichbare Anfallstelle im Sinne von § 3 Absatz 11,"@de .

dtd:LA-id1893 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nimmt Sachverständige und sonstige Prüfer nach erfolgter Anzeige gemäß § 27 Absatz 1 oder 2 in das Prüferregister auf und veröffentlicht dieses im Internet, kann gemäß § 27 Absatz 1 Satz 2 oder Absatz 2 Satz 2 entsprechende Nachweise fordern und eine Aufnahme in das Prüferregister im Einzelfall ablehnen sowie gemäß § 27 Absatz 4 einen registrierten Sachverständigen oder sonstigen Prüfer aus dem Register entfernen,"@de .

dtd:LA-id1894 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ist befugt, im Einvernehmen mit dem Bundeskartellamt Prüfleitlinien zu entwickeln, die von den Systemprüfern und den registrierten Sachverständigen sowie von Wirtschaftsprüfern, Steuerberatern und vereidigten Buchprüfern bei Prüfungen im Rahmen dieses Gesetzes zu beachten sind,"@de .

dtd:LA-id1895 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "übermittelt gemäß § 15 Absatz 2 des Umweltstatistikgesetzes vom 16. August 2005 (BGBl. I S. 2446), das zuletzt durch Artikel 1 des Gesetzes vom 22. September 2021 (BGBl. I S. 4363) geändert worden ist, den statistischen Ämtern der Länder und dem Statistischen Bundesamt auf Anforderung die für die Erhebung nach § 5a Absatz 2 bis 6 des Umweltstatistikgesetzes erforderlichen Namen, Anschriften und E-Mail-Adressen der in diese Erhebungen einbezogenen Stellen,"@de .

dtd:LA-id1896 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "übermittelt gemäß § 14 Absatz 2 Nummer 4 Buchstabe a des Umweltstatistikgesetzes den statistischen Ämtern der Länder und dem Statistischen Bundesamt auf Anforderung die die für die Erhebung nach § 5a des Umweltstatistikgesetzes erforderlichen Daten, soweit sie der Zentralen Stelle aufgrund ihrer Pflichten nach diesem Gesetz vorliegen,"@de .

dtd:LA-id1897 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "stellt dem Umweltbundesamt gemäß § 8 Absatz 2 Satz 2 bis 4 des Einwegkunststofffondsgesetzes vom 11. Mai 2023 (BGBl. 2023 I Nr. 124), in der jeweils geltenden Fassung, die vorhandenen Registerangaben nach § 9 einschließlich der notwendigen technischen Informationen zum Datenabruf zur Verfügung,"@de .

dtd:LA-id1898 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "verwendet die gemäß § 8 Absatz 3 Satz 1 des Einwegkunststofffondsgesetzes vom Umweltbundesamt übermittelten Registerangaben zur Erfüllung ihrer Aufgaben und legt gemäß § 8 Absatz 3 Satz 2 des Einwegkunststofffondsgesetzes im Einvernehmen mit dem Umweltbundesamt das Format des elektronischen Datenaustauschs fest und"@de .

dtd:LA-id1899 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ist befugt, die mit der Erfüllung der ihr nach diesem Absatz zugewiesenen Aufgaben notwendigerweise zusammenhängenden Tätigkeiten durchzuführen."@de .

dtd:LA-id1900 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "errichtet und betreibt die für die Registrierung nach § 9 und die Übermittlung der Daten nach den §§ 10, 11 und 20 erforderlichen elektronischen Datenverarbeitungssysteme,"@de .

dtd:LA-id1901 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "stellt für die wettbewerbsneutrale Ausschreibung von Sammelleistungen gemäß § 23 Absatz 10 Satz 2 den Zugang zu einer elektronischen Ausschreibungsplattform zur Verfügung,"@de .

dtd:LA-id1902 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "schließt Finanzierungsvereinbarungen nach § 25 Absatz 1 Satz 2 mit den Systemen und Betreibern von Branchenlösungen,"@de .

dtd:LA-id1903 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kann Finanzierungsvereinbarungen nach § 25 Absatz 1 Satz 2 kündigen, wenn Systeme oder Betreiber von Branchenlösungen ihre gegenüber der Zentralen Stelle bestehenden gesetzlichen oder vertraglichen Pflichten in erheblichem Maße verletzen, insbesondere indem sie wiederholt Meldepflichten, die Auswirkungen auf die Finanzierung der Zentralen Stelle haben, trotz Aufforderung nicht, nicht richtig oder nicht vollständig erfüllen, mit der Entrichtung eines nicht unerheblichen Teils der vereinbarten Umlage im Verzug sind oder die nach § 25 Absatz 6 geforderte Sicherheit nicht leisten,"@de .

dtd:LA-id1904 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "führt mindestens einmal jährlich eine Schulung nach § 27 Absatz 3 durch und kann im Anwendungsbereich dieses Gesetzes zusätzliche Fortbildungsveranstaltungen für registrierte Sachverständige anbieten,"@de .


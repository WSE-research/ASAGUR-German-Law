    dtd:has_Size dtd:normal ;
    dtd:has_Value "Markennamen, unter denen der Hersteller seine Verpackungen in Verkehr bringt;"@de .

dtd:LA-id1802 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben zu den Verpackungen, die der Hersteller in Verkehr bringt, aufgeschlüsselt nach systembeteiligungspflichtigen Verpackungen gemäß § 3 Absatz 8, den jeweiligen Verpackungen gemäß § 15 Absatz 1 Satz 1 Nummer 1 bis 5 und Einweggetränkeverpackungen, die gemäß § 31 der Pfandpflicht unterliegen;"@de .

dtd:LA-id1803 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erklärung, dass sämtliche Angaben nach diesem Absatz der Wahrheit entsprechen."@de .


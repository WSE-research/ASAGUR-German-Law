    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verkaufs- und Umverpackungen, für die wegen Systemunverträglichkeit nach § 7 Absatz 5 eine Systembeteiligung nicht möglich ist,"@de .

dtd:LA-id1826 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verkaufsverpackungen schadstoffhaltiger Füllgüter oder"@de .


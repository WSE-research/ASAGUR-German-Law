    dtd:has_Size dtd:normal ;
    dtd:has_Value "Transportverpackungen,"@de .

dtd:LA-id1824 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verkaufs- und Umverpackungen, die nach Gebrauch typischerweise nicht bei privaten Endverbrauchern als Abfall anfallen,"@de .


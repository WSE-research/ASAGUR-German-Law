    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Abs. 1 Satz 1 nicht sicherstellt, dass der Kältemittelverlust einen dort genannten Grenzwert nicht überschreitet,"@de .

dtd:LA-id398 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Absatz 1 Satz 2 den Zugang zu einer Verbindungsstelle nicht sicherstellt,"@de .

dtd:LA-id399 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Abs. 2 Satz 1 eine Einrichtung nicht oder nicht rechtzeitig überprüft,"@de .

dtd:LA-id400 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Abs. 3 eine Klimaanlage befüllt,"@de .

dtd:LA-id401 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 5 Abs. 1 Satz 1 oder § 6 Absatz 1 Satz 1, auch in Verbindung mit Satz 2, eine dort genannte Tätigkeit durchführt,"@de .

dtd:LA-id402 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 8 Absatz 1 Satz 1 ein dort genanntes Unternehmen beauftragt,"@de .

dtd:LA-id403 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 8 Absatz 1 Satz 2, Absatz 2, Absatz 3 oder Absatz 4 nicht sicherstellt, dass eine dort genannte Tätigkeit durch dort genannte Personen durchgeführt wird,"@de .

dtd:LA-id404 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 9 Absatz 2 fluorierte Treibhausgase verkauft oder kauft oder"@de .

dtd:LA-id405 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 9 Absatz 3 eine dort genannte Einrichtung verkauft."@de .

dtd:LA-id406 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Absatz 2 Satz 3 eine dort genannte Aufzeichnung nicht, nicht richtig oder nicht vollständig führt oder"@de .

dtd:LA-id407 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Absatz 2 Satz 4 eine dort genannte Aufzeichnung nicht oder nicht mindestens fünf Jahre aufbewahrt oder nicht oder nicht rechtzeitig vorlegt."@de .

dtd:LA-id408 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:LA-id928 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "natürliche Personen mit Wohnsitz oder gewöhnlichem Aufenthalt im Inland,"@de .

dtd:LA-id929 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "juristische Personen und rechtsfähige Personengesellschaften mit Sitz oder Ort der Leitung im Inland,"@de .

dtd:LA-id930 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zweigniederlassungen ausländischer juristischer Personen oder rechtsfähiger Personengesellschaften, wenn die Zweigniederlassungen ihre Leitung im Inland haben und es für sie eine gesonderte Buchführung gibt, und"@de .

dtd:LA-id931 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebsstätten ausländischer juristischer Personen oder rechtsfähiger Personengesellschaften im Inland, wenn die Betriebsstätten ihre Verwaltung im Inland haben."@de .

dtd:LA-id932 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "natürliche Personen mit Wohnsitz oder gewöhnlichem Aufenthalt in der Europäischen Union,"@de .

dtd:LA-id933 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "juristische Personen oder rechtsfähige Personengesellschaften mit Sitz oder Ort der Leitung in der Europäischen Union,"@de .

dtd:LA-id934 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zweigniederlassungen juristischer Personen, deren Sitz oder Ort der Leitung in einem Drittland liegt, wenn die Zweigniederlassungen ihre Leitung in der Europäischen Union haben und es für sie eine gesonderte Buchführung gibt, und"@de .

dtd:LA-id935 a dtd:LA ;
    dtd:has_Size dtd:normal ;

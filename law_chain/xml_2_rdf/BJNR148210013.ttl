    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anteile an einem Wertpapiersammelbestand oder an einer Sammelschuldbuchforderung,"@de .

dtd:LA-id942 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rechte auf Lieferung oder Zuteilung von Wertpapieren im Sinne der Nummern 1 und 2."@de .


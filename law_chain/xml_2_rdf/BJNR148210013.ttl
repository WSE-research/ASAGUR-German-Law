    dtd:has_Size dtd:normal ;
    dtd:has_Value "zwischenstaatliche Vereinbarungen umzusetzen, denen die gesetzgebenden Körperschaften in der Form eines Bundesgesetzes zugestimmt haben."@de .

dtd:LA-id957 a dtd:LA ;
    dtd:has_Size dtd:normal ;

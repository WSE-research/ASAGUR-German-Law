dtd:LA-id989 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "innerhalb von zwei Monaten nach dem Erlangen der Kenntnis vom Abschluss des schuldrechtlichen Vertrags über den Erwerb ein Prüfverfahren eröffnet und"@de .

dtd:LA-id990 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "innerhalb von vier Monaten nach dem vollständigen Eingang der nach Absatz 2 Satz 2 und 4 bestimmten Unterlagen die Beschränkungen oder Handlungspflichten anordnet."@de .

dtd:LA-id991 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von einem unmittelbaren oder mittelbaren Erwerber, einem Veräußerer oder einem inländischen Unternehmen eine Auskunft oder Unterlagen nach Absatz 2 Satz 5 nachfordert oder"@de .

dtd:LA-id992 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit den am Erwerb Beteiligten vertragliche Regelungen zum Schutz der in § 4 Absatz 1 Nummer 1, 4 oder 4a genannten Rechtsgüter verhandelt."@de .

dtd:LA-id993 a dtd:LA ;

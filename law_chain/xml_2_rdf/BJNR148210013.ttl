    dtd:has_Size dtd:normal ;
    dtd:has_Value "Waren aus Drittländern ins Inland liefert oder liefern lässt und über die Lieferung der Waren bestimmt oder"@de .

dtd:LA-id920 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall von Software oder Technologie über deren Übertragung aus Drittländern ins Inland einschließlich ihrer Bereitstellung auf elektronischem Weg im Inland bestimmt."@de .

dtd:LA-id921 a dtd:LA ;
    dtd:has_Size dtd:normal ;

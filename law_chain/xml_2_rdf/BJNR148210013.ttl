    dtd:has_Value "die Übertragung von Software oder Technologie einschließlich ihrer Bereitstellung auf elektronischem Weg für natürliche und juristische Personen im Inland."@de .

dtd:LA-id923 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Freizone gebraucht, verbraucht oder verarbeitet werden oder"@de .


dtd:LA-id958 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Güter, die zur Durchführung militärischer Aktionen bestimmt sind."@de .

dtd:LA-id959 a dtd:LA ;
    dtd:has_Size dtd:normal ;

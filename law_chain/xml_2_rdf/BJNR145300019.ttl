    dtd:has_Value "einem Kleiderspind von ausreichender Größe, mindestens 475 Liter Rauminhalt, und"@de .

dtd:LA-id909 a dtd:LA ;
    dtd:has_Value "einer Kommode oder einem entsprechenden Behältnis von mindestens 56 Liter Rauminhalt."@de .

dtd:LA-id910 a dtd:LA ;
    dtd:has_Value "einem kleinen Schrank für den Toilettenartikelbedarf der Besatzungsmitglieder,"@de .

dtd:LA-id911 a dtd:LA ;
    dtd:has_Value "einem fest angebrachten, aufklappbaren oder ausziehbaren Tisch oder Pult,"@de .

dtd:LA-id912 a dtd:LA ;
    dtd:has_Value "einem Spiegel,"@de .

dtd:LA-id913 a dtd:LA ;
    dtd:has_Value "einer Steckdose,"@de .

dtd:LA-id914 a dtd:LA ;
    dtd:has_Value "einem Anschluss an eine Gemeinschaftsantenne,"@de .

dtd:LA-id915 a dtd:LA ;
    dtd:has_Value "einem Bücherbrett,"@de .

dtd:LA-id916 a dtd:LA ;
    dtd:has_Value "Kleiderhaken und"@de .

dtd:LA-id917 a dtd:LA ;
    dtd:has_Value "den erforderlichen Sitzgelegenheiten."@de .

dtd:LA-id918 a dtd:LA ;
    dtd:has_Value "Kochgeräten,"@de .

dtd:LA-id919 a dtd:LA ;
    dtd:has_Value "einem Doppelspülbecken, einem Handwaschbecken und einer Einrichtung mit Einmalhandtüchern,"@de .

dtd:LA-id920 a dtd:LA ;
    dtd:has_Value "einem Anschluss für kaltes und warmes Trinkwasser,"@de .

dtd:LA-id921 a dtd:LA ;
    dtd:has_Value "den für die Unterbringung der beweglichen Kochgeräte und des Essgeschirrs erforderlichen Schränken, Regalen und Geschirrgestellen aus einem geeigneten, nicht rostenden Werkstoff,"@de .

dtd:LA-id922 a dtd:LA ;
    dtd:has_Value "einer Abluftanlage und"@de .

dtd:LA-id923 a dtd:LA ;

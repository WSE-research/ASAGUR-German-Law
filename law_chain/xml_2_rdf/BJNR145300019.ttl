dtd:LA-id924 a dtd:LA ;
    dtd:has_Value "auf Schiffen mit einer Bruttoraumzahl von bis zu 200, die keine Fischereifahrzeuge sind, und"@de .

dtd:LA-id925 a dtd:LA ;
    dtd:has_Value "auf Fischereifahrzeugen mit einer Länge von weniger als 24 Metern."@de .

dtd:LA-id926 a dtd:LA ;
    dtd:has_Value "ein Doppelspülbecken mit einem Anschluss für kaltes und warmes Trinkwasser,"@de .

dtd:LA-id927 a dtd:LA ;
    dtd:has_Value "ein Spender für Einmalhandtücher,"@de .

dtd:LA-id928 a dtd:LA ;
    dtd:has_Value "ein Kühlschrank, der leicht zugänglich ist und dessen Fassungsvermögen für die Bedürfnisse der Besatzungsmitglieder, die die Messen besuchen, ausreicht,"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "überwiegende Gründe des Gemeinwohls die Befreiung erfordern."@de .

dtd:LA-id412 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Aufstellung von Bauleitplänen"@de .

dtd:LA-id413 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Unterhaltung der Straßen und Wege sowie Gewässer."@de .


    dtd:has_Value ""@de .

dtd:P-id277 a dtd:P ;
    dtd:has_Value ""@de .

dtd:P-id279 a dtd:P ;
    dtd:has_Value ""@de .

dtd:P-id281 a dtd:P ;
    dtd:has_Value "Für Strom aus Anlagen, deren Zuschläge in einem Zuschlagsverfahren eines Gebotstermins vor dem 1. Dezember 2022 ermittelt worden sind, ist diese Verordnung in der am 28. Juli 2022 geltenden Fassung anzuwenden."@de .

dtd:P-id283 a dtd:P ;
    dtd:has_Value "Diese Verordnung tritt zum 31. Dezember 2028 außer Kraft."@de .


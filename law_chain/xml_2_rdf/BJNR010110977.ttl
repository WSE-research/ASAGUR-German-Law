dtd:LA-id168 a dtd:LA ;
    dtd:has_DL dtd:DL-id108 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Luftfahrzeugen mit Strahl- oder Turbinenantrieba)das Rollen, Schweben oder Anschwimmen von der Park- zur Startposition und das Rollen, Schweben oder Abschwimmen nach dem Verlassen der Start- und Landebahn oder des Landepunktes zur Parkposition, b)der Betrieb im Stand vom Anlassen des Triebwerkes bis zum Stillstand des Triebwerkes sowie die Bewegung bei laufendem Triebwerk zum Zwecke von Funktionsprüfungen oder Positionswechsel, "@de .

dtd:LA-id169 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Rollen, Schweben oder Anschwimmen von der Park- zur Startposition und das Rollen, Schweben oder Abschwimmen nach dem Verlassen der Start- und Landebahn oder des Landepunktes zur Parkposition,"@de .

dtd:LA-id170 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Betrieb im Stand vom Anlassen des Triebwerkes bis zum Stillstand des Triebwerkes sowie die Bewegung bei laufendem Triebwerk zum Zwecke von Funktionsprüfungen oder Positionswechsel,"@de .

dtd:LA-id171 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Starrflüglern mit Kolbentriebwerk das Rollen auf nicht ordnungsgemäß ausgebauter und befestigter Oberfläche, die nicht durch Angehörige des Flugbetriebspersonals oder durch einen Luftfahrzeugführer vorher erkundet ist,"@de .

dtd:LA-id172 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Luftnotfall der Absprung mit dem Fallschirm,"@de .

dtd:LA-id173 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Luftrettungsdienst oder in der Ausbildung dazu Dienstverrichtungen im Gefahrenbereich der Rotoren eines Drehflüglers oder beim Abseilen oder Aufseilen an einem Drehflügler."@de .

dtd:LA-id174 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Besatzung eines Starrflüglers mit Strahl- oder Turbinenantrieb gehören,"@de .

dtd:LA-id175 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Ausbildung zum Angehörigen der Besatzung, zum Fluglehrer oder zum Testpiloten stehen oder auf einen anderen Luftfahrzeugtyp umgeschult werden,"@de .

dtd:LA-id176 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Lehrpersonal für die fliegerische Ausbildung oder zum Prüfpersonal für die Abnahme fliegerischer Prüfungen gehören,"@de .

dtd:LA-id177 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dienstverrichtungen nach § 1 Abs. 3 vornehmen,"@de .

dtd:LA-id178 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen besonders gefährlichen Auftrag (§ 3 Abs. 1) durchführen,"@de .

dtd:LA-id179 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Besatzung eines Luftfahrzeuges gehören, das sich in einem besonders gefährlichen Flug- oder Betriebszustand (§ 3 Abs. 3) befindet,"@de .

dtd:LA-id180 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in einer Flughöhe von weniger als 500 Meter über Grund,"@de .

dtd:LA-id181 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Verlastung oder Abwurf von Gegenständen,"@de .

dtd:LA-id182 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Luftrettungseinsatz, dessen Durchführung mit einer besonderen Lebensgefahr verbunden ist,"@de .

dtd:LA-id183 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Langsamflug, Kunstflug oder Verbandsflug,"@de .

dtd:LA-id184 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Durchführung von Messungen im Rahmen der Flugsicherung oder Wettererkundung (Meßflug),"@de .

dtd:LA-id185 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Gebirge bei einem seitlichen Abstand von weniger als 20 Meter zu einer Steilwand,"@de .

dtd:LA-id186 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Erprobung oder zum Nachfliegen von neuen Luftfahrzeugtypen oder Luftfahrzeugen im Rahmen einer beabsichtigten Änderung des bisherigen Verwendungszwecks,"@de .

dtd:LA-id187 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Abnahme von neuen Luftfahrzeugen,"@de .

dtd:LA-id188 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Überprüfung von überholten Luftfahrzeugen oder neuen oder erneuerten wesentlichen Luftfahrzeugteilen,"@de .

dtd:LA-id189 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Durchführung von Triebwerks- und Geräteerprobungen."@de .

dtd:LA-id190 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Dauer des Start- und Landevorganges (§ 1 Abs. 2),"@de .

dtd:LA-id191 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Dauer eines zur Durchführung des Flugauftrages notwendigen Durchfliegens von Schlechtwettergebieten, wenn das Luftfahrzeug nach Instrumentenflugregeln fliegen muß,"@de .

dtd:LA-id192 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn das Luftfahrzeug steuerungsunfähig ist."@de .

dtd:LA-id193 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Helmtauchers vom Schließen bis zum Öffnen des Helmfensters,"@de .

dtd:LA-id194 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Schwimmtauchers vom Aufsetzen bis zum Absetzen der Schwimmaske."@de .

dtd:LA-id195 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bergführer sind oder an Bergführerlehrgängen teilnehmen,"@de .

dtd:LA-id196 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus dienstlichen Gründen Bergnothilfe leisten,"@de .

dtd:LA-id197 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Bergnothilfe ausgebildet werden oder"@de .

dtd:LA-id198 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ausbildungspersonal für die Fels- und Eisausbildung sind,"@de .

dtd:P-id200 a dtd:P ;
    dtd:has_Value "(+++ Textnachweis ab: 1.1.1977 +++)"@de .

dtd:P-id202 a dtd:P ;
    dtd:has_Value "Auf Grund des § 43 Abs. 3 Satz 2 und 3 des Beamtenversorgungsgesetzes vom 24. August 1976 (BGBl. I S. 2485) verordnet die Bundesregierung mit Zustimmung des Bundesrates:"@de .

dtd:P-id204 a dtd:P ;
    dtd:has_Value "(1) Flugdienst im Sinne des § 43 Abs. 3 Satz 1 Nr. 1 des Beamtenversorgungsgesetzes ist jeder Aufenthalt, der an Bord eines Luftfahrzeuges zur Durchführung eines Flugauftrages oder eines sonstigen dienstlichen Auftrages vom Beginn des Starts bis zur Beendigung der Landung erforderlich ist."@de .

dtd:P-id205 a dtd:P ;
    dtd:has_Value "(2) Der Start beginnt nach der Freigabe zum Start oder aus eigenem Entschluß des verantwortlichen Luftfahrzeugführers mit der Bewegung des Luftfahrzeuges zum Zwecke des Abhebens und endet mit Erreichen der Reiseflughöhe oder der durch Flugauftrag vorgeschriebenen Mindestflughöhe. Die Landung beginnt mit der Freigabe zur Landung oder aus eigenem Entschluß des verantwortlichen Luftfahrzeugführers und endet bei Starrflüglern mit dem Verlassen der Start- und Landebahn, bei Drehflüglern mit dem Aufsetzen oder dem Ausrollen."@de .

dtd:P-id206 a dtd:P ;
    dtd:has_DL dtd:DL-id99 ;
    dtd:has_Value "(3) Zum Flugdienst gehören auch 1.bei Luftfahrzeugen mit Strahl- oder Turbinenantrieba)das Rollen, Schweben oder Anschwimmen von der Park- zur Startposition und das Rollen, Schweben oder Abschwimmen nach dem Verlassen der Start- und Landebahn oder des Landepunktes zur Parkposition, b)der Betrieb im Stand vom Anlassen des Triebwerkes bis zum Stillstand des Triebwerkes sowie die Bewegung bei laufendem Triebwerk zum Zwecke von Funktionsprüfungen oder Positionswechsel,  2.bei Starrflüglern mit Kolbentriebwerk das Rollen auf nicht ordnungsgemäß ausgebauter und befestigter Oberfläche, die nicht durch Angehörige des Flugbetriebspersonals oder durch einen Luftfahrzeugführer vorher erkundet ist, 3.im Luftnotfall der Absprung mit dem Fallschirm, 4.im Luftrettungsdienst oder in der Ausbildung dazu Dienstverrichtungen im Gefahrenbereich der Rotoren eines Drehflüglers oder beim Abseilen oder Aufseilen an einem Drehflügler. "@de .

dtd:P-id208 a dtd:P ;
    dtd:has_DL dtd:DL-id113 ;
    dtd:has_Value "(1) Beamte, die 1.zur Besatzung eines Starrflüglers mit Strahl- oder Turbinenantrieb gehören, 2.in der Ausbildung zum Angehörigen der Besatzung, zum Fluglehrer oder zum Testpiloten stehen oder auf einen anderen Luftfahrzeugtyp umgeschult werden, 3.zum Lehrpersonal für die fliegerische Ausbildung oder zum Prüfpersonal für die Abnahme fliegerischer Prüfungen gehören, 4.Dienstverrichtungen nach § 1 Abs. 3 vornehmen, 5.einen besonders gefährlichen Auftrag (§ 3 Abs. 1) durchführen, 6.zur Besatzung eines Luftfahrzeuges gehören, das sich in einem besonders gefährlichen Flug- oder Betriebszustand (§ 3 Abs. 3) befindet, sind Angehörige des besonders gefährdeten fliegenden Personals."@de .

dtd:P-id209 a dtd:P ;
    dtd:has_Value "(2) Für Beamte, die auf Grund eines dienstlich erteilten Auftrages in einem Luftfahrzeug des Bundes, eines Landes oder der verbündeten Streitkräfte mitfliegen, gilt Absatz 1 sinngemäß."@de .

dtd:P-id211 a dtd:P ;
    dtd:has_DL dtd:DL-id126 ;
    dtd:has_Value "(1) Ein besonders gefährlicher Auftrag (§ 2 Abs. 1 Nr. 5) liegt vor bei vorgeschriebenen Flügen 1.in einer Flughöhe von weniger als 500 Meter über Grund, 2.mit Verlastung oder Abwurf von Gegenständen, 3.im Luftrettungseinsatz, dessen Durchführung mit einer besonderen Lebensgefahr verbunden ist, 4.im Langsamflug, Kunstflug oder Verbandsflug, 5.zur Durchführung von Messungen im Rahmen der Flugsicherung oder Wettererkundung (Meßflug), 6.im Gebirge bei einem seitlichen Abstand von weniger als 20 Meter zu einer Steilwand, 7.zur Erprobung oder zum Nachfliegen von neuen Luftfahrzeugtypen oder Luftfahrzeugen im Rahmen einer beabsichtigten Änderung des bisherigen Verwendungszwecks, 8.zur Abnahme von neuen Luftfahrzeugen, 9.zur Überprüfung von überholten Luftfahrzeugen oder neuen oder erneuerten wesentlichen Luftfahrzeugteilen, 10.zur Durchführung von Triebwerks- und Geräteerprobungen.  "@de .

dtd:P-id212 a dtd:P ;
    dtd:has_Value "(2) Einem besonders gefährlichen Auftrag im Sinne des Absatzes 1 Nr. 1, 3, 4 und 6 stehen die Fälle gleich, in denen sich abweichend von dem erteilten Flugauftrag die Notwendigkeit der dort bezeichneten Flugarten erst nach dem Start auf Grund der die Flugbedingungen beeinflussenden Umstände ergibt."@de .

dtd:P-id213 a dtd:P ;
    dtd:has_DL dtd:DL-id147 ;
    dtd:has_Value "(3) Ein besonders gefährlicher Flug- oder Betriebszustand (§ 2 Abs. 1 Nr. 6) liegt vor 1.für die Dauer des Start- und Landevorganges (§ 1 Abs. 2), 2.für die Dauer eines zur Durchführung des Flugauftrages notwendigen Durchfliegens von Schlechtwettergebieten, wenn das Luftfahrzeug nach Instrumentenflugregeln fliegen muß, 3.wenn das Luftfahrzeug steuerungsunfähig ist. "@de .

dtd:P-id215 a dtd:P ;
    dtd:has_Value "(1) Beamte, die zu Unterwasserarbeiten mit einem Helmtauchgerät ausgebildet, in Übung gehalten oder eingesetzt werden, sind Helmtaucher. Beamte, die zu Unterwasserarbeiten mit einem Leichttauchgerät ausgebildet, in Übung gehalten oder eingesetzt werden, sind Schwimmtaucher."@de .

dtd:P-id216 a dtd:P ;
    dtd:has_DL dtd:DL-id154 ;
    dtd:has_Value "(2) Besonders gefährlicher Tauchdienst ist jede Dienstverrichtung, 1.des Helmtauchers vom Schließen bis zum Öffnen des Helmfensters, 2.des Schwimmtauchers vom Aufsetzen bis zum Absetzen der Schwimmaske. "@de .

dtd:P-id218 a dtd:P ;
    dtd:has_DL dtd:DL-id159 ;
    dtd:has_Value "(1) Beamte, die 1.Bergführer sind oder an Bergführerlehrgängen teilnehmen, 2.aus dienstlichen Gründen Bergnothilfe leisten, 3.für die Bergnothilfe ausgebildet werden oder 4.Ausbildungspersonal für die Fels- und Eisausbildung sind, sind während der Dienstverrichtung nach Absatz 2 Beamte im Bergrettungsdienst."@de .

dtd:P-id219 a dtd:P ;
    dtd:has_Value "(2) Bergrettungsdienst ist jede Dienstverrichtung, die beim Einsatz oder bei der Ausbildung zur Bergnothilfe ausgeübt wird, und zwar im Felsklettern ab Schwierigkeitsgrad III, im Eisgehen ab Schwierigkeitsgrad II oder unter sonstigen Bedingungen, mit denen eine besondere Lebensgefahr verbunden ist. Ausbildung sind auch alle Dienstverrichtungen im Sinne des Satzes 1, die notwendig sind, um den Beamten für die Bergnothilfe in Übung zu halten."@de .

dtd:P-id221 a dtd:P ;
    dtd:has_Value "(1) Beamte, die zur Untersuchung von Munition dienstlich eingesetzt, und Beamte, die dabei als Hilfskräfte tätig sind, gehören während des dienstlichen Umgangs mit Munition (Absatz 3) zum besonders gefährdeten Munitionsuntersuchungspersonal."@de .

dtd:P-id222 a dtd:P ;
    dtd:has_Value "(2) Munition sind alle Gegenstände, die Explosivstoffe enthalten oder aus Explosivstoffen bestehen. Zur Erzeugung von Feuer, Rauch und künstlichem Nebel oder einer anderen Wirkung können die Gegenstände auch andere Stoffe enthalten."@de .

dtd:P-id223 a dtd:P ;
    dtd:has_Value "(3) Dienstlicher Umgang mit Munition ist das dienstlich angeordnete Untersuchen (Prüfen und Feststellen des Zustandes) von Munition, deren Zustand zweifelhaft oder deren Herkunft unbekannt ist. Dazu gehören alle Dienstverrichtungen, die mit der Untersuchung im Zusammenhang stehen, insbesondere das Suchen, Markieren, Freilegen, Befördern, Zerlegen und Vernichten sowie das Entfernen, Auswechseln und Hinzufügen von Teilen."@de .

dtd:P-id225 a dtd:P ;
    dtd:has_Value "(1) Polizeivollzugsbeamte, die in den zur Bekämpfung schwerer Gewaltkriminalität gebildeten Verbänden der Bundespolizei und entsprechenden Polizeiverbänden der Länder dienstlich eingesetzt oder ausgebildet werden, sind Angehörige von Verbänden für besondere polizeiliche Einsätze. Entsprechendes gilt für andere Beamte, die gemeinsam mit den in Satz 1 bezeichneten Beamten oder wie diese besonders zur Bekämpfung schwerer Gewaltkriminalität eingesetzt oder ausgebildet werden."@de .

dtd:P-id226 a dtd:P ;
    dtd:has_Value "(2) Besonders gefährlich ist eine Diensthandlung, die beim besonderen polizeilichen Einsatz zur Bekämpfung schwerer Gewaltkriminalität oder in einer Sonderausbildung dazu vorgenommen wird und nach der Art des Einsatzes oder der Sonderausbildung über die im Polizeidienst übliche Gefährdung hinausgeht."@de .

dtd:P-id228 a dtd:P ;
    dtd:has_Value "Beamte, die unter einem schwebenden Drehflügelflugzeug Außenlasten an diesem Flugzeug ein- oder aushängen oder die Verbindung einer Steuerleitung zwischen Flugzeug und Außenlast herstellen oder lösen, befinden sich im Einsatz im Sinne des § 43 Abs. 3 Satz 1 Nr. 6 des Beamtenversorgungsgesetzes. Der Einsatz umfaßt auch die Ausbildung und Erprobung."@de .

dtd:P-id230 a dtd:P ;
    dtd:has_Value "Für Angestellte und Arbeiter, zu deren Dienstobliegenheiten Tätigkeiten der in § 43 Abs. 3 Satz 1 Nr. 1 bis 6 des Beamtenversorgungsgesetzes bezeichneten Art gehören, gelten die §§ 1 bis 8 entsprechend."@de .

dtd:P-id232 a dtd:P ;
    dtd:has_Value "Diese Verordnung gilt nach § 14 des Dritten Überleitungsgesetzes in Verbindung mit § 108 Satz 2 des Beamtenversorgungsgesetzes auch im Land Berlin."@de .

dtd:P-id234 a dtd:P ;
    dtd:has_Value "Diese Verordnung tritt mit Wirkung vom 1. Januar 1977 in Kraft."@de .


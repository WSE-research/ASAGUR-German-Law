dtd:LA-id195 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "daß Berlin auf Grund des Einigungsvertrages Hauptstadt des vereinigten Deutschlands ist,"@de .

dtd:LA-id196 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "daß der Deutsche Bundestag seinen politischen Willen vielfach bekundet hat, daß nach der Herstellung der deutschen Einheit Parlament und Regierung wieder in der deutschen Hauptstadt Berlin, die in über 40 Jahren deutscher Teilung ein Symbol des Willens zur deutschen Einheit war, ihren Sitz haben sollen,"@de .

dtd:LA-id197 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "daß Bonn in Wahrnehmung der Aufgaben als provisorische Bundeshauptstadt Wesentliches zum Aufbau und zur Identifikation des demokratischen, an bundesstaatlichen Prinzipien orientierten Deutschlands geleistet hat,"@de .

dtd:LA-id198 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf der Grundlage seines Beschlusses vom 20. Juni 1991 zur Vollendung der Einheit Deutschlands sowie seines Beschlusses zum dritten Zwischenbericht der Konzeptkommission des Ältestenrates vom 10. März 1994 und"@de .

dtd:LA-id199 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Kenntnis der Entscheidungen der Bundesregierung vom 3. Juni 1992 sowie vom 12. Oktober 1993"@de .

dtd:LA-id200 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sicherstellung einer dauerhaften und fairen Arbeitsteilung zwischen der Bundeshauptstadt Berlin und der Bundesstadt Bonn."@de .

dtd:LA-id201 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ansiedlung des Kernbereichs der Regierungsfunktionen in der Bundeshauptstadt Berlin."@de .

dtd:LA-id202 a dtd:LA ;
    dtd:has_DL dtd:DL-id121 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erhalt und Förderung politischer Funktionen in der Bundesstadt Bonn in folgenden Politikbereichen:a)Bildung und Wissenschaft, Kultur, Forschung und Technologie, Telekommunikation, b)Umwelt und Gesundheit, c)Ernährung, Landwirtschaft und Forsten, d)Entwicklungspolitik, nationale, internationale und supranationale Einrichtungen, e)Verteidigung. "@de .


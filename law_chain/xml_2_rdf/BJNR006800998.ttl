dtd:LA-id54 a dtd:LA ;
    dtd:has_DL dtd:DL-id47 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dem Bundesministerium der Finanzen werden aus dem Geschäftsbereich des Bundesministeriums für Post und Telekommunikation übertragen:-die Zuständigkeit für die Bundesanstalt für Post und Telekommunikation Deutsche Bundespost, die Museumsstiftung Post und Telekommunikation und die Unfallkasse Post und Telekom, -die beamten- und personalvertretungsrechtlichen Zuständigkeiten für die Beamten der Aktiengesellschaften Deutsche Telekom AG, Deutsche Post AG und Deutsche Postbank AG, -die Befugnis zur Herausgabe von Postwertzeichen. "@de .

dtd:LA-id55 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die der Bundesdruckerei GmbH durch Dienstleistungsüberlassungsvertrag zugeordneten Beamten werden in den Geschäftsbereich des Bundesministeriums der Finanzen übergeleitet."@de .

dtd:LA-id56 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zuständigkeit für die Bundesanstalt für Post und Telekommunikation Deutsche Bundespost, die Museumsstiftung Post und Telekommunikation und die Unfallkasse Post und Telekom,"@de .

dtd:LA-id57 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die beamten- und personalvertretungsrechtlichen Zuständigkeiten für die Beamten der Aktiengesellschaften Deutsche Telekom AG, Deutsche Post AG und Deutsche Postbank AG,"@de .

dtd:LA-id58 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Befugnis zur Herausgabe von Postwertzeichen."@de .

dtd:LA-id59 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dem Bundesministerium für Wirtschaft werden alle übrigen Zuständigkeiten aus dem Geschäftsbereich des Bundesministeriums für Post und Telekommunikation übertragen."@de .

dtd:P-id61 a dtd:P ;
    dtd:has_Value ""@de .

dtd:P-id63 a dtd:P ;
    dtd:has_Value "(+++ Textnachweis ab: 17.12.1997 +++)"@de .

dtd:P-id65 a dtd:P ;
    dtd:has_Value "Nachstehend mache ich den Organisationserlaß des Bundeskanzlers vom 17. Dezember 1997 bekannt, der mit Wirkung vom gleichen Tage in Kraft tritt:"@de .

dtd:P-id67 a dtd:P ;
    dtd:has_Value "Das Bundesministerium für Post und Telekommunikation wird mit dem Ablauf des 31. Dezember 1997 aufgelöst."@de .

dtd:P-id69 a dtd:P ;
    dtd:has_DL dtd:DL-id42 ;
    dtd:has_Value "Mit Wirkung vom 1. Januar 1998 tritt folgendes in Kraft: 1.Dem Bundesministerium der Finanzen werden aus dem Geschäftsbereich des Bundesministeriums für Post und Telekommunikation übertragen:-die Zuständigkeit für die Bundesanstalt für Post und Telekommunikation Deutsche Bundespost, die Museumsstiftung Post und Telekommunikation und die Unfallkasse Post und Telekom, -die beamten- und personalvertretungsrechtlichen Zuständigkeiten für die Beamten der Aktiengesellschaften Deutsche Telekom AG, Deutsche Post AG und Deutsche Postbank AG, -die Befugnis zur Herausgabe von Postwertzeichen. Die der Bundesdruckerei GmbH durch Dienstleistungsüberlassungsvertrag zugeordneten Beamten werden in den Geschäftsbereich des Bundesministeriums der Finanzen übergeleitet.  2.Dem Bundesministerium für Wirtschaft werden alle übrigen Zuständigkeiten aus dem Geschäftsbereich des Bundesministeriums für Post und Telekommunikation übertragen. "@de .

dtd:P-id71 a dtd:P ;
    dtd:has_Value "Die Einzelheiten des Überganges werden zwischen den beteiligten Bundesministern geregelt und dem Chef des Bundeskanzleramtes mitgeteilt."@de .


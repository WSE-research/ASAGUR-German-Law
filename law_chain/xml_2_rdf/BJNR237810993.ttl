dtd:LA-id342 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unternehmerischer Bereich; er umfaßt das Erbringen von Eisenbahnverkehrsleistungen sowie das Betreiben der Eisenbahninfrastruktur;"@de .

dtd:LA-id343 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verwaltungsbereich."@de .

dtd:LA-id344 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erfüllung der in § 20 Abs. 1 und 2 bestimmten Übertragungsverpflichtungen,"@de .

dtd:LA-id345 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wahrnehmung der hoheitlichen Aufgaben, die bis zum Inkrafttreten dieses Gesetzes von den in § 1 genannten Sondervermögen wahrgenommen worden sind, bis zur Errichtung des Eisenbahn-Bundesamtes gemäß § 2 Abs. 1 des Gesetzes über die Eisenbahnverkehrsverwaltung des Bundes vom 27. Dezember 1993 (BGBl. I S. 2378, 2394),"@de .

dtd:LA-id346 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verwaltung des Personals, welches gemäß § 12 Abs. 2 und 3 des Deutsche Bahn Gründungsgesetzes vom 27. Dezember 1993 (BGBl. I S. 2378, 2386) der Deutsche Bahn Aktiengesellschaft zugewiesen ist,"@de .

dtd:LA-id347 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Unterstützung der Verwaltung der zinspflichtigen Verbindlichkeiten des Bundeseisenbahnvermögens,"@de .

dtd:LA-id348 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verwaltung und Verwertung der Liegenschaften, die im Sinne des § 20 Abs. 1 nicht bahnnotwendig sind."@de .

dtd:LA-id349 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Einvernehmen mit dem Bundesministerium des Innern, für Bau und Heimat und dem Bundesministerium der Finanzen nach Maßgabe des § 26 Absatz 1 des Bundesbeamtengesetzes die Laufbahnen beim Bundeseisenbahnvermögen selbständig zu gestalten und Ausnahmeregelungen zu treffen,"@de .

dtd:LA-id350 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Einvernehmen mit dem Bundesministerium des Innern, für Bau und Heimat für die zugewiesenen Beamten besondere Arbeitszeitvorschriften zu erlassen und dabei von § 88 des Bundesbeamtengesetzes abweichende Regelungen über die Verpflichtung der Beamten, über die regelmäßige wöchentliche Arbeitszeit hinaus Dienst zu tun, sowie über den Ausgleich von Mehrarbeit zu treffen,"@de .

dtd:LA-id351 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dienstliche Gründe beim Bundeseisenbahnvermögen oder"@de .

dtd:LA-id352 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dienstliche oder betriebliche Gründe bei einer Gesellschaft, der der Beamte gemäß § 12 Abs. 2 und 3 des Deutsche Bahn Gründungsgesetzes zugewiesen ist,"@de .

dtd:LA-id353 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Mitglieder mit mitversicherten Angehörigen den halben Beitragssatz der Rentner der Bahnbetriebskrankenkasse,"@de .

dtd:LA-id354 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Mitglieder ohne mitversicherte Angehörige zwei Drittel des vorgenannten Beitragssatzes"@de .

dtd:LA-id355 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personaleinsatz und Personalkosten,"@de .

dtd:LA-id356 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verwaltung der Verbindlichkeiten,"@de .

dtd:LA-id357 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verwertung von Grundstücken."@de .

dtd:LA-id358 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "inwieweit die in dem Übergabebescheid genannten Liegenschaften bahnnotwendig sind,"@de .

dtd:LA-id359 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ob die Ablehnung des Erlasses eines Übergabebescheids hinsichtlich einer beanspruchten Liegenschaft rechtmäßig ist."@de .

dtd:LA-id360 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Eisenbahninfrastruktur bei Geltendmachung des Anspruches auf Übertragung der Liegenschaften ausschließlich oder ganz überwiegend für Zwecke des Schienenpersonennahverkehrs genutzt wird,"@de .

dtd:LA-id361 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Eisenbahnen des Bundes zum Erbringen von Verkehrsleistungen nicht mehr bereit sind,"@de .

dtd:LA-id362 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Vereinbarung mit Aufgabenträgern über das Erbringen von Verkehrsleistungen auf der Eisenbahninfrastruktur der Eisenbahnen des Bundes über die Finanzierung des Betriebes der Eisenbahninfrastruktur nicht zustande gekommen ist sowie"@de .

dtd:LA-id363 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Aufgabenträger das Erbringen von Verkehrsleistungen im Schienenpersonennahverkehr für mindestens 15 Jahre und das Betreiben der Eisenbahninfrastruktur für mindestens 30 Jahre garantiert."@de .

dtd:LA-id364 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebliche Sozialeinrichtungen der bisherigen Bundeseisenbahnen"@de .


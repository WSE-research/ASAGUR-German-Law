dtd:LA-id1396 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Eigentümer der Sportboote oder Wassermotorräder,"@de .

dtd:LA-id1397 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Personen, die Sportboote oder Wassermotorräder vermieten (Unternehmer) und deren Gehilfen, wenn diese den Unternehmer selbständig vertreten,"@de .

dtd:LA-id1398 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mieter, Bootsführer und Insassen der Sportboote oder Wassermotorräder."@de .

dtd:LA-id1399 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sportboote"@de .

dtd:LA-id1400 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wasserfahrzeuge mit oder ohne Maschinenantrieb, die für Sport- und Erholungszwecke gebaut worden sind und ausschließlich für Sport- oder Erholungszwecke oder für die Ausbildung zum Führen von Sportbooten verwendet werden und die für nicht mehr als zwölf Personen zuzüglich Fahrzeugführer und Besatzung zugelassen sind,"@de .

dtd:LA-id1401 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "große Sportboote"@de .

dtd:LA-id1402 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sportboote mit Kajüte und Übernachtungsmöglichkeiten, die für Fahrten seewärts der Basislinie (Küstenmeer, küstennahe Seegewässer, Hohe See) geeignet und bestimmt sind, insbesondere Segel- und Motoryachten,"@de .

dtd:LA-id1403 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kleine Sportboote"@de .

dtd:LA-id1404 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sportboote, die für Fahrten binnenwärts der Basislinie (andere Gewässer) oder in Strandnähe geeignet und bestimmt sind, insbesondere offene Segel-, Motor-, Ruder-, Falt-, Schlauch- und Wassertretboote,"@de .

dtd:LA-id1405 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wassermotorräder"@de .

dtd:LA-id1406 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wasserfahrzeuge mit weniger als vier Meter Länge, die einen Verbrennungsmotor mit Strahlpumpenantrieb als Hauptantriebsquelle verwenden und die dazu ausgelegt sind, von einer oder mehreren Personen gefahren zu werden, die nicht in, sondern auf dem Rumpf sitzen, stehen oder knien,"@de .

dtd:LA-id1407 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vermietung"@de .

dtd:LA-id1408 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die gegen Entgelt erfolgende Überlassung eines Sportbootes oder Wassermotorrades zum Gebrauch an laufend wechselnde Mieter ohne Gestellung eines Bootsführers oder einer Besatzung und ohne dass der Mieter das Sportboot gewerbsmäßig nutzt,"@de .

dtd:LA-id1409 a dtd:LA ;
    dtd:has_BR dtd:BR-id250 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gewerbsmäßige Nutzungder Einsatz von Sportbooten gegen ein Entgelt,"@de .

dtd:LA-id1410 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "anerkannte Organisation"@de .

dtd:LA-id1411 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine nach der Richtlinie 2009/15/EG anerkannte Klassifikationsgesellschaft, mit der ein Auftragsverhältnis im Sinne des Artikels 5 Absatz 2 der Richtlinie 2009/15/EG begründet worden ist,"@de .

dtd:LA-id1412 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berufsgenossenschaft"@de .

dtd:LA-id1413 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Dienststelle Schiffssicherheit bei der Berufsgenossenschaft Verkehrswirtschaft Post-Logistik Telekommunikation."@de .

dtd:LA-id1414 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Wasserfahrzeug seine Eigenschaft als Sportboot im Sinne dieser Verordnung verliert oder wesentliche Ausrüstungsgegenstände funktionsuntüchtig oder nicht mehr vorhanden sind oder"@de .

dtd:LA-id1415 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Sportboot mit Gestellung eines Bootsführers oder einer Besatzung oder zum Zweck der gewerbsmäßigen Nutzung vermietet wird und nicht über ein gültiges Sicherheitszeugnis nach § 14 verfügt."@de .

dtd:LA-id1416 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name, Wohnsitz oder Sitz und - soweit vorhanden - Betriebsstätte des Antragstellers, bei natürlichen Personen auch Geburtstag und Geburtsort,"@de .

dtd:LA-id1417 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben darüber, ob der Antragsteller bereits ein Bootszeugnis oder ein Sicherheitszeugnis der Berufsgenossenschaft für das Sportboot oder Wassermotorrad besitzt, besessen oder beantragt hat,"@de .

dtd:LA-id1418 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben über die Art des Sportbootes oder Wassermotorrades und die Personenzahl, die höchstens befördert werden soll,"@de .

dtd:LA-id1419 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben darüber, in welchem Fahrtgebiet das Sportboot oder Wassermotorrad benutzt werden soll."@de .

dtd:LA-id1420 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die vorgeschriebenen Kennzeichnungen und Kennzeichen besitzt,"@de .

dtd:LA-id1421 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein von der Zulassungsbehörde für dieses Sportboot oder Wassermotorrad ausgestelltes Bootszeugnis nach dem Muster der Anlage 1 besitzt,"@de .

dtd:LA-id1422 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die in dem Bootszeugnis nach Nummer 2 festgelegten Bedingungen und Auflagen erfüllt und"@de .

dtd:LA-id1423 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die in dem Bootszeugnis nach Nummer 2 vorgeschriebene Ausrüstung an Bord hat."@de .

dtd:LA-id1424 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, die die notwendigen Kenntnisse und Fähigkeiten zur Bedienung und Führung des Sportbootes oder Wassermotorrades offensichtlich nicht besitzen,"@de .

dtd:LA-id1425 a dtd:LA ;
    dtd:has_Size dtd:normal ;

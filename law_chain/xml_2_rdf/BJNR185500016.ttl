dtd:LA-id208 a dtd:LA ;
    dtd:has_Value "der Vermittlungsgegenstand gerichtlich anhängig ist oder in der Vergangenheit anhängig war,"@de .

dtd:LA-id209 a dtd:LA ;
    dtd:has_Value "die Streitigkeit durch außergerichtlichen Vergleich beigelegt wurde,"@de .

dtd:LA-id210 a dtd:LA ;

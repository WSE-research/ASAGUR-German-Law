dtd:P-id46 a dtd:P ;
    dtd:has_Value "Auf Grund des Artikels V Nr. 1 Abs. 13 Satz 2 des BEG-Schlußgesetzes vom 14. September 1965 (BGBl. I S. 1315) verordnet die Bundesregierung mit Zustimmung des Bundesrates:"@de .

dtd:P-id48 a dtd:P ;
    dtd:has_Value "(1) Berechtigte, denen ein dreifacher Steigerungsbetrag nach Artikel V Nr. 1 Abs. 10 Buchstabe c des BEG-Schlußgesetzes gewährt worden ist, erhalten als zusätzliche Beihilfe einen Betrag von 1.750 Deutsche Mark."@de .

dtd:P-id49 a dtd:P ;
    dtd:has_Value "(2) Berechtigte, denen ein vierfacher Steigerungsbetrag nach Artikel V Nr. 1 Abs. 10 Buchstabe d des BEG-Schlußgesetzes gewährt worden ist, erhalten als zusätzliche Beihilfe einen Betrag von 2.400 Deutsche Mark."@de .

dtd:P-id51 a dtd:P ;
    dtd:has_Value "Die Vorschriften des Artikels V Nr. 2 bis 5 des BEG-Schlußgesetzes finden Anwendung."@de .

dtd:P-id53 a dtd:P ;
    dtd:has_Value "Diese Verordnung gilt nach § 14 des Dritten Überleitungsgesetzes in Verbindung mit Artikel XI Abs. 2 des BEG-Schlußgesetzes auch im Land Berlin."@de .

dtd:P-id55 a dtd:P ;
    dtd:has_Value "Diese Verordnung tritt am Tage nach der Verkündung in Kraft."@de .


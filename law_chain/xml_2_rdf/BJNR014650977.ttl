dtd:LA-id301 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorsteuern im Sinne des Umsatzsteuergesetzes bleiben außer Ansatz. Berücksichtigt werden können:"@de .


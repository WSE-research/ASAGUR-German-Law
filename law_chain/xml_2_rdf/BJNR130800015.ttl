dtd:P-id1566 a dtd:P ;
    dtd:has_B dtd:B-id277 ;
    dtd:has_Value "1. Schwerpunkt Kerzenherstellung"@de .

dtd:P-id1567 a dtd:P ;
    dtd:has_B dtd:B-id278 ;
    dtd:has_Value "2. Schwerpunkt Wachsbildnerei"@de .


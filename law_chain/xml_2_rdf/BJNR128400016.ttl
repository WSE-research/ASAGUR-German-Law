dtd:LA-id572 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Arbeitstag aus Anlass des 25-, 40- und 50-jährigen Dienstjubiläums."@de .

dtd:LA-id573 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer amts-, vertrauens- oder versorgungsärztlich angeordneten Untersuchung,"@de .

dtd:LA-id574 a dtd:LA ;

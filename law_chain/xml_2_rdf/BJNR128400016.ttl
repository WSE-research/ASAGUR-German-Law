dtd:LA-id541 a dtd:LA ;
    dtd:has_Value "zur Ausübung einer ehrenamtlichen Tätigkeit oder eines öffentlichen Ehrenamtes, wenn die Übernahme der Tätigkeit auf einer gesetzlichen Vorschrift beruht."@de .

dtd:LA-id542 a dtd:LA ;
    dtd:has_Value "in einer öffentlichen zwischenstaatlichen oder überstaatlichen Einrichtung,"@de .

dtd:LA-id543 a dtd:LA ;
    dtd:has_Value "in der Verwaltung eines Mitgliedstaates der Europäischen Union oder"@de .

dtd:LA-id544 a dtd:LA ;

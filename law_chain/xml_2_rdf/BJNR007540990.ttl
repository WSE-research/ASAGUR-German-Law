dtd:LA-id45 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wir übertragen"@de .

dtd:LA-id46 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Oberpostdirektionen und"@de .

dtd:LA-id47 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Posttechnischen Zentralamt"@de .

dtd:LA-id48 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "- je für ihren Geschäftsbereich - die Befugnis,"@de .

dtd:LA-id49 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 70 des Bundesbeamtengesetzes über die Zustimmung zur Annahme von Belohnungen oder Geschenken zu entscheiden, die Beamten, auch nach Beendigung des Beamtenverhältnisses, in bezug auf ihr Amt gewährt werden,"@de .

dtd:LA-id50 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 8 Abs. 1 der Verordnung über die Gewährung von Jubiläumszuwendungen an Beamte und Richter des Bundes Beamten Jubiläumszuwendungen zu gewähren oder zu versagen."@de .

dtd:LA-id51 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bei Belohnungen oder Geschenken, die einem Beamten nach Beendigung des Beamtenverhältnisses gewährt werden, ist für Entscheidungen nach Abschnitt 1 Nr. 1.1 dieser Anordnung diejenige Behörde zuständig, deren Geschäftsbereich der Beamte zuletzt angehört hat."@de .

dtd:LA-id52 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wir übertragen"@de .

dtd:LA-id53 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Oberpostdirektionen - soweit sie sich für besondere Fälle die Entscheidung vorbehalten -, dem Posttechnischen Zentralamt und den Postämtern mit Verwaltungsdienst - je für ihren Geschäftsbereich - die Befugnis,"@de .

dtd:LA-id54 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 64 des Bundesbeamtengesetzes von einem Beamten die Übernahme und Fortführung einer Nebentätigkeit im öffentlichen Dienst zu verlangen,"@de .

dtd:LA-id55 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 65 Abs. 4 des Bundesbeamtengesetzes einem Beamten Nebentätigkeiten zu genehmigen und zu versagen sowie Genehmigungen zu widerrufen,"@de .

dtd:LA-id56 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 69a Abs. 3 des Bundesbeamtengesetzes Ruhestandsbeamten und früheren Beamten mit Versorgungsbezügen die Aufnahme einer Beschäftigung oder Erwerbstätigkeit zu untersagen; zuständig für solche Entscheidungen ist die letzte Beschäftigungsbehörde."@de .

dtd:LA-id57 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wir bestimmen, daß"@de .

dtd:LA-id58 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Oberpostdirektionen,"@de .

dtd:LA-id59 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Posttechnische Zentralamt und"@de .

dtd:LA-id60 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Postämter mit Verwaltungsdienst"@de .

dtd:LA-id61 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "- je für ihren Geschäftsbereich -"@de .

dtd:LA-id62 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 60 des Bundesbeamtengesetzes einem Beamten aus zwingenden dienstlichen Gründen die Führung seiner Dienstgeschäfte verbieten dürfen."@de .

dtd:LA-id63 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Für besondere Fälle behalten wir uns Entscheidungen nach den Nummern 1 bis 4 dieser Anordnung vor."@de .

dtd:LA-id64 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Diese Anordnung tritt am Tage nach der Veröffentlichung im Bundesgesetzblatt in Kraft."@de .


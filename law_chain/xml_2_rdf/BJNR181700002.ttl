dtd:LA-id1180 a dtd:LA ;
    dtd:has_DL dtd:DL-id945 ;
    dtd:has_Value "unter den nach § 50 Absatz 1 bis 4 jeweils dafür maßgebenden Voraussetzungena)die befristete oder unbefristete Untersagung der Ausübung von Befugnissen (Fahrverbot) (§ 50 Absatz 1 und 4), erforderlichenfalls mit Auflagen (§ 50 Absatz 2),b)die Entziehung einer Berechtigung (§ 50 Absatz 2) oderc)die Erlaubnis, ein minderes Befähigungszeugnis auszustellen (§ 50 Absatz 3),"@de .

dtd:LA-id1181 a dtd:LA ;
    dtd:has_Value "die befristete oder unbefristete Untersagung der Ausübung von Befugnissen (Fahrverbot) (§ 50 Absatz 1 und 4), erforderlichenfalls mit Auflagen (§ 50 Absatz 2),"@de .

dtd:LA-id1182 a dtd:LA ;
    dtd:has_Value "die Entziehung einer Berechtigung (§ 50 Absatz 2) oder"@de .


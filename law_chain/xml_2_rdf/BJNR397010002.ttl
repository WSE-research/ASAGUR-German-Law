dtd:LA-id2866 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf festgesetzten Veranstaltungen im Sinne des Titels IV der Gewerbeordnung (Messen, Ausstellungen, Märkte), ausgenommen die Entgegennahme von Bestellungen auf Messen und Ausstellungen,"@de .

dtd:LA-id2867 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Volksfesten, Schützenfesten, Märkten, Sammlertreffen oder ähnlichen öffentlichen Veranstaltungen, ausgenommen das Überlassen der benötigten Schusswaffen oder Munition in einer Schießstätte sowie von Munition, die Teil einer Sammlung (§ 17 Abs. 1) oder für eine solche bestimmt ist."@de .

dtd:LA-id2868 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vom bisherigen Besitzer weitergenutzt werden sowie"@de .

dtd:LA-id2869 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Dauer der gemeinschaftlichen Aufbewahrung auch von berechtigten Personen mitgenutzt werden, die mit dem bisherigen Besitzer nach Nummer 1 in häuslicher Gemeinschaft leben."@de .

dtd:LA-id2870 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anforderungen an technische Sicherungssysteme zur Verhinderung einer unberechtigten Wegnahme oder Nutzung von Schusswaffen,"@de .

dtd:LA-id2871 a dtd:LA ;

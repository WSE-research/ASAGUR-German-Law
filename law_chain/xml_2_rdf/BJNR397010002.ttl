dtd:LA-id2858 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Verlangen der nach Absatz 3 zuständigen Überwachungsbehörde ihr diese Waffen oder diese Munition vorzuführen und"@de .

dtd:LA-id2859 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Berechtigung zum Verbringen oder zur Mitnahme nachzuweisen."@de .

dtd:LA-id2860 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für das Überlassen und Versenden der in Satz 1 bezeichneten Gegenstände an staatliche Stellen in einem dieser Staaten und in den Fällen, in denen Unternehmen Schusswaffen zur Durchführung von Kooperationsvereinbarungen zwischen Staaten oder staatlichen Stellen überlassen werden, sofern durch Vorlage einer Bescheinigung von Behörden des Empfangsstaates nachgewiesen wird, dass diesen Behörden der Erwerb bekannt ist, oder"@de .

dtd:LA-id2861 a dtd:LA ;

    dtd:has_Size dtd:normal ;
    dtd:has_Value "die durch sie hervorgebracht oder zu ihrer Begehung oder Vorbereitung gebraucht worden oder bestimmt gewesen sind,"@de .

dtd:LA-id3101 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die obersten Bundes- und Landesbehörden und die Deutsche Bundesbank,"@de .


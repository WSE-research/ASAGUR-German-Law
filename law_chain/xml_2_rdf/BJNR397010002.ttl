    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen aus anderen Staaten, denen der Schutz der in den Nummern 1 und 2 genannten Personen obliegt,"@de .

dtd:LA-id3108 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie bis zum 1. Juli 2018 einen Antrag nach § 40 Absatz 4 stellt und"@de .


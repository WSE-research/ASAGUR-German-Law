dtd:LA-id3078 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 10 Absatz 2 Satz 4 oder § 37i eine Mitteilung nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig macht,"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus denen nicht geschossen werden kann und"@de .

dtd:LA-id3122 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nicht mit allgemein gebräuchlichen Werkzeugen so umgebaut oder verändert werden können, dass aus ihnen Munition, Ladungen oder Geschosse verschossen werden können."@de .

dtd:LA-id3123 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine angriffsunfähig machende Wirkung durch ein gezieltes Versprühen oder Ausstoßen von Reiz- oder anderen Wirkstoffen oder"@de .


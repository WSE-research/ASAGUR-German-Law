    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine gesundheitsschädliche Wirkung durch eine andere als kinetische Energie, insbesondere durch ein gezieltes Ausstrahlen einer elektromagnetischen Strahlung,"@de .

dtd:LA-id3125 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Antrieb von Geschossen oder Wirkstoffen oder"@de .


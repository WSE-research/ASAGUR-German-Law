dtd:LA-id2853 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Brauchtumsschützen, die bis zu drei Einzellader- oder Repetier-Langwaffen nach Anlage 1 Abschnitt 3 der Kategorie C und die dafür bestimmte Munition zur Teilnahme an einer Brauchtumsveranstaltung mitnehmen."@de .

dtd:LA-id2854 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Waffen oder Munition, die durch Inhaber einer Erlaubnis zum Erwerb oder Besitz für diese Waffen oder Munition mitgenommen werden,"@de .

dtd:LA-id2855 a dtd:LA ;

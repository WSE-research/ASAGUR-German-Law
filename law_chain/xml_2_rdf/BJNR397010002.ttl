    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 13 Absatz 3 Satz 2 oder § 20 Absatz 1 die Ausstellung einer Waffenbesitzkarte oder die Eintragung in eine Waffenbesitzkarte nicht oder nicht rechtzeitig beantragt,"@de .

dtd:LA-id3080 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 21 Absatz 6, § 24 Absatz 6, § 27 Absatz 1 Satz 6 oder Absatz 2 Satz 2, § 30 Satz 3, § 34 Absatz 4 oder 5 Satz 1, § 37 Absatz 1 Satz 1, auch in Verbindung mit Satz 2, entgegen § 37a Satz 1, auch in Verbindung mit Satz 3, entgegen § 37a Satz 2, § 37b Absatz 1 Satz 1, Absatz 2 Satz 1 oder Absatz 3, § 37c Absatz 1, § 37d Absatz 1 oder 2, § 40 Absatz 5 Satz 1 oder § 58 Absatz 19 Satz 1 eine Anzeige nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig erstattet,"@de .

dtd:LA-id3081 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 24 Abs. 1, auch in Verbindung mit einer Rechtsverordnung nach § 25 Nummer 1 oder Nr. 2 Buchstabe a, oder § 24 Absatz 4 Satz 1 und 2, auch in Verbindung mit einer Rechtsverordnung nach § 25 Nummer 1, eine Angabe, ein Zeichen oder die Bezeichnung der Munition auf der Schusswaffe nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig anbringt oder Munition nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig mit einem besonderen Kennzeichen versieht,"@de .

dtd:LA-id3082 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 24 Absatz 5 eine Schusswaffe oder Munition anderen gewerbsmäßig überlässt,"@de .

dtd:LA-id3083 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ohne Erlaubnis nach § 27 Abs. 1 Satz 1 eine Schießstätte betreibt oder ihre Beschaffenheit oder die Art ihrer Benutzung wesentlich ändert,"@de .

dtd:LA-id3084 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 27 Abs. 3 Satz 1 Nr. 1 und 2 einem Kind oder Jugendlichen das Schießen gestattet oder entgegen § 27 Abs. 6 Satz 2 nicht sicherstellt, dass die Aufsichtsperson nur einen Schützen bedient,"@de .

dtd:LA-id3085 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 27 Abs. 3 Satz 2 Unterlagen nicht aufbewahrt oder entgegen § 27 Abs. 3 Satz 3 diese nicht herausgibt,"@de .

dtd:LA-id3086 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 27 Abs. 5 Satz 2 eine Bescheinigung nicht mitführt,"@de .

dtd:LA-id3087 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 33 Abs. 1 Satz 1 eine Schusswaffe oder Munition nicht anmeldet oder nicht oder nicht rechtzeitig vorführt,"@de .

dtd:LA-id3088 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 34 Abs. 1 Satz 1 eine nicht erlaubnispflichtige Waffe oder nicht erlaubnispflichtige Munition einem Nichtberechtigten überlässt,"@de .

dtd:LA-id3089 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 35 Abs. 1 Satz 4 die Urkunden nicht aufbewahrt oder nicht, nicht vollständig oder nicht rechtzeitig Einsicht gewährt,"@de .

dtd:LA-id3090 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 35 Abs. 2 einen Hinweis nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig gibt oder die Erfüllung einer dort genannten Pflicht nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig protokolliert,"@de .

dtd:LA-id3091 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 37g Absatz 1 ein dort genanntes Dokument nicht oder nicht rechtzeitig vorlegt,"@de .

dtd:LA-id3092 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 38 Absatz 1 Satz 1 oder Absatz 2 ein dort genanntes Dokument nicht mit sich führt oder nicht oder nicht rechtzeitig aushändigt,"@de .

dtd:LA-id3093 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 39 Abs. 1 Satz 1 eine Auskunft nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig erteilt,"@de .

dtd:LA-id3094 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 42a Abs. 1 eine Anscheinswaffe, eine dort genannte Hieb- oder Stoßwaffe oder ein dort genanntes Messer führt,"@de .

dtd:LA-id3095 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 46 Abs. 1 Satz 1, auch in Verbindung mit Satz 2, eine Ausfertigung der Erlaubnisurkunde nicht oder nicht rechtzeitig zurückgibt oder"@de .


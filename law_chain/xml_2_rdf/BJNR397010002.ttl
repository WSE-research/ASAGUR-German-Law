    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zollverwaltung"@de .

dtd:LA-id3105 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Staatsgäste aus anderen Staaten,"@de .

dtd:LA-id3106 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstige erheblich gefährdete Personen des öffentlichen Lebens aus anderen Staaten, die sich besuchsweise im Geltungsbereich dieses Gesetzes aufhalten, und"@de .


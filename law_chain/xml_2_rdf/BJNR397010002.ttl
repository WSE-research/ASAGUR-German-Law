    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Erzeugung von Schall- oder Lichtimpulsen"@de .

dtd:LA-id3127 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Reichweite und Sprühdauer begrenzt sind und"@de .

dtd:LA-id3128 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Nachweis der gesundheitlichen Unbedenklichkeit, der Reichweiten- und der Sprühdauerbegrenzung ein amtliches Prüfzeichen tragen;"@de .

dtd:LA-id3129 a dtd:LA ;

dtd:LA-id214 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Prüfung der meisterhaften Verrichtung der gebräuchlichen Arbeiten (Teil I),"@de .

dtd:LA-id215 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Prüfung der erforderlichen fachtheoretischen Kenntnisse (Teil II),"@de .

dtd:LA-id216 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Prüfung der erforderlichen betriebswirtschaftlichen, kaufmännischen und rechtlichen Kenntnisse (Teil III) und"@de .

dtd:LA-id217 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Prüfung der erforderlichen berufs- und arbeitspädagogischen Kenntnisse (Teil IV)."@de .

dtd:LA-id218 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kundenwünsche ermitteln, Kunden beraten, Auftragsverhandlungen führen und Auftragsziele festlegen, Leistungen kalkulieren und Angebote erstellen,"@de .

dtd:LA-id219 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufgaben der technischen und kaufmännischen Betriebsführung, der Betriebsorganisation, der Personalplanung und des Personaleinsatzes wahrnehmen, insbesondere unter Berücksichtigung der betrieblichen Aus- und Weiterbildung, des Qualitätsmanagements, der Haftung sowie des Arbeitsschutzes, der Arbeitssicherheit und des Umweltschutzes, einschließlich der Verwendung lösemittelarmer oder wasserbasierender, lösemittelfreier Produkte; Informationssysteme nutzen,"@de .

dtd:LA-id220 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufträge durchführen unter Berücksichtigung von Fertigungsverfahren, Instandhaltungsalternativen, berufsbezogenen Gesetzen, Normen, Regeln und Vorschriften sowie des Personalbedarfs und der Ausbildung; Auftragsbearbeitung und Auftragsabwicklung organisieren, planen und überwachen,"@de .

dtd:LA-id221 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "technische Arbeitspläne, technische Zeichnungen, Skizzen, Entwürfe und Modelle unter Berücksichtigung kreativer Gestaltungsaspekte auch unter Einsatz von rechnergestützten Systemen erstellen,"@de .

dtd:LA-id222 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gold- und Silberschmiedearbeiten planen, entwerfen, herstellen, montieren und instand halten, dabei insbesondere die Bedeutung der Stilkunde, der sakralen Symbolik, der Heraldik, der Kunstgeschichte sowie der historischen und zeitgemäßen Entwicklung der Gold- und Silberschmiedekunst berücksichtigen,"@de .

dtd:LA-id223 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arten und Eigenschaften zu verarbeitender Werkstoffe, einschließlich der Verfahren zur Oberflächenbehandlung bei der Planung und Fertigung von Gold- und Silberschmiedearbeiten berücksichtigen,"@de .

dtd:LA-id224 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Edelsteine, Perlen, Natur- und Kunststoffe sowie deren Synthesen, Dubletten und Imitationen prüfen, unterscheiden und bewerten,"@de .

dtd:LA-id225 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mechanische, chemische und elektrochemische Be- und Verarbeitungsverfahren zur Fertigung von Gold- und Silberschmiedearbeiten beherrschen, insbesondere Spanen, Umformen und Fügen,"@de .

dtd:LA-id226 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Legieren, Schmelzen und Gießen von Edelmetallen beherrschen, Guss- und Formteile gestaltend bearbeiten,"@de .

dtd:LA-id227 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Oberflächen unter Berücksichtigung kreativer Gestaltungsaspekte bearbeiten und veredeln,"@de .

dtd:LA-id228 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fehler und Schäden an Gold- und Silberschmiedearbeiten feststellen, Maßnahmen zur Beseitigung von Fehlern und Schäden beherrschen, Ergebnisse bewerten und dokumentieren,"@de .

dtd:LA-id229 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leistungen abnehmen und protokollieren, Nachkalkulation durchführen."@de .

dtd:LA-id230 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Meisterprüfungsprojekt und ein darauf bezogenes Fachgespräch,"@de .

dtd:LA-id231 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Situationsaufgabe."@de .

dtd:LA-id232 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein mit Juwelen oder edlen Steinen auszufassendes Schmuckstück oder ein Schmuckstück ohne Steine,"@de .

dtd:LA-id233 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein profanes oder sakrales Erzeugnis aus edlen Metallen."@de .

dtd:LA-id234 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Entwurf, Werkstattzeichnung, Kalkulation und Arbeitsplan,"@de .

dtd:LA-id235 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anfertigung der Gold- oder Silberschmiedearbeit."@de .

dtd:LA-id236 a dtd:LA ;
    dtd:has_Size dtd:normal ;

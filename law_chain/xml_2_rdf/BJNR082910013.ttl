dtd:LA-id65 a dtd:LA ;
    dtd:has_DL dtd:DL-id57 ;
    dtd:has_Value "der Antrag der drittbetroffenen Person, gemäß Artikel 36 Absatz 2 der Europäischen Menschenrechtskonvention Stellung zu nehmen oder an der mündlichen Verhandlung teilzunehmen, aa)erfolgreich war oderbb)Aussicht auf Erfolg hat und nicht mutwillig ist und"@de .

dtd:LA-id66 a dtd:LA ;
    dtd:has_Value "erfolgreich war oder"@de .


    dtd:has_BR dtd:BR-id132,
        dtd:BR-id133 ;
    dtd:has_Value "(Fundstelle: BGBl. I 2019, 2261 – 2262)"@de ;
    dtd:has_noindex dtd:noindex-id141 .


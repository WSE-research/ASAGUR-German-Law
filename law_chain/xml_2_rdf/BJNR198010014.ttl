dtd:LA-id559 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "technische Systeme zur elektronischen Erhebung von Gebühren für die Benutzung von mautpflichtigen Streckennetzen mit Kraftfahrzeugen (elektronische Mautsysteme) und"@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Erbringen mautdienstbezogener Leistungen, auch in der Form der Mitwirkung Privater bei der Erhebung."@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "öffentliche Straßen,"@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bauwerke im Verlauf öffentlicher Straßen, insbesondere Tunnel und Brücken, und"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fähren, soweit sie Teil einer öffentlichen Straße sind,"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "elektronische Mautsysteme, soweit die Mauterhebung keine Benutzung eines Fahrzeuggerätes durch ein mautpflichtiges Fahrzeug erfordert oder nicht durch automatische Kennzeichenerkennung automatisch festgestellt wird, dass sich ein Fahrzeug auf dem mautpflichtigen Streckennetz befindet,"@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kleine, rein örtliche Mautsysteme, bei denen die Kosten für die Anpassung an die Anforderungen nach diesem Gesetz außer Verhältnis zu dem erzielten Nutzen stünden, es sei denn, in den der Erhebung der Maut zu Grunde liegenden Rechtsvorschriften des Bundes oder der Länder ist etwas Abweichendes bestimmt, oder"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Parkgebühren."@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Value "Satellitenortung,"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Value "Mobilfunk nach der GSM/GPRS-Norm,"@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Value "Mikrowellentechnik (5,8 GHz)."@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seinen Sitz oder eine ständige Niederlassung in Deutschland hat,"@de .

dtd:LA-id571 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über ein Qualitätsmanagementsystem verfügt, das nach den allgemein anerkannten Regeln der Technik zertifiziert ist,"@de .

dtd:LA-id572 a dtd:LA ;
    dtd:has_DL dtd:DL-id400 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über a)die technische Ausrüstung undb)die EG-Konformitätserklärung oder das Zertifikat zur Bescheinigung der Konformität der Interoperabilitätskomponenten nach Anhang III der Durchführungsverordnung (EU) 2020/204 der Kommission vom 28. November 2019 über detaillierte Pflichten der Anbieter des europäischen elektronischen Mautdienstes, den Mindestinhalt der Vorgabe für das EETS-Gebiet, elektronische Schnittstellen und Anforderungen an Interoperabilitätskomponenten sowie zur Aufhebung der Entscheidung 2009/750/EG (ABl. L 43 vom 17.2.2020, S. 49) verfügt,"@de .

dtd:LA-id573 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die technische Ausrüstung und"@de .

dtd:LA-id574 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die EG-Konformitätserklärung oder das Zertifikat zur Bescheinigung der Konformität der Interoperabilitätskomponenten nach Anhang III der Durchführungsverordnung (EU) 2020/204 der Kommission vom 28. November 2019 über detaillierte Pflichten der Anbieter des europäischen elektronischen Mautdienstes, den Mindestinhalt der Vorgabe für das EETS-Gebiet, elektronische Schnittstellen und Anforderungen an Interoperabilitätskomponenten sowie zur Aufhebung der Entscheidung 2009/750/EG (ABl. L 43 vom 17.2.2020, S. 49) verfügt,"@de .

dtd:LA-id575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Befähigung zum Erbringen mautdienstbezogener Leistungen besitzt,"@de .

dtd:LA-id576 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über eine angemessene finanzielle Leistungsfähigkeit verfügt, um die Einrichtung und ordnungsgemäße Führung eines Geschäftsbetriebs zum Erbringen mautdienstbezogener Leistungen in den nach § 12 Absatz 1 abzudeckenden Mitgliedstaaten der Europäischen Union und den anderen Vertragsstaaten des Abkommens über den Europäischen Wirtschaftsraum zu gewährleisten,"@de .

dtd:LA-id577 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über einen auf seine Veranlassung im Rahmen eines Audits geprüften Risikomanagementplan verfügt, der die Risiken im Zusammenhang mit dem Erbringen der mautdienstbezogenen Leistungen benennt und bewertet sowie Maßnahmen zur Verringerung dieser Risiken vorsieht, sowie"@de .

dtd:LA-id578 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gewähr für die erforderliche Zuverlässigkeit für die Einrichtung und ordnungsgemäße Führung eines Geschäftsbetriebs zum Erbringen mautdienstbezogener Leistungen in den Mitgliedstaaten der Europäischen Union und den anderen Vertragsstaaten des Abkommens über den Europäischen Wirtschaftsraum bietet."@de .

dtd:LA-id579 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Nichtregistrierung des Antragstellers als Anbieter zwei Jahre nach bestandskräftiger oder rechtskräftiger Ablehnung des Antrags auf Registrierung,"@de .

dtd:LA-id580 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Registrierung des Antragstellers als Anbieter zwei Jahre nach bestandskräftiger oder rechtskräftiger Beendigung der Registrierung"@de .

dtd:LA-id581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die von den Anbietern zu zahlenden Entgelte nach Absatz 3 und die Bankgarantie oder ein gleichwertiges Finanzinstrument nach Absatz 4,"@de .

dtd:LA-id582 a dtd:LA ;
    dtd:has_DL dtd:DL-id415 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Verfahren zur Abwicklung der Mitwirkung bei der Mauterhebung durch die Anbieter hinsichtlich a)der Voraussetzungen für die Zulassung als Anbieter,b)der Mitwirkung der Anbieter bei der Berechnung der Maut und der Maut-Basisdaten nach § 17,c)der elektronischen Schnittstellen nach Anhang I der Durchführungsverordnung (EU) 2020/204, einschließlich des Anlegens von Sperrlisten und Nutzerlisten, den Zugriff auf und die Übermittlung von Sperrlisten und Nutzerlisten oder Daten daraus,d)des Formats für die Übermittlung der Positionsdaten, der für die Höhe der Maut maßgeblichen Merkmale der Fahrzeugklassifizierung und der Daten des Mautbuchungsnachweises,e)der Termine und der Häufigkeit der Übermittlung dieser Daten,f)der Richtigkeit der Positionsdaten, der für die Höhe der Maut maßgeblichen Merkmale der Fahrzeugklassifizierung und der Daten des Mautbuchungsnachweises,g)der Betriebsbereitschaft,h)der Fakturierungsgrundsätze,i)der Zahlungsgrundsätze,j)der Geschäftsbedingungen, einschließlich der Methode der Berechnung der Vergütung, die von der für die Erhebung der Maut zuständigen Behörde des Bundes oder eines Landes an die Anbieter zu zahlen ist, und einschließlich der Anforderungen an die Dienstleistungsqualität,k)der Unterstützung der Kontrolle der Einhaltung der Mautpflicht durch die Teilsysteme der Anbieter,l)der Überwachung der Anbieter,m)des Umgangs mit Änderungen undn)der Vermittlungsstelle nach § 28."@de .

dtd:LA-id583 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Voraussetzungen für die Zulassung als Anbieter,"@de .

dtd:LA-id584 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Mitwirkung der Anbieter bei der Berechnung der Maut und der Maut-Basisdaten nach § 17,"@de .

dtd:LA-id585 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der elektronischen Schnittstellen nach Anhang I der Durchführungsverordnung (EU) 2020/204, einschließlich des Anlegens von Sperrlisten und Nutzerlisten, den Zugriff auf und die Übermittlung von Sperrlisten und Nutzerlisten oder Daten daraus,"@de .

dtd:LA-id586 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Formats für die Übermittlung der Positionsdaten, der für die Höhe der Maut maßgeblichen Merkmale der Fahrzeugklassifizierung und der Daten des Mautbuchungsnachweises,"@de .

dtd:LA-id587 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Termine und der Häufigkeit der Übermittlung dieser Daten,"@de .

dtd:LA-id588 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Richtigkeit der Positionsdaten, der für die Höhe der Maut maßgeblichen Merkmale der Fahrzeugklassifizierung und der Daten des Mautbuchungsnachweises,"@de .

dtd:LA-id589 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Betriebsbereitschaft,"@de .

dtd:LA-id590 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Fakturierungsgrundsätze,"@de .

dtd:LA-id591 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Zahlungsgrundsätze,"@de .

dtd:LA-id592 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Geschäftsbedingungen, einschließlich der Methode der Berechnung der Vergütung, die von der für die Erhebung der Maut zuständigen Behörde des Bundes oder eines Landes an die Anbieter zu zahlen ist, und einschließlich der Anforderungen an die Dienstleistungsqualität,"@de .

dtd:LA-id593 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Unterstützung der Kontrolle der Einhaltung der Mautpflicht durch die Teilsysteme der Anbieter,"@de .

dtd:LA-id594 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Überwachung der Anbieter,"@de .

dtd:LA-id595 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Umgangs mit Änderungen und"@de .

dtd:LA-id596 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Vermittlungsstelle nach § 28."@de .

dtd:LA-id597 a dtd:LA ;
    dtd:has_Value "der Betreiber eine Vergütung für Kosten zur Erfüllung von Anforderungen und Verpflichtungen erhält, die nicht für die Anbieter gelten, und"@de .

dtd:LA-id598 a dtd:LA ;
    dtd:has_Value "die zuständige Behörde die Vergütung der Anbieter um feste Entgelte für die Kosten reduziert, die ihr für die Bereitstellung, den Betrieb und die Instandhaltung eines den Anforderungen des Mautdienstes entsprechenden Systems in ihrem mautpflichtigen Streckennetz entstehen, einschließlich der Zulassungskosten, sofern die Kosten für die Bereitstellung, den Betrieb und die Instandhaltung eines den Anforderungen des Mautdienstes entsprechenden Systems in ihrem mautpflichtigen Streckennetz nicht in der Maut enthalten sind."@de .

dtd:LA-id599 a dtd:LA ;
    dtd:has_Value "öffentlich-rechtlichen Vertrag zwischen der für die Erhebung der Maut zuständigen Behörde und dem Anbieter oder"@de .

dtd:LA-id600 a dtd:LA ;
    dtd:has_Value "Verwaltungsakt der für die Erhebung der Maut zuständigen Behörde"@de .

dtd:LA-id601 a dtd:LA ;
    dtd:has_Value "das mautpflichtige Streckennetz, insbesondere dessen geografische Ausdehnung und die Infrastrukturen, für die Maut erhoben wird,"@de .

dtd:LA-id602 a dtd:LA ;
    dtd:has_Value "die Art der Maut und die Erhebungsgrundsätze,"@de .

dtd:LA-id603 a dtd:LA ;
    dtd:has_Value "die Fahrzeuge, für die Maut erhoben wird,"@de .

dtd:LA-id604 a dtd:LA ;
    dtd:has_Value "die Merkmale für die Fahrzeugklassifizierung und ihre Zuordnung zur Gebührenstruktur des Bundes oder Landes,"@de .

dtd:LA-id605 a dtd:LA ;
    dtd:has_Value "die geforderten Mautbuchungsnachweise."@de .

dtd:LA-id606 a dtd:LA ;

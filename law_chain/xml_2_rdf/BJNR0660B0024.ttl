dtd:LA-id234 a dtd:LA ;
    dtd:has_Value "die zentrale Antrags- und Registerstelle nach § 10 des Bundeskrebsregisterdatengesetzes sowie"@de .

dtd:LA-id235 a dtd:LA ;
    dtd:has_Value "die beteiligten Vertrauensstellen dieser Einrichtungen,"@de .


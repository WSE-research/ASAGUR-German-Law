dtd:LA-id13026 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kapitalerträge, bei denen auf Grund einer Nichtveranlagungs-Bescheinigung einer natürlichen Person nach § 44a Absatz 2 Satz 1 Nummer 2 vom Steuerabzug Abstand genommen oder eine Erstattung vorgenommen wurde."@de .

dtd:LA-id13027 a dtd:LA ;
    dtd:has_Size dtd:normal ;

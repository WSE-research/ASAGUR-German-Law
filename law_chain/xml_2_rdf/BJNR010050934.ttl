dtd:LA-id12029 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bezüge im Sinne des § 22 Nummer 1 Satz 2, soweit diese von einer nicht von der Körperschaftsteuer befreiten Körperschaft, Personenvereinigung oder Vermögensmasse stammen."@de .

dtd:LA-id12030 a dtd:LA ;
    dtd:has_Size dtd:normal ;

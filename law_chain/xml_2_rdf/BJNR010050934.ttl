    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn nachträglich festgestellt wird, dass die Voraussetzungen der unbeschränkten Einkommensteuerpflicht im Sinne des § 1 Absatz 2 oder Absatz 3 oder des § 1a nicht vorgelegen haben; § 39 Absatz 7 ist sinngemäß anzuwenden;"@de .

dtd:LA-id13116 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Fällen des § 2 Absatz 7 Satz 3;"@de .

dtd:LA-id13117 a dtd:LA ;
    dtd:has_DL dtd:DL-id10939 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Einkünfte aus nichtselbständiger Arbeit im Sinne des § 49 Absatz 1 Nummer 4, a)wenn als Lohnsteuerabzugsmerkmal ein Freibetrag nach § 39a Absatz 4 gebildet worden ist und der im Kalenderjahr insgesamt erzielte Arbeitslohn höher ist als die Summe aus dem Grundfreibetrag (§ 32a Absatz 1 Satz 2 Nummer 1), dem Arbeitnehmer-Pauschbetrag (§ 9a Satz 1 Nummer 1 Buchstabe a) und dem Sonderausgaben-Pauschbetrag (§ 10c Satz 1),b)wenn die Veranlagung zur Einkommensteuer beantragt wird (§ 46 Absatz 2 Nummer 8) oderc)in den Fällen des § 46 Absatz 2 Nummer 2, 5 und 5a;"@de .

dtd:LA-id13118 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn als Lohnsteuerabzugsmerkmal ein Freibetrag nach § 39a Absatz 4 gebildet worden ist und der im Kalenderjahr insgesamt erzielte Arbeitslohn höher ist als die Summe aus dem Grundfreibetrag (§ 32a Absatz 1 Satz 2 Nummer 1), dem Arbeitnehmer-Pauschbetrag (§ 9a Satz 1 Nummer 1 Buchstabe a) und dem Sonderausgaben-Pauschbetrag (§ 10c Satz 1),"@de .

dtd:LA-id13119 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn die Veranlagung zur Einkommensteuer beantragt wird (§ 46 Absatz 2 Nummer 8) oder"@de .

dtd:LA-id13120 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Fällen des § 46 Absatz 2 Nummer 2, 5 und 5a;"@de .

dtd:LA-id13121 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Einkünfte im Sinne des § 50a Absatz 1 Nummer 1, 2 und 4, wenn die Veranlagung zur Einkommensteuer beantragt wird;"@de .

dtd:LA-id13122 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Einkünfte aus Kapitalvermögen im Sinne des § 49 Absatz 1 Nummer 5 Satz 1 Buchstabe a, auf die § 20 Absatz 1 Nummer 6 Satz 2 anzuwenden ist, wenn die Veranlagung zur Einkommensteuer beantragt wird."@de .

dtd:LA-id13123 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an der inländischen Veranstaltung international bedeutsamer kultureller und sportlicher Ereignisse, um deren Ausrichtung ein internationaler Wettbewerb stattfindet, oder"@de .

dtd:LA-id13124 a dtd:LA ;

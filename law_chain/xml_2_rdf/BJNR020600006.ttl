    dtd:has_Value "berufsbezogene Arbeitsschutz- und Unfallverhütungsvorschriften anwenden"@de .

dtd:LA-id551 a dtd:LA ;
    dtd:has_Value "Verhaltensweisen bei Unfällen beschreiben sowie erste Maßnahmen einleiten"@de .

dtd:LA-id552 a dtd:LA ;
    dtd:has_Value "Vorschriften des vorbeugenden Brandschutzes anwenden; Verhaltensweisen bei Bränden beschreiben und Maßnahmen zur Brandbekämpfung ergreifen"@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Value "mögliche Umweltbelastungen durch den Ausbildungsbetrieb und seinen Beitrag zum Umweltschutz an Beispielen erklären"@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_Value "für den Ausbildungsbetrieb geltende Regelungen des Umweltschutzes anwenden"@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_Value "Möglichkeiten der wirtschaftlichen und umweltschonenden Energie- und Materialverwendung nutzen"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Value "Abfälle vermeiden; Stoffe und Materialien einer umweltschonenden Entsorgung zuführen"@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Value "Arbeitsaufträge erfassen, Vorgaben auf Umsetzbarkeit prüfen"@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Value "Arbeitsschritte vorbereiten und festlegen, Arbeitsmittel zusammenstellen"@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Value "Dokumentationen erstellen"@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Value "Arbeitsergebnisse kontrollieren und bewerten"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Value "Arbeitsabläufe unter Berücksichtigung betrieblicher, rechtlicher, ökonomischer, ökologischer und terminlicher Vorgaben planen"@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Value "Aufgaben im Team planen und umsetzen, Ergebnisse gemeinsam abstimmen und auswerten"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Value "Gespräche situationsgerecht führen, Konfliktlösungsmöglichkeiten anwenden"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Value "Arbeitsabläufe mit am Arbeitsprozess beteiligten Bereichen abstimmen, insbesondere Anweisungen geben und entgegennehmen"@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Value "Grundlagen des Funkverkehrs anwenden"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Value "Informationen, auch in einer Fremdsprache, beschaffen, bewerten und nutzen"@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Value "Vorschriften zum Datenschutz beachten"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Value "Daten erfassen, sichern und pflegen"@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Value "Arbeitsaufgaben mit Hilfe von Informations- und Kommunikationstechniken bearbeiten"@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_Value "Sachverhalte darstellen, deutsche und englische Fachbegriffe anwenden; Auskünfte in Englisch erteilen"@de .

dtd:LA-id571 a dtd:LA ;
    dtd:has_Value "Organisation, Funktion und Bedeutung des Hafens im gesamtwirtschaftlichen Prozess erläutern"@de .

dtd:LA-id572 a dtd:LA ;
    dtd:has_Value "Betriebe der Hafenschifffahrt im logistischen Prozess mit vor- und nachgelagerten Dienstleistungen unterscheiden"@de .

dtd:LA-id573 a dtd:LA ;
    dtd:has_Value "Verwaltung des Hafens erläutern"@de .

dtd:LA-id574 a dtd:LA ;
    dtd:has_Value "Umschlagseinrichtungen hinsichtlich Funktion und Besonderheiten unterscheiden"@de .

dtd:LA-id575 a dtd:LA ;
    dtd:has_Value "An- und Auslieferpapiere für den Im- und Export sowie Begleitpapiere überprüfen"@de .

dtd:LA-id576 a dtd:LA ;
    dtd:has_Value "Ladungsarten, insbesondere Trockengüter, Tankladungen und Container unter Berücksichtigung ihrer Besonderheiten unterscheiden"@de .

dtd:LA-id577 a dtd:LA ;
    dtd:has_Value "Maßnahmen bei Ladungsschäden ergreifen"@de .

dtd:LA-id578 a dtd:LA ;
    dtd:has_Value "Wasserfahrzeuge unter Berücksichtigung des Einsatzes unterscheiden"@de .

dtd:LA-id579 a dtd:LA ;
    dtd:has_Value "bei logistischen Planungs- und Organisationsprozessen mitwirken, Informationsfluss sicherstellen, bei Abweichungen Maßnahmen ergreifen"@de .

dtd:LA-id580 a dtd:LA ;
    dtd:has_Value "Wasserfahrzeuge in Betrieb nehmen, losmachen, festmachen und verholen"@de .

dtd:LA-id581 a dtd:LA ;
    dtd:has_Value "Hafenfahrzeuge unter Beachtung einschlägiger Vorschriften steuern"@de .

dtd:LA-id582 a dtd:LA ;
    dtd:has_Value "Person-über-Bord-Manöver ausführen"@de .

dtd:LA-id583 a dtd:LA ;
    dtd:has_Value "An- und Ablegemanöver ohne Anhang planen und unter Aufsicht durchführen"@de .

dtd:LA-id584 a dtd:LA ;
    dtd:has_Value "Wasserstände und Strömungsverhältnisse ermitteln und berücksichtigen"@de .

dtd:LA-id585 a dtd:LA ;
    dtd:has_Value "Navigationshilfs- und Kommunikationsmittel bedienen"@de .

dtd:LA-id586 a dtd:LA ;
    dtd:has_Value "Gewässer im Einsatzgebiet mit ihren Kaistrecken und Landmarken sowie wasserbauliche Anlagen unterscheiden und beim Führen von Wasserfahrzeugen berücksichtigen"@de .

dtd:LA-id587 a dtd:LA ;
    dtd:has_Value "Sichtzeichen und Schallsignale von Fahrzeugen und Schifffahrtszeichen entsprechend der im Einsatzgebiet gültigen Rechtsvorschriften berücksichtigen und anwenden"@de .

dtd:LA-id588 a dtd:LA ;
    dtd:has_Value "Schlepp- oder Schubverbände zusammenstellen und koppeln"@de .

dtd:LA-id589 a dtd:LA ;
    dtd:has_Value "Koppelmanöver unter Aufsicht durchführen"@de .

dtd:LA-id590 a dtd:LA ;
    dtd:has_Value "An- und Ablegemanöver mit geschlepptem Anhang unter Aufsicht durchführen"@de .

dtd:LA-id591 a dtd:LA ;
    dtd:has_Value "Einfluss von Stabilität und Trimm auf das Manövrierverhalten von Hafenfahrzeugen berücksichtigen"@de .

dtd:LA-id592 a dtd:LA ;
    dtd:has_Value "Anlagen in Betrieb nehmen, bedienen und überwachen"@de .

dtd:LA-id593 a dtd:LA ;
    dtd:has_Value "Gültigkeit von Zulassungsdokumenten für den nautischen und technischen Betrieb beachten, bei fehlerhaften und ungültigen Unterlagen Maßnahmen ergreifen"@de .

dtd:LA-id594 a dtd:LA ;
    dtd:has_Value "Vorschriften für die Besetzung von Wasserfahrzeugen anwenden"@de .

dtd:LA-id595 a dtd:LA ;
    dtd:has_Value "Regelungen und Vorschriften, insbesondere für den Transport von Gütern und die Beförderung von Personen, anwenden"@de .

dtd:LA-id596 a dtd:LA ;
    dtd:has_Value "Gespräche kundenorientiert führen"@de .

dtd:LA-id597 a dtd:LA ;
    dtd:has_Value "Kundenwünsche beachten"@de .

dtd:LA-id598 a dtd:LA ;
    dtd:has_Value "qualitätsbewusst handeln und zur Qualitätssicherung beitragen"@de .

dtd:LA-id599 a dtd:LA ;
    dtd:has_Value "zur kontinuierlichen Verbesserung von Arbeitsabläufen im eigenen Arbeitsbereich beitragen"@de .

dtd:LA-id600 a dtd:LA ;
    dtd:has_Value "Betriebsmittel sowie Werk- und Hilfsstoffe einsetzen, Betriebsstoffe übernehmen"@de .

dtd:LA-id601 a dtd:LA ;
    dtd:has_Value "Konservierungs- und Reinigungsmittel einsetzen, Bestimmungen des Umwelt- und Gesundheitsschutzes beachten"@de .

dtd:LA-id602 a dtd:LA ;
    dtd:has_Value "seemännische Gebrauchsknoten einsetzen"@de .

dtd:LA-id603 a dtd:LA ;
    dtd:has_Value "Werkstoffe bearbeiten"@de .

dtd:LA-id604 a dtd:LA ;
    dtd:has_Value "Arten und Eigenschaften von Draht- und Fasertauwerk unterscheiden, Tauwerk pflegen und spleißen"@de .

dtd:LA-id605 a dtd:LA ;
    dtd:has_Value "Rettungsmittel und technische Einrichtungen nach Vorschriften pflegen und warten"@de .

dtd:LA-id606 a dtd:LA ;
    dtd:has_Value "Arbeitsgeschirre unter Berücksichtigung einschlägiger Vorschriften einsetzen"@de .

dtd:LA-id607 a dtd:LA ;
    dtd:has_Value "Rettungsmittel und persönliche Schutzausrüstungen verwenden"@de .

dtd:LA-id608 a dtd:LA ;
    dtd:has_Value "Störungen im Schiffsbetrieb erkennen und bewerten, Maßnahmen zur Beseitigung veranlassen und durchführen"@de .

dtd:LA-id609 a dtd:LA ;
    dtd:has_Value "Hilfs- und Sofortmaßnahmen in Notfällen, insbesondere bei Havarien und Bränden, ergreifen"@de .

dtd:LA-id610 a dtd:LA ;
    dtd:has_Value "verunglückte Personen retten und Maßnahmen der ersten Hilfe durchführen"@de .


dtd:LA-id89 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Lagerstätte nach den vorgegebenen natürlichen Verhältnissen innerhalb eines Steinkohlenbergbaugebietes und nach bergwirtschaftlichen und bergtechnischen Erfordernissen abbauen kann, um insbesondere einen geeigneten Zuschnitt der Baufelder für die einzelnen Steinkohlenbergwerke sicherzustellen;"@de .

dtd:LA-id90 a dtd:LA ;

    dtd:has_Value "Anfahrvorgängen nach § 3 Absatz 1 Satz 2,"@de .

dtd:LA-id986 a dtd:LA ;
    dtd:has_Value "Funktionstests nach § 28,"@de .

dtd:LA-id987 a dtd:LA ;
    dtd:has_Value "Probeabrufen und Testfahrten nach § 29 oder"@de .

dtd:LA-id988 a dtd:LA ;
    dtd:has_Value "Nachbesserungen nach § 30."@de .


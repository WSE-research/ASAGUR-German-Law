    dtd:has_Value "Arbeitsgeräte, Werkzeuge, Maschinen und Anlagen hinsichtlich Material, Funktion und Einsatz auswählen und unter Berücksichtigung der Sicherheitsbestimmungen einsetzen"@de .

dtd:LA-id972 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arbeitsgeräte, Werkzeuge, Maschinen und Anlagen reinigen und warten"@de .

dtd:LA-id973 a dtd:LA ;

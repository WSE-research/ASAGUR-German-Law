    dtd:has_Value "Maschinen und Anlagen unter Berücksichtigung der Sicherheitsbestimmungen einrichten, Prozessdaten einstellen, Prozesse überwachen, Verfahrensparameter korrigieren"@de .

dtd:LA-id975 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Störungen feststellen und Maßnahmen zur Störungsbeseitigung ergreifen"@de .

dtd:LA-id976 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "prozessbezogene Berechnungen durchführen"@de .

dtd:LA-id977 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ziele, Aufgaben und betrieblichen Aufbau der Qualitätssicherung unterscheiden"@de .

dtd:LA-id978 a dtd:LA ;
    dtd:has_Size dtd:normal ;

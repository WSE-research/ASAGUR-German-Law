dtd:LA-id368 a dtd:LA ;
    dtd:has_Value "welche Regressionsfunktion der Analyse zugrunde liegt,"@de .

dtd:LA-id369 a dtd:LA ;
    dtd:has_Value "welche Merkmale sich mit welchem Einfluss auf die Miethöhe auswirken, ob dieser Einfluss statistisch signifikant ist und welches Signifikanzniveau dabei zugrunde gelegt wird,"@de .

dtd:LA-id370 a dtd:LA ;

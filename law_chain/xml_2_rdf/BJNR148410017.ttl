dtd:LA-id348 a dtd:LA ;
    dtd:has_Value "Familienname, Geburtsname, Vornamen und Doktorgrad des Fluggastes,"@de .

dtd:LA-id349 a dtd:LA ;
    dtd:has_Value "Angaben zum Fluggastdaten-Buchungscode,"@de .

dtd:LA-id350 a dtd:LA ;
    dtd:has_Value "Datum der Buchung und der Flugscheinausstellung,"@de .


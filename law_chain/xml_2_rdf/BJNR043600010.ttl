dtd:LA-id1008 a dtd:LA ;
    dtd:has_Value "Pflichtqualifikationen nach § 4 Absatz 2 Abschnitt A und C,"@de .

dtd:LA-id1009 a dtd:LA ;
    dtd:has_Value "zwei im Ausbildungsvertrag festzulegende Wahlqualifikationen nach § 4 Absatz 2 Abschnitt B."@de .

dtd:LA-id1010 a dtd:LA ;
    dtd:has_Value "die im Ausbildungsrahmenplan für die ersten 18 Monate genannten Fertigkeiten, Kenntnisse und Fähigkeiten sowie"@de .

dtd:LA-id1011 a dtd:LA ;
    dtd:has_Value "den im Berufsschulunterricht zu vermittelnden Lehrstoff, soweit er den im Ausbildungsrahmenplan genannten Fertigkeiten, Kenntnissen und Fähigkeiten entspricht."@de .

dtd:LA-id1012 a dtd:LA ;
    dtd:has_Value "„Steuern und Regeln von Anlagen zur Aufbereitung von Roh-, Faser- und Hilfsstoffen sowie Qualitätssicherung“ sowie"@de .

dtd:LA-id1013 a dtd:LA ;
    dtd:has_Value "„Aufbereiten von Roh-, Faser- und Hilfsstoffen sowie Instandhaltung“."@de .

dtd:LA-id1014 a dtd:LA ;
    dtd:has_Value "die Arbeitsschritte, die zur Aufbereitung von Zellstoff, Holzstoff, Altpapier und Rückstoff notwendig sind, festzulegen sowie entsprechende Aggregate und Anlagen zu bedienen,"@de .

dtd:LA-id1015 a dtd:LA ;
    dtd:has_Value "Faser- und Hilfsstoffe entsprechend den Auftragsanforderungen einzusetzen,"@de .

dtd:LA-id1016 a dtd:LA ;
    dtd:has_Value "qualitätssichernde Maßnahmen bei der Aufbereitung von Roh-, Faser- und Hilfsstoffen zu planen und durchzuführen,"@de .

dtd:LA-id1017 a dtd:LA ;
    dtd:has_Value "Arbeits- und Prüfmittel festzulegen,"@de .

dtd:LA-id1018 a dtd:LA ;
    dtd:has_Value "technische Unterlagen zu nutzen sowie"@de .

dtd:LA-id1019 a dtd:LA ;
    dtd:has_Value "Ergebnisse der Qualitätssicherung zu dokumentieren."@de .

dtd:LA-id1020 a dtd:LA ;
    dtd:has_Value "die Arbeitsabläufe, die zum Aufbereiten von Roh-, Faser- und Hilfsstoffen notwendig sind, unter Beachtung wirtschaftlicher, technischer und organisatorischer Vorgaben selbständig und kundenorientiert zu planen, durchzuführen und zu dokumentieren,"@de .

dtd:LA-id1021 a dtd:LA ;
    dtd:has_Value "den Einsatz von Roh- und Faserstoffen zu planen,"@de .

dtd:LA-id1022 a dtd:LA ;
    dtd:has_Value "die Frischwasser- und Abwasseraufbereitung zu überwachen, Störungen zu beseitigen sowie Untersuchungen zur Wasserqualität durchzuführen und auszuwerten,"@de .

dtd:LA-id1023 a dtd:LA ;
    dtd:has_Value "Kundenanforderungen und Anforderungen des Arbeits-, Gesundheits- und Umweltschutzes beim Aufbereiten von Roh-, Faser- und Hilfsstoffen einzuhalten sowie Anforderungen der Wirtschaftlichkeit zu berücksichtigen,"@de .

dtd:LA-id1024 a dtd:LA ;
    dtd:has_Value "instand haltende Arbeiten unter Verwendung von Schalt- und Funktionsplänen zu planen und durchzuführen und"@de .

dtd:LA-id1025 a dtd:LA ;
    dtd:has_Value "Störungen an Steuer- und Regeleinrichtungen festzustellen und Maßnahmen zu ihrer Behebung zu ergreifen."@de .

dtd:LA-id1026 a dtd:LA ;
    dtd:has_Value "die im Ausbildungsrahmenplan genannten Fertigkeiten, Kenntnisse und Fähigkeiten sowie"@de .

dtd:LA-id1027 a dtd:LA ;
    dtd:has_Value "den im Berufsschulunterricht zu vermittelnden Lehrstoff, soweit er den im Ausbildungsrahmenplan genannten Fertigkeiten, Kenntnissen und Fähigkeiten entspricht."@de .

dtd:LA-id1028 a dtd:LA ;
    dtd:has_Value "„Bedienen von Anlagen zur Herstellung von Papier, Karton, Pappe und Zellstoff“,"@de .

dtd:LA-id1029 a dtd:LA ;
    dtd:has_Value "„Steuern und Regeln von Anlagen zur Herstellung von Papier, Karton, Pappe und Zellstoff“,"@de .

dtd:LA-id1030 a dtd:LA ;
    dtd:has_Value "„Durchführen einer betrieblichen Aufgabe“ sowie"@de .

dtd:LA-id1031 a dtd:LA ;
    dtd:has_Value "„Wirtschafts- und Sozialkunde“."@de .

dtd:LA-id1032 a dtd:LA ;
    dtd:has_Value "Maschinen und Anlagen zur Herstellung von Papier, Karton, Pappe und Zellstoff einzurichten, zu bedienen und zu überwachen sowie Maßnahmen zur Behebung von Störungen zu ergreifen,"@de .

dtd:LA-id1033 a dtd:LA ;
    dtd:has_Value "Maschinen und Anlagen zum Veredeln, Ausrüsten und Verpacken einzurichten, zu bedienen und zu überwachen sowie Maßnahmen zur Behebung von Störungen zu ergreifen und"@de .

dtd:LA-id1034 a dtd:LA ;
    dtd:has_Value "Steuerungen von Regel- und Messeinrichtungen zu bedienen sowie Prozess- und Qualitätsleitsysteme zu nutzen."@de .

dtd:LA-id1035 a dtd:LA ;
    dtd:has_Value "Arbeitsabläufe zur Herstellung, Veredelung und Ausrüstung von Papier, Karton, Pappe und Zellstoff unter Beachtung wirtschaftlicher, technischer und organisatorischer Vorgaben selbständig und kundenorientiert zu planen, durchzuführen und zu dokumentieren,"@de .

dtd:LA-id1036 a dtd:LA ;
    dtd:has_Value "Anlagen und Anlagenteile zu inspizieren sowie Maßnahmen zur Fehlerbehebung zu ergreifen,"@de .

dtd:LA-id1037 a dtd:LA ;
    dtd:has_Value "qualitätssichernde Maßnahmen an Anlagen zur Herstellung von Papier, Karton, Pappe und Zellstoff zu planen und durchzuführen, Faser- und Hilfsstoffe zu prüfen und einzusetzen sowie Endprodukte zu prüfen,"@de .

dtd:LA-id1038 a dtd:LA ;
    dtd:has_Value "im Team sowie mit vor- und nachgelagerten Funktions- und Servicebereichen zu kommunizieren,"@de .

dtd:LA-id1039 a dtd:LA ;
    dtd:has_Value "beim Steuern und Regeln von Anlagen zur Herstellung von Papier, Karton, Pappe und Zellstoff Maßnahmen zur Arbeitsorganisation, zur Sicherheit und zum Gesundheitsschutz bei der Arbeit, zum Umweltschutz, zu rationeller Energieverwendung und zur Qualitätssicherung zu berücksichtigen sowie"@de .

dtd:LA-id1040 a dtd:LA ;
    dtd:has_Value "Informations- und Kommunikationstechnologien anzuwenden."@de .

dtd:LA-id1041 a dtd:LA ;
    dtd:has_Value "Problemstellungen zu erkennen und Arbeitsaufgaben abzuleiten, Arbeitsabläufe, die für die Durchführung der betrieblichen Aufgabe notwendig sind, zu planen, Material und Informationen zu beschaffen sowie Zeitpläne zu erstellen,"@de .

dtd:LA-id1042 a dtd:LA ;
    dtd:has_Value "Aufträge insbesondere unter Berücksichtigung technischer Dokumente und unter Einhaltung der Arbeitssicherheit und des Umweltschutzes durchzuführen,"@de .

dtd:LA-id1043 a dtd:LA ;
    dtd:has_Value "Arbeitsabläufe zu kontrollieren und bei Abweichungen Änderungen vorzunehmen sowie Arbeitsergebnisse zu prüfen,"@de .

dtd:LA-id1044 a dtd:LA ;

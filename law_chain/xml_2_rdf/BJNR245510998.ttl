dtd:LA-id223 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schiffe, Luftfahrzeuge, Plattformen oder sonstige auf See errichtete Anlagen, die sich auf oder über der Hohen See in dem Gebiet befinden, das als ausschließliche Wirtschaftszone der Bundesrepublik Deutschland völkerrechtlich anerkannt ist,"@de .

dtd:LA-id224 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schiffe und Luftfahrzeuge, die berechtigt sind, die Bundesflagge oder das Staatszugehörigkeitszeichen der Bundesrepublik Deutschland zu führen,"@de .

dtd:LA-id225 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Plattformen oder sonstige auf Hoher See errichtete Anlagen, die im Eigentum deutscher natürlicher oder juristischer Personen stehen,"@de .

dtd:LA-id226 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schiffe oder Luftfahrzeuge, die im Geltungsbereich dieses Gesetzes mit den einzubringenden, einzuleitenden oder zu verbrennenden Abfällen oder anderen Stoffen und Gegenständen beladen worden sind."@de .

dtd:LA-id227 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jede in die Hohe See erfolgende Beseitigung von Abfällen oder sonstigen Stoffen von Schiffen, Luftfahrzeugen, Plattformen oder sonstigen auf See errichteten Anlagen aus,"@de .

dtd:LA-id228 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jede in die Hohe See erfolgende Beseitigung von Schiffen, Luftfahrzeugen, Plattformen oder sonstigen auf See errichteten Anlagen,"@de .

dtd:LA-id229 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jede Lagerung von Abfällen oder sonstigen Stoffen auf dem Meeresboden und im Meeresuntergrund von Schiffen, Luftfahrzeugen, Plattformen oder sonstigen auf Hoher See errichteten Anlagen aus,"@de .

dtd:LA-id230 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Aufgabe von Plattformen oder sonstigen auf Hoher See errichteten Anlagen insbesondere durch deren teilweises oder vollständiges Versenken vor Ort in der Absicht, sich dieser Anlagen zu entledigen,"@de .

dtd:LA-id231 a dtd:LA ;
    dtd:has_Value "jede Zuführung von Stoffen und Gegenständen in die Hohe See im Rahmen des marinen Geo-Engineerings."@de .

dtd:LA-id232 a dtd:LA ;
    dtd:has_Value "der konventionellen Aqua- und Marikultur und"@de .

dtd:LA-id233 a dtd:LA ;
    dtd:has_Value "zur Schaffung künstlicher Riffe."@de .

dtd:LA-id234 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Baggergut,"@de .

dtd:LA-id235 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Urnen zur Seebestattung (Behältnisse, die mit der Asche aus der Verbrennung eines menschlichen Leichnams gefüllt sind),"@de .

dtd:LA-id236 a dtd:LA ;
    dtd:has_Value "Stoffe und Gegenstände, die im Rahmen von Maßnahmen des marinen Geo-Engineerings, die in der Anlage aufgeführt worden sind, eingebracht werden."@de .

dtd:LA-id237 a dtd:LA ;
    dtd:has_Value "keine Stoffe und Gegenstände in internationalen oder nationalen Meeresschutzgebieten eingebracht werden und die Einbringung von Stoffen und Gegenständen außerhalb solcher Schutzgebiete keine nachteiligen Auswirkungen auf diese haben kann,"@de .

dtd:LA-id238 a dtd:LA ;
    dtd:has_Value "Verschmutzungen, erhebliche nachteilige Auswirkungen und Gefahren für die Meeresumwelt, die Ökosysteme, die biologische Vielfalt, die menschliche Gesundheit und für die zulässige Nutzung der Meere verhindert werden,"@de .

dtd:LA-id239 a dtd:LA ;
    dtd:has_Value "Vorsorge gegen Verschmutzungen, erhebliche nachteilige Auswirkungen und Gefahren nach Nummer 2 getroffen wird,"@de .

dtd:LA-id240 a dtd:LA ;
    dtd:has_Value "keine erhebliche nachteilige Veränderung der Wasserbeschaffenheit zu besorgen ist und"@de .

dtd:LA-id241 a dtd:LA ;
    dtd:has_Value "Abfälle vermieden, nicht zu vermeidende Abfälle verwertet und nicht zu verwertende Abfälle ohne Beeinträchtigung des Wohls der Allgemeinheit beseitigt werden."@de .

dtd:LA-id242 a dtd:LA ;
    dtd:has_Value "von Beginn an ausreichend finanziert sind,"@de .

dtd:LA-id243 a dtd:LA ;
    dtd:has_Value "entsprechend dem Stand von Wissenschaft und Technik durchgeführt werden,"@de .

dtd:LA-id244 a dtd:LA ;
    dtd:has_Value "nicht aus wirtschaftlichen Interessen durchgeführt werden,"@de .

dtd:LA-id245 a dtd:LA ;
    dtd:has_Value "zur Qualitätssicherung durch unabhängige Fachwissenschaftler überprüft werden und"@de .

dtd:LA-id246 a dtd:LA ;
    dtd:has_Value "mit ihren Ergebnissen in wissenschaftlichen Fachzeitschriften veröffentlicht werden."@de .

dtd:LA-id247 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 4 Satz 1,"@de .

dtd:LA-id248 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ohne Erlaubnis nach § 5 Absatz 1 oder"@de .

dtd:LA-id249 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen einer mit einer Erlaubnis nach § 5 Absatz 1 verbundenen vollziehbaren Auflage"@de .

dtd:LA-id250 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Einvernehmen mit dem Bundesministerium für Verkehr und digitale Infrastruktur und dem Bundesministerium für Wirtschaft und Energie das Verfahren zur Erteilung der Erlaubnisse nach § 5 sowie die frühzeitige Öffentlichkeitsbeteiligung zu regeln;"@de .

dtd:LA-id251 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Einvernehmen mit dem Bundesministerium der Finanzen die Gebühren für die Erlaubniserteilung nach § 5 zu bestimmen und dabei feste Sätze oder Rahmensätze vorzusehen. Die Gebührensätze sind so zu bemessen, daß der mit den Amtshandlungen verbundene Personal- und Sachaufwand gedeckt wird."@de .

dtd:LA-id252 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 4 Satz 1 Abfälle oder sonstige Stoffe oder Gegenstände in die See einbringt,"@de .

dtd:LA-id253 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ohne Erlaubnis nach § 5 Absatz 1 Satz 1 Stoffe oder Gegenstände einbringt,"@de .

dtd:LA-id254 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Bedingung nach § 5 Absatz 2 Satz 1 nicht einhält,"@de .

dtd:LA-id255 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Auflage nach § 5 Absatz 2 Satz 1 zuwiderhandelt,"@de .

dtd:LA-id256 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 6 Abfälle oder sonstige Stoffe verbrennt oder"@de .

dtd:LA-id257 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 7 Satz 2 eine Meldung nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig macht."@de .

dtd:LA-id258 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Atomgesetz in der Fassung der Bekanntmachung vom 15. Juli 1985 (BGBl. I S. 1565), zuletzt geändert durch Artikel 1 des Gesetzes vom 6. April 1998 (BGBl. I S. 694);"@de .

dtd:LA-id259 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Gesetz vom 21. März 1956 über das internationale Übereinkommen zur Verhütung der Verschmutzung der See durch Öl 1954 (BGBl. 1956 II S. 379), zuletzt geändert durch Artikel 279 des Einführungsgesetzes zum Strafgesetzbuch vom 2. März 1974 (BGBl. I S. 469, 626);"@de .

dtd:LA-id260 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Gesetz vom 23. Dezember 1981 zu dem Internationalen Übereinkommen von 1973 zur Verhütung der Meeresverschmutzung durch Schiffe und zu dem Protokoll von 1978 zu diesem Übereinkommen (BGBl. 1982 II S. 2), zuletzt geändert durch Artikel 4 des Gesetzes vom 17. Juli 1997 (BGBl. I S. 1832);"@de .


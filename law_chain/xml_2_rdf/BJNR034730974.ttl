    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Mehrkostenausgleich nach § 3 Abs. 1 bis 4 sowie die Zuschüsse nach den §§ 4, 5 und 7 sowie nach § 6 in der bis zum 1. Januar 1990 geltenden Fassung dieses Gesetzes zu berechnen und das Vorliegen der Zuschußvoraussetzungen zu überprüfen,"@de .

dtd:LA-id296 a dtd:LA ;
    dtd:has_Size dtd:normal ;

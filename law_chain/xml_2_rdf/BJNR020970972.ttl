    dtd:has_Value "das Grundpfandrecht in diesem Staat die Rückzahlung und Verzinsung der Forderungen sicherstellt und"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gesamtbetrag dieser Beleihungen das haftende Eigenkapital der Bausparkasse nicht übersteigt."@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wegen der geringen Höhe des Darlehensbetrages eine Erklärung des Darlehensnehmers als ausreichend erscheint, in der er sich gegenüber der Bausparkasse verpflichtet, eine mögliche Sicherung durch Grundpfandrechte nicht durch eine Verpfändung des als Pfandobjekt in Betracht kommenden Gegenstandes für eine andere Verbindlichkeit oder durch seine Veräußerung zu verhindern oder"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Sicherung wegen der geringen Höhe des Darlehensbetrages nicht erforderlich erscheint."@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "inländische Körperschaften und Anstalten des öffentlichen Rechts,"@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Europäische Union, ihre Mitgliedstaaten, andere Vertragsstaaten des Abkommens über den Europäischen Wirtschaftsraum und die Europäische Investitionsbank,"@de .

dtd:LA-id571 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:LA-id511 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fremde Gelder von Kreditinstituten und sonstigen Kapitalsammelstellen aufnehmen,"@de .

dtd:LA-id512 a dtd:LA ;

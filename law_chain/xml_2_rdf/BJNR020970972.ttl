dtd:LA-id561 a dtd:LA ;
    dtd:has_Value "die Wartezeiten unangemessen lang sind,"@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Value "die Zuteilung nicht gewährleistet erscheint oder"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Value "die Erfüllung der von der Bausparkasse in den Bausparverträgen übernommenen Verpflichtungen nicht gewährleistet erscheint."@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Staat Vollmitglied der Organisation für wirtschaftliche Zusammenarbeit und Entwicklung ist,"@de .

dtd:LA-id565 a dtd:LA ;

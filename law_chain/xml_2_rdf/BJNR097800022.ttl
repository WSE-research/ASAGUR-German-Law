dtd:P-id68 a dtd:P ;
    dtd:has_BR dtd:BR-id54 ;
    dtd:has_Value " (+++ Textnachweis ab: 1.7.2022 +++) Das G wurde als Artikel 3 des G v. 28.6.2022 I 975 vom Bundestag beschlossen. Es ist gem. Art. 4 Abs. 1 dieses G am 1.7.2022 in Kraft getreten.   "@de ;
    dtd:has_pre dtd:pre-id84 .

dtd:P-id70 a dtd:P ;
    dtd:has_Value "(1) Der aktuelle Rentenwert beträgt ab dem 1. Juli 2022 36,02 Euro."@de .

dtd:P-id71 a dtd:P ;
    dtd:has_Value "(2) Der aktuelle Rentenwert (Ost) beträgt ab dem 1. Juli 2022 35,52 Euro."@de .

dtd:P-id73 a dtd:P ;
    dtd:has_Value "Der Ausgleichsbedarf beträgt ab dem 1. Juli 2022 1,0000."@de .

dtd:P-id75 a dtd:P ;
    dtd:has_Value "Das Sicherungsniveau vor Steuern beträgt für das Jahr 2022 48,14 Prozent."@de .

dtd:P-id77 a dtd:P ;
    dtd:has_Value "(1) Der allgemeine Rentenwert in der Alterssicherung der Landwirte beträgt ab dem 1. Juli 2022 16,63 Euro."@de .

dtd:P-id78 a dtd:P ;
    dtd:has_Value "(2) Der allgemeine Rentenwert (Ost) in der Alterssicherung der Landwirte beträgt ab dem 1. Juli 2022 16,37 Euro."@de .

dtd:P-id80 a dtd:P ;
    dtd:has_Value "(1) Der Anpassungsfaktor für die zum 1. Juli 2022 anzupassenden Geldleistungen der gesetzlichen Unfallversicherung im Sinne des § 44 Absatz 4 und des § 95 des Siebten Buches Sozialgesetzbuch beträgt 1,0535."@de .

dtd:P-id81 a dtd:P ;
    dtd:has_Value "(2) Die vom Jahresarbeitsverdienst abhängigen Geldleistungen und das Pflegegeld der gesetzlichen Unfallversicherung im Sinne des § 215 Absatz 5 des Siebten Buches Sozialgesetzbuch für Versicherungsfälle, die vor dem 1. Juli 2022 eingetreten sind, werden zum 1. Juli 2022 angepasst. Der Anpassungsfaktor beträgt 1,0612."@de .

dtd:P-id83 a dtd:P ;
    dtd:has_DL dtd:DL-id60 ;
    dtd:has_Value "Das Pflegegeld der gesetzlichen Unfallversicherung beträgt vom 1. Juli 2022 an 1.für Versicherungsfälle, auf die § 44 Absatz 2 des Siebten Buches Sozialgesetzbuch anzuwenden ist, zwischen 408 Euro und 1 624 Euro monatlich,2.für Versicherungsfälle, auf die § 215 Absatz 5 des Siebten Buches Sozialgesetzbuch anzuwenden ist, zwischen 395 Euro und 1 585 Euro."@de .


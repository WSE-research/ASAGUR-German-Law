dtd:LA-id516 a dtd:LA ;
    dtd:has_Value "Anzeigen der Aufnahme der betrieblichen Tätigkeit durch Sammler, Beförderer, Händler und Makler von Abfällen nach § 53 Absatz 1 Satz 1 des Kreislaufwirtschaftsgesetzes und"@de .

dtd:LA-id517 a dtd:LA ;
    dtd:has_Value "Erlaubnisse für Sammler, Beförderer, Händler und Makler von gefährlichen Abfällen nach § 54 Absatz 1 Satz 1 des Kreislaufwirtschaftsgesetzes."@de .

dtd:LA-id518 a dtd:LA ;
    dtd:has_DL dtd:DL-id328 ;
    dtd:has_Value "wegen Verletzung von Vorschriften a)des Strafrechts über gemeingefährliche Delikte oder Delikte gegen die Umwelt,b)des Immissionsschutz-, Abfall-, Wasser-, Natur- und Landschaftsschutz-, Chemikalien-, Gentechnik- oder Atom- und Strahlenschutzrechts,c)des Lebensmittel-, Arzneimittel-, Pflanzenschutz- oder Infektionsschutzrechts,d)des Gewerbe-, Arbeitsschutz-, Transport- oder Gefahrgutrechts odere)des Betäubungsmittel-, Waffen- oder Sprengstoffrechtsinnerhalb der letzten fünf Jahre vor Anzeige der Aufnahme der betrieblichen Tätigkeit oder der Beantragung der Erlaubnis mit einer Geldbuße in Höhe von mehr als zweitausendfünfhundert Euro belegt oder zu einer Strafe verurteilt worden ist oder"@de .

dtd:LA-id519 a dtd:LA ;
    dtd:has_Value "des Strafrechts über gemeingefährliche Delikte oder Delikte gegen die Umwelt,"@de .

dtd:LA-id520 a dtd:LA ;
    dtd:has_Value "des Immissionsschutz-, Abfall-, Wasser-, Natur- und Landschaftsschutz-, Chemikalien-, Gentechnik- oder Atom- und Strahlenschutzrechts,"@de .

dtd:LA-id521 a dtd:LA ;
    dtd:has_Value "des Lebensmittel-, Arzneimittel-, Pflanzenschutz- oder Infektionsschutzrechts,"@de .

dtd:LA-id522 a dtd:LA ;
    dtd:has_Value "des Gewerbe-, Arbeitsschutz-, Transport- oder Gefahrgutrechts oder"@de .

dtd:LA-id523 a dtd:LA ;
    dtd:has_Value "des Betäubungsmittel-, Waffen- oder Sprengstoffrechts"@de .

dtd:LA-id524 a dtd:LA ;
    dtd:has_Value "wiederholt oder grob pflichtwidrig gegen die in Nummer 1 genannten Vorschriften verstoßen hat."@de .

dtd:LA-id525 a dtd:LA ;
    dtd:has_Value "ein Hochschul- oder Fachhochschulstudium abgeschlossen hat,"@de .

dtd:LA-id526 a dtd:LA ;
    dtd:has_Value "eine kaufmännische oder technische Fachschul- oder Berufsausbildung besitzt oder"@de .

dtd:LA-id527 a dtd:LA ;
    dtd:has_Value "eine Qualifikation als Meister vorweisen kann."@de .

dtd:LA-id528 a dtd:LA ;
    dtd:has_Value "des Sammelns oder Beförderns von Abfällen die erworbenen Kenntnisse des Betroffenen nicht auf die angezeigte, sondern auf die jeweils andere Tätigkeit beziehen,"@de .

dtd:LA-id529 a dtd:LA ;
    dtd:has_Value "des Handelns mit Abfällen die erworbenen Kenntnisse des Betroffenen nicht auf die angezeigte, sondern auf die Tätigkeit des Sammelns oder Beförderns beziehen oder"@de .

dtd:LA-id530 a dtd:LA ;
    dtd:has_Value "des Makelns von Abfällen die erworbenen Kenntnisse des Betroffenen nicht auf die angezeigte, sondern auf die Tätigkeit des Sammelns, Beförderns oder Handelns von Abfällen beziehen."@de .

dtd:LA-id531 a dtd:LA ;
    dtd:has_Value "während einer zweijährigen praktischen Tätigkeit erworbene Kenntnisse über die Tätigkeit, für die der Betrieb die Erlaubnis beantragt, sowie"@de .

dtd:LA-id532 a dtd:LA ;
    dtd:has_Value "die Teilnahme an einem oder mehreren von der zuständigen Behörde anerkannten Lehrgängen, in denen Kenntnisse entsprechend der Anlage 1 vermittelt werden."@de .

dtd:LA-id533 a dtd:LA ;
    dtd:has_Value "ein Hochschul- oder Fachhochschulstudium abgeschlossen hat,"@de .

dtd:LA-id534 a dtd:LA ;
    dtd:has_Value "eine kaufmännische oder technische Fachschul- oder Berufsausbildung besitzt oder"@de .

dtd:LA-id535 a dtd:LA ;
    dtd:has_Value "eine Qualifikation als Meister vorweisen kann."@de .

dtd:LA-id536 a dtd:LA ;
    dtd:has_Value "des Sammelns oder Beförderns von gefährlichen Abfällen die erworbenen Kenntnisse des Betroffenen nicht auf die beantragte, sondern auf die jeweils andere Tätigkeit beziehen,"@de .

dtd:LA-id537 a dtd:LA ;
    dtd:has_Value "des Handelns mit gefährlichen Abfällen die erworbenen Kenntnisse des Betroffenen nicht auf die beantragte, sondern auf die Tätigkeit des Sammelns oder Beförderns von gefährlichen Abfällen beziehen oder"@de .

dtd:LA-id538 a dtd:LA ;
    dtd:has_Value "des Makelns von gefährlichen Abfällen die erworbenen Kenntnisse des Betroffenen nicht auf die beantragte, sondern auf die Tätigkeit des Sammelns, Beförderns oder Handelns von gefährlichen Abfällen beziehen."@de .

dtd:LA-id539 a dtd:LA ;
    dtd:has_Citation dtd:Citation-id224 ;
    dtd:has_Value "der Vordruck nach Anlage 2 in elektronischer Form vorgehalten wird; das Feld \"Unterschrift\" im Vordruck nach Anlage 2 entfällt; und"@de .

dtd:LA-id540 a dtd:LA ;
    dtd:has_DL dtd:DL-id377 ;
    dtd:has_Value "die Möglichkeit geschaffen wird a)für Entsorgungsfachbetriebe, der Anzeige das Zertifikat nach § 56 Absatz 3 des Kreislaufwirtschaftsgesetzes beizufügen undb)für Sammler, Beförderer, Händler und Makler von gefährlichen Abfällen, die einen EMAS-Standort betreiben, der Anzeige die Registrierungsurkunde beizufügen."@de .

dtd:LA-id541 a dtd:LA ;
    dtd:has_Value "für Entsorgungsfachbetriebe, der Anzeige das Zertifikat nach § 56 Absatz 3 des Kreislaufwirtschaftsgesetzes beizufügen und"@de .

dtd:LA-id542 a dtd:LA ;
    dtd:has_Value "für Sammler, Beförderer, Händler und Makler von gefährlichen Abfällen, die einen EMAS-Standort betreiben, der Anzeige die Registrierungsurkunde beizufügen."@de .

dtd:LA-id543 a dtd:LA ;
    dtd:has_Value "jederzeit Anzeigen nach Absatz 1 Satz 1 über das informationstechnische System erstattet werden können und"@de .

dtd:LA-id544 a dtd:LA ;
    dtd:has_Value "nach den Artikeln 24, 25 und 32 der Verordnung (EU) 2016/679 des Europäischen Parlaments und des Rates vom 27. April 2016 zum Schutz natürlicher Personen bei der Verarbeitung personenbezogener Daten, zum freien Datenverkehr und zur Aufhebung der Richtlinie 95/46/EG (Datenschutz-Grundverordnung) (ABl. L 119 vom 4.5.2016, S. 1) entsprechende technische und organisatorische Sicherungsmaßnahmen ergriffen werden."@de .

dtd:LA-id545 a dtd:LA ;
    dtd:has_Value "die Gewerbeanmeldung,"@de .

dtd:LA-id546 a dtd:LA ;
    dtd:has_Value "ein Auszug aus dem Handels-, Vereins- oder Genossenschaftsregister, sofern eine Eintragung erfolgt ist,"@de .

dtd:LA-id547 a dtd:LA ;
    dtd:has_Value "eine firmenbezogene Auskunft, Belegart 9, aus dem Gewerbezentralregister, sofern es sich bei dem Unternehmen um eine juristische Person oder Personenvereinigung handelt,"@de .

dtd:LA-id548 a dtd:LA ;
    dtd:has_DL dtd:DL-id404 ;
    dtd:has_Value "eine personenbezogene Auskunft, Belegart 9, aus dem Gewerbezentralregister für a)den Inhaber undb)die für die Leitung und Beaufsichtigung des Betriebes verantwortlichen Personen, sofern solche vorhanden sind,"@de .

dtd:LA-id549 a dtd:LA ;
    dtd:has_Value "den Inhaber und"@de .

dtd:LA-id550 a dtd:LA ;
    dtd:has_Value "die für die Leitung und Beaufsichtigung des Betriebes verantwortlichen Personen, sofern solche vorhanden sind,"@de .

dtd:LA-id551 a dtd:LA ;
    dtd:has_DL dtd:DL-id409 ;
    dtd:has_Value "ein Führungszeugnis, Belegart OG, a)des Inhabers undb)der für die Leitung und Beaufsichtigung des Betriebes verantwortlichen Personen, sofern solche vorhanden sind,"@de .

dtd:LA-id552 a dtd:LA ;
    dtd:has_Value "des Inhabers und"@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Value "der für die Leitung und Beaufsichtigung des Betriebes verantwortlichen Personen, sofern solche vorhanden sind,"@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_DL dtd:DL-id414 ;
    dtd:has_Value "ein Nachweis über die Fachkunde a)des Inhabers, soweit er für die Leitung des Betriebes verantwortlich ist, undb)der für die Leitung und Beaufsichtigung des Betriebes verantwortlichen Personen, sofern solche vorhanden sind,"@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_Value "des Inhabers, soweit er für die Leitung des Betriebes verantwortlich ist, und"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Value "der für die Leitung und Beaufsichtigung des Betriebes verantwortlichen Personen, sofern solche vorhanden sind,"@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Value "der Nachweis einer Betriebshaftpflichtversicherung und einer auf die jeweilige Tätigkeit bezogenen Umwelthaftpflichtversicherung, sofern solche Versicherungen vorhanden sind, sowie"@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Value "der Nachweis der Kraftfahrzeug-Haftpflichtversicherung bei Sammlern und Beförderern von Abfällen, die gefährliche Abfälle auf öffentlichen Straßen befördern."@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Value "das Datum des Eingangs des vollständigen Antrages,"@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Value "einen Hinweis auf die Genehmigungsfiktion nach § 54 Absatz 6 Satz 2 des Kreislaufwirtschaftsgesetzes in Verbindung mit § 42a des Verwaltungsverfahrensgesetzes,"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Value "das Datum des Beginns und des Endes der Frist für die Genehmigungsfiktion sowie"@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Value "einen Hinweis auf mögliche Rechtsbehelfe im Zusammenhang mit der Erlaubnis."@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Value "der Vordruck nach Anlage 3 in elektronischer Form vorgehalten wird und"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Value "für den Antragsteller die Möglichkeit geschaffen wird, die Unterlagen nach § 9 Absatz 3 Satz 1 beizufügen."@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Value "jederzeit Erlaubnisse nach Absatz 1 Satz 1 über das informationstechnische System beantragt werden können und"@de .

dtd:LA-id566 a dtd:LA ;

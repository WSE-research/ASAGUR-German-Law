dtd:LA-id1547 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(1) Wer rechtswidrig Daten löscht, unterdrückt, unbrauchbar macht oder verändert, wird mit Freiheitsstrafe bis zu zwei Jahren, Verurteilung auf Bewährung oder mit Geldstrafe bestraft."@de .

dtd:LA-id1548 a dtd:LA ;

dtd:LA-id3109 a dtd:LA ;
    dtd:has_Value "hoheitsrechtlicher Aufgaben oder"@de .

dtd:LA-id3110 a dtd:LA ;
    dtd:has_Value "von Aufgaben, die zur Sicherung des Staates oder des öffentlichen Lebens nicht ausschließlich Personen übertragen werden dürfen, die in einem privatrechtlichen Arbeitsverhältnis stehen."@de .

dtd:LA-id3111 a dtd:LA ;
    dtd:has_Value "zur späteren Verwendung auf Lebenszeit oder"@de .

dtd:LA-id3112 a dtd:LA ;
    dtd:has_Value "zur Übertragung eines Amtes mit leitender Funktion."@de .

dtd:LA-id3113 a dtd:LA ;
    dtd:has_Value "der Ableistung eines Vorbereitungsdienstes oder"@de .

dtd:LA-id3114 a dtd:LA ;
    dtd:has_Value "der vorübergehenden Wahrnehmung von Aufgaben nach § 5."@de .

dtd:LA-id3115 a dtd:LA ;
    dtd:has_DL dtd:DL-id2433 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Deutsche oder Deutscher im Sinne des Artikels 116 Absatz 1 des Grundgesetzes ist oder die Staatsangehörigkeit a)eines anderen Mitgliedstaates der Europäischen Union oderb)eines anderen Vertragsstaates des Abkommens über den Europäischen Wirtschaftsraum oderc)eines Drittstaates, dem die Bundesrepublik Deutschland und die Europäische Union vertraglich einen entsprechenden Anspruch auf Anerkennung der Berufsqualifikationen eingeräumt haben,besitzt,"@de .

dtd:LA-id3116 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines anderen Mitgliedstaates der Europäischen Union oder"@de .

dtd:LA-id3117 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines anderen Vertragsstaates des Abkommens über den Europäischen Wirtschaftsraum oder"@de .

dtd:LA-id3118 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines Drittstaates, dem die Bundesrepublik Deutschland und die Europäische Union vertraglich einen entsprechenden Anspruch auf Anerkennung der Berufsqualifikationen eingeräumt haben,"@de .

dtd:LA-id3119 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gewähr dafür bietet, jederzeit für die freiheitliche demokratische Grundordnung im Sinne des Grundgesetzes einzutreten, und"@de .

dtd:LA-id3120 a dtd:LA ;
    dtd:has_DL dtd:DL-id2440 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "a)die für die entsprechende Laufbahn vorgeschriebene Vorbildung besitzt oderb)die erforderliche Befähigung durch Lebens- und Berufserfahrung erworben hat."@de .

dtd:LA-id3121 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für die entsprechende Laufbahn vorgeschriebene Vorbildung besitzt oder"@de .

dtd:LA-id3122 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die erforderliche Befähigung durch Lebens- und Berufserfahrung erworben hat."@de .

dtd:LA-id3123 a dtd:LA ;
    dtd:has_Value "Begründung des Beamtenverhältnisses,"@de .

dtd:LA-id3124 a dtd:LA ;
    dtd:has_Value "Umwandlung des Beamtenverhältnisses in ein solches anderer Art,"@de .

dtd:LA-id3125 a dtd:LA ;
    dtd:has_Value "Verleihung eines anderen Amtes mit anderem Endgrundgehalt und anderer Amtsbezeichnung oder"@de .

dtd:LA-id3126 a dtd:LA ;
    dtd:has_Value "Verleihung eines anderen Amtes mit anderer Amtsbezeichnung beim Wechsel der Laufbahngruppe."@de .

dtd:LA-id3127 a dtd:LA ;
    dtd:has_Value "bei der Begründung des Beamtenverhältnisses die Wörter „unter Berufung in das Beamtenverhältnis“ mit dem die Art des Beamtenverhältnisses bestimmenden Zusatz „auf Lebenszeit“, „auf Probe“, „auf Widerruf“ oder „als Ehrenbeamtin“ oder „als Ehrenbeamter“ oder „auf Zeit“ mit der Angabe der Zeitdauer der Berufung,"@de .

dtd:LA-id3128 a dtd:LA ;
    dtd:has_Value "bei der Umwandlung des Beamtenverhältnisses in ein solches anderer Art die diese Art bestimmenden Wörter nach Nummer 1 und"@de .

dtd:LA-id3129 a dtd:LA ;
    dtd:has_Value "bei der Verleihung eines Amtes die Amtsbezeichnung."@de .

dtd:LA-id3130 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die in § 7 bezeichneten Voraussetzungen erfüllt und"@de .

dtd:LA-id3131 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich in einer Probezeit in vollem Umfang bewährt hat."@de .

dtd:LA-id3132 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kriterien und das Verfahren der Bewährungsfeststellung,"@de .

dtd:LA-id3133 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mindestprobezeit sowie Ausnahmen von der Mindestprobezeit,"@de .

dtd:LA-id3134 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verlängerung der Probezeit und die Anrechnung von Zeiten gleichwertiger Tätigkeiten auf die Probezeit."@de .

dtd:LA-id3135 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie nicht der in § 10 Abs. 2 vorgeschriebenen Form entspricht,"@de .

dtd:LA-id3136 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie von einer sachlich unzuständigen Behörde ausgesprochen wurde oder"@de .

dtd:LA-id3137 a dtd:LA ;
    dtd:has_DL dtd:DL-id2480 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Zeitpunkt der Ernennung a)nach § 7 Absatz 1 Satz 1 Nummer 1 keine Ernennung erfolgen durfte und keine Ausnahme nach § 7 Abs. 3 zugelassen war oderb)die Fähigkeit zur Wahrnehmung öffentlicher Ämter nicht vorlag."@de .

dtd:LA-id3138 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 7 Absatz 1 Satz 1 Nummer 1 keine Ernennung erfolgen durfte und keine Ausnahme nach § 7 Abs. 3 zugelassen war oder"@de .

dtd:LA-id3139 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Fähigkeit zur Wahrnehmung öffentlicher Ämter nicht vorlag."@de .

dtd:LA-id3140 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall des Absatzes 1 Nummer 1 aus der Urkunde oder aus dem Akteninhalt eindeutig hervorgeht, dass die für die Ernennung zuständige Stelle ein bestimmtes Beamtenverhältnis begründen oder ein bestehendes Beamtenverhältnis in ein solches anderer Art umwandeln wollte, für das die sonstigen Voraussetzungen vorliegen, und die oder der Dienstvorgesetzte dies schriftlich festgestellt hat; das Gleiche gilt, wenn die Angabe der Dauer fehlt, die Dauer aber durch Rechtsvorschrift bestimmt ist,"@de .

dtd:LA-id3141 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall des Absatzes 1 Nr. 2 die sachlich zuständige Behörde die Ernennung bestätigt oder"@de .

dtd:LA-id3142 a dtd:LA ;

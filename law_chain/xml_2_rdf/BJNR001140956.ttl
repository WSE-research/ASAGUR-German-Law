    dtd:has_Size dtd:normal ;
    dtd:has_Value "er innerhalb der drei Monate schriftlich erklärt, dass er mit der Fortsetzung des Wehrdienstverhältnisses nicht einverstanden ist, mit dem Tag der Abgabe der Erklärung."@de .

dtd:LA-id3492 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dauerhaft nicht dienstfähig sind (§ 64),"@de .

dtd:LA-id3493 a dtd:LA ;

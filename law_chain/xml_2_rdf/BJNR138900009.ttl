    dtd:has_Size dtd:normal ;
    dtd:has_Value "der jeweilige Betreiber der Eisenbahninfrastruktur im Eisenbahnverkehr oder"@de .

dtd:LA-id3176 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Schiffsführer in der Binnenschifffahrt"@de .

dtd:LA-id3177 a dtd:LA ;

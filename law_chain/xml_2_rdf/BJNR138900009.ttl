    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Schüttgut-Container,"@de .

dtd:LA-id3146 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Fahrzeug für die Beförderung in loser Schüttung,"@de .

dtd:LA-id3147 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Batterie-Fahrzeug,"@de .

dtd:LA-id3148 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein MEMU,"@de .

dtd:LA-id3149 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Wagen für die Beförderung in loser Schüttung,"@de .


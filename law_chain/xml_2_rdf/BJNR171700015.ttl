dtd:LA-id51 a dtd:LA ;
    dtd:has_Value "nach § 33 Absatz 1 in Verbindung mit Absatz 3 Nummer 1 des Bundesdisziplinargesetzes die Kürzung der Dienstbezüge bis zum Höchstmaß für die Beamtinnen und Beamten festzusetzen,"@de .

dtd:LA-id52 a dtd:LA ;
    dtd:has_Value "nach § 34 Absatz 1 in Verbindung mit Absatz 2 Satz 1 des Bundesdisziplinargesetzes Disziplinarklage gegen die Beamtinnen und Beamten zu erheben,"@de .

dtd:LA-id53 a dtd:LA ;

    dtd:has_Value "bestehende familienfördernde Leistungen und Hilfen für Kinder und Familien, einschließlich der besonderen Rechte im Arbeitsleben,"@de .

dtd:LA-id541 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorsorgeuntersuchungen bei Schwangerschaft und die Kosten der Entbindung,"@de .

dtd:LA-id542 a dtd:LA ;

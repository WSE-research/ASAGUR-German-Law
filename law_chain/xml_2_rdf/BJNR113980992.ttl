    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Pseudonym der Schwangeren,"@de .

dtd:LA-id590 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den voraussichtlichen Geburtstermin und"@de .


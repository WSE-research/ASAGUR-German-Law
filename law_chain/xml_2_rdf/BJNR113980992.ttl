    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Pseudonym,"@de .

dtd:LA-id586 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Geburtsort und das Geburtsdatum des Kindes,"@de .

dtd:LA-id587 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Name und die Anschrift der geburtshilflichen Einrichtung oder der zur Leistung von Geburtshilfe berechtigten Person, bei der die Anmeldung nach Absatz 4 erfolgt ist, und"@de .

dtd:LA-id588 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anschrift der Beratungsstelle."@de .


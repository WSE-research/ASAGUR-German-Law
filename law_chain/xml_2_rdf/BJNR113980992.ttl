    dtd:has_Value "Bundesland, in dem der Schwangerschaftsabbruch vorgenommen wird, und Bundesland oder Staat im Ausland, in dem die Schwangere wohnt,"@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vornahme in Arztpraxis oder Krankenhaus und im Falle der Vornahme des Eingriffs im Krankenhaus die Dauer des Krankenhausaufenthaltes."@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name und Anschrift der Einrichtung nach § 13 Abs. 1;"@de .

dtd:LA-id571 a dtd:LA ;

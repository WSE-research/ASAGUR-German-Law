    dtd:has_U dtd:U-id2054 ;
    dtd:has_Value "Widerrufsbelehrung"@de .

dtd:B-id2055 a dtd:B ;
    dtd:has_U dtd:U-id2056 ;
    dtd:has_Value "Abschnitt 1"@de .

dtd:B-id2057 a dtd:B ;
    dtd:has_U dtd:U-id2058 ;
    dtd:has_Value "Widerrufsrecht, Widerrufsfolgen und besondere Hinweise"@de .

dtd:B-id2059 a dtd:B ;
    dtd:has_U dtd:U-id2060 ;
    dtd:has_Value "Widerrufsrecht"@de .

dtd:B-id2061 a dtd:B ;
    dtd:has_Value "Sie können Ihre Vertragserklärung innerhalb einer Frist von [14]"@de .

dtd:B-id2062 a dtd:B ;
    dtd:has_Value "Tagen ohne Angabe von Gründen in Textform (z. B. Brief, Fax, E-Mail) widerrufen."@de .

dtd:B-id2063 a dtd:B ;
    dtd:has_Value "Die Widerrufsfrist beginnt, nachdem Ihnen"@de .

dtd:B-id2064 a dtd:B ;
    dtd:has_Value "der Versicherungsschein,"@de .

dtd:B-id2065 a dtd:B ;
    dtd:has_Value "die Vertragsbestimmungen,"@de .

dtd:B-id2066 a dtd:B ;
    dtd:has_Value "diese Belehrung,"@de .

dtd:B-id2067 a dtd:B ;
    dtd:has_Value "das Informationsblatt zu Versicherungsprodukten,"@de .

dtd:B-id2068 a dtd:B ;
    dtd:has_Value "und die weiteren in Abschnitt 2 aufgeführten Informationen"@de .

dtd:B-id2069 a dtd:B ;
    dtd:has_Value "jeweils in Textform zugegangen sind."@de .

dtd:B-id2070 a dtd:B ;
    dtd:has_Value "Zur Wahrung der Widerrufsfrist genügt die rechtzeitige Absendung des Widerrufs. Der Widerruf ist zu richten an:"@de .

dtd:B-id2071 a dtd:B ;
    dtd:has_U dtd:U-id2072 ;
    dtd:has_Value "Widerrufsfolgen"@de .

dtd:B-id2073 a dtd:B ;
    dtd:has_Value "Im Falle eines wirksamen Widerrufs endet der Versicherungsschutz und der Versicherer hat Ihnen den auf die Zeit nach Zugang des Widerrufs entfallenden Teil der Prämien zu erstatten, wenn Sie zugestimmt haben, dass der Versicherungsschutz vor dem Ende der Widerrufsfrist beginnt. Den Teil der Prämie, der auf die Zeit bis zum Zugang des Widerrufs entfällt, darf der Versicherer in diesem Fall einbehalten; dabei handelt es sich um [einen Betrag in Höhe von…]"@de .

dtd:B-id2074 a dtd:B ;
    dtd:has_Value "Der Versicherer hat zurückzuzahlende Beträge unverzüglich, spätestens 30 Tage nach Zugang des Widerrufs, zu erstatten."@de .

dtd:B-id2075 a dtd:B ;
    dtd:has_Value "Beginnt der Versicherungsschutz nicht vor dem Ende der Widerrufsfrist, so hat der wirksame Widerruf zur Folge, dass empfangene Leistungen zurückzugewähren und gezogene Nutzungen (z. B. Zinsen) herauszugeben sind."@de .

dtd:B-id2076 a dtd:B ;
    dtd:has_U dtd:U-id2077 ;
    dtd:has_Value "Besondere Hinweise"@de .

dtd:B-id2078 a dtd:B ;
    dtd:has_Value "Ihr Widerrufsrecht erlischt, wenn der Vertrag auf Ihren ausdrücklichen Wunsch sowohl von Ihnen als auch vom Versicherer vollständig erfüllt ist, bevor Sie Ihr Widerrufsrecht ausgeübt haben."@de .

dtd:B-id2079 a dtd:B ;
    dtd:has_U dtd:U-id2080 ;
    dtd:has_Value "Abschnitt 2"@de .

dtd:B-id2081 a dtd:B ;
    dtd:has_U dtd:U-id2082 ;
    dtd:has_Value "Auflistung der für den Fristbeginn erforderlichen weiteren Informationen"@de .

dtd:B-id2083 a dtd:B ;
    dtd:has_U dtd:U-id2084 ;
    dtd:has_Value "Unterabschnitt 1"@de .

dtd:B-id2085 a dtd:B ;
    dtd:has_U dtd:U-id2086 ;
    dtd:has_Value "Informationspflichten bei allen Versicherungszweigen"@de .

dtd:B-id2087 a dtd:B ;
    dtd:has_U dtd:U-id2088 ;
    dtd:has_Value "Unterabschnitt 2"@de .

dtd:B-id2089 a dtd:B ;
    dtd:has_U dtd:U-id2090 ;
    dtd:has_Value "Zusätzliche Informationspflichten bei dieser Berufsunfähigkeitsversicherung"@de .

dtd:B-id2091 a dtd:B ;
    dtd:has_U dtd:U-id2092 ;
    dtd:has_Value "Unterabschnitt 3"@de .

dtd:B-id2093 a dtd:B ;
    dtd:has_U dtd:U-id2094 ;
    dtd:has_Value "Zusätzliche Informationspflichten bei dieser Krankenversicherung"@de .

dtd:B-id2095 a dtd:B ;
    dtd:has_Value "(Ort), (Datum), (Unterschrift des Versicherungsnehmers)"@de .

dtd:B-id2096 a dtd:B ;
    dtd:has_Value "Gestaltungshinweise:"@de .

dtd:B-id2097 a dtd:B ;
    dtd:has_Value "„das Basisinformationsblatt zur Verfügung gestellt worden ist und“"@de .

dtd:B-id2098 a dtd:B ;
    dtd:has_Value "„das PEPP-Basisinformationsblatt zur Verfügung gestellt worden ist und“"@de .

dtd:B-id2099 a dtd:B ;
    dtd:has_Value "„der Versicherungsschein,“"@de .

dtd:B-id2100 a dtd:B ;
    dtd:has_Value "„das Informationsblatt zu Versicherungsprodukten“"@de .

dtd:B-id2101 a dtd:B ;
    dtd:has_Value "„das Informationsblatt zu Versicherungsprodukten“"@de .

dtd:B-id2102 a dtd:B ;
    dtd:has_Value "„das Produktinformationsblatt“"@de .

dtd:B-id2103 a dtd:B ;
    dtd:has_Value "„Die Widerrufsfrist beginnt zudem nicht, bevor Ihnen mindestens eine Woche nach Abgabe Ihrer Vertragserklärung die Belehrung über das Widerrufsrecht und das Informationsblatt zu Versicherungsprodukten erneut in Textform zugegangen sind.“"@de .

dtd:B-id2104 a dtd:B ;
    dtd:has_Value "„Den Rückkaufswert einschließlich der Überschussanteile hat der Versicherer Ihnen auszuzahlen.“"@de .

dtd:B-id2105 a dtd:B ;
    dtd:has_Value "„Haben Sie Ihr Widerrufsrecht hinsichtlich des Versicherungsvertrages wirksam ausgeübt, so sind Sie auch an einen mit dem Versicherungsvertrag zusammenhängenden Vertrag nicht mehr gebunden. Ein zusammenhängender Vertrag liegt vor, wenn er einen Bezug zu dem widerrufenen Vertrag aufweist und eine Dienstleistung des Versicherers oder eines Dritten auf der Grundlage einer Vereinbarung zwischen dem Dritten und dem Versicherer betrifft. Eine Vertragsstrafe darf weder vereinbart noch verlangt werden.“"@de .

dtd:B-id2106 a dtd:B ;
    dtd:has_Value "„Unterabschnitt 3“"@de .

dtd:B-id2107 a dtd:B ;
    dtd:has_Value "„Unterabschnitt 2“"@de .


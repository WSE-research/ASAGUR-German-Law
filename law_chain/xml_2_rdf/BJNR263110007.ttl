dtd:I-id2108 a dtd:I ;
    dtd:has_Value " 2."@de .

dtd:I-id2109 a dtd:I ;
    dtd:has_Value "die Identität einer Vertreterin oder eines Vertreters des Versicherers in dem Mitgliedstaat der Europäischen Union, in dem Sie Ihren Wohnsitz haben, wenn es eine solche Vertreterin oder einen solchen Vertreter gibt, oder die Identität einer anderen gewerblich tätigen Person als dem Versicherer, wenn Sie mit dieser geschäftlich zu tun haben, und die Eigenschaft, in der diese Person gegenüber Ihnen tätig wird;"@de .

dtd:I-id2110 a dtd:I ;
    dtd:has_Value "b)"@de .

dtd:I-id2111 a dtd:I ;
    dtd:has_Value "jede andere Anschrift, die für die Geschäftsbeziehung zwischen einer Vertreterin oder einem Vertreter des Versicherers oder einer anderen gewerblich tätigen Person gemäß Nummer 2 und Ihnen maßgeblich ist, bei juristischen Personen, Personenvereinigungen oder -gruppen auch den Namen eines Vertretungsberechtigten; soweit die Mitteilung durch Übermittlung der Vertragsbestimmungen einschließlich der Allgemeinen Versicherungsbedingungen erfolgt, bedürfen die Informationen einer hervorgehobenen und deutlich gestalteten Form;"@de .

dtd:I-id2112 a dtd:I ;
    dtd:has_Value " 5."@de .

dtd:I-id2113 a dtd:I ;
    dtd:has_Value "Angaben über das Bestehen eines Garantiefonds oder anderer Entschädigungsregelungen; Name und Anschrift des Garantiefonds sind anzugeben;"@de .

dtd:I-id2114 a dtd:I ;
    dtd:has_Value "a)"@de .

dtd:I-id2115 a dtd:I ;
    dtd:has_Value "gegebenenfalls zusätzlich anfallende Kosten unter Angabe des insgesamt zu zahlenden Betrages sowie mögliche weitere Steuern, Gebühren oder Kosten, die nicht über den Versicherer abgeführt oder von ihm in Rechnung gestellt werden;"@de .

dtd:I-id2116 a dtd:I ;
    dtd:has_Value "b)"@de .

dtd:I-id2117 a dtd:I ;
    dtd:has_Value "alle Kosten, die Ihnen für die Benutzung von Fernkommunikationsmitteln entstehen, wenn solche zusätzlichen Kosten in Rechnung gestellt werden;"@de .

dtd:I-id2118 a dtd:I ;
    dtd:has_Value "10."@de .

dtd:I-id2119 a dtd:I ;
    dtd:has_Value "die Befristung der Gültigkeitsdauer der zur Verfügung gestellten Informationen, beispielsweise die Gültigkeitsdauer befristeter Angebote, insbesondere hinsichtlich des Preises;"@de .

dtd:I-id2120 a dtd:I ;
    dtd:has_Value "11."@de .

dtd:I-id2121 a dtd:I ;
    dtd:has_Value "den Hinweis, dass sich die Finanzdienstleistung auf Finanzinstrumente bezieht, die wegen ihrer spezifischen Merkmale oder der durchzuführenden Vorgänge mit speziellen Risiken behaftet sind oder deren Preis Schwankungen auf dem Finanzmarkt unterliegt, auf die der Versicherer keinen Einfluss hat, und dass in der Vergangenheit erwirtschaftete Beträge kein Indikator für künftige Erträge sind; die jeweiligen Umstände und Risiken sind zu bezeichnen;"@de .

dtd:I-id2122 a dtd:I ;
    dtd:has_Value "b)"@de .

dtd:I-id2123 a dtd:I ;
    dtd:has_Value "Angaben zur Mindestlaufzeit des Vertrages;"@de .

dtd:I-id2124 a dtd:I ;
    dtd:has_Value "insbesondere zu den vertraglichen Kündigungsbedingungen einschließlich etwaiger Vertragsstrafen"@de .

dtd:I-id2125 a dtd:I ;
    dtd:has_Value "eine Vertragsklausel über das auf den Vertrag anwendbare Recht oder über das zuständige Gericht;"@de .

dtd:I-id2126 a dtd:I ;
    dtd:has_Value " 7."@de .

dtd:I-id2127 a dtd:I ;
    dtd:has_Value "Angaben über die der Versicherung zugrunde liegenden Fonds und die Art der darin enthaltenen Vermögenswerte;"@de .

dtd:I-id2128 a dtd:I ;
    dtd:has_Value " 9."@de .

dtd:I-id2129 a dtd:I ;
    dtd:has_Value "die Minderung der Wertentwicklung durch Kosten in Prozentpunkten (Effektivkosten) bis zum Beginn der Auszahlungsphase;"@de .


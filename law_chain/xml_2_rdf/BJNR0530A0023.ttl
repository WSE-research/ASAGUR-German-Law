    dtd:has_Value "eine Kopie der Dokumente nach § 1 Absatz 1 der Zahlungskonto-Identitätsprüfungsverordnung und"@de .

dtd:LA-id231 a dtd:LA ;
    dtd:has_Value "einer der in § 12 Absatz 1 Satz 1 Nummer 2 bis 4 des Geldwäschegesetzes vorgesehenen Nachweise sowie"@de .

dtd:LA-id232 a dtd:LA ;
    dtd:has_DL dtd:DL-id184 ;
    dtd:has_Value "bei nicht natürlichen Personen a)eine Kopie eines der in § 12 Absatz 2 Nummer 1 und 2 des Geldwäschegesetzes genannten Nachweise undb)die gültige Kennung für Rechtsträger."@de .

dtd:LA-id233 a dtd:LA ;

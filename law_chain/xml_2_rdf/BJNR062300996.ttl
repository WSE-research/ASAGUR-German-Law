dtd:LA-id613 a dtd:LA ;
    dtd:has_DL dtd:DL-id393 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fortbildungsabschlüsse zu öffentlich-rechtlich geregelten Prüfungen auf der Grundlage a)der §§ 53 bis 53d und 54 des Berufsbildungsgesetzes,b)der §§ 42 bis 42d, 42f, 45 und 51a der Handwerksordnung oderc)der nach § 122 Absatz 2 bis 4 der Handwerksordnung weiter anzuwendenden Prüfungsregelungen,"@de .

dtd:LA-id614 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der §§ 53 bis 53d und 54 des Berufsbildungsgesetzes,"@de .

dtd:LA-id615 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der §§ 42 bis 42d, 42f, 45 und 51a der Handwerksordnung oder"@de .

dtd:LA-id616 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der nach § 122 Absatz 2 bis 4 der Handwerksordnung weiter anzuwendenden Prüfungsregelungen,"@de .

dtd:LA-id617 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gleichwertige Fortbildungsabschlüsse nach bundes- oder landesrechtlichen Regelungen oder"@de .

dtd:LA-id618 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gleichwertige Fortbildungsabschlüsse an anerkannten Ergänzungsschulen auf der Grundlage staatlich genehmigter Prüfungsordnungen."@de .

dtd:LA-id619 a dtd:LA ;
    dtd:has_DL dtd:DL-id405 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Vollzeitform, wenn a)sie mindestens 400 Unterrichtsstunden umfassen (Mindestdauer),b)sie innerhalb von 36 Kalendermonaten abgeschlossen werden (maximaler Vollzeit-Zeitrahmen) undc)in der Regel in jeder Woche an vier Werktagen mindestens 25 Unterrichtsstunden stattfinden (Vollzeit-Fortbildungsdichte);"@de .

dtd:LA-id620 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie mindestens 400 Unterrichtsstunden umfassen (Mindestdauer),"@de .

dtd:LA-id621 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie innerhalb von 36 Kalendermonaten abgeschlossen werden (maximaler Vollzeit-Zeitrahmen) und"@de .

dtd:LA-id622 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Regel in jeder Woche an vier Werktagen mindestens 25 Unterrichtsstunden stattfinden (Vollzeit-Fortbildungsdichte);"@de .

dtd:LA-id623 a dtd:LA ;
    dtd:has_DL dtd:DL-id412 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Teilzeitform, wenn a)sie mindestens 400 Unterrichtsstunden umfassen (Mindestdauer),b)sie innerhalb von 48 Kalendermonaten abgeschlossen werden (maximaler Teilzeit-Zeitrahmen) undc)im Durchschnitt mindestens 18 Unterrichtsstunden je Monat stattfinden (Teilzeit-Fortbildungsdichte)."@de .

dtd:LA-id624 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie mindestens 400 Unterrichtsstunden umfassen (Mindestdauer),"@de .

dtd:LA-id625 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie innerhalb von 48 Kalendermonaten abgeschlossen werden (maximaler Teilzeit-Zeitrahmen) und"@de .

dtd:LA-id626 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Durchschnitt mindestens 18 Unterrichtsstunden je Monat stattfinden (Teilzeit-Fortbildungsdichte)."@de .

dtd:LA-id627 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach der Akkreditierungs- und Zulassungsverordnung Arbeitsförderung vom 2. April 2012 (BGBl. I S. 504) anerkannt worden ist oder"@de .

dtd:LA-id628 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein System zur Sicherung der Qualität anwendet und"@de .

dtd:LA-id629 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den beantragten Bewilligungszeitraum bereits Leistungen nach dem Bundesausbildungsförderungsgesetz bewilligt worden sind, es sei denn, der Teilnehmer oder die Teilnehmerin hat für den Bewilligungszeitraum noch keine Leistungen nach dem Bundesausbildungsförderungsgesetz erhalten und hat für diesen Bewilligungszeitraum auf Leistungen nach dem Bundesausbildungsförderungsgesetz verzichtet,"@de .

dtd:LA-id630 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für sie Arbeitslosengeld bei beruflicher Weiterbildung nach dem Dritten Buch Sozialgesetzbuch oder nach § 6 Absatz 1 des Beruflichen Rehabilitierungsgesetzes geleistet wird,"@de .

dtd:LA-id631 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arbeitslosengeld bei Arbeitslosigkeit nach dem Dritten Buch Sozialgesetzbuch geleistet wird und es sich um eine Maßnahme in Vollzeitform handelt,"@de .

dtd:LA-id632 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Gründungszuschuss nach den §§ 93 und 94 des Dritten Buches Sozialgesetzbuch geleistet wird und es sich um eine Maßnahme in Vollzeitform handelt,"@de .

dtd:LA-id633 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leistungen zur Rehabilitation nach den für einen Rehabilitationsträger im Sinne des Neunten Buches Sozialgesetzbuch geltenden Vorschriften erbracht werden oder"@de .

dtd:LA-id634 a dtd:LA ;
    dtd:has_Value "Qualifizierungsgeld nach dem Dritten Buch Sozialgesetzbuch geleistet wird."@de .

dtd:LA-id635 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "inhaltlich einem im Fortbildungsplan angegebenen Maßnahmeabschnitt entspricht oder"@de .

dtd:LA-id636 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen im Fortbildungsplan angegebenen Maßnahmeabschnitt, der nicht mehr angeboten wird, weitgehend ersetzt"@de .

dtd:LA-id637 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die besonderen Umstände des Einzelfalles dies rechtfertigen und"@de .

dtd:LA-id638 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine zumutbare Möglichkeit nicht besteht, Fortbildungsstoff im Rahmen einer Verlängerung der Förderungshöchstdauer nach § 11 Absatz 2 Satz 1 nachzuholen."@de .

dtd:LA-id639 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Deutschen im Sinne des Grundgesetzes,"@de .

dtd:LA-id640 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unionsbürgern, die ein Recht auf Daueraufenthalt im Sinne des Freizügigkeitsgesetzes/EU besitzen, sowie anderen Ausländern, die eine Niederlassungserlaubnis oder eine Erlaubnis zum Daueraufenthalt nach dem Aufenthaltsgesetz besitzen,"@de .

dtd:LA-id641 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ehegatten, Lebenspartnern und Kindern von Unionsbürgern, die unter den Voraussetzungen des § 3 Absatz 1 und 3 des Freizügigkeitsgesetzes/EU gemeinschaftsrechtlich freizügigkeitsberechtigt sind oder denen diese Rechte als Kinder nur deshalb nicht zustehen, weil sie 21 Jahre oder älter sind und von ihren Eltern oder deren Ehegatten oder Lebenspartnern keinen Unterhalt erhalten,"@de .

dtd:LA-id642 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unionsbürgern, die Ehegatte, Lebenspartner oder Kind eines Deutschen oder einer Deutschen sind, unter den Voraussetzungen des § 2 Absatz 2 des Freizügigkeitsgesetzes/EU freizügigkeitsberechtigt sind und ihren ständigen Wohnsitz im Inland haben,"@de .

dtd:LA-id643 a dtd:LA ;

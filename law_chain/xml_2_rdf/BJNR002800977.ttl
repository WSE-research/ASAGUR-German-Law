dtd:LA-id576 a dtd:LA ;
    dtd:has_Value "Menschen, insbesondere die menschliche Gesundheit,"@de .

dtd:LA-id577 a dtd:LA ;
    dtd:has_Value "Tiere, Pflanzen und die biologische Vielfalt,"@de .


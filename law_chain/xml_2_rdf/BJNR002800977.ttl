    dtd:has_Value "die Angabe des Namens und des Wohnsitzes oder Sitzes des Antragstellers,"@de .

dtd:LA-id645 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angabe, daß ein Vorbescheid erteilt wird, und die Angabe der Rechtsgrundlage,"@de .

dtd:LA-id646 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die genaue Bezeichnung des Gegenstandes des Vorbescheides,"@de .

dtd:LA-id647 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Voraussetzungen und Vorbehalte, unter denen der Vorbescheid erteilt wird,"@de .

dtd:LA-id648 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Begründung, aus der die wesentlichen tatsächlichen und rechtlichen Gründe, die die Behörde zu ihrer Entscheidung bewogen haben, und die Behandlung der erhobenen Einwendungen hervorgehen sollen."@de .

dtd:LA-id649 a dtd:LA ;

    dtd:has_Value "kulturelles Erbe und sonstige Sachgüter sowie"@de .

dtd:LA-id580 a dtd:LA ;
    dtd:has_Value "die Wechselwirkungen zwischen den in den Nummern 1 bis 4 genannten Schutzgütern."@de .

dtd:LA-id581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angabe des Namens und des Wohnsitzes oder des Sitzes des Antragstellers,"@de .

dtd:LA-id582 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angabe, ob eine Genehmigung oder ein Vorbescheid beantragt wird,"@de .


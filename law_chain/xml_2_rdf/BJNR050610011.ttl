dtd:LA-id179 a dtd:LA ;
    dtd:has_Value "Gerichte des Bundes und der Länder,"@de .

dtd:LA-id180 a dtd:LA ;
    dtd:has_Value "Dienststellen der Bundeswehr und der stationierten Streitkräfte,"@de .

dtd:LA-id181 a dtd:LA ;
    dtd:has_Value "Aufgabenträger im Gesundheitswesen,"@de .

dtd:LA-id182 a dtd:LA ;

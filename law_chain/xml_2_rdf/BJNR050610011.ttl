    dtd:has_Value "bei erheblichen Störungen der Versorgung mit Postdienstleistungen, insbesondere infolge von Naturkatastrophen, besonders schweren Unglücksfällen, Sabotagehandlungen, terroristischen Anschlägen oder sonstigen vergleichbaren Ereignissen oder im Spannungs- oder Verteidigungsfall sowie"@de .

dtd:LA-id171 a dtd:LA ;
    dtd:has_DL dtd:DL-id132 ;
    dtd:has_Value "im Rahmen a)internationaler Vereinbarungen zur Notfallbewältigung,b)der Zusammenarbeit mit den Vereinten Nationen oderc)von Bündnisverpflichtungen."@de .

dtd:LA-id172 a dtd:LA ;

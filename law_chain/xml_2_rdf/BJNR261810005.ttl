dtd:LA-id3189 a dtd:LA ;
    dtd:has_Value "das eindeutige Ordnungsmerkmal, die Domaininformationen und die Landzuordnung,"@de .

dtd:LA-id3190 a dtd:LA ;
    dtd:has_Value "die betroffenen Erzeugnisse oder mit Lebensmitteln verwechselbare Produkte."@de .


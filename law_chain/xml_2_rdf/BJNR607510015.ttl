    dtd:has_SUP dtd:SUP-id147 ;
    dtd:has_Value "y = 212,13 * (x-1,75) –1,7583 für x ≥ 15"@de .

dtd:P-id458 a dtd:P ;
    dtd:has_Value "Der Risikofaktor ergibt sich als Quotient aus dem für das Gewässer aktuell anzuwendenden Abdrifteckwert und dem Referenzwert."@de .

dtd:P-id459 a dtd:P ;
    dtd:has_Value "Folgende Risikofaktoren wurden für die Ermittlung der Expositionsklasse errechnet: Abstand Böschungsoberkante – Mitte erste BaumreiheRisikofaktor 3,50 m (nur periodisch wasserführende Gewässer)277 5,00 m130 6,00 m 93 7,00 m 72 8,00 m 58 9,00 m 4910,00 m 4115,00 m 2120,00 m 1220,00 m mit 90 % abdriftmindernder Technik (Referenz)  1"@de ;

    dtd:has_Value "den festgestellten Jahresabschluss unverzglich nach seiner Feststellung zusammen mit dem Lagebericht und dem Prfungsbericht nach  321 des Handelsgesetzbuchs zu bermitteln."@de .

dtd:P-id1314 a dtd:P ;
    dtd:has_BR dtd:BR-id472 ;
    dtd:has_Value " (+++ Textnachweis Geltung ab: 1.1.1986 +++)(+++ nderungen aufgrund EinigVtr vgl. 4. Abschn.  21 - 26 u. 5. Abschn. +++)berschrift: IdF d. Art. 1 Nr. 1 G v. 22.12.1981 I 1568 mWv 1.7.1982"@de ;

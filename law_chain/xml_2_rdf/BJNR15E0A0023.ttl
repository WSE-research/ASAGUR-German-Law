    dtd:has_Value "Verordnung (EU) 2019/2033."@de .

dtd:Footnote-id2228 a dtd:Footnote ;
    dtd:has_FnZ "2"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F828184_02_BJNR15E0A0023BJNE004400000"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Hier sind negative Ergebnisbeiträge aus den Sicherungsgeschäften mit den Kursreserven der gesicherten Aktiva zu verrechnen."@de .

dtd:Footnote-id2229 a dtd:Footnote ;
    dtd:has_FnZ "3"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F828184_03_BJNR15E0A0023BJNE004400000"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Einschließlich laufender Erträge aus Beteiligungen und Erträgen aus Ergebnisabführungsverträgen."@de .

dtd:Footnote-id2230 a dtd:Footnote ;
    dtd:has_FnZ "4"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F828184_04_BJNR15E0A0023BJNE004400000"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Einschließlich der Gewinne und Verluste aus Devisentermingeschäften unabhängig davon, ob es sich um zins- oder kursbedingte Aufwendungen oder Erträge handelt."@de .

dtd:Footnote-id2231 a dtd:Footnote ;
    dtd:has_FnZ "5"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F828184_05_BJNR15E0A0023BJNE004400000"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Hier sind die Ergebnisse aus Warenverkehr und Nebenbetrieben sowie alle anderen ordentlichen Ergebnisse aus dem nichtzinsabhängigen Geschäft einzuordnen."@de .

dtd:Footnote-id2232 a dtd:Footnote ;
    dtd:has_FnZ "6"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F828184_06_BJNR15E0A0023BJNE004400000"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Einschließlich Aufwendungen für vertraglich vereinbarte feste Tätigkeitsvergütungen an die persönlich haftenden Gesellschafter. Aufwendungen für von fremden Arbeitgebern angemietete Arbeitskräfte sind dem anderen Verwaltungsaufwand zuzurechnen."@de .


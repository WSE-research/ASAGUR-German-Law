    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Prüfungszeit beträgt 180 Minuten."@de .

dtd:LA-id752 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der Prüfling soll nachweisen, dass er allgemeine wirtschaftliche und gesellschaftliche Zusammenhänge der Berufs- und Arbeitswelt darstellen und beurteilen kann;"@de .


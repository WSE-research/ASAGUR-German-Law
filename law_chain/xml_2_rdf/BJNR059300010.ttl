    dtd:has_Size dtd:normal ;
    dtd:has_Value "technische Unterlagen, insbesondere Fertigungsvorschriften, Normen, Sicherheitsbestimmungen, Arbeitsanweisungen, Merkblätter und Richtlinien, anwenden"@de .

dtd:LA-id762 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "konstruktive Zusammenhänge zwischen Gestell und Gerüst sowie textilen Flächen, Folien und Verbundstoffen berücksichtigen"@de .

dtd:LA-id763 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Umsetzungsvorschläge unter Berücksichtigung von technischen Vorgaben, Einsatz, Funktion, Flächengestaltung und Kundenanforderungen erarbeiten"@de .

dtd:LA-id764 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Werk- und Hilfsstoffe nach Art und Struktur unterscheiden und auswählen"@de .

dtd:LA-id765 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verarbeitungs- und Gebrauchsanforderungen beachten und nach Verwendungszweck unterscheiden"@de .

dtd:LA-id766 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "textile Flächengebilde, Folien und Verbundstoffe nach Eigenschaften unterscheiden und einsetzen"@de .

dtd:LA-id767 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einfluss von Werkstoffeigenschaften auf Fertigprodukte berücksichtigen"@de .

dtd:LA-id768 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zubehör nach technischen und statischen Vorgaben auswählen und einsetzen"@de .


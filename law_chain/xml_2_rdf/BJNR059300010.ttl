    dtd:has_Value "Prozessdaten einstellen, Produktionsprozesse überwachen, Verfahrensparameter korrigieren"@de .

dtd:LA-id776 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Störungen feststellen und Maßnahmen zur Störungsbeseitigung ergreifen"@de .

dtd:LA-id777 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorbeugende Instandhaltung durchführen, insbesondere Verschleißteile kontrollieren, austauschen und Austausch veranlassen"@de .

dtd:LA-id778 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Werk- und Hilfsstoffe legen, nach Rapport ablängen, Fadenlauf berücksichtigen"@de .

dtd:LA-id779 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schnittformen, insbesondere nach Schnittschablonen und Zeichnungen, übertragen, Schnittmaße kontrollieren"@de .

dtd:LA-id780 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Teile zuschneiden, kontrollieren und kennzeichnen"@de .

dtd:LA-id781 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schnittteile zusammenstellen und zuordnen, Materialreste sortieren, lagern und umweltgerecht entsorgen"@de .

dtd:LA-id782 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schnittteile, Verbindungsteile und Zubehör nach Arbeitsauftrag bereitstellen"@de .

dtd:LA-id783 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schnittschablonen prüfen und anfertigen, Schnittbilder optimieren"@de .

dtd:LA-id784 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nähmaterialien, insbesondere Nadeln, Garne und Zwirne, nach Verwendungszweck auswählen"@de .


dtd:LA-id754 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Prüfungszeit beträgt 60 Minuten."@de .

dtd:LA-id755 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Gesamtergebnis mit mindestens „ausreichend“,"@de .

dtd:LA-id756 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in mindestens zwei Prüfungsbereichen mit mindestens „ausreichend“ und"@de .

dtd:LA-id757 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in keinem Prüfungsbereich mit „ungenügend“"@de .

dtd:LA-id758 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arten, Aufbau und Funktion von technischen Textilien, insbesondere für Sonnenschutz, Umweltschutz, Bautechnik, Transport- und Schutztechnik, unterscheiden"@de .

dtd:LA-id759 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Skizzen und Fachzeichnungen erstellen und anwenden"@de .

dtd:LA-id760 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Funktion, Proportion und Lage von Objekten berücksichtigen"@de .


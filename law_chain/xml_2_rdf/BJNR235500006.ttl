dtd:LA-id1399 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mittel:"@de .

dtd:LA-id1400 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sera, Impfstoffe und Antigene, die unter Verwendung von Krankheitserregern oder auf biotechnischem Wege hergestellt werden und zur Verhütung, Erkennung oder Heilung von Tierseuchen bestimmt sind;"@de .

dtd:LA-id1401 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sera:"@de .

dtd:LA-id1402 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mittel, die spezifische Antikörper oder Bestandteile von solchen Antikörpern enthalten und die dazu bestimmt sind, bei Tieren wegen dieser Antikörper oder deren Bestandteile angewendet zu werden, sowie Mittel, die aus Blut, Organen, Organteilen oder Organsekreten einschließlich Eiern gewonnen werden, spezifische Antikörper enthalten und dazu bestimmt sind, ohne am oder im tierischen Körper angewendet zu werden, den Zustand oder die Funktion des tierischen Körpers erkennen zu lassen oder der Erkennung übertragbarer Krankheiten und deren Erreger zu dienen;"@de .

dtd:LA-id1403 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Impfstoffe:"@de .

dtd:LA-id1404 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mittel, die dazu bestimmt sind, an Tieren oder tierischen Embryonen zur Erzeugung spezifischer Abwehr- oder Schutzstoffe angewendet zu werden;"@de .

dtd:LA-id1405 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Antigene:"@de .

dtd:LA-id1406 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mittel, ausgenommen Sera und Impfstoffe, die dazu bestimmt sind, außerhalb des tierischen Körpers angewendet zu werden oder bei Anwendung am oder im tierischen Körper Reaktionen des Immunsystems auszulösen;"@de .

dtd:LA-id1407 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Immunmodulatoren:"@de .

dtd:LA-id1408 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Antigene, die Stoffe enthalten, die dazu bestimmt sind, unspezifische Reaktionen des Immunsystems zu steigern oder zu schwächen;"@de .

dtd:LA-id1409 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ausgangsstoffe:"@de .

dtd:LA-id1410 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Stoffe, die zur Herstellung eines Mittels verwendet werden;"@de .

dtd:LA-id1411 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Charge:"@de .

dtd:LA-id1412 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die in einem einheitlichen Herstellungsgang erzeugte Menge eines Mittels;"@de .

dtd:LA-id1413 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wartezeit:"@de .

dtd:LA-id1414 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zeit, die bei bestimmungsgemäßer Anwendung des Mittels nach dessen letzter Anwendung am Tier bis zur Gewinnung von Lebensmitteln, die von diesem Tier stammen, zum Schutz der Gesundheit von Mensch und Tier einzuhalten ist und die gewährleistet, dass Rückstände in diesen Lebensmitteln, die nach der Verordnung (EU) Nr. 37/2010 der Kommission vom 22. Dezember 2009 über pharmakologisch wirksame Stoffe und ihre Einstufung hinsichtlich der Rückstandshöchstmengen in Lebensmitteln tierischen Ursprungs (ABl. L 15 vom 20.1.2010, S. 1) in der jeweils geltenden Fassung festgelegten zulässigen Höchstmengen für pharmakologisch wirksame Stoffe nicht überschreiten;"@de .

dtd:LA-id1415 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "pharmazeutischer Unternehmer:"@de .

dtd:LA-id1416 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Inhaber der Zulassung oder derjenige, der Mittel unter seinem Namen abgibt;"@de .

dtd:LA-id1417 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Agentur:"@de .

dtd:LA-id1418 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Europäische Arzneimittel-Agentur im Sinne des Artikels 55 der Verordnung (EG) Nr. 726/2004 des Europäischen Parlaments und des Rates vom 31. März 2004 zur Festlegung von Gemeinschaftsverfahren für die Genehmigung und Überwachung von Human- und Tierarzneimitteln und zur Errichtung einer Europäischen Arzneimittel-Agentur (ABl. EU Nr. L 136 S. 1) in der jeweils geltenden Fassung;"@de .

dtd:LA-id1419 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Qualität:"@de .

dtd:LA-id1420 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beschaffenheit eines Mittels, die nach Identität, Gehalt, Reinheit, sonstigen chemischen, physikalischen oder biologischen Eigenschaften oder durch das Herstellungsverfahren bestimmt wird;"@de .

dtd:LA-id1421 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nebenwirkung:"@de .

dtd:LA-id1422 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine bei bestimmungsgemäßem Gebrauch auftretende schädliche und unbeabsichtigte Reaktion auf ein Mittel;"@de .


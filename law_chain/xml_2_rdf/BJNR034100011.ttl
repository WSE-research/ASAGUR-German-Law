dtd:LA-id228 a dtd:LA ;
    dtd:has_Value "denen ausschließlich Pflichten als Fahrzeugführer, Triebfahrzeugführer, Schiffsführer, Besatzung in der Binnenschifffahrt, Betreiber einer Annahmestelle in der Binnenschifffahrt, Empfänger, Reisender, Hersteller und Rekonditionierer von Verpackungen, Wiederaufarbeiter von Verpackungen und Großpackmitteln (IBC) und als Stelle für Inspektionen und Prüfungen von IBC zugewiesen sind,"@de .

dtd:LA-id229 a dtd:LA ;
    dtd:has_Value "denen ausschließlich Pflichten als Auftraggeber des Absenders zugewiesen sind und die an der Beförderung gefährlicher Güter von nicht mehr als 50 Tonnen netto je Kalenderjahr beteiligt sind, ausgenommen radioaktive Stoffe der Klasse 7 und gefährliche Güter der Beförderungskategorie 0 nach Absatz 1.1.3.6.3 ADR,"@de .

dtd:LA-id230 a dtd:LA ;
    dtd:has_Value "denen ausschließlich Pflichten als Entlader zugewiesen sind und die an der Beförderung gefährlicher Güter von nicht mehr als 50 Tonnen netto je Kalenderjahr beteiligt sind,"@de .

dtd:LA-id231 a dtd:LA ;
    dtd:has_Value "deren Tätigkeit sich auf die Beförderung gefährlicher Güter erstreckt, die von den Vorschriften des ADR/RID/ADN/IMDG-Code freigestellt sind,"@de .

dtd:LA-id232 a dtd:LA ;
    dtd:has_Value "deren Tätigkeit sich auf die Beförderung gefährlicher Güter im Straßen-, Eisenbahn-, Binnenschiffs- oder Seeverkehr erstreckt, deren Mengen die in Unterabschnitt 1.1.3.6 ADR festgelegten höchstzulässigen Mengen nicht überschreiten,"@de .

dtd:LA-id233 a dtd:LA ;
    dtd:has_Value "deren Tätigkeit sich auf die Beförderung gefährlicher Güter erstreckt, die nach den Bedingungen des Kapitels 3.3, 3.4 und 3.5 ADR/RID/ADN/IMDG-Code freigestellt sind, und"@de .

dtd:LA-id234 a dtd:LA ;
    dtd:has_Value "die gefährliche Güter von nicht mehr als 50 Tonnen netto je Kalenderjahr für den Eigenbedarf in Erfüllung betrieblicher Aufgaben befördern, wobei dies bei radioaktiven Stoffen nur für solche der UN-Nummern 2908 bis 2911 gilt."@de .

dtd:LA-id235 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erteilung der Schulungsnachweise nach § 4,"@de .

dtd:LA-id236 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anerkennung und Überwachung der Lehrgänge nach § 5 Absatz 1,"@de .

dtd:LA-id237 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erteilung von Ausnahmen von § 5 Absatz 3 und § 6 Absatz 3,"@de .

dtd:LA-id238 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Durchführung der Prüfungen nach § 6 Absatz 1 bis 4 und"@de .

dtd:LA-id239 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Umschreibung eines Schulungsnachweises nach § 7 Absatz 3 in einen Schulungsnachweis nach § 4."@de .

dtd:LA-id240 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Art der gefährlichen Güter unterteilt nach Klassen,"@de .

dtd:LA-id241 a dtd:LA ;
    dtd:has_DL dtd:DL-id171 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gesamtmenge der gefährlichen Güter in einer der folgenden vier Stufen: 1.bis 5 Tonnen,2.mehr als 5 Tonnen bis 50 Tonnen,3.mehr als 50 Tonnen bis 1 000 Tonnen,4.mehr als 1 000 Tonnen,"@de .

dtd:LA-id242 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bis 5 Tonnen,"@de .

dtd:LA-id243 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mehr als 5 Tonnen bis 50 Tonnen,"@de .

dtd:LA-id244 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mehr als 50 Tonnen bis 1 000 Tonnen,"@de .

dtd:LA-id245 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mehr als 1 000 Tonnen,"@de .

dtd:LA-id246 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zahl und Art der Unfälle mit gefährlichen Gütern über die ein Unfallbericht nach Unterabschnitt 1.8.3.6 ADR/RID/ADN erstellt worden ist,"@de .

dtd:LA-id247 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstige Angaben, die nach Auffassung des Gefahrgutbeauftragten für die Beurteilung der Sicherheitslage wichtig sind, und"@de .

dtd:LA-id248 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben, ob das Unternehmen an der Beförderung gefährlicher Güter nach Abschnitt 1.10.3 ADR/RID/ADN oder 1.4.3 IMDG-Code beteiligt gewesen ist."@de .

dtd:LA-id249 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vor seiner Bestellung im Besitz eines gültigen und auf die Tätigkeiten des Unternehmens abgestellten Schulungsnachweises nach § 4 ist,"@de .

dtd:LA-id250 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle zur Wahrnehmung seiner Tätigkeit erforderlichen sachdienlichen Auskünfte und Unterlagen erhält, soweit sie die Beförderung gefährlicher Güter betreffen,"@de .

dtd:LA-id251 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die notwendigen Mittel zur Aufgabenwahrnehmung erhält,"@de .

dtd:LA-id252 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jederzeit seine Vorschläge und Bedenken unmittelbar der entscheidenden Stelle im Unternehmen vortragen kann,"@de .

dtd:LA-id253 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu vorgesehenen Vorschlägen auf Änderung oder Anträgen auf Abweichungen von den Vorschriften über die Beförderung gefährlicher Güter Stellung nehmen kann und"@de .

dtd:LA-id254 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle Aufgaben, die ihm nach § 8 übertragen worden sind, ordnungsgemäß erfüllen kann."@de .

dtd:LA-id255 a dtd:LA ;
    dtd:has_DL dtd:DL-id200 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Unternehmer a)entgegen § 3 Absatz 1 Satz 1 einen Gefahrgutbeauftragten nicht, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig bestellt,b)entgegen § 3 Absatz 3 einen Gefahrgutbeauftragten bestellt oder die Funktion des Gefahrgutbeauftragten selbst wahrnimmt, ohne im Besitz eines gültigen Schulungsnachweises nach § 4 zu sein,c)einer vollziehbaren Anordnung nach § 3 Absatz 4 zuwiderhandelt,d)entgegen § 9 Absatz 2 Nummer 1 nicht dafür sorgt, dass der Gefahrgutbeauftragte im Besitz eines dort genannten Schulungsnachweises ist,e)entgegen § 9 Absatz 2 Nummer 6 nicht dafür sorgt, dass der Gefahrgutbeauftragte alle Aufgaben ordnungsgemäß erfüllen kann,f)entgegen § 9 Absatz 3 eine dort genannte Aufzeichnung oder den Jahresbericht nicht oder nicht mindestens fünf Jahre aufbewahrt oder nicht oder nicht rechtzeitig vorlegt,g)entgegen § 9 Absatz 4 den Namen des Gefahrgutbeauftragten nicht oder nicht rechtzeitig bekannt gibt oderh)entgegen § 9 Absatz 5 den Unfallbericht nicht oder nicht rechtzeitig vorlegt,"@de .

dtd:LA-id256 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Absatz 1 Satz 1 einen Gefahrgutbeauftragten nicht, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig bestellt,"@de .

dtd:LA-id257 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Absatz 3 einen Gefahrgutbeauftragten bestellt oder die Funktion des Gefahrgutbeauftragten selbst wahrnimmt, ohne im Besitz eines gültigen Schulungsnachweises nach § 4 zu sein,"@de .

dtd:LA-id258 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Anordnung nach § 3 Absatz 4 zuwiderhandelt,"@de .


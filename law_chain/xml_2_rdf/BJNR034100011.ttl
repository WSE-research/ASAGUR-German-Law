    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 9 Absatz 2 Nummer 6 nicht dafür sorgt, dass der Gefahrgutbeauftragte alle Aufgaben ordnungsgemäß erfüllen kann,"@de .

dtd:LA-id261 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 9 Absatz 3 eine dort genannte Aufzeichnung oder den Jahresbericht nicht oder nicht mindestens fünf Jahre aufbewahrt oder nicht oder nicht rechtzeitig vorlegt,"@de .

dtd:LA-id262 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 9 Absatz 4 den Namen des Gefahrgutbeauftragten nicht oder nicht rechtzeitig bekannt gibt oder"@de .

dtd:LA-id263 a dtd:LA ;

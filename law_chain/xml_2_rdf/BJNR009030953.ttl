dtd:LA-id1061 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bundesautobahnen,"@de .

dtd:LA-id1062 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bundesstraßen mit den Ortsdurchfahrten (§ 5 Abs. 4)."@de .

dtd:LA-id1063 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Straßenkörper; das sind besonders der Straßengrund, der Straßenunterbau, die Straßendecke, die Brücken, Tunnel, Durchlässe, Dämme, Gräben, Entwässerungsanlagen, Böschungen, Stützmauern, Lärmschutzanlagen, Trenn-, Seiten-, Rand- und Sicherheitsstreifen;"@de .

dtd:LA-id1064 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Luftraum über dem Straßenkörper;"@de .

dtd:LA-id1065 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Zubehör; das sind die Verkehrszeichen, die Verkehrseinrichtungen und -anlagen aller Art, die der Sicherheit oder Leichtigkeit des Straßenverkehrs oder dem Schutz der Anlieger dienen, und die Bepflanzung;"@de .

dtd:LA-id1066 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einrichtungen zur Erhebung von Maut und zur Kontrolle der Einhaltung der Mautpflicht;"@de .

dtd:LA-id1067 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Nebenanlagen; das sind solche Anlagen, die überwiegend den Aufgaben der Straßenbauverwaltung der Bundesfernstraßen dienen, z. B. Straßenmeistereien, Gerätehöfe, Lager, Lagerplätze, Entnahmestellen, Hilfsbetriebe und -einrichtungen;"@de .

dtd:LA-id1068 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Nebenbetriebe an den Bundesautobahnen (§ 15 Abs. 1)."@de .

dtd:LA-id1069 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Zusammenhang mit der Errichtung oder erheblichen Änderung baulicher Anlagen, wenn die oberste Landesstraßenbaubehörde oder, soweit dem Bund die Verwaltung einer Bundesfernstraße zusteht, das Fernstraßen-Bundesamt nach § 9 Absatz 2 zugestimmt oder nach § 9 Absatz 8 eine Ausnahme zugelassen haben,"@de .

dtd:LA-id1070 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in einem Flurbereinigungsverfahren auf Grund des Wege- und Gewässerplans."@de .

dtd:LA-id1071 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Hochbauten jeder Art in einer Entfernung bis zu 40 Meter bei Bundesautobahnen und bis zu 20 Meter bei Bundesstraßen außerhalb der zur Erschließung der anliegenden Grundstücke bestimmten Teile der Ortsdurchfahrten, jeweils gemessen vom äußeren Rand der befestigten Fahrbahn,"@de .

dtd:LA-id1072 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bauliche Anlagen, die außerhalb der zur Erschließung der anliegenden Grundstücke bestimmten Teile der Ortsdurchfahrten über Zufahrten oder Zugänge an Bundesstraßen unmittelbar oder mittelbar angeschlossen werden sollen."@de .

dtd:LA-id1073 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bauliche Anlagen längs der Bundesautobahnen in einer Entfernung bis zu 100 Meter und längs der Bundesstraßen außerhalb der zur Erschließung der anliegenden Grundstücke bestimmten Teile der Ortsdurchfahrten bis zu 40 Meter, gemessen vom äußeren Rand der befestigten Fahrbahn, errichtet, erheblich geändert oder anders genutzt werden sollen,"@de .

dtd:LA-id1074 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bauliche Anlagen auf Grundstücken, die außerhalb der zur Erschließung der anliegenden Grundstücke bestimmten Teile der Ortsdurchfahrten über Zufahrten oder Zugänge an Bundesstraßen unmittelbar oder mittelbar angeschlossen sind, erheblich geändert oder anders genutzt werden sollen."@de .

dtd:LA-id1075 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "demjenigen Träger der Straßenbaulast zur Last, der die Änderung verlangt oder hätte verlangen müssen,"@de .

dtd:LA-id1076 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den beteiligten Trägern der Straßenbaulast zur Last, die die Änderung verlangen oder hätten verlangen müssen, und zwar im Verhältnis der Fahrbahnbreiten der an der Kreuzung beteiligten Straßenäste nach der Änderung."@de .

dtd:LA-id1077 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Umfang der Kosten nach den §§ 12 und 12a näher bestimmt wird;"@de .

dtd:LA-id1078 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "näher bestimmt wird, welche Teile der Kreuzungsanlage nach § 13 Abs. 1 und 2 zu der einen oder anderen Straße gehören;"@de .

dtd:LA-id1079 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Berechnung und die Zahlung von Ablösungsbeträgen nach § 13 Abs. 3 und nach § 13a Abs. 2 näher bestimmt sowie dazu ein Verfahren zur gütlichen Beilegung von Streitigkeiten festgelegt werden."@de .

dtd:LA-id1080 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der vom Straßenverkehr auf der Umleitungsstrecke ausgehende Lärm um mindestens 3 Dezibel (A) erhöht wird,"@de .

dtd:LA-id1081 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Beurteilungspegel 64 Dezibel (A) am Tage (6.00 Uhr bis 22.00 Uhr) oder 54 Dezibel (A) in der Nacht (22.00 Uhr bis 6.00 Uhr) überschreitet und"@de .

dtd:LA-id1082 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Verkehrszunahme verursacht wird, die ab Sperrung der Bundesfernstraße voraussichtlich länger als zwei Jahre andauern wird."@de .

dtd:LA-id1083 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "um einen oder mehrere durchgehende Fahrstreifen für den Kraftfahrzeugverkehr baulich erweitert wird oder"@de .

dtd:LA-id1084 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in sonstiger Weise erheblich baulich umgestaltet wird."@de .

dtd:LA-id1085 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Zuge des Wiederaufbaus nach einer Naturkatastrophe erforderlich ist, um die Bundesfernstraße vor Naturereignissen zu schützen, und in einem räumlich begrenzten Korridor entlang des Trassenverlaufs erfolgt oder"@de .

dtd:LA-id1086 a dtd:LA ;
    dtd:has_Size dtd:normal ;

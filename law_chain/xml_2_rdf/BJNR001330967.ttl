    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 19 erhält folgende Fassung:\"§ 19 "@de .

dtd:LA-id193 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bei rückerstattungsrechtlichen Schadensersatzansprüchen auf Zahlung einer Rente werden die bis zum 5. Juli 1959 fällig gewesenen Beträge mit der Maßgabe zusammengerechnet, daß Reichsmarkbeträge im Verhältnis 10:1, Beträge in französischen Franken im Verhältnis 100:0,8507 in Deutsche Mark umgerechnet werden. Die ab 6. Juli 1959 zu zahlende Rente ist zu kapitalisieren. Der Kapitalwert der Rente ist nach den Vorschriften des Bewertungsgesetzes zu errechnen.\""@de .

dtd:LA-id194 a dtd:LA ;
    dtd:has_DL dtd:DL-id134 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "\"§ 19 "@de .

dtd:LA-id195 a dtd:LA ;

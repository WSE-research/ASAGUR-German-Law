dtd:entry-id1437 a dtd:entry ;
    dtd:has_BR dtd:BR-id618 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Aussprache, Betonung, Sprechrhythmus und Intonation sind stark von der ersten Sprache oder regionalen Varianten beeinflusst und beeinträchtigen in der Regel die Verständlichkeit."@de ;
    dtd:has_align dtd:left ;
    dtd:has_colname "col2"@de ;
    dtd:has_colsep "1"@de ;
    dtd:has_valign dtd:top .

dtd:entry-id1438 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Beherrscht nur begrenzt einige einfache, auswendig gelernte grammatische Strukturen und Satzmuster."@de ;
    dtd:has_align dtd:left ;
    dtd:has_colname "col3"@de ;
    dtd:has_colsep "1"@de ;
    dtd:has_valign dtd:top .

dtd:entry-id1439 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Beschränkter Umfang des Vokabulars, das nur vereinzelte Wörter und auswendig gelernte Phrasen umfasst."@de ;
    dtd:has_align dtd:left ;
    dtd:has_colname "col4"@de ;
    dtd:has_colsep "1"@de ;
    dtd:has_valign dtd:top .

dtd:entry-id1440 a dtd:entry ;
    dtd:has_BR dtd:BR-id619 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Kann sehr kurze, vereinzelte, auswendig gelernte Äußerungen mit häufigen Pausen produzieren. Verwendet ablenkende Füllwörter bei der Suche nach Ausdrücken und der Artikulation weniger bekannter Wörter."@de ;
    dtd:has_align dtd:left ;
    dtd:has_colname "col5"@de ;
    dtd:has_colsep "1"@de ;
    dtd:has_valign dtd:top .

dtd:entry-id1441 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Verständnis ist auf vereinzelte, auswendig gelernte Phrasen begrenzt, wenn diese deutlich und langsam artikuliert werden."@de ;
    dtd:has_align dtd:left ;
    dtd:has_colname "col6"@de ;
    dtd:has_colsep "1"@de ;
    dtd:has_valign dtd:top .


dtd:P-id68 a dtd:P ;
    dtd:has_Value "Auf Grund des § 37 Abs. 2 des Jugendarbeitsschutzgesetzes vom 9. August 1960 (Bundesgesetzbl. I S. 665), zuletzt geändert durch das Bundesurlaubsgesetz vom 8. Januar 1963 (Bundesgesetzbl. I S. 2), wird mit Zustimmung des Bundesrates verordnet:"@de .

dtd:P-id70 a dtd:P ;
    dtd:has_Value "(1) Weibliche Jugendliche dürfen in Betrieben und bei Veranstaltungen aller Art als Nackttänzerinnen, Schönheitstänzerinnen oder Schleiertänzerinnen oder mit ähnlichen sie sittlich gefährdenden Tätigkeiten, insbesondere wenn sie dabei unbekleidet oder fast unbekleidet sind, nicht beschäftigt werden."@de .

dtd:P-id71 a dtd:P ;
    dtd:has_Value "(2) Weibliche Jugendliche dürfen als Tanzdamen, Eintänzerinnen, Tisch- oder Bardamen nicht beschäftigt werden."@de .

dtd:P-id72 a dtd:P ;
    dtd:has_Value "(3)"@de .

dtd:P-id73 a dtd:P ;
    dtd:has_Value "(4)"@de .

dtd:P-id75 a dtd:P ;
    dtd:has_Value "-"@de .

dtd:P-id77 a dtd:P ;
    dtd:has_Value "Zuwiderhandlungen gegen § 22 Abs. 1 Nr. 2 des Jugendarbeitsschutzgesetzes in Verbindung mit einem Beschäftigungsverbot nach § 1 dieser Verordnung werden nach § 58 Abs. 1 Nr. 18, Abs. 3 bis 6 des Jugendarbeitsschutzgesetzes geahndet."@de .

dtd:P-id79 a dtd:P ;
    dtd:has_Value "Diese Verordnung gilt nach § 14 des Dritten Überleitungsgesetzes vom 4. Januar 1952 (Bundesgesetzbl. I S. 1) in Verbindung mit § 74 des Jugendarbeitsschutzgesetzes auch im Land Berlin."@de .

dtd:P-id81 a dtd:P ;
    dtd:has_Value "Diese Verordnung tritt am ersten Tag des zweiten auf die Verkündung folgenden Kalendermonats in Kraft."@de .


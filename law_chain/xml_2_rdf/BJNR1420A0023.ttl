dtd:LA-id79 a dtd:LA ;
    dtd:has_Value "des § 69 Absatz 2 in Verbindung mit § 68 Absatz 2 Satz 1 und § 228b, des § 160 Nummer 2 in Verbindung mit § 159, § 68 Absatz 2 Satz 1 und § 228b sowie des § 275b in Verbindung mit § 275a des Sechsten Buches Sozialgesetzbuch, von denen § 69 Absatz 2 zuletzt durch Artikel 4 Nummer 3 Buchstabe b des Gesetzes vom 22. Dezember 2011 (BGBl. I S. 3057), § 68 Absatz 2 und § 159 zuletzt durch Artikel 5 Nummer 1 Buchstabe b und Nummer 4 des Gesetzes vom 2. Dezember 2006 (BGBl. I S. 2742) sowie § 228b und § 275a zuletzt durch Artikel 1 Nummer 11 und 31 des Gesetzes vom 17. Juli 2017 (BGBl. I S. 2575) geändert worden sind,"@de .

dtd:LA-id80 a dtd:LA ;
    dtd:has_Value "des § 6 Absatz 6 und 7 des Fünften Buches Sozialgesetzbuch, dessen Absatz 7 durch Artikel 1 Nummer 1 Buchstabe c des Gesetzes vom 23. Dezember 2002 (BGBl. I S. 4637) eingefügt und dessen Absatz 6 durch Artikel 4 des Gesetzes vom 2. Dezember 2006 (BGBl. I S. 2742) geändert worden ist,"@de .

dtd:LA-id81 a dtd:LA ;
    dtd:has_Value "des § 17 Absatz 2 Satz 1 in Verbindung mit § 18 des Vierten Buches Sozialgesetzbuch, dessen § 18 durch Artikel 3 Nummer 4 des Gesetzes vom 17. Juli 2017 (BGBl. I S. 2575) geändert worden ist,"@de .

dtd:LA-id82 a dtd:LA ;
    dtd:has_Value "in der allgemeinen Rentenversicherung auf 90 600 Euro jährlich; umgerechnet auf den Monat ergeben sich 7 550 Euro, und"@de .

dtd:LA-id83 a dtd:LA ;
    dtd:has_Value "in der knappschaftlichen Rentenversicherung auf 111 600 Euro jährlich; umgerechnet auf den Monat ergeben sich 9 300 Euro."@de .

dtd:LA-id84 a dtd:LA ;
    dtd:has_Value "in der allgemeinen Rentenversicherung auf 89 400 Euro jährlich; umgerechnet auf den Monat ergeben sich 7 450 Euro, und"@de .

dtd:LA-id85 a dtd:LA ;
    dtd:has_Value "in der knappschaftlichen Rentenversicherung auf 110 400 Euro jährlich; umgerechnet auf den Monat ergeben sich 9 200 Euro."@de .

dtd:P-id101 a dtd:P ;
    dtd:has_DL dtd:DL-id69 ;
    dtd:has_Value "(1) Die Beitragsbemessungsgrenze nach § 159 des Sechsten Buches Sozialgesetzbuch wird für das Jahr 2024 wie folgt festgesetzt: 1.in der allgemeinen Rentenversicherung auf 90 600 Euro jährlich; umgerechnet auf den Monat ergeben sich 7 550 Euro, und2.in der knappschaftlichen Rentenversicherung auf 111 600 Euro jährlich; umgerechnet auf den Monat ergeben sich 9 300 Euro."@de .

dtd:P-id102 a dtd:P ;
    dtd:has_DL dtd:DL-id74 ;
    dtd:has_Value "(2) Die Beitragsbemessungsgrenze (Ost) nach § 275a des Sechsten Buches Sozialgesetzbuch wird für das Jahr 2024 wie folgt festgesetzt: 1.in der allgemeinen Rentenversicherung auf 89 400 Euro jährlich; umgerechnet auf den Monat ergeben sich 7 450 Euro, und2.in der knappschaftlichen Rentenversicherung auf 110 400 Euro jährlich; umgerechnet auf den Monat ergeben sich 9 200 Euro."@de .

dtd:P-id103 a dtd:P ;
    dtd:has_Value "(3) Anlage 2 zum Sechsten Buch Sozialgesetzbuch wird um den Zeitraum „01.01.2024 – 31.12.2024“ und um die jeweiligen Jahresbeträge ergänzt. Anlage 2a zum Sechsten Buch Sozialgesetzbuch wird um den Zeitraum „01.01.2024 – 31.12.2024“ und um die jeweiligen Jahresbeträge ergänzt."@de .

dtd:P-id105 a dtd:P ;
    dtd:has_Value "Diese Verordnung tritt am 1. Januar 2024 in Kraft."@de .

dtd:P-id107 a dtd:P ;
    dtd:has_Value "Der Bundesrat hat zugestimmt."@de .

dtd:P-id87 a dtd:P ;
    dtd:has_BR dtd:BR-id58 ;
    dtd:has_Value " (+++ Textnachweis ab: 1.1.2024 +++)"@de ;
    dtd:has_pre dtd:pre-id108 .

dtd:P-id89 a dtd:P ;
    dtd:has_DL dtd:DL-id61,
        dtd:DL-id66 ;
    dtd:has_Value "Auf Grund –des § 69 Absatz 2 in Verbindung mit § 68 Absatz 2 Satz 1 und § 228b, des § 160 Nummer 2 in Verbindung mit § 159, § 68 Absatz 2 Satz 1 und § 228b sowie des § 275b in Verbindung mit § 275a des Sechsten Buches Sozialgesetzbuch, von denen § 69 Absatz 2 zuletzt durch Artikel 4 Nummer 3 Buchstabe b des Gesetzes vom 22. Dezember 2011 (BGBl. I S. 3057), § 68 Absatz 2 und § 159 zuletzt durch Artikel 5 Nummer 1 Buchstabe b und Nummer 4 des Gesetzes vom 2. Dezember 2006 (BGBl. I S. 2742) sowie § 228b und § 275a zuletzt durch Artikel 1 Nummer 11 und 31 des Gesetzes vom 17. Juli 2017 (BGBl. I S. 2575) geändert worden sind,–des § 6 Absatz 6 und 7 des Fünften Buches Sozialgesetzbuch, dessen Absatz 7 durch Artikel 1 Nummer 1 Buchstabe c des Gesetzes vom 23. Dezember 2002 (BGBl. I S. 4637) eingefügt und dessen Absatz 6 durch Artikel 4 des Gesetzes vom 2. Dezember 2006 (BGBl. I S. 2742) geändert worden ist,verordnet die Bundesregierung und auf Grund –des § 17 Absatz 2 Satz 1 in Verbindung mit § 18 des Vierten Buches Sozialgesetzbuch, dessen § 18 durch Artikel 3 Nummer 4 des Gesetzes vom 17. Juli 2017 (BGBl. I S. 2575) geändert worden ist,verordnet das Bundesministerium für Arbeit und Soziales:"@de .

dtd:P-id91 a dtd:P ;
    dtd:has_Value "(1) Die Bezugsgröße nach § 18 Absatz 1 des Vierten Buches Sozialgesetzbuch für das Jahr 2024 beträgt 42 420 Euro. Umgerechnet auf den Monat ergeben sich 3 535 Euro."@de .

dtd:P-id92 a dtd:P ;
    dtd:has_Value "(2) Die Bezugsgröße (Ost) nach § 18 Absatz 2 des Vierten Buches Sozialgesetzbuch für das Jahr 2024 beträgt 41 580 Euro. Umgerechnet auf den Monat ergeben sich 3 465 Euro."@de .

dtd:P-id94 a dtd:P ;
    dtd:has_Value "(1) Die Jahresarbeitsentgeltgrenze nach § 6 Absatz 6 des Fünften Buches Sozialgesetzbuch wird für das Jahr 2024 auf 69 300 Euro festgesetzt. Umgerechnet auf den Monat ergeben sich 5 775 Euro."@de .

dtd:P-id95 a dtd:P ;
    dtd:has_Value "(2) Die Jahresarbeitsentgeltgrenze nach § 6 Absatz 7 des Fünften Buches Sozialgesetzbuch wird für das Jahr 2024 auf 62 100 Euro festgesetzt. Umgerechnet auf den Monat ergeben sich 5 175 Euro."@de .

dtd:P-id97 a dtd:P ;
    dtd:has_Value "(1) Das Durchschnittsentgelt nach § 69 Absatz 2 Satz 1 Nummer 1 des Sechsten Buches Sozialgesetzbuch für das Jahr 2022 beträgt 42 053 Euro."@de .

dtd:P-id98 a dtd:P ;
    dtd:has_Value "(2) Das vorläufige Durchschnittsentgelt nach § 69 Absatz 2 Satz 1 Nummer 2 des Sechsten Buches Sozialgesetzbuch für das Jahr 2024 beträgt 45 358 Euro."@de .

dtd:P-id99 a dtd:P ;
    dtd:has_Value "(3) Anlage 1 zum Sechsten Buch Sozialgesetzbuch wird entsprechend ergänzt."@de .


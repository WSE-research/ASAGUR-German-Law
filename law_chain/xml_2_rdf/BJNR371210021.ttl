    dtd:has_B dtd:B-id202 ;
    dtd:has_BR dtd:BR-id41,
        dtd:BR-id42 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einzugsgebiete nach § 3 Nummer 13 WHG"@de .


dtd:LA-id1432 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "muss das Tier seiner Art und seinen Bedrfnissen entsprechend angemessen ernhren, pflegen und verhaltensgerecht unterbringen,"@de .

dtd:LA-id1433 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "darf die Mglichkeit des Tieres zu artgemer Bewegung nicht so einschrnken, dass ihm Schmerzen oder vermeidbare Leiden oder Schden zugefgt werden,"@de .

dtd:LA-id1434 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "muss ber die fr eine angemessene Ernhrung, Pflege und verhaltensgerechte Unterbringung des Tieres erforderlichen Kenntnisse und Fhigkeiten verfgen."@de .

dtd:LA-id1435 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hinsichtlich der Bewegungsmglichkeit oder der Gemeinschaftsbedrfnisse der Tiere,"@de .

dtd:LA-id1436 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an Rume, Kfige, andere Behltnisse und sonstige Einrichtungen zur Unterbringung von Tieren sowie an die Beschaffenheit von Anbinde-, Ftterungs- und Trnkvorrichtungen,"@de .

dtd:LA-id1437 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hinsichtlich der Lichtverhltnisse und des Raumklimas bei der Unterbringung der Tiere,"@de .

dtd:LA-id1438 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an die Pflege einschlielich der berwachung der Tiere; hierbei kann das Bundesministerium auch vorschreiben, dass Aufzeichnungen ber die Ergebnisse der berwachung zu machen, aufzubewahren und der zustndigen Behrde auf Verlangen vorzulegen sind,"@de .

dtd:LA-id1439 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an Kenntnisse und Fhigkeiten von Personen, die Tiere halten, betreuen oder zu betreuen haben und an den Nachweis dieser Kenntnisse und Fhigkeiten,"@de .

dtd:LA-id1440 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an Sicherheitsvorkehrungen im Falle technischer Strungen oder im Brandfall."@de .

dtd:LA-id1441 a dtd:LA ;
    dtd:has_DL dtd:DL-id504 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anforderungena)hinsichtlich der Transportfhigkeit von Tieren,b)an Transportmittel fr Tiere"@de .

dtd:LA-id1442 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "festlegen,"@de .

dtd:LA-id1443 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hinsichtlich der Transportfhigkeit von Tieren,"@de .

dtd:LA-id1444 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an Transportmittel fr Tiere"@de .

dtd:LA-id1445 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bestimmte Transportmittel und Versendungsarten fr die Befrderung bestimmter Tiere, insbesondere die Versendung als Nachnahme, verbieten oder beschrnken,"@de .

dtd:LA-id1446 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bestimmte Transportmittel und Versendungsarten fr die Befrderung bestimmter Tiere vorschreiben,"@de .

dtd:LA-id1447 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorschreiben, dass bestimmte Tiere bei der Befrderung von einem Betreuer begleitet werden mssen,"@de .

dtd:LA-id1448 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorschreiben, dass Personen, die Tiertransporte durchfhren oder hierbei mitwirken, bestimmte Kenntnisse und Fhigkeiten haben und diese nachweisen mssen,"@de .

dtd:LA-id1449 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorschriften ber das Verladen, Entladen, Unterbringen, Ernhren und Pflegen der Tiere erlassen,"@de .

dtd:LA-id1450 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Voraussetzung fr die Durchfhrung von Tiertransporten bestimmte Bescheinigungen, Erklrungen oder Meldungen vorschreiben sowie deren Ausstellung und Aufbewahrung regeln,"@de .

dtd:LA-id1451 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorschreiben, dass, wer gewerbsmig Tiertransporte durchfhrt, einer Erlaubnis der zustndigen Behrde bedarf oder bei der zustndigen Behrde registriert sein muss, sowie die Voraussetzungen und das Verfahren bei der Erteilung der Erlaubnis und bei der Registrierung regeln,"@de .

dtd:LA-id1452 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorschreiben, dass, wer Tiere whrend des Transports in einer Einrichtung oder einem Betrieb ernhren, pflegen oder unterbringen will, einer Erlaubnis der zustndigen Behrde bedarf, und die Voraussetzungen und das Verfahren der Erteilung der Erlaubnis regeln, soweit dies zur Durchfhrung von Rechtsakten der Europischen Gemeinschaft oder der Europischen Union erforderlich ist."@de .

dtd:LA-id1453 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach Absatz 1, soweit sie Anforderungen an die Haltung von Tieren festlegen, die zur Verwendung in Tierversuchen bestimmt sind oder deren Gewebe oder Organe dazu bestimmt sind, zu wissenschaftlichen Zwecken verwendet zu werden,"@de .

dtd:LA-id1454 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach Absatz 2 Satz 1, soweit sie die Befrderung von Tieren regeln, die zur Verwendung in Tierversuchen bestimmt sind oder deren Gewebe oder Organe dazu bestimmt sind, zu wissenschaftlichen Zwecken verwendet zu werden."@de .

dtd:LA-id1455 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Tier auer in Notfllen Leistungen abzuverlangen, denen es wegen seines Zustandes offensichtlich nicht gewachsen ist oder die offensichtlich seine Krfte bersteigen,"@de .

dtd:LA-id1456 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Tier, an dem Eingriffe und Behandlungen vorgenommen worden sind, die einen leistungsmindernden krperlichen Zustand verdecken, Leistungen abzuverlangen, denen es wegen seines krperlichen Zustandes nicht gewachsen ist,"@de .

dtd:LA-id1457 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an einem Tier im Training oder bei sportlichen Wettkmpfen oder hnlichen Veranstaltungen Manahmen, die mit erheblichen Schmerzen, Leiden oder Schden verbunden sind und die die Leistungsfhigkeit von Tieren beeinflussen knnen, sowie an einem Tier bei sportlichen Wettkmpfen oder hnlichen Veranstaltungen Dopingmittel anzuwenden,"@de .

dtd:LA-id1458 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein gebrechliches, krankes, abgetriebenes oder altes, im Haus, Betrieb oder sonst in Obhut des Menschen gehaltenes Tier, fr das ein Weiterleben mit nicht behebbaren Schmerzen oder Leiden verbunden ist, zu einem anderen Zweck als zur unverzglichen schmerzlosen Ttung zu veruern oder zu erwerben; dies gilt nicht fr die unmittelbare Abgabe eines kranken Tieres an eine Person oder Einrichtung, der eine Genehmigung nach  8 Absatz 1 Satz 1 und, wenn es sich um ein Wirbeltier handelt, erforderlichenfalls eine Genehmigung nach Vorschriften, die auf Grund des  9 Absatz 3 Nummer 1 und 2 erlassen worden sind, fr Versuche an solchen Tieren erteilt worden ist,"@de .

dtd:LA-id1459 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein im Haus, Betrieb oder sonst in Obhut des Menschen gehaltenes Tier auszusetzen oder es zurckzulassen, um sich seiner zu entledigen oder sich der Halter- oder Betreuerpflicht zu entziehen,"@de .

dtd:LA-id1460 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein gezchtetes oder aufgezogenes Tier einer wildlebenden Art in der freien Natur auszusetzen oder anzusiedeln, das nicht auf die zum berleben in dem vorgesehenen Lebensraum erforderliche artgeme Nahrungsaufnahme vorbereitet und an das Klima angepasst ist; die Vorschriften des Jagdrechts und des Naturschutzrechts bleiben unberhrt,"@de .

dtd:LA-id1461 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier auszubilden oder zu trainieren, sofern damit erhebliche Schmerzen, Leiden oder Schden fr das Tier verbunden sind,"@de .

dtd:LA-id1462 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier zu einer Filmaufnahme, Schaustellung, Werbung oder hnlichen Veranstaltung heranzuziehen, sofern damit Schmerzen, Leiden oder Schden fr das Tier verbunden sind,"@de .

dtd:LA-id1463 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier an einem anderen lebenden Tier auf Schrfe abzurichten oder zu prfen,"@de .

dtd:LA-id1464 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier auf ein anderes Tier zu hetzen, soweit dies nicht die Grundstze weidgerechter Jagdausbung erfordern,"@de .

dtd:LA-id1465 a dtd:LA ;
    dtd:has_DL dtd:DL-id547 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier zu einem derartig aggressiven Verhalten auszubilden oder abzurichten, dass dieses Verhaltena)bei ihm selbst zu Schmerzen, Leiden oder Schden fhrt oderb)im Rahmen jeglichen artgemen Kontaktes mit Artgenossen bei ihm selbst oder einem Artgenossen zu Schmerzen oder vermeidbaren Leiden oder Schden fhrt oderc)seine Haltung nur unter Bedingungen zulsst, die bei ihm zu Schmerzen oder vermeidbaren Leiden oder Schden fhren,"@de .

dtd:LA-id1466 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei ihm selbst zu Schmerzen, Leiden oder Schden fhrt oder"@de .

dtd:LA-id1467 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Rahmen jeglichen artgemen Kontaktes mit Artgenossen bei ihm selbst oder einem Artgenossen zu Schmerzen oder vermeidbaren Leiden oder Schden fhrt oder"@de .

dtd:LA-id1468 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seine Haltung nur unter Bedingungen zulsst, die bei ihm zu Schmerzen oder vermeidbaren Leiden oder Schden fhren,"@de .

dtd:LA-id1469 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Tier durch Anwendung von Zwang Futter einzuverleiben, sofern dies nicht aus gesundheitlichen Grnden erforderlich ist,"@de .

dtd:LA-id1470 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Tier Futter darzureichen, das dem Tier erhebliche Schmerzen, Leiden oder Schden bereitet,"@de .

dtd:LA-id1471 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Gert zu verwenden, das durch direkte Stromeinwirkung das artgeme Verhalten eines Tieres, insbesondere seine Bewegung, erheblich einschrnkt oder es zur Bewegung zwingt und dem Tier dadurch nicht unerhebliche Schmerzen, Leiden oder Schden zufgt, soweit dies nicht nach bundes- oder landesrechtlichen Vorschriften zulssig ist,"@de .

dtd:LA-id1472 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier als Preis oder Belohnung bei einem Wettbewerb, einer Verlosung, einem Preisausschreiben oder einer hnlichen Veranstaltung auszuloben,"@de .

dtd:LA-id1473 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier fr eigene sexuelle Handlungen zu nutzen oder fr sexuelle Handlungen Dritter abzurichten oder zur Verfgung zu stellen und dadurch zu artwidrigem Verhalten zu zwingen."@de .

dtd:LA-id1474 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach Satz 2 gezchtete Tiere mit den Eigenschaften, die fr den jeweiligen Zweck erforderlich sind, nicht zur Verfgung stehen oder"@de .

dtd:LA-id1475 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die jeweiligen wissenschaftlichen Zwecke die Verwendung von Tieren erforderlich machen, die nicht nach Satz 2 gezchtet worden sind."@de .

dtd:LA-id1476 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie bei Notschlachtungen nach den gegebenen Umstnden nicht mglich ist,"@de .

dtd:LA-id1477 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zustndige Behrde eine Ausnahmegenehmigung fr ein Schlachten ohne Betubung (Schchten) erteilt hat; sie darf die Ausnahmegenehmigung nur insoweit erteilen, als es erforderlich ist, den Bedrfnissen von Angehrigen bestimmter Religionsgemeinschaften im Geltungsbereich dieses Gesetzes zu entsprechen, denen zwingende Vorschriften ihrer Religionsgemeinschaft das Schchten vorschreiben oder den Genuss von Fleisch nicht geschchteter Tiere untersagen oder"@de .

dtd:LA-id1478 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dies als Ausnahme durch Rechtsverordnung nach  4b Nr. 3 bestimmt ist."@de .

dtd:LA-id1479 a dtd:LA ;
    dtd:has_DL dtd:DL-id573 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "a)das Schlachten von Fischen und anderen kaltbltigen Tieren zu regeln,b)bestimmte Ttungsarten und Betubungsverfahren nher zu regeln, vorzuschreiben, zuzulassen oder zu verbieten,c)die Voraussetzungen nher zu regeln, unter denen Schlachtungen im Sinne des  4a Abs. 2 Nr. 2 vorgenommen werden drfen,d)nhere Vorschriften ber Art und Umfang der zum Betuben oder Tten von Wirbeltieren erforderlichen Kenntnisse und Fhigkeiten sowie ber das Verfahren zu deren Nachweis zu erlassen,e)nicht gewerbliche Ttigkeiten zu bestimmen, die den Erwerb des Sachkundenachweises zum Tten von Wirbeltieren erfordern,"@de .

dtd:LA-id1480 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "um sicherzustellen, dass den Tieren nicht mehr als unvermeidbare Schmerzen zugefgt werden,"@de .

dtd:LA-id1481 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Schlachten von Fischen und anderen kaltbltigen Tieren zu regeln,"@de .

dtd:LA-id1482 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bestimmte Ttungsarten und Betubungsverfahren nher zu regeln, vorzuschreiben, zuzulassen oder zu verbieten,"@de .

dtd:LA-id1483 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Voraussetzungen nher zu regeln, unter denen Schlachtungen im Sinne des  4a Abs. 2 Nr. 2 vorgenommen werden drfen,"@de .

dtd:LA-id1484 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nhere Vorschriften ber Art und Umfang der zum Betuben oder Tten von Wirbeltieren erforderlichen Kenntnisse und Fhigkeiten sowie ber das Verfahren zu deren Nachweis zu erlassen,"@de .

dtd:LA-id1485 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht gewerbliche Ttigkeiten zu bestimmen, die den Erwerb des Sachkundenachweises zum Tten von Wirbeltieren erfordern,"@de .

dtd:LA-id1486 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Schlachten von Tieren im Rahmen der Bestimmungen des Europischen bereinkommens vom 10. Mai 1979 ber den Schutz von Schlachttieren (BGBl. 1983 II S. 770) nher zu regeln,"@de .

dtd:LA-id1487 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fr das Schlachten von Geflgel Ausnahmen von der Betubungspflicht zu bestimmen."@de .

dtd:LA-id1488 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit sie das Betuben oder Tten mittels gefhrlicher Stoffe oder Gemische im Sinne des Chemikaliengesetzes oder darauf bezogene Voraussetzungen fr den Erwerb eines Sachkundenachweises betreffen, des Einvernehmens der Bundesministerien fr Wirtschaft und Energie sowie fr Umwelt, Naturschutz und nukleare Sicherheit,"@de .

dtd:LA-id1489 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit sie das Betuben oder Tten von Tieren, die zur Verwendung in Tierversuchen bestimmt sind oder deren Gewebe oder Organe dazu bestimmt sind, zu wissenschaftlichen Zwecken verwendet zu werden, oder darauf bezogene Voraussetzungen fr den Erwerb eines Sachkundenachweises betreffen, des Einvernehmens des Bundesministeriums fr Bildung und Forschung."@de .

dtd:LA-id1490 a dtd:LA ;
    dtd:has_DL dtd:DL-id598 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fr den Fall, dass eine Ttung der Kken a)nach tierseuchenrechtlichen Bestimmungen vorgeschrieben oder angeordnet worden ist oderb)im Einzelfall aus Grnden des Tierschutzes erforderlich ist,"@de .

dtd:LA-id1491 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach tierseuchenrechtlichen Bestimmungen vorgeschrieben oder angeordnet worden ist oder"@de .

dtd:LA-id1492 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Einzelfall aus Grnden des Tierschutzes erforderlich ist,"@de .

dtd:LA-id1493 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fr nicht schlupffhige Kken,"@de .

dtd:LA-id1494 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fr Stubenkken nach Artikel 1 Nummer 1 Buchstabea der Verordnung (EG) Nr. 543/2008 der Kommission vom 16. Juni 2008 mit Durchfhrungsvorschriften zur Verordnung (EG) Nr. 1234/2007 des Rates hinsichtlich der Vermarktungsnormen fr Geflgelfleisch (ABl. L 157 vom 17.6.2008, S. 46; L 8 vom 13.1.2009, S. 33), die zuletzt durch die Verordnung (EU) Nr. 519/2013 (ABl. L 158 vom 10.6.2013, S. 74) gendert worden ist, und"@de .

dtd:LA-id1495 a dtd:LA ;
    dtd:has_DL dtd:DL-id603 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fr Kken, a)die zur Verwendung in Tierversuchen bestimmt sind oderb)deren Gewebe oder Organe dazu bestimmt sind, zu wissenschaftlichen Zwecken verwendet zu werden."@de .

dtd:LA-id1496 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zur Verwendung in Tierversuchen bestimmt sind oder"@de .

dtd:LA-id1497 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "deren Gewebe oder Organe dazu bestimmt sind, zu wissenschaftlichen Zwecken verwendet zu werden."@de .

dtd:LA-id1498 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Eingriff an einem Hhnerei vorzunehmen, der den Tod des Hhnerembryos verursacht, oder"@de .

dtd:LA-id1499 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Abbruch des Brutvorgangs vorzunehmen, der den Tod des Hhnerembryos verursacht."@de .


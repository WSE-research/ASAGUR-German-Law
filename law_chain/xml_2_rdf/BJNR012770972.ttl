dtd:LA-id1432 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "muss das Tier seiner Art und seinen Bedürfnissen entsprechend angemessen ernähren, pflegen und verhaltensgerecht unterbringen,"@de .

dtd:LA-id1433 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "darf die Möglichkeit des Tieres zu artgemäßer Bewegung nicht so einschränken, dass ihm Schmerzen oder vermeidbare Leiden oder Schäden zugefügt werden,"@de .

dtd:LA-id1434 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "muss über die für eine angemessene Ernährung, Pflege und verhaltensgerechte Unterbringung des Tieres erforderlichen Kenntnisse und Fähigkeiten verfügen."@de .

dtd:LA-id1435 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hinsichtlich der Bewegungsmöglichkeit oder der Gemeinschaftsbedürfnisse der Tiere,"@de .

dtd:LA-id1436 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an Räume, Käfige, andere Behältnisse und sonstige Einrichtungen zur Unterbringung von Tieren sowie an die Beschaffenheit von Anbinde-, Fütterungs- und Tränkvorrichtungen,"@de .

dtd:LA-id1437 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hinsichtlich der Lichtverhältnisse und des Raumklimas bei der Unterbringung der Tiere,"@de .

dtd:LA-id1438 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an die Pflege einschließlich der Überwachung der Tiere; hierbei kann das Bundesministerium auch vorschreiben, dass Aufzeichnungen über die Ergebnisse der Überwachung zu machen, aufzubewahren und der zuständigen Behörde auf Verlangen vorzulegen sind,"@de .

dtd:LA-id1439 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an Kenntnisse und Fähigkeiten von Personen, die Tiere halten, betreuen oder zu betreuen haben und an den Nachweis dieser Kenntnisse und Fähigkeiten,"@de .

dtd:LA-id1440 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an Sicherheitsvorkehrungen im Falle technischer Störungen oder im Brandfall."@de .

dtd:LA-id1441 a dtd:LA ;
    dtd:has_DL dtd:DL-id504 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anforderungena)hinsichtlich der Transportfähigkeit von Tieren,b)an Transportmittel für Tiere"@de .

dtd:LA-id1442 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "festlegen,"@de .

dtd:LA-id1443 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hinsichtlich der Transportfähigkeit von Tieren,"@de .

dtd:LA-id1444 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an Transportmittel für Tiere"@de .

dtd:LA-id1445 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bestimmte Transportmittel und Versendungsarten für die Beförderung bestimmter Tiere, insbesondere die Versendung als Nachnahme, verbieten oder beschränken,"@de .

dtd:LA-id1446 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bestimmte Transportmittel und Versendungsarten für die Beförderung bestimmter Tiere vorschreiben,"@de .

dtd:LA-id1447 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorschreiben, dass bestimmte Tiere bei der Beförderung von einem Betreuer begleitet werden müssen,"@de .

dtd:LA-id1448 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorschreiben, dass Personen, die Tiertransporte durchführen oder hierbei mitwirken, bestimmte Kenntnisse und Fähigkeiten haben und diese nachweisen müssen,"@de .

dtd:LA-id1449 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorschriften über das Verladen, Entladen, Unterbringen, Ernähren und Pflegen der Tiere erlassen,"@de .

dtd:LA-id1450 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Voraussetzung für die Durchführung von Tiertransporten bestimmte Bescheinigungen, Erklärungen oder Meldungen vorschreiben sowie deren Ausstellung und Aufbewahrung regeln,"@de .

dtd:LA-id1451 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorschreiben, dass, wer gewerbsmäßig Tiertransporte durchführt, einer Erlaubnis der zuständigen Behörde bedarf oder bei der zuständigen Behörde registriert sein muss, sowie die Voraussetzungen und das Verfahren bei der Erteilung der Erlaubnis und bei der Registrierung regeln,"@de .

dtd:LA-id1452 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorschreiben, dass, wer Tiere während des Transports in einer Einrichtung oder einem Betrieb ernähren, pflegen oder unterbringen will, einer Erlaubnis der zuständigen Behörde bedarf, und die Voraussetzungen und das Verfahren der Erteilung der Erlaubnis regeln, soweit dies zur Durchführung von Rechtsakten der Europäischen Gemeinschaft oder der Europäischen Union erforderlich ist."@de .

dtd:LA-id1453 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach Absatz 1, soweit sie Anforderungen an die Haltung von Tieren festlegen, die zur Verwendung in Tierversuchen bestimmt sind oder deren Gewebe oder Organe dazu bestimmt sind, zu wissenschaftlichen Zwecken verwendet zu werden,"@de .

dtd:LA-id1454 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach Absatz 2 Satz 1, soweit sie die Beförderung von Tieren regeln, die zur Verwendung in Tierversuchen bestimmt sind oder deren Gewebe oder Organe dazu bestimmt sind, zu wissenschaftlichen Zwecken verwendet zu werden."@de .

dtd:LA-id1455 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Tier außer in Notfällen Leistungen abzuverlangen, denen es wegen seines Zustandes offensichtlich nicht gewachsen ist oder die offensichtlich seine Kräfte übersteigen,"@de .

dtd:LA-id1456 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Tier, an dem Eingriffe und Behandlungen vorgenommen worden sind, die einen leistungsmindernden körperlichen Zustand verdecken, Leistungen abzuverlangen, denen es wegen seines körperlichen Zustandes nicht gewachsen ist,"@de .

dtd:LA-id1457 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an einem Tier im Training oder bei sportlichen Wettkämpfen oder ähnlichen Veranstaltungen Maßnahmen, die mit erheblichen Schmerzen, Leiden oder Schäden verbunden sind und die die Leistungsfähigkeit von Tieren beeinflussen können, sowie an einem Tier bei sportlichen Wettkämpfen oder ähnlichen Veranstaltungen Dopingmittel anzuwenden,"@de .

dtd:LA-id1458 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein gebrechliches, krankes, abgetriebenes oder altes, im Haus, Betrieb oder sonst in Obhut des Menschen gehaltenes Tier, für das ein Weiterleben mit nicht behebbaren Schmerzen oder Leiden verbunden ist, zu einem anderen Zweck als zur unverzüglichen schmerzlosen Tötung zu veräußern oder zu erwerben; dies gilt nicht für die unmittelbare Abgabe eines kranken Tieres an eine Person oder Einrichtung, der eine Genehmigung nach § 8 Absatz 1 Satz 1 und, wenn es sich um ein Wirbeltier handelt, erforderlichenfalls eine Genehmigung nach Vorschriften, die auf Grund des § 9 Absatz 3 Nummer 1 und 2 erlassen worden sind, für Versuche an solchen Tieren erteilt worden ist,"@de .

dtd:LA-id1459 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein im Haus, Betrieb oder sonst in Obhut des Menschen gehaltenes Tier auszusetzen oder es zurückzulassen, um sich seiner zu entledigen oder sich der Halter- oder Betreuerpflicht zu entziehen,"@de .

dtd:LA-id1460 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein gezüchtetes oder aufgezogenes Tier einer wildlebenden Art in der freien Natur auszusetzen oder anzusiedeln, das nicht auf die zum Überleben in dem vorgesehenen Lebensraum erforderliche artgemäße Nahrungsaufnahme vorbereitet und an das Klima angepasst ist; die Vorschriften des Jagdrechts und des Naturschutzrechts bleiben unberührt,"@de .

dtd:LA-id1461 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier auszubilden oder zu trainieren, sofern damit erhebliche Schmerzen, Leiden oder Schäden für das Tier verbunden sind,"@de .

dtd:LA-id1462 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier zu einer Filmaufnahme, Schaustellung, Werbung oder ähnlichen Veranstaltung heranzuziehen, sofern damit Schmerzen, Leiden oder Schäden für das Tier verbunden sind,"@de .

dtd:LA-id1463 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier an einem anderen lebenden Tier auf Schärfe abzurichten oder zu prüfen,"@de .

dtd:LA-id1464 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier auf ein anderes Tier zu hetzen, soweit dies nicht die Grundsätze weidgerechter Jagdausübung erfordern,"@de .

dtd:LA-id1465 a dtd:LA ;
    dtd:has_DL dtd:DL-id547 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier zu einem derartig aggressiven Verhalten auszubilden oder abzurichten, dass dieses Verhaltena)bei ihm selbst zu Schmerzen, Leiden oder Schäden führt oderb)im Rahmen jeglichen artgemäßen Kontaktes mit Artgenossen bei ihm selbst oder einem Artgenossen zu Schmerzen oder vermeidbaren Leiden oder Schäden führt oderc)seine Haltung nur unter Bedingungen zulässt, die bei ihm zu Schmerzen oder vermeidbaren Leiden oder Schäden führen,"@de .

dtd:LA-id1466 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei ihm selbst zu Schmerzen, Leiden oder Schäden führt oder"@de .

dtd:LA-id1467 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Rahmen jeglichen artgemäßen Kontaktes mit Artgenossen bei ihm selbst oder einem Artgenossen zu Schmerzen oder vermeidbaren Leiden oder Schäden führt oder"@de .

dtd:LA-id1468 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seine Haltung nur unter Bedingungen zulässt, die bei ihm zu Schmerzen oder vermeidbaren Leiden oder Schäden führen,"@de .

dtd:LA-id1469 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Tier durch Anwendung von Zwang Futter einzuverleiben, sofern dies nicht aus gesundheitlichen Gründen erforderlich ist,"@de .

dtd:LA-id1470 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Tier Futter darzureichen, das dem Tier erhebliche Schmerzen, Leiden oder Schäden bereitet,"@de .

dtd:LA-id1471 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Gerät zu verwenden, das durch direkte Stromeinwirkung das artgemäße Verhalten eines Tieres, insbesondere seine Bewegung, erheblich einschränkt oder es zur Bewegung zwingt und dem Tier dadurch nicht unerhebliche Schmerzen, Leiden oder Schäden zufügt, soweit dies nicht nach bundes- oder landesrechtlichen Vorschriften zulässig ist,"@de .

dtd:LA-id1472 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier als Preis oder Belohnung bei einem Wettbewerb, einer Verlosung, einem Preisausschreiben oder einer ähnlichen Veranstaltung auszuloben,"@de .

dtd:LA-id1473 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier für eigene sexuelle Handlungen zu nutzen oder für sexuelle Handlungen Dritter abzurichten oder zur Verfügung zu stellen und dadurch zu artwidrigem Verhalten zu zwingen."@de .

dtd:LA-id1474 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach Satz 2 gezüchtete Tiere mit den Eigenschaften, die für den jeweiligen Zweck erforderlich sind, nicht zur Verfügung stehen oder"@de .

dtd:LA-id1475 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die jeweiligen wissenschaftlichen Zwecke die Verwendung von Tieren erforderlich machen, die nicht nach Satz 2 gezüchtet worden sind."@de .

dtd:LA-id1476 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie bei Notschlachtungen nach den gegebenen Umständen nicht möglich ist,"@de .

dtd:LA-id1477 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zuständige Behörde eine Ausnahmegenehmigung für ein Schlachten ohne Betäubung (Schächten) erteilt hat; sie darf die Ausnahmegenehmigung nur insoweit erteilen, als es erforderlich ist, den Bedürfnissen von Angehörigen bestimmter Religionsgemeinschaften im Geltungsbereich dieses Gesetzes zu entsprechen, denen zwingende Vorschriften ihrer Religionsgemeinschaft das Schächten vorschreiben oder den Genuss von Fleisch nicht geschächteter Tiere untersagen oder"@de .

dtd:LA-id1478 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dies als Ausnahme durch Rechtsverordnung nach § 4b Nr. 3 bestimmt ist."@de .

dtd:LA-id1479 a dtd:LA ;
    dtd:has_DL dtd:DL-id573 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "a)das Schlachten von Fischen und anderen kaltblütigen Tieren zu regeln,b)bestimmte Tötungsarten und Betäubungsverfahren näher zu regeln, vorzuschreiben, zuzulassen oder zu verbieten,c)die Voraussetzungen näher zu regeln, unter denen Schlachtungen im Sinne des § 4a Abs. 2 Nr. 2 vorgenommen werden dürfen,d)nähere Vorschriften über Art und Umfang der zum Betäuben oder Töten von Wirbeltieren erforderlichen Kenntnisse und Fähigkeiten sowie über das Verfahren zu deren Nachweis zu erlassen,e)nicht gewerbliche Tätigkeiten zu bestimmen, die den Erwerb des Sachkundenachweises zum Töten von Wirbeltieren erfordern,"@de .

dtd:LA-id1480 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "um sicherzustellen, dass den Tieren nicht mehr als unvermeidbare Schmerzen zugefügt werden,"@de .

dtd:LA-id1481 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Schlachten von Fischen und anderen kaltblütigen Tieren zu regeln,"@de .

dtd:LA-id1482 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bestimmte Tötungsarten und Betäubungsverfahren näher zu regeln, vorzuschreiben, zuzulassen oder zu verbieten,"@de .

dtd:LA-id1483 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Voraussetzungen näher zu regeln, unter denen Schlachtungen im Sinne des § 4a Abs. 2 Nr. 2 vorgenommen werden dürfen,"@de .

dtd:LA-id1484 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nähere Vorschriften über Art und Umfang der zum Betäuben oder Töten von Wirbeltieren erforderlichen Kenntnisse und Fähigkeiten sowie über das Verfahren zu deren Nachweis zu erlassen,"@de .

dtd:LA-id1485 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht gewerbliche Tätigkeiten zu bestimmen, die den Erwerb des Sachkundenachweises zum Töten von Wirbeltieren erfordern,"@de .

dtd:LA-id1486 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Schlachten von Tieren im Rahmen der Bestimmungen des Europäischen Übereinkommens vom 10. Mai 1979 über den Schutz von Schlachttieren (BGBl. 1983 II S. 770) näher zu regeln,"@de .

dtd:LA-id1487 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für das Schlachten von Geflügel Ausnahmen von der Betäubungspflicht zu bestimmen."@de .

dtd:LA-id1488 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit sie das Betäuben oder Töten mittels gefährlicher Stoffe oder Gemische im Sinne des Chemikaliengesetzes oder darauf bezogene Voraussetzungen für den Erwerb eines Sachkundenachweises betreffen, des Einvernehmens der Bundesministerien für Wirtschaft und Energie sowie für Umwelt, Naturschutz und nukleare Sicherheit,"@de .

dtd:LA-id1489 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit sie das Betäuben oder Töten von Tieren, die zur Verwendung in Tierversuchen bestimmt sind oder deren Gewebe oder Organe dazu bestimmt sind, zu wissenschaftlichen Zwecken verwendet zu werden, oder darauf bezogene Voraussetzungen für den Erwerb eines Sachkundenachweises betreffen, des Einvernehmens des Bundesministeriums für Bildung und Forschung."@de .

dtd:LA-id1490 a dtd:LA ;
    dtd:has_DL dtd:DL-id598 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den Fall, dass eine Tötung der Küken a)nach tierseuchenrechtlichen Bestimmungen vorgeschrieben oder angeordnet worden ist oderb)im Einzelfall aus Gründen des Tierschutzes erforderlich ist,"@de .

dtd:LA-id1491 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach tierseuchenrechtlichen Bestimmungen vorgeschrieben oder angeordnet worden ist oder"@de .

dtd:LA-id1492 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Einzelfall aus Gründen des Tierschutzes erforderlich ist,"@de .

dtd:LA-id1493 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für nicht schlupffähige Küken,"@de .

dtd:LA-id1494 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Stubenküken nach Artikel 1 Nummer 1 Buchstabe a der Verordnung (EG) Nr. 543/2008 der Kommission vom 16. Juni 2008 mit Durchführungsvorschriften zur Verordnung (EG) Nr. 1234/2007 des Rates hinsichtlich der Vermarktungsnormen für Geflügelfleisch (ABl. L 157 vom 17.6.2008, S. 46; L 8 vom 13.1.2009, S. 33), die zuletzt durch die Verordnung (EU) Nr. 519/2013 (ABl. L 158 vom 10.6.2013, S. 74) geändert worden ist, und"@de .

dtd:LA-id1495 a dtd:LA ;
    dtd:has_DL dtd:DL-id603 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Küken, a)die zur Verwendung in Tierversuchen bestimmt sind oderb)deren Gewebe oder Organe dazu bestimmt sind, zu wissenschaftlichen Zwecken verwendet zu werden."@de .

dtd:LA-id1496 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zur Verwendung in Tierversuchen bestimmt sind oder"@de .

dtd:LA-id1497 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "deren Gewebe oder Organe dazu bestimmt sind, zu wissenschaftlichen Zwecken verwendet zu werden."@de .

dtd:LA-id1498 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Eingriff an einem Hühnerei vorzunehmen, der den Tod des Hühnerembryos verursacht, oder"@de .

dtd:LA-id1499 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:I-id119 a dtd:I ;
    dtd:has_Value "Friedrich-Ebert-Stiftung e. V.."@de .

dtd:LA-id143 a dtd:LA ;
    dtd:has_Value "einen Beitrag zum Verständnis der Zeitgeschichte und der weiteren Entwicklung der Bundesrepublik Deutschland sowie"@de .

dtd:LA-id144 a dtd:LA ;
    dtd:has_Value "zur Aufarbeitung, Darstellung und Weiterentwicklung der Verantwortung Deutschlands in der Außen-, Sicherheits- und Wirtschaftspolitik im europäischen und globalen Umfeld zu leisten;"@de .

dtd:LA-id145 a dtd:LA ;
    dtd:has_Value "Kenntnisse zu den geopolitischen und wirtschaftlichen Herausforderungen des 21. Jahrhunderts in Europa und der Welt zu vertiefen und zu erweitern."@de .

dtd:LA-id146 a dtd:LA ;
    dtd:has_Value "Einrichtung und Betrieb eines Helmut-Schmidt-Zentrums als öffentlich zugängliche Erinnerungsstätte in Hamburg, das im Rahmen des Stiftungszwecks eine ständige zeitgeschichtliche Ausstellung errichtet und pflegt, Sonderausstellungen und Veranstaltungen durchführt sowie wissenschaftliche Arbeitsmöglichkeiten bietet,"@de .

dtd:LA-id147 a dtd:LA ;
    dtd:has_Value "Unterhalt, Betrieb und Nutzung des Anwesens der Eheleute Schmidt in Hamburg-Langenhorn als authentischer Geschichtsort sowie für eine angemessene öffentliche Nutzung im Sinne des Stiftungszwecks,"@de .

dtd:LA-id148 a dtd:LA ;
    dtd:has_Value "regelmäßiges Wirken wie die Organisation und Durchführung von Veranstaltungen in der Hauptstadt Berlin und an anderen Orten als dem Stiftungssitz,"@de .

dtd:LA-id149 a dtd:LA ;
    dtd:has_Value "Forschung und Förderung wissenschaftlicher Arbeiten sowie Veröffentlichungen im Sinne des Stiftungszwecks, insbesondere unter Berücksichtigung von zukunftsgerichteten Fragestellungen,"@de .

dtd:LA-id150 a dtd:LA ;
    dtd:has_Value "Förderung der Zusammenarbeit mit anderen nationalen und internationalen Einrichtungen, soweit sie dem Stiftungszweck dient."@de .

dtd:LA-id151 a dtd:LA ;
    dtd:has_Value "das Kuratorium,"@de .

dtd:LA-id152 a dtd:LA ;
    dtd:has_Value "der Vorstand."@de .

dtd:P-id154 a dtd:P ;
    dtd:has_BR dtd:BR-id115 ;
    dtd:has_Value " (+++ Textnachweis ab: 1.1.2017 +++)(+++ Zur Anwendung vgl. §§ 10 Abs. 2, 11 Abs. 2 +++)"@de ;

    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Ansässigkeitsstaat des Berufskraftfahrers, des Lokomotivführers oder des Begleitpersonals,"@de .

dtd:LA-id178 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in einem oder mehreren Drittstaaten oder"@de .

dtd:LA-id179 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Ansässigkeitsstaat des Berufskraftfahrers, des Lokomotivführers oder des Begleitpersonals sowie in einem oder mehreren Drittstaaten"@de .


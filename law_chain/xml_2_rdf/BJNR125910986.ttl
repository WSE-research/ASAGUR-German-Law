dtd:P-id137 a dtd:P ;
    dtd:has_Value "Der Anpassungsfaktor für die vom 1. Juli 1987 an anzupassenden Geldleistungen der gesetzlichen Unfallversicherung beträgt 1,0303."@de .

dtd:P-id139 a dtd:P ;
    dtd:has_Value "Das Pflegegeld beträgt vom 1. Juli 1987 an zwischen 414 Deutsche Mark und 1.656 Deutsche Mark monatlich."@de .


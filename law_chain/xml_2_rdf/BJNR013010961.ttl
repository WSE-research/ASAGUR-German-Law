dtd:LA-id4891 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach zehn Jahren, wenn die Pflichtverletzung eine Maßnahme nach § 90 Absatz 1 Nummer 4 oder Absatz 2 Nummer 4 rechtfertigt,"@de .

dtd:LA-id4892 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach 20 Jahren, wenn die Pflichtverletzung eine Maßnahme nach § 90 Absatz 1 Nummer 5 oder Absatz 2 Nummer 5 rechtfertigt."@de .


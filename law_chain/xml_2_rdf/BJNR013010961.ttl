dtd:LA-id4899 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn nachträglich bekannt wird, daß er nicht hätte zum Beisitzer berufen werden dürfen;"@de .

dtd:LA-id4900 a dtd:LA ;
    dtd:has_Size dtd:normal ;

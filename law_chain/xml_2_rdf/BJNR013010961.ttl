    dtd:has_Value "eines wegen desselben Verhaltens eingeleiteten Straf- oder Bußgeldverfahrens,"@de .

dtd:LA-id4894 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines wegen desselben Verhaltens eingeleiteten vorrangigen berufsaufsichtlichen Verfahrens und"@de .

dtd:LA-id4895 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Aussetzung des Verfahrens nach § 111."@de .

dtd:LA-id4896 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer das 65. Lebensjahr vollendet hat;"@de .

dtd:LA-id4897 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer in den letzten vier Jahren Mitglied des Vorstandes gewesen ist;"@de .

dtd:LA-id4898 a dtd:LA ;
    dtd:has_Size dtd:normal ;

    dtd:has_Value "wer die Verpflichtung zur Kostenerstattung durch Verpflichtungserklärung oder Vertrag übernommen hat, oder"@de .

dtd:LA-id183 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den eine Verpflichtung zur Kostenerstattung gesetzlich oder hoheitlich angeordnet ist oder der für die Verpflichtung eines anderen zur Kostenerstattung gesetzlich haftet."@de .

dtd:LA-id184 a dtd:LA ;

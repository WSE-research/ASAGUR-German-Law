dtd:LA-id636 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Herstellung der Darreichungsform von Arzneimitteln,"@de .

dtd:LA-id637 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arzneimittelforschung, Entwicklung, Herstellung, Prüfung von Arzneimitteln, Tätigkeiten in der Arzneimittelzulassung, Pharmakovigilanz und Risikoabwehr in der pharmazeutischen Industrie,"@de .

dtd:LA-id638 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arzneimittelprüfung in einem Laboratorium für die Prüfung von Arzneimitteln,"@de .

dtd:LA-id639 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Lagerung, Qualitätserhaltung und Vertrieb von Arzneimitteln auf der Großhandelsstufe,"@de .

dtd:LA-id640 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bevorratung, Herstellung, Prüfung, Lagerung, Vertrieb und Abgabe von unbedenklichen und wirksamen Arzneimitteln der erforderlichen Qualität in der Öffentlichkeit zugänglichen Apotheken,"@de .

dtd:LA-id641 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Herstellung, Prüfung, Lagerung und Abgabe von unbedenklichen und wirksamen Arzneimitteln der erforderlichen Qualität in Krankenhäusern,"@de .

dtd:LA-id642 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Information und Beratung über Arzneimittel als solche, einschließlich ihrer angemessenen Verwendung,"@de .

dtd:LA-id643 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Meldung von unerwünschten Arzneimittelwirkungen an die zuständigen Behörden,"@de .

dtd:LA-id644 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "personalisierte Unterstützung von Patienten bei Selbstmedikation,"@de .

dtd:LA-id645 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beiträge zu örtlichen oder landesweiten gesundheitsbezogenen Kampagnen,"@de .

dtd:LA-id646 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tätigkeiten im Arzneimittel-, Apotheken- und Medizinproduktewesen der öffentlichen Gesundheitsverwaltung in Behörden des Bundes, der Länder und der Kommunen sowie in Körperschaften des öffentlichen Rechts und in Berufs- und Fachverbänden,"@de .

dtd:LA-id647 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tätigkeiten in Lehre und Forschung an Universitäten sowie in der Lehre an Lehranstalten und Berufsschulen in pharmazeutischen Fachgebieten."@de .

dtd:LA-id648 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id649 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich nicht eines Verhaltens schuldig gemacht hat, aus dem sich seine Unwürdigkeit oder Unzuverlässigkeit zur Ausübung des Apothekerberufs ergibt,"@de .

dtd:LA-id650 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht in gesundheitlicher Hinsicht zur Ausübung des Berufs ungeeignet ist,"@de .

dtd:LA-id651 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach einer Gesamtausbildungszeit von fünf Jahren, von denen zwölf Monate auf die praktische Ausbildung entfallen müssen, die pharmazeutische Prüfung im Geltungsbereich dieses Gesetzes bestanden hat,"@de .

dtd:LA-id652 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über die für die Ausübung der Berufstätigkeit erforderlichen Kenntnisse der deutschen Sprache verfügt."@de .

dtd:LA-id653 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von der früheren Tschechoslowakei verliehen wurden und die Aufnahme des Berufs des Apothekers gestatten oder aus denen hervorgeht, dass die Ausbildung im Falle der Tschechischen Republik und der Slowakei vor dem 1. Januar 1993 aufgenommen wurde, oder"@de .

dtd:LA-id654 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von der früheren Sowjetunion verliehen wurden und die Aufnahme des Berufs des Apothekers gestatten oder aus denen hervorgeht, dass die Ausbildung im Falle Estlands vor dem 20. August 1991, im Falle Lettlands vor dem 21. August 1991, im Falle Litauens vor dem 11. März 1990 aufgenommen wurde, oder"@de .

dtd:LA-id655 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vom früheren Jugoslawien verliehen wurden und die Aufnahme des Berufs des Apothekers gestatten oder aus denen hervorgeht, dass die Ausbildung im Falle Sloweniens vor dem 25. Juni 1991 aufgenommen wurde,"@de .

dtd:LA-id656 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ausbildung der Antragsteller sich hinsichtlich der beruflichen Tätigkeit auf Fächer einschließlich der praktischen Ausbildungsteile bezieht, die sich wesentlich von der deutschen Ausbildung unterscheiden, oder"@de .

dtd:LA-id657 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Apothekerberuf eine oder mehrere reglementierte Tätigkeiten umfasst, die in dem Staat, der den Ausbildungsnachweis ausgestellt hat, nicht Bestandteil dieses Berufs sind, und sich die deutsche Ausbildung auf Fächer bezieht, die sich wesentlich von denen unterscheiden, die von der Ausbildung des Antragstellers abgedeckt werden."@de .

dtd:LA-id658 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Identitätsnachweis,"@de .

dtd:LA-id659 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine tabellarische Aufstellung der absolvierten Ausbildungsgänge und der ausgeübten Erwerbstätigkeiten,"@de .

dtd:LA-id660 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine amtlich beglaubigte Kopie der Befähigungsnachweise oder des Ausbildungsnachweises, der zur Aufnahme des entsprechenden Berufs berechtigt, sowie gegebenenfalls eine Bescheinigung über die von der betreffenden Person erworbene Berufserfahrung,"@de .

dtd:LA-id661 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall von Absatz 3 eine Bescheinigung über die Berechtigung zur Berufsausübung im Herkunftsstaat und Unterlagen, die geeignet sind darzulegen, im Inland den Apothekerberuf ausüben zu wollen,"@de .

dtd:LA-id662 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Unterlagen, die von den zuständigen Behörden des Herkunftsmitgliedstaats ausgestellt wurden und belegen, dass die Erfordernisse nach Absatz 1 Satz 1 Nr. 2 erfüllt werden oder, wenn im Herkunftsmitgliedstaat die vorgenannten Unterlagen nicht ausgestellt werden, eine eidesstattliche Erklärung oder – in den Staaten, in denen es keine eidesstattliche Erklärung gibt – eine feierliche Erklärung, die die betreffende Person vor einer zuständigen Justiz- oder Verwaltungsbehörde oder gegebenenfalls vor einem Notar oder einer entsprechend bevollmächtigten Berufsorganisation des Herkunftsmitgliedstaats, der eine diese eidesstattliche oder feierliche Erklärung bestätigende Bescheinigung ausstellt, abgegeben hat,"@de .

dtd:LA-id663 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Nachweis nach Absatz 1 Satz 1 Nr. 3, wobei ein entsprechender Nachweis, der im Herkunftsmitgliedstaat gefordert wird, anerkannt wird oder, wenn im Herkunftsmitgliedstaat kein derartiger Nachweis verlangt wird, eine von einer zuständigen Behörde des Herkunftsmitgliedstaats ausgestellte Bescheinigung,"@de .

dtd:LA-id664 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Bescheinigung der zuständigen Behörden des Herkunftsmitgliedstaats, aus der hervorgeht, dass die Nachweise über die geforderten Ausbildungsvoraussetzungen den in der Richtlinie verlangten Nachweisen entsprechen,"@de .

dtd:LA-id665 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Fällen der Absätze 2 und 3 zusätzliche Nachweise, um feststellen zu können, ob die Ausbildung wesentliche Unterschiede gegenüber der Ausbildung aufweist, die in diesem Gesetz und in der Rechtsverordnung nach § 5 Absatz 1 geregelt ist,"@de .

dtd:LA-id666 a dtd:LA ;
    dtd:has_DL dtd:DL-id547 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den Fall, dass sich Ausbildungsnachweise nach Artikel 3 Abs. 1 Buchstabe c der Richtlinie 2005/36/EG, die von der zuständigen Behörde eines Mitgliedstaats oder eines Vertragsstaates des Abkommens über den Europäischen Wirtschaftsraum oder eines Vertragsstaates, dem Deutschland und die Europäische Gemeinschaft oder Deutschland und die Europäische Union vertraglich einen entsprechenden Rechtsanspruch eingeräumt haben, ausgestellt wurden, auf eine Ausbildung beziehen, die ganz oder teilweise in einer rechtmäßig im Hoheitsgebiet eines anderen der oben genannten Staaten niedergelassenen Einrichtung absolviert wurde, Unterlagen darüber, a)ob der Ausbildungsgang in der betreffenden Einrichtung von der Ausbildungseinrichtung des Ausstellungsmitgliedstaats offiziell bescheinigt worden ist,b)ob der ausgestellte Ausbildungsnachweis dem entspricht, der verliehen worden wäre, wenn der Ausbildungsgang vollständig im Ausstellungsmitgliedstaat absolviert worden wäre, undc)ob mit dem Ausbildungsnachweis im Hoheitsgebiet des Ausstellungsmitgliedstaats dieselben beruflichen Rechte verliehen werden.Die Nachweise nach Satz 1 Nr. 3 und 4 dürfen bei ihrer Vorlage nicht älter als drei Monate sein. Haben die zuständigen Behörden berechtigte Zweifel an der Authentizität der in dem jeweiligen Herkunftsmitgliedstaat ausgestellten Bescheinigungen und Ausbildungsnachweise, können sie von den zuständigen Behörden des Herkunftsmitgliedstaats eine Bestätigung der Authentizität dieser Bescheinigungen und Nachweise sowie eine Bestätigung darüber verlangen, dass der Antragsteller die Mindestanforderungen der Ausbildung erfüllt, die in Artikel 44 der Richtlinie 2005/36/EG verlangt werden. Haben die zuständigen Behörden berechtigte Zweifel an der Berechtigung zur Ausübung des Apothekerberufs, können sie von den zuständigen Behörden eines Mitgliedstaates eine Bestätigung verlangen, aus der sich ergibt, dass dem Antragsteller die Ausübung des Apothekerberufs nicht auf Grund eines schwerwiegenden standeswidrigen Verhaltens oder einer Verurteilung wegen strafbarer Handlungen dauerhaft oder vorübergehend untersagt worden ist."@de .

dtd:LA-id667 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ob der Ausbildungsgang in der betreffenden Einrichtung von der Ausbildungseinrichtung des Ausstellungsmitgliedstaats offiziell bescheinigt worden ist,"@de .

dtd:LA-id668 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ob der ausgestellte Ausbildungsnachweis dem entspricht, der verliehen worden wäre, wenn der Ausbildungsgang vollständig im Ausstellungsmitgliedstaat absolviert worden wäre, und"@de .

dtd:LA-id669 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ob mit dem Ausbildungsnachweis im Hoheitsgebiet des Ausstellungsmitgliedstaats dieselben beruflichen Rechte verliehen werden."@de .

dtd:LA-id670 a dtd:LA ;
    dtd:has_Value "den Widerruf, die Rücknahme oder das Ruhen der Approbation oder der Erlaubnis, die sofort vollziehbar oder unanfechtbar sind,"@de .

dtd:LA-id671 a dtd:LA ;
    dtd:has_Value "die sofort vollziehbare oder unanfechtbare Einschränkung der Ausübung des Apothekerberufs,"@de .

dtd:LA-id672 a dtd:LA ;
    dtd:has_Value "den Verzicht auf die Approbation oder die Erlaubnis,"@de .

dtd:LA-id673 a dtd:LA ;
    dtd:has_Value "das Verbot der Ausübung des Apothekerberufs durch unanfechtbare gerichtliche Entscheidung oder"@de .

dtd:LA-id674 a dtd:LA ;
    dtd:has_Value "das vorläufige Berufsverbot durch gerichtliche Entscheidung."@de .

dtd:LA-id675 a dtd:LA ;
    dtd:has_Value "die zur Identifizierung der betroffenen Person erforderlichen Angaben, insbesondere Name, Vorname, Geburtsdatum und Geburtsort,"@de .

dtd:LA-id676 a dtd:LA ;
    dtd:has_Value "Beruf der betroffenen Person,"@de .

dtd:LA-id677 a dtd:LA ;
    dtd:has_Value "Angaben über die Behörde oder das Gericht, die oder das die Entscheidung getroffen hat,"@de .

dtd:LA-id678 a dtd:LA ;
    dtd:has_Value "Umfang der Entscheidung oder des Verzichts und"@de .

dtd:LA-id679 a dtd:LA ;
    dtd:has_Value "Zeitraum, in dem die Entscheidung oder der Verzicht gilt."@de .

dtd:LA-id680 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine der Voraussetzungen nach § 4 Abs. 1 Satz 1 Nr. 2 und 3 nicht vorgelegen hat oder"@de .

dtd:LA-id681 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die pharmazeutische Prüfung nach § 4 Abs. 1 Satz 1 Nr. 4 nicht bestanden oder"@de .

dtd:LA-id682 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nachzuweisende pharmazeutische Ausbildung nach § 4 Abs. 1 Satz 2, Abs. 1a, Absatz 2 oder 3 nicht abgeschlossen war."@de .

dtd:LA-id683 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegen den Apotheker wegen des Verdachts einer Straftat, aus der sich seine Unwürdigkeit oder Unzuverlässigkeit zur Ausübung des Apothekerberufs ergeben kann, ein Strafverfahren eingeleitet ist,"@de .

dtd:LA-id684 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine der Voraussetzungen nach § 4 Abs. 1 Satz 1 Nr. 3 nicht mehr gegeben ist,"@de .

dtd:LA-id685 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zweifel bestehen, ob die Voraussetzungen nach § 4 Abs. 1 Satz 1 Nr. 3 noch erfüllt sind und der Apotheker sich weigert, sich einer von der zuständigen Behörde angeordneten amts- oder fachärztlichen Untersuchung zu unterziehen oder"@de .

dtd:LA-id686 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn bekannt wird, dass der Apotheker nicht über die Kenntnisse der deutschen Sprache verfügt, die für die Ausübung des Apothekerberufs in Deutschland erforderlich sind."@de .

dtd:LA-id687 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Nachweis über seine Staatsangehörigkeit,"@de .

dtd:LA-id688 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Bescheinigung darüber, dass er in einem Mitgliedstaat rechtmäßig als Apotheker niedergelassen ist und ihm die Ausübung dieses Berufs zum Zeitpunkt der Vorlage der Bescheinigung nicht, auch nicht vorübergehend, untersagt ist und keine Vorstrafen vorliegen,"@de .

dtd:LA-id689 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seinen Berufsqualifikationsnachweis und"@de .

dtd:LA-id690 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Erklärung des Dienstleistungserbringers, dass er über die zur Erbringung der Dienstleistung erforderlichen Kenntnisse der deutschen Sprache verfügt;"@de .

dtd:LA-id691 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "er in Deutschland rechtmäßig zur Ausübung des Apothekerberufs niedergelassen ist,"@de .

dtd:LA-id692 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihm die Ausübung dieser Tätigkeit zum Zeitpunkt der Vorlage der Bescheinigung nicht, auch nicht vorübergehend, untersagt ist und"@de .

dtd:LA-id693 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "er über einen erforderlichen Berufsqualifikationsnachweis verfügt."@de .


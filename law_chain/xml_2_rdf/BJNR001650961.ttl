    dtd:has_Value "einem psychiatrischen Krankenhaus (§ 63 des Strafgesetzbuches, § 126a der Strafprozeßordnung) oder"@de .

dtd:LA-id435 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Entziehungsanstalt (§ 64 des Strafgesetzbuches, § 126a der Strafprozeßordnung)"@de .


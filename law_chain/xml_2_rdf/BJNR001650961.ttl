dtd:LA-id396 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Polizeivollzugsbeamten des Bundes (§ 1 des Bundespolizeibeamtengesetzes vom 19. Juli 1960 - Bundesgesetzbl. I S. 569);"@de .

dtd:LA-id397 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beamten des Zollgrenzdienstes (Grenzaufsichtsdienst und Grenzabfertigungsdienst), des Zollfahndungsdienstes, des Bewachungs- und Begleitungsdienstes und die übrigen Beamten der Bundesfinanzbehörden, die mit Vollzugsaufgaben betraut sind;"@de .

dtd:LA-id398 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id399 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beamten der Wasserstraßen- und Schifffahrtsverwaltung des Bundes mit strom- und schifffahrtspolizeilichen Befugnissen;"@de .

dtd:LA-id400 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id401 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beauftragten des Bundesamtes für Logistik und Mobilität, soweit sie mit Überwachungsaufgaben nach den §§ 11 bis 13 des Güterkraftverkehrsgesetzes betraut sind;"@de .

dtd:LA-id402 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beamten der Bundesgerichte und der Behörden der Bundesjustizverwaltung, die mit Vollzugs- und Sicherungsaufgaben betraut sind;"@de .

dtd:LA-id403 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "andere Personen, die durch die zuständigen Bundesbehörden mit Aufgaben betraut sind, die den unter den Nummern 1 bis 7 aufgeführten Beamten obliegen;"@de .

dtd:LA-id404 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die der Dienstgewalt von Bundesbehörden unterstehenden Personen, die mit Aufgaben der Strafverfolgung oder der Verfolgung von Ordnungswidrigkeiten im Sinne des Gesetzes über Ordnungswidrigkeiten betraut sind, wenn sie sich in Ausübung dieser Tätigkeit im Vollzugsdienst befinden."@de .

dtd:LA-id405 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gefahr besteht, daß er die Vollzugsbeamten oder Dritte angreift, oder wenn er Widerstand leistet;"@de .

dtd:LA-id406 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "er zu fliehen versucht oder wenn bei Würdigung aller Tatsachen, besonders der persönlichen Verhältnisse und der Umstände, die einer Flucht entgegenstehen, zu befürchten ist, daß er sich aus dem Gewahrsam befreien wird;"@de .

dtd:LA-id407 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Selbstmordgefahr besteht."@de .

dtd:LA-id408 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Polizeivollzugsbeamten des Bundes (§ 1 des Bundespolizeibeamtengesetzes vom 19. Juli 1960 - Bundesgesetzbl. I S. 569);"@de .

dtd:LA-id409 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Beamten des Grenzaufsichtsdienstes und denen des Grenzabfertigungsdienstes, wenn sie Grenzaufsichtsdienst verrichten, des Zollfahndungsdienstes und des Bewachungs- und Begleitungsdienstes;"@de .

dtd:LA-id410 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id411 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Beamten der Wasserstraßen- und Schifffahrtsverwaltung des Bundes mit strom- und schifffahrtspolizeilichen Befugnissen nach näherer Anweisung des Bundesministeriums für Verkehr und digitale Infrastruktur;"@de .

dtd:LA-id412 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id413 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den mit Vollzugs- und Sicherungsaufgaben betrauten Beamten der Bundesgerichte und der Behörden der Bundesjustizverwaltung;"@de .

dtd:LA-id414 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "anderen Personen, die durch die zuständigen Bundesbehörden mit Aufgaben betraut sind, die den unter den Nummern 1 bis 6 aufgeführten Beamten obliegen;"@de .

dtd:LA-id415 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den der Dienstgewalt von Bundesbehörden unterstehenden Personen, die mit Aufgaben der Strafverfolgung betraut sind, wenn sie sich in Ausübung dieser Tätigkeit im Vollzugsdienst befinden."@de .

dtd:LA-id416 a dtd:LA ;
    dtd:has_DL dtd:DL-id366 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "um die unmittelbar bevorstehende Ausführung oder die Fortsetzung einer rechtswidrigen Tat zu verhindern, die sich den Umständen nacha)als ein Verbrechenoder  b)als ein Vergehen, das unter Anwendung oder Mitführung von Schußwaffen oder Sprengstoffen begangen werden soll oder ausgeführt wird, "@de .

dtd:LA-id417 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "darstellt;"@de .

dtd:LA-id418 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als ein Verbrechen"@de .

dtd:LA-id419 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "oder"@de .

dtd:LA-id420 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als ein Vergehen, das unter Anwendung oder Mitführung von Schußwaffen oder Sprengstoffen begangen werden soll oder ausgeführt wird,"@de .

dtd:LA-id421 a dtd:LA ;
    dtd:has_DL dtd:DL-id371 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "um eine Person, die sich der Festnahme oder der Feststellung ihrer Person durch die Flucht zu entziehen versucht, anzuhalten, wenn siea)bei einer rechtswidrigen Tat auf frischer Tat betroffen wird, die sich den Umständen nach als ein Verbrechen darstellt oder als ein Vergehen, das unter Anwendung oder Mitführung von Schußwaffen oder Sprengstoffen begangen wird, b)eines Verbrechens dringend verdächtig ist oder c)eines Vergehens dringend verdächtig ist und Anhaltspunkte befürchten lassen, daß sie von einer Schußwaffe oder einem Sprengstoff Gebrauch machen werde; "@de .

dtd:LA-id422 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einer rechtswidrigen Tat auf frischer Tat betroffen wird, die sich den Umständen nach als ein Verbrechen darstellt oder als ein Vergehen, das unter Anwendung oder Mitführung von Schußwaffen oder Sprengstoffen begangen wird,"@de .

dtd:LA-id423 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines Verbrechens dringend verdächtig ist oder"@de .

dtd:LA-id424 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines Vergehens dringend verdächtig ist und Anhaltspunkte befürchten lassen, daß sie von einer Schußwaffe oder einem Sprengstoff Gebrauch machen werde;"@de .

dtd:LA-id425 a dtd:LA ;
    dtd:has_DL dtd:DL-id378 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Vereitlung der Flucht oder zur Wiederergreifung einer Person, die sich in amtlichem Gewahrsam befindet oder befanda)zur Verbüßung einer Freiheitsstrafe wegen einer Straftat mit Ausnahme des Strafarrestes, b)zum Vollzug der Unterbringung in der Sicherungsverwahrung, c)wegen des dringenden Verdachts eines Verbrechens, d)auf Grund richterlichen Haftbefehls oder e)sonst wegen des dringenden Verdachts eines Vergehens, wenn zu befürchten ist, daß sie von einer Schußwaffe oder einem Sprengstoff Gebrauch machen werde; "@de .

dtd:LA-id426 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Verbüßung einer Freiheitsstrafe wegen einer Straftat mit Ausnahme des Strafarrestes,"@de .

dtd:LA-id427 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Vollzug der Unterbringung in der Sicherungsverwahrung,"@de .

dtd:LA-id428 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wegen des dringenden Verdachts eines Verbrechens,"@de .

dtd:LA-id429 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Grund richterlichen Haftbefehls oder"@de .

dtd:LA-id430 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonst wegen des dringenden Verdachts eines Vergehens, wenn zu befürchten ist, daß sie von einer Schußwaffe oder einem Sprengstoff Gebrauch machen werde;"@de .

dtd:LA-id431 a dtd:LA ;
    dtd:has_DL dtd:DL-id389 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegen eine Person, die mit Gewalt einen Gefangenen oder jemanden, dessen Unterbringung ina)der Sicherungsverwahrung (§ 66 des Strafgesetzbuches), b)einem psychiatrischen Krankenhaus (§ 63 des Strafgesetzbuches, § 126a der Strafprozeßordnung) oder c)einer Entziehungsanstalt (§ 64 des Strafgesetzbuches, § 126a der Strafprozeßordnung) "@de .

dtd:LA-id432 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "angeordnet ist, aus dem amtlichen Gewahrsam zu befreien versucht."@de .

dtd:LA-id433 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Sicherungsverwahrung (§ 66 des Strafgesetzbuches),"@de .


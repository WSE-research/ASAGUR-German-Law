dtd:LA-id914 a dtd:LA ;
    dtd:has_Value "die Laufbahn des mittleren Polizeivollzugsdienstes in der Bundespolizei,"@de .

dtd:LA-id915 a dtd:LA ;
    dtd:has_Value "die Laufbahn des gehobenen Polizeivollzugsdienstes in der Bundespolizei und"@de .

dtd:LA-id916 a dtd:LA ;
    dtd:has_Value "die Laufbahn des höheren Polizeivollzugsdienstes in der Bundespolizei."@de .

dtd:LA-id917 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im mittleren Polizeivollzugsdienst in der Bundespolizei die Dienstbezeichnung „Polizeimeisteranwärterin“ oder „Polizeimeisteranwärter“,"@de .

dtd:LA-id918 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im gehobenen Polizeivollzugsdienst in der Bundespolizei die Dienstbezeichnung „Polizeikommissaranwärterin“ oder „Polizeikommissaranwärter“ und"@de .

dtd:LA-id919 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im höheren Polizeivollzugsdienst in der Bundespolizei die Dienstbezeichnung „Polizeiratanwärterin“ oder „Polizeiratanwärter“."@de .

dtd:LA-id920 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Mutterschutzes,"@de .

dtd:LA-id921 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Kinderbetreuung, höchstens jedoch um drei Jahre je Kind, sowie"@de .

dtd:LA-id922 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Pflege naher Angehöriger (Eltern, Schwiegereltern, Ehegattinnen oder Ehegatten, Lebenspartnerinnen oder Lebenspartner, Geschwister oder Kinder), für die eine Pflegestufe nach § 15 des Elften Buches Sozialgesetzbuch festgestellt worden war oder ist und die von der Bewerberin oder dem Bewerber aus gesetzlicher oder sittlicher Verpflichtung in dem in § 15 Absatz 3 des Elften Buches Sozialgesetzbuch genannten Umfang gepflegt worden sind oder werden; dabei kann das Höchstalter jedoch höchstens um drei Jahre je Angehörige oder Angehörigen angehoben werden."@de .

dtd:LA-id923 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Berufs- oder Hochschulabschluss besitzt, der der Verwendung in der Laufbahn besonders förderlich ist, und"@de .

dtd:LA-id924 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch eine mindestens dreijährige hauptberufliche Tätigkeit berufliche Erfahrungen erworben hat, die der Verwendung in der Laufbahn besonders förderlich sind."@de .

dtd:LA-id925 a dtd:LA ;
    dtd:has_Value "das Höchstalter nach § 5 Absatz 3 und 4 noch nicht überschritten haben,"@de .

dtd:LA-id926 a dtd:LA ;
    dtd:has_Value "ein mindestens dreijähriges Hochschulstudium erfolgreich abgeschlossen haben und über Kenntnisse und Fähigkeiten verfügen, die der Verwendung im Polizeivollzugsdienst besonders förderlich sind, und"@de .

dtd:LA-id927 a dtd:LA ;
    dtd:has_Value "eine zweite Staatsprüfung bestanden haben."@de .

dtd:LA-id928 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Polizeivollzugsdienstes eines Bundeslandes,"@de .

dtd:LA-id929 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des kriminalpolizeilichen Vollzugsdienstes des Bundes oder eines Bundeslandes oder"@de .

dtd:LA-id930 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Polizeivollzugsdienstes beim Deutschen Bundestag."@de .

dtd:LA-id931 a dtd:LA ;
    dtd:has_DL dtd:DL-id796 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beamtinnen und Beamte versetzt werden, wenn sie a)die Bildungsvoraussetzungen nach Anlage 2 erfüllen undb)erfolgreich an einem Auswahlverfahren, das für Regelbewerberinnen und Regelbewerber vorgesehen ist, teilgenommen haben,"@de .

dtd:LA-id932 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bildungsvoraussetzungen nach Anlage 2 erfüllen und"@de .

dtd:LA-id933 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erfolgreich an einem Auswahlverfahren, das für Regelbewerberinnen und Regelbewerber vorgesehen ist, teilgenommen haben,"@de .

dtd:LA-id934 a dtd:LA ;
    dtd:has_DL dtd:DL-id801 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Polizeivollzugsbeamtinnen und Polizeivollzugsbeamte in eine höhere Laufbahn des Polizeivollzugsdienstes wechseln, wenn sie a)die Bildungsvoraussetzungen nach Anlage 2 erfüllen, undb)erfolgreich an einem Auswahlverfahren, das für Regelbewerberinnen und Regelbewerber vorgesehen ist, teilgenommen haben,"@de .

dtd:LA-id935 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bildungsvoraussetzungen nach Anlage 2 erfüllen, und"@de .

dtd:LA-id936 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erfolgreich an einem Auswahlverfahren, das für Regelbewerberinnen und Regelbewerber vorgesehen ist, teilgenommen haben,"@de .

dtd:LA-id937 a dtd:LA ;
    dtd:has_DL dtd:DL-id806 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Polizeivollzugsbeamtinnen und Polizeivollzugsbeamte im Flugdienst der Bundespolizei abweichend von § 17 Absatz 4 des Bundesbeamtengesetzes in die Laufbahn des gehobenen Polizeivollzugsdienstes wechseln, wenn sie a)die Bildungsvoraussetzungen nach Anlage 2 erfüllen undb)erfolgreich an einem Auswahlverfahren teilgenommen haben,"@de .

dtd:LA-id938 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bildungsvoraussetzungen nach Anlage 2 erfüllen und"@de .

dtd:LA-id939 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erfolgreich an einem Auswahlverfahren teilgenommen haben,"@de .

dtd:LA-id940 a dtd:LA ;
    dtd:has_DL dtd:DL-id811 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bewerberinnen und Bewerber nach Maßgabe der §§ 19 bis 21 der Bundeslaufbahnverordnung eingestellt werden, wenn sie a)die Bildungsvoraussetzungen nach Anlage 2 erfüllen undb)die Altershöchstgrenze nach § 5 Absatz 3 und 4 nicht überschritten haben."@de .

dtd:LA-id941 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bildungsvoraussetzungen nach Anlage 2 erfüllen und"@de .

dtd:LA-id942 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Altershöchstgrenze nach § 5 Absatz 3 und 4 nicht überschritten haben."@de .

dtd:LA-id943 a dtd:LA ;
    dtd:has_Value "für Polizeivollzugsbeamtinnen und Polizeivollzugsbeamte des mittleren und des gehobenen Polizeivollzugsdienstes mindestens drei Monate und"@de .

dtd:LA-id944 a dtd:LA ;
    dtd:has_Value "für Polizeivollzugsbeamtinnen und Polizeivollzugsbeamte des höheren Polizeivollzugsdienstes mindestens sechs Monate."@de .

dtd:LA-id945 a dtd:LA ;
    dtd:has_Value "für den bisherigen Dienstposten die gleichen Anforderungen gelten wie für den zur Erprobung übertragenen Dienstposten und"@de .

dtd:LA-id946 a dtd:LA ;
    dtd:has_Value "die unterschiedliche Bewertung lediglich auf geringfügigen Unterschieden bei den Aufgaben und in der Verantwortung beruht."@de .

dtd:LA-id947 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Beginn des Aufstiegs in den gehobenen Polizeivollzugsdienst in der Bundespolizei in einer Dienstzeit von drei Jahren bewährt haben und noch nicht 50 Jahre alt sind oder"@de .

dtd:LA-id948 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Zulassung zum Aufstieg in den höheren Polizeivollzugsdienst in der Bundespolizei in einer Dienstzeit von drei Jahren im gehobenen Dienst bewährt haben und noch nicht 45 Jahre alt sind."@de .

dtd:LA-id949 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über die Zulassung zum Aufstieg das Bundespolizeipräsidium entscheidet,"@de .

dtd:LA-id950 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Falle des § 36 Absatz 4 Satz 8 der Bundeslaufbahnverordnung die Teilnahme am Auswahlverfahren einmal, bei erfolgreicher Teilnahme auch mehrfach wiederholt werden kann."@de .

dtd:LA-id951 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein dienstliches Bedürfnis besteht und"@de .

dtd:LA-id952 a dtd:LA ;
    dtd:has_DL dtd:DL-id841 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Polizeivollzugsbeamtinnen und Polizeivollzugsbeamten a)bei Beginn des Aufstiegs noch nicht 57 Jahre alt sind,b)sich in einer Dienstzeit von mindestens zehn Jahren bewährt haben,c)sich im Amt der Polizeihauptmeisterin oder des Polizeihauptmeisters mindestens drei Jahre bewährt haben,d)in den letzten beiden dienstliche Beurteilungen überdurchschnittlich bewertet worden sind unde)erfolgreich an einem Auswahlverfahren teilgenommen haben."@de .

dtd:LA-id953 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Beginn des Aufstiegs noch nicht 57 Jahre alt sind,"@de .

dtd:LA-id954 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich in einer Dienstzeit von mindestens zehn Jahren bewährt haben,"@de .

dtd:LA-id955 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich im Amt der Polizeihauptmeisterin oder des Polizeihauptmeisters mindestens drei Jahre bewährt haben,"@de .

dtd:LA-id956 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den letzten beiden dienstliche Beurteilungen überdurchschnittlich bewertet worden sind und"@de .

dtd:LA-id957 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erfolgreich an einem Auswahlverfahren teilgenommen haben."@de .

dtd:LA-id958 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dafür ein dienstliches Bedürfnis besteht und"@de .

dtd:LA-id959 a dtd:LA ;
    dtd:has_DL dtd:DL-id857 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Polizeivollzugsbeamtinnen und Polizeivollzugsbeamten a)bei Beginn des Aufstiegs noch nicht 40 Jahre alt sind,b)sich in dem von der Bundespolizei geförderten Spitzensport mindestens drei Jahre bewährt haben,c)den Vorbereitungsdienst für den mittleren Polizeivollzugsdienst in der Bundespolizei mit einem mindestens überdurchschnittlichen Ergebnis abgeschlossen haben undd)erfolgreich an einem Auswahlverfahren teilgenommen haben.Bei der Bemessung der Bewährungszeit sind Zeiten einer Teilzeitbeschäftigung wie Zeiten einer Vollzeitbeschäftigung zu behandeln, soweit nicht zwingende sachliche Gründe entgegenstehen."@de .

dtd:LA-id960 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Beginn des Aufstiegs noch nicht 40 Jahre alt sind,"@de .

dtd:LA-id961 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich in dem von der Bundespolizei geförderten Spitzensport mindestens drei Jahre bewährt haben,"@de .

dtd:LA-id962 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Vorbereitungsdienst für den mittleren Polizeivollzugsdienst in der Bundespolizei mit einem mindestens überdurchschnittlichen Ergebnis abgeschlossen haben und"@de .

dtd:LA-id963 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erfolgreich an einem Auswahlverfahren teilgenommen haben."@de .

dtd:LA-id964 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über die Zulassung zum Aufstieg – abweichend von § 36 Absatz 6 der Bundeslaufbahnverordnung – das Bundespolizeipräsidium entscheidet,"@de .

dtd:LA-id965 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall des § 36 Absatz 4 Satz 8 der Bundeslaufbahnverordnung die Teilnahme am Auswahlverfahren einmal, bei erfolgreicher Teilnahme auch mehrfach wiederholt werden kann."@de .

dtd:LA-id966 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zulassung vor Ablauf des 31. Dezember 2023 erfolgt,"@de .

dtd:LA-id967 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Zulassung ein dienstliches Bedürfnis besteht und"@de .

dtd:LA-id968 a dtd:LA ;
    dtd:has_DL dtd:DL-id878 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Polizeivollzugsbeamtinnen und Polizeivollzugsbeamten a)bei Beginn des Aufstiegs noch nicht 55 Jahre alt sind,b)sich in einer Dienstzeit von mindestens zehn Jahren bewährt haben,c)sich im Amt der Ersten Polizeihauptkommissarin oder des Ersten Polizeihauptkommissars mindestens drei Jahre bewährt haben,d)in der letzten dienstlichen Beurteilung in ihrer Besoldungsgruppe mindestens mit der Note B 1 beurteilt worden sind unde)erfolgreich an einem Auswahlverfahren teilgenommen haben."@de .


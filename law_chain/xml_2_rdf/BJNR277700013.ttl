dtd:P-id1761 a dtd:P ;
    dtd:has_Value "Die Portfolioumschlagsrate eines Sondervermögens, einer Investmentaktiengesellschaft mit veränderlichem Kapital oder einer offenen Investmentkommanditgesellschaft wird folgendermaßen ermittelt: Der niedrigere Betrag des Gegenwertes der Käufe und Verkäufe der Vermögensgegenstände des betreffenden Berichtszeitraums wird durch den durchschnittlichen Nettoinventarwert dividiert. Der durchschnittliche Nettoinventarwert ist das arithmetische Mittel der ermittelten Nettoinventarwerte der Vermögensgegenstände."@de .

dtd:P-id1762 a dtd:P ;
    dtd:has_Value "Die Portfolioumschlagsrate ist nach folgender Formel zu berechnen und in Prozent anzugeben:"@de .

dtd:P-id1763 a dtd:P ;
    dtd:has_B dtd:B-id558 ;
    dtd:has_Value "Portfolioumschlagsrate = Min(X ,Y)/M"@de .

dtd:P-id1764 a dtd:P ;
    dtd:has_Value "Der kleinere Wert von X oder Y = Min (X,Y)"@de .

dtd:P-id1765 a dtd:P ;
    dtd:has_Value "Wertpapierkäufe = X"@de .

dtd:P-id1766 a dtd:P ;
    dtd:has_Value "Wertpapierverkäufe = Y"@de .

dtd:P-id1767 a dtd:P ;
    dtd:has_Value "durchschnittlicher Nettoinventarwert = M"@de .


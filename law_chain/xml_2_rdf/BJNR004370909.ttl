    dtd:has_Size dtd:normal ;
    dtd:has_Value "ergeben sich acht oder mehr Punkte, gilt der Inhaber einer Fahrerlaubnis als ungeeignet zum Fhren von Kraftfahrzeugen und die Fahrerlaubnis ist zu entziehen."@de .

dtd:LA-id2989 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "unabhngig davon bercksichtigt, ob nach deren Begehung bereits Manahmen ergriffen worden sind,"@de .

dtd:LA-id2990 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nur dann bercksichtigt, wenn deren Tilgungsfrist zu dem in Satz 5 genannten Zeitpunkt noch nicht abgelaufen war."@de .

dtd:LA-id2991 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ermahnung auf fnf Punkte,"@de .

dtd:LA-id2992 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verwarnung auf sieben Punkte,"@de .

dtd:LA-id2993 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value " 315c Absatz 1 Nummer 1 Buchstabe a des Strafgesetzbuches,"@de .

dtd:LA-id2994 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den  316 oder 323a des Strafgesetzbuches oder"@de .

dtd:LA-id2995 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den  24a oder 24c"@de .

dtd:LA-id2996 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fr die verkehrspdagogische Teilmanahme Fahrlehrer, die ber eine Seminarerlaubnis Verkehrspdagogik nach  46 des Fahrlehrergesetzes und"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine zur Durchfhrung der verkehrspsychologischen Teilmanahme geeignete rumliche und sachliche Ausstattung nachweist."@de .

dtd:LA-id3006 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vom Inhaber der Seminarerlaubnis Verkehrspsychologie lngstens neun Monate nach der Ausstellung der Teilnahmebescheinigung fr die Durchfhrung des jeweiligen Fahreignungsseminars verwendet werden,"@de .

dtd:LA-id3007 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vom Inhaber der Seminarerlaubnis Verkehrspsychologie der Bundesanstalt fr Straenwesen bermittelt und von dieser zur Evaluierung nach  4b verwendet werden,"@de .

dtd:LA-id3008 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:LA-id2960 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das 30. Lebensjahr vollendet hat,"@de .

dtd:LA-id2961 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens seit fnf Jahren eine gltige Fahrerlaubnis der Klasse C1 besitzt und"@de .

dtd:LA-id2962 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Zeitpunkt der Einweisungs- und Prfungsfahrten im Fahreignungsregister mit nicht mehr als zwei Punkten belastet ist,"@de .

dtd:LA-id2963 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seine Teilnahme an einem Aufbauseminar anzuordnen und hierfr eine Frist zu setzen, wenn er eine schwerwiegende oder zwei weniger schwerwiegende Zuwiderhandlungen begangen hat,"@de .

dtd:LA-id2964 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihn schriftlich zu verwarnen und ihm nahezulegen, innerhalb von zwei Monaten an einer verkehrspsychologischen Beratung nach Absatz 7 teilzunehmen, wenn er nach Teilnahme an einem Aufbauseminar innerhalb der Probezeit eine weitere schwerwiegende oder zwei weitere weniger schwerwiegende Zuwiderhandlungen begangen hat,"@de .

dtd:LA-id2965 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihm die Fahrerlaubnis zu entziehen, wenn er nach Ablauf der in Nummer 2 genannten Frist innerhalb der Probezeit eine weitere schwerwiegende oder zwei weitere weniger schwerwiegende Zuwiderhandlungen begangen hat."@de .

dtd:LA-id2966 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach  3 oder nach  4 Absatz 5 Satz 1 Nummer 3 dieses Gesetzes, weil innerhalb der Probezeit Zuwiderhandlungen begangen wurden, oder nach  69 oder  69b des Strafgesetzbuches,"@de .

dtd:LA-id2967 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach Absatz 3, weil einer Anordnung zur Teilnahme an einem Aufbauseminar nicht nachgekommen wurde,"@de .

dtd:LA-id2968 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "persnlich zuverlssig ist,"@de .

dtd:LA-id2969 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ber den Abschluss eines Hochschulstudiums als Diplom-Psychologe oder eines gleichwertigen Masterabschlusses in Psychologie verfgt und"@de .


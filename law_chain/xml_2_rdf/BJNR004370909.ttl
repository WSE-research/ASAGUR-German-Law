    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Befhigung zum Fhren von Kraftfahrzeugen in einer theoretischen und praktischen Prfung nachgewiesen hat,"@de .

dtd:LA-id2946 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erste Hilfe leisten kann und"@de .


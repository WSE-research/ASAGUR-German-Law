    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Tätigkeit einer Seelotsin oder eines Seelotsen ohne Bestallung nach § 7 oder ohne Erlaubnis nach § 42 Abs. 1 ausübt,"@de .

dtd:LA-id870 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 23 Abs. 1 Satz 1 die Kapitänin oder den Kapitän nicht berät,"@de .


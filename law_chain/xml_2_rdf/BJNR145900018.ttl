dtd:LA-id1017 a dtd:LA ;
    dtd:has_Value "der Unterwarnow und Breitling und"@de .

dtd:LA-id1018 a dtd:LA ;
    dtd:has_Value "im Wolgaster Hafengebiet"@de .

dtd:LA-id1019 a dtd:LA ;
    dtd:has_DL dtd:DL-id714 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Für Fahrgastschiffe auf Wasserstraßen der Zone 2-Binnen gelten abweichend von Kapitel 19 ES-TRIN folgende Bestimmungen: a)Bei Fahrgastschiffen ohne Schottendeck muss der Sicherheitsabstand abweichend von Artikel 19.04 Nummer 1 Satz 2 ES-TRIN mindestens 0,80 m betragen.b)Einzelrettungsmittel nach Artikel 19.09 Nummer 4 ES-TRIN können durch Sammelrettungsmittel nach Artikel 19.09 Nummer 5 ES-TRIN in Verbindung mit Nummer 7 und 9 ersetzt werden."@de .

dtd:LA-id1020 a dtd:LA ;
    dtd:has_Size dtd:normal ;

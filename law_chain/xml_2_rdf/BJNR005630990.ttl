    dtd:has_Value "Anatomie und Physiologie des Menschen, insbesondere des Sehsystems,"@de .

dtd:LA-id1087 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Allgemeine Augenheilkunde einschließlich Arzneimittel,"@de .

dtd:LA-id1088 a dtd:LA ;
    dtd:has_Size dtd:normal ;

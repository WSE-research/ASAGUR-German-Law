dtd:P-id566 a dtd:P ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id576 ;
    dtd:has_Value "( Fundstelle des Originaltextes: BGBl. I 2002, 3227 - 3228;bzgl. der einzelnen Änderungen vgl. Fußnote )"@de ;

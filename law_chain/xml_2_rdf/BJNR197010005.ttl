dtd:LA-id7096 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betreiber von Gasversorgungsnetzen"@de .

dtd:LA-id7097 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "natürliche oder juristische Personen oder rechtlich unselbständige Organisationseinheiten eines Energieversorgungsunternehmens, die Gasversorgungsnetze betreiben,"@de .


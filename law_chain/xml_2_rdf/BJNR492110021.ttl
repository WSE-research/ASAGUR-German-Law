dtd:LA-id612 a dtd:LA ;
    dtd:has_Value "Leistungen von Gebietskörperschaften des öffentlichen Rechts, soweit es sich nicht um Leistungen handelt, für die Benutzungsgebühren oder privatrechtliche Entgelte zu entrichten sind;"@de .

dtd:LA-id613 a dtd:LA ;
    dtd:has_Value "Waren und Leistungen, soweit für sie auf Grund von Rechtsvorschriften eine Werbung untersagt ist;"@de .

dtd:LA-id614 a dtd:LA ;
    dtd:has_Value "mündliche Angebote, die ohne Angabe von Preisen abgegeben werden;"@de .

dtd:LA-id615 a dtd:LA ;
    dtd:has_Value "Warenangebote bei Versteigerungen."@de .

dtd:LA-id616 a dtd:LA ;
    dtd:has_Value "dem Angebot oder der Werbung eindeutig zuzuordnen sowie"@de .

dtd:LA-id617 a dtd:LA ;
    dtd:has_Value "leicht erkennbar und deutlich lesbar oder sonst gut wahrnehmbar zu machen."@de .

dtd:LA-id618 a dtd:LA ;
    dtd:has_Value "„Arbeits- oder Mengenpreis“ den verbrauchsabhängigen Preis je Mengeneinheit einschließlich der Umsatzsteuer und aller besonderen Verbrauchssteuern für die leitungsgebundene Abgabe von Elektrizität, Gas, Fernwärme oder Wasser;"@de .

dtd:LA-id619 a dtd:LA ;
    dtd:has_Value "„Fertigpackung“ eine Verpackung im Sinne des § 42 Absatz 1 des Mess- und Eichgesetzes;"@de .

dtd:LA-id620 a dtd:LA ;
    dtd:has_Value "„Gesamtpreis“ den Preis, der einschließlich der Umsatzsteuer und sonstiger Preisbestandteile für eine Ware oder eine Leistung zu zahlen ist;"@de .

dtd:LA-id621 a dtd:LA ;
    dtd:has_Value "„Grundpreis“ den Preis je Mengeneinheit einer Ware einschließlich der Umsatzsteuer und sonstiger Preisbestandteile;"@de .

dtd:LA-id622 a dtd:LA ;
    dtd:has_Value "„lose Ware“ unverpackte Ware, die durch den Unternehmer in Anwesenheit der Verbraucher, durch die Verbraucher selbst oder auf deren Veranlassung abgemessen wird;"@de .

dtd:LA-id623 a dtd:LA ;
    dtd:has_Value "„offene Packung“ eine Verkaufseinheit im Sinne des § 42 Absatz 2 Nummer 1 des Mess- und Eichgesetzes;"@de .

dtd:LA-id624 a dtd:LA ;
    dtd:has_Value "„Selbstabfüllung“ die Abgabe von flüssiger loser Ware, die durch die Verbraucher selbst in die jeweilige Umverpackung abgefüllt wird;"@de .

dtd:LA-id625 a dtd:LA ;
    dtd:has_Value "„Unternehmer“ jede natürliche oder juristische Person im Sinne des § 2 Absatz 1 Nummer 8 des Gesetzes gegen den unlauteren Wettbewerb in der Fassung der Bekanntmachung vom 3. März 2010 (BGBl. I S. 254), das zuletzt durch Artikel 1 des Gesetzes vom 10. August 2021 (BGBl. I S. 3504) geändert worden ist, in der am 28. Mai 2022 geltenden Fassung;"@de .

dtd:LA-id626 a dtd:LA ;
    dtd:has_Value "„Verbraucher“ jede natürliche Person im Sinne des § 13 des Bürgerlichen Gesetzbuchs."@de .

dtd:LA-id627 a dtd:LA ;
    dtd:has_Value "Waren, die über ein Nenngewicht oder Nennvolumen von weniger als 10 Gramm oder 10 Milliliter verfügen;"@de .

dtd:LA-id628 a dtd:LA ;
    dtd:has_Value "Waren, die verschiedenartige Erzeugnisse enthalten, die nicht miteinander vermischt oder vermengt sind;"@de .

dtd:LA-id629 a dtd:LA ;
    dtd:has_Value "Waren, die von kleinen Direktvermarktern, insbesondere Hofläden, Winzerbetrieben oder Imkern, sowie kleinen Einzelhandelsgeschäften, insbesondere Kiosken, mobilen Verkaufsstellen oder Ständen auf Märkten oder Volksfesten, angeboten werden, bei denen die Warenausgabe überwiegend im Wege der Bedienung erfolgt, es sei denn, dass das Warensortiment im Rahmen eines Vertriebssystems bezogen wird;"@de .

dtd:LA-id630 a dtd:LA ;
    dtd:has_Value "Waren, die im Rahmen einer Dienstleistung angeboten werden;"@de .

dtd:LA-id631 a dtd:LA ;
    dtd:has_Value "Waren, die in Getränke- und Verpflegungsautomaten angeboten werden;"@de .

dtd:LA-id632 a dtd:LA ;
    dtd:has_Value "Kau- und Schnupftabak mit einem Nenngewicht bis 25 Gramm;"@de .

dtd:LA-id633 a dtd:LA ;
    dtd:has_Value "kosmetische Mittel, die ausschließlich der Färbung oder Verschönerung der Haut, des Haares oder der Nägel dienen;"@de .

dtd:LA-id634 a dtd:LA ;
    dtd:has_Value "Parfüms und parfümierte Duftwässer, die mindestens 3 Volumenprozent Duftöl und mindestens 70 Volumenprozent reinen Ethylalkohol enthalten."@de .

dtd:LA-id635 a dtd:LA ;
    dtd:has_Value "dass die für Waren oder Leistungen geforderten Preise die Umsatzsteuer und sonstige Preisbestandteile enthalten und"@de .

dtd:LA-id636 a dtd:LA ;
    dtd:has_Value "ob zusätzlich Fracht-, Liefer- oder Versandkosten oder sonstige Kosten anfallen."@de .

dtd:LA-id637 a dtd:LA ;
    dtd:has_Value "bei Waren oder Leistungen, für die Liefer- oder Leistungsfristen von mehr als vier Monaten bestehen, soweit zugleich die voraussichtlichen Liefer- und Leistungsfristen angegeben werden, oder"@de .

dtd:LA-id638 a dtd:LA ;
    dtd:has_Value "bei Waren oder Leistungen, die im Rahmen von Dauerschuldverhältnissen erbracht werden."@de .

dtd:LA-id639 a dtd:LA ;
    dtd:has_Value "individuellen Preisermäßigungen;"@de .


    dtd:has_Value "nach dem Tod der betroffenen Person folgende Angehörige: a)die Ehegattin oder der Ehegatte,b)die oder der Verlobte,c)die Eltern,d)die Kinder unde)die Geschwister."@de .

dtd:LA-id89 a dtd:LA ;
    dtd:has_Value "die Ehegattin oder der Ehegatte,"@de .

dtd:LA-id90 a dtd:LA ;
    dtd:has_Value "die oder der Verlobte,"@de .

dtd:LA-id91 a dtd:LA ;
    dtd:has_Value "die Eltern,"@de .

dtd:LA-id92 a dtd:LA ;
    dtd:has_Value "die Kinder und"@de .


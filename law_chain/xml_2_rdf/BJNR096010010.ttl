dtd:LA-id305 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ultraviolette Strahlung ist die optische Strahlung im Wellenlängenbereich von 100 bis 400 Nanometer (UV-Strahlung); das Spektrum der UV-Strahlung wird unterteilt in UV-A-Strahlung (315 bis 400 Nanometer), UV-B-Strahlung (280 bis 315 Nanometer) und UV-C-Strahlung (100 bis 280 Nanometer);"@de .

dtd:LA-id306 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sichtbare Strahlung ist die optische Strahlung im Wellenlängenbereich von 380 bis 780 Nanometer;"@de .

dtd:LA-id307 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Infrarotstrahlung ist die optische Strahlung im Wellenlängenbereich von 780 Nanometer bis 1 Millimeter (IR-Strahlung); das Spektrum der IR-Strahlung wird unterteilt in IR-A-Strahlung (780 bis 1 400 Nanometer), IR-B-Strahlung (1 400 bis 3 000 Nanometer) und IR-C-Strahlung (3 000 Nanometer bis 1 Millimeter)."@de .

dtd:LA-id308 a dtd:LA ;
    dtd:has_Value "Art, Ausmaß und Dauer der Exposition durch künstliche optische Strahlung,"@de .

dtd:LA-id309 a dtd:LA ;
    dtd:has_Value "der Wellenlängenbereich der künstlichen optischen Strahlung,"@de .

dtd:LA-id310 a dtd:LA ;
    dtd:has_Value "die in § 6 genannten Expositionsgrenzwerte,"@de .

dtd:LA-id311 a dtd:LA ;
    dtd:has_Value "alle Auswirkungen auf die Gesundheit und Sicherheit von Beschäftigten, die besonders gefährdeten Gruppen angehören,"@de .

dtd:LA-id312 a dtd:LA ;
    dtd:has_Value "alle möglichen Auswirkungen auf die Sicherheit und Gesundheit von Beschäftigten, die sich aus dem Zusammenwirken von künstlicher optischer Strahlung und fotosensibilisierenden chemischen Stoffen am Arbeitsplatz ergeben können,"@de .

dtd:LA-id313 a dtd:LA ;
    dtd:has_Value "alle indirekten Auswirkungen auf die Sicherheit und Gesundheit der Beschäftigten, zum Beispiel durch Blendung, Brand- und Explosionsgefahr,"@de .

dtd:LA-id314 a dtd:LA ;
    dtd:has_Value "die Verfügbarkeit und die Möglichkeit des Einsatzes alternativer Arbeitsmittel und Ausrüstungen, die zu einer geringeren Exposition der Beschäftigten führen (Substitutionsprüfung),"@de .

dtd:LA-id315 a dtd:LA ;
    dtd:has_Value "Erkenntnisse aus arbeitsmedizinischen Vorsorgeuntersuchungen sowie hierzu allgemein zugängliche, veröffentlichte Informationen,"@de .

dtd:LA-id316 a dtd:LA ;
    dtd:has_Value "die Exposition der Beschäftigten durch künstliche optische Strahlung aus mehreren Quellen,"@de .

dtd:LA-id317 a dtd:LA ;
    dtd:has_Value "die Herstellerangaben zu optischen Strahlungsquellen und anderen Arbeitsmitteln,"@de .

dtd:LA-id318 a dtd:LA ;
    dtd:has_Value "die Klassifizierung der Lasereinrichtungen und gegebenenfalls der in den Lasereinrichtungen zum Einsatz kommenden Laser nach dem Stand der Technik,"@de .

dtd:LA-id319 a dtd:LA ;
    dtd:has_Value "die Klassifizierung von inkohärenten optischen Strahlungsquellen nach dem Stand der Technik, von denen vergleichbare Gefährdungen wie bei Lasern der Klassen 3R, 3B oder 4 ausgehen können,"@de .

dtd:LA-id320 a dtd:LA ;
    dtd:has_Value "die Arbeitsplatz- und Expositionsbedingungen, die zum Beispiel im Normalbetrieb, bei Einrichtvorgängen sowie bei Instandhaltungs- und Reparaturarbeiten auftreten können."@de .

dtd:LA-id321 a dtd:LA ;
    dtd:has_Value "den vorhandenen Arbeitsplatz- und Expositionsbedingungen hinsichtlich der betreffenden künstlichen optischen Strahlung angepasst sein und"@de .

dtd:LA-id322 a dtd:LA ;
    dtd:has_Value "geeignet sein, die jeweiligen physikalischen Größen zu bestimmen; die Messergebnisse müssen die Entscheidung erlauben, ob die in § 6 genannten Expositionsgrenzwerte eingehalten werden."@de .

dtd:LA-id323 a dtd:LA ;
    dtd:has_Value "bei der Durchführung der Gefährdungsbeurteilung nach § 3,"@de .

dtd:LA-id324 a dtd:LA ;
    dtd:has_Value "bei der Durchführung der notwendigen Schutzmaßnahmen nach § 7 und"@de .

dtd:LA-id325 a dtd:LA ;
    dtd:has_Value "bei der Überwachung des sicheren Betriebs von Lasern nach Satz 1."@de .

dtd:LA-id326 a dtd:LA ;
    dtd:has_Value "alternative Arbeitsverfahren, welche die Exposition der Beschäftigten durch künstliche optische Strahlung verringern,"@de .

dtd:LA-id327 a dtd:LA ;
    dtd:has_Value "Auswahl und Einsatz von Arbeitsmitteln, die in geringerem Maße künstliche optische Strahlung emittieren,"@de .

dtd:LA-id328 a dtd:LA ;
    dtd:has_Value "technische Maßnahmen zur Verringerung der Exposition der Beschäftigten durch künstliche optische Strahlung, falls erforderlich auch unter Einsatz von Verriegelungseinrichtungen, Abschirmungen oder vergleichbaren Sicherheitseinrichtungen,"@de .

dtd:LA-id329 a dtd:LA ;
    dtd:has_Value "Wartungsprogramme für Arbeitsmittel, Arbeitsplätze und Anlagen,"@de .

dtd:LA-id330 a dtd:LA ;
    dtd:has_Value "die Gestaltung und die Einrichtung der Arbeitsstätten und Arbeitsplätze,"@de .

dtd:LA-id331 a dtd:LA ;
    dtd:has_Value "organisatorische Maßnahmen zur Begrenzung von Ausmaß und Dauer der Exposition,"@de .

dtd:LA-id332 a dtd:LA ;
    dtd:has_Value "Auswahl und Einsatz einer geeigneten persönlichen Schutzausrüstung,"@de .

dtd:LA-id333 a dtd:LA ;
    dtd:has_Value "die Verwendung der Arbeitsmittel nach den Herstellerangaben."@de .

dtd:LA-id334 a dtd:LA ;
    dtd:has_Value "die mit der Tätigkeit verbundenen Gefährdungen,"@de .

dtd:LA-id335 a dtd:LA ;
    dtd:has_Value "die durchgeführten Maßnahmen zur Beseitigung oder zur Minimierung der Gefährdung unter Berücksichtigung der Arbeitsplatzbedingungen,"@de .

dtd:LA-id336 a dtd:LA ;
    dtd:has_Value "die Expositionsgrenzwerte und ihre Bedeutung,"@de .

dtd:LA-id337 a dtd:LA ;
    dtd:has_Value "die Ergebnisse der Expositionsermittlung zusammen mit der Erläuterung ihrer Bedeutung und der Bewertung der damit verbundenen möglichen Gefährdungen und gesundheitlichen Folgen,"@de .

dtd:LA-id338 a dtd:LA ;
    dtd:has_Value "die Beschreibung sicherer Arbeitsverfahren zur Minimierung der Gefährdung auf Grund der Exposition durch künstliche optische Strahlung,"@de .

dtd:LA-id339 a dtd:LA ;
    dtd:has_Value "die sachgerechte Verwendung der persönlichen Schutzausrüstung."@de .

dtd:LA-id340 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gefährdungsbeurteilung einschließlich der Dokumentation,"@de .

dtd:LA-id341 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Art, Ausmaß und Dauer der Exposition durch die künstliche optische Strahlung,"@de .

dtd:LA-id342 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Wellenlängenbereich der künstlichen optischen Strahlung,"@de .

dtd:LA-id343 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Stand der Technik bezüglich der Tätigkeiten und der Arbeitsverfahren sowie zu den technischen, organisatorischen und persönlichen Schutzmaßnahmen,"@de .

dtd:LA-id344 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Lösungsvorschlägen, wie die Exposition der Beschäftigten reduziert werden kann, um die Expositionswerte einzuhalten, sowie einen Zeitplan hierfür."@de .

dtd:LA-id345 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Absatz 3 Satz 1 Beschäftigte eine Tätigkeit aufnehmen lässt,"@de .

dtd:LA-id346 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Absatz 4 Satz 1 und 2 eine Gefährdungsbeurteilung nicht richtig, nicht vollständig oder nicht rechtzeitig dokumentiert,"@de .

dtd:LA-id347 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 4 Absatz 1 Satz 1 nicht sicherstellt, dass eine Messung oder eine Berechnung nach dem Stand der Technik durchgeführt wird,"@de .

dtd:LA-id348 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 5 Absatz 1 Satz 1 nicht sicherstellt, dass die Gefährdungsbeurteilung, die Messungen oder die Berechnungen von fachkundigen Personen durchgeführt werden,"@de .

dtd:LA-id349 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 5 Absatz 2 Satz 1 einen Laserschutzbeauftragten nicht schriftlich bestellt,"@de .

dtd:LA-id350 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 5 Absatz 2 Satz 2 einen Laserschutzbeauftragten bestellt, der nicht über die für seine Aufgaben erforderlichen Fachkenntnisse verfügt,"@de .

dtd:LA-id351 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 7 Absatz 3 Satz 1 einen Arbeitsbereich nicht kennzeichnet,"@de .

dtd:LA-id352 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 7 Absatz 3 Satz 4 einen Arbeitsbereich nicht abgrenzt,"@de .


dtd:LA-id378 a dtd:LA ;
    dtd:has_Value "die Art der zu verrichtenden Tätigkeit objektiv berücksichtigen,"@de .

dtd:LA-id379 a dtd:LA ;
    dtd:has_Value "auf für weibliche und männliche Beschäftigte gemeinsamen Kriterien beruhen,"@de .

dtd:LA-id380 a dtd:LA ;
    dtd:has_Value "die einzelnen Differenzierungskriterien diskriminierungsfrei gewichten sowie"@de .

dtd:LA-id381 a dtd:LA ;
    dtd:has_Value "insgesamt durchschaubar sein."@de .

dtd:LA-id382 a dtd:LA ;
    dtd:has_Value "Arbeitnehmerinnen und Arbeitnehmer,"@de .

dtd:LA-id383 a dtd:LA ;
    dtd:has_Value "Beamtinnen und Beamte des Bundes sowie der sonstigen der Aufsicht des Bundes unterstehenden Körperschaften, Anstalten und Stiftungen des öffentlichen Rechts,"@de .

dtd:LA-id384 a dtd:LA ;
    dtd:has_Value "Richterinnen und Richter des Bundes,"@de .

dtd:LA-id385 a dtd:LA ;
    dtd:has_Value "Soldatinnen und Soldaten,"@de .

dtd:LA-id386 a dtd:LA ;
    dtd:has_Value "die zu ihrer Berufsbildung Beschäftigten sowie"@de .

dtd:LA-id387 a dtd:LA ;
    dtd:has_Value "die in Heimarbeit Beschäftigten sowie die ihnen Gleichgestellten."@de .

dtd:LA-id388 a dtd:LA ;
    dtd:has_Value "in den Fällen des § 14 sowie in den Fällen einer gesetzlichen Entgeltregelung ist das Vergleichsentgelt der Beschäftigten des jeweils anderen Geschlechts anzugeben, die in die gleiche Entgelt- oder Besoldungsgruppe eingruppiert sind wie der oder die auskunftverlangende Beschäftigte;"@de .

dtd:LA-id389 a dtd:LA ;
    dtd:has_Value "in den Fällen des § 15 ist das Vergleichsentgelt aller Beschäftigten des jeweils anderen Geschlechts anzugeben, die die erfragte Vergleichstätigkeit oder die nach § 15 Absatz 4 ermittelte Vergleichstätigkeit ausüben."@de .

dtd:LA-id390 a dtd:LA ;
    dtd:has_Value "nur Entgeltregelungen, die in demselben Betrieb und bei demselben Arbeitgeber angewendet werden,"@de .

dtd:LA-id391 a dtd:LA ;
    dtd:has_Value "keine regional unterschiedlichen Entgeltregelungen bei demselben Arbeitgeber und"@de .

dtd:LA-id392 a dtd:LA ;
    dtd:has_Value "keinen Vergleich der Beschäftigtengruppen nach § 5 Absatz 2 untereinander."@de .

dtd:LA-id393 a dtd:LA ;
    dtd:has_Value "ihre Maßnahmen zur Förderung der Gleichstellung von Frauen und Männern und deren Wirkungen sowie"@de .

dtd:LA-id394 a dtd:LA ;
    dtd:has_Value "ihre Maßnahmen zur Herstellung von Entgeltgleichheit für Frauen und Männer."@de .

dtd:LA-id395 a dtd:LA ;
    dtd:has_Value "zu der durchschnittlichen Gesamtzahl der Beschäftigten sowie"@de .

dtd:LA-id396 a dtd:LA ;
    dtd:has_Value "zu der durchschnittlichen Zahl der Vollzeit- und Teilzeitbeschäftigten."@de .


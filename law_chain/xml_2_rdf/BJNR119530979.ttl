    dtd:has_Value "die Einfuhr von Gegenständen im Inland oder in den österreichischen Gebieten Jungholz und Mittelberg (Einfuhrumsatzsteuer);"@de .

dtd:LA-id3867 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der innergemeinschaftliche Erwerb im Inland gegen Entgelt."@de .


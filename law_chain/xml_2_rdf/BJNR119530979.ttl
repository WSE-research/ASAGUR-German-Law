    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine juristische Person, die nicht Unternehmer ist oder die den Gegenstand nicht für ihr Unternehmen erwirbt, und"@de .

dtd:LA-id3893 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gesamtbetrag der Entgelte für Erwerbe im Sinne des Absatzes 1 Nr. 1 und des Absatzes 2 hat den Betrag von 12 500 Euro im vorangegangenen Kalenderjahr nicht überstiegen und wird diesen Betrag im laufenden Kalenderjahr voraussichtlich nicht übersteigen (Erwerbsschwelle)."@de .

dtd:LA-id3894 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "motorbetriebene Landfahrzeuge mit einem Hubraum von mehr als 48 Kubikzentimetern oder einer Leistung von mehr als 7,2 Kilowatt;"@de .

dtd:LA-id3895 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wasserfahrzeuge mit einer Länge von mehr als 7,5 Metern;"@de .

dtd:LA-id3896 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Luftfahrzeuge, deren Starthöchstmasse mehr als 1.550 Kilogramm beträgt."@de .

dtd:LA-id3897 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Landfahrzeug nicht mehr als 6.000 Kilometer zurückgelegt hat oder wenn seine erste Inbetriebnahme im Zeitpunkt des Erwerbs nicht mehr als sechs Monate zurückliegt;"@de .

dtd:LA-id3898 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wasserfahrzeug nicht mehr als 100 Betriebsstunden auf dem Wasser zurückgelegt hat oder wenn seine erste Inbetriebnahme im Zeitpunkt des Erwerbs nicht mehr als drei Monate zurückliegt;"@de .

dtd:LA-id3899 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Luftfahrzeug nicht länger als 40 Betriebsstunden genutzt worden ist oder wenn seine erste Inbetriebnahme im Zeitpunkt des Erwerbs nicht mehr als drei Monate zurückliegt."@de .

dtd:LA-id3900 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Inland ansässige ständige diplomatische Missionen und berufskonsularische Vertretungen,"@de .

dtd:LA-id3901 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Inland ansässige zwischenstaatliche Einrichtungen,"@de .

dtd:LA-id3902 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Inland stationierte Streitkräfte anderer Vertragsparteien des Nordatlantikvertrags oder"@de .

dtd:LA-id3903 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Inland stationierte Streitkräfte anderer Mitgliedstaaten, die an einer Verteidigungsanstrengung teilnehmen, die zur Durchführung einer Tätigkeit der Union im Rahmen der Gemeinsamen Sicherheits- und Verteidigungspolitik unternommen wird."@de .

dtd:LA-id3904 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit natürliche Personen, einzeln oder zusammengeschlossen, einem Unternehmen so eingegliedert sind, dass sie den Weisungen des Unternehmers zu folgen verpflichtet sind,"@de .

dtd:LA-id3905 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn eine juristische Person nach dem Gesamtbild der tatsächlichen Verhältnisse finanziell, wirtschaftlich und organisatorisch in das Unternehmen des Organträgers eingegliedert ist (Organschaft). Die Wirkungen der Organschaft sind auf Innenleistungen zwischen den im Inland gelegenen Unternehmensteilen beschränkt. Diese Unternehmensteile sind als ein Unternehmen zu behandeln. Hat der Organträger seine Geschäftsleitung im Ausland, gilt der wirtschaftlich bedeutendste Unternehmensteil im Inland als der Unternehmer."@de .

dtd:LA-id3906 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der von einer juristischen Person des öffentlichen Rechts im Kalenderjahr aus gleichartigen Tätigkeiten erzielte Umsatz voraussichtlich 17 500 Euro jeweils nicht übersteigen wird oder"@de .

dtd:LA-id3907 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vergleichbare, auf privatrechtlicher Grundlage erbrachte Leistungen ohne Recht auf Verzicht (§ 9) einer Steuerbefreiung unterliegen."@de .

dtd:LA-id3908 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Leistungen aufgrund gesetzlicher Bestimmungen nur von juristischen Personen des öffentlichen Rechts erbracht werden dürfen oder"@de .

dtd:LA-id3909 a dtd:LA ;
    dtd:has_DL dtd:DL-id2147 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zusammenarbeit durch gemeinsame spezifische öffentliche Interessen bestimmt wird. Dies ist regelmäßig der Fall, wenn a)die Leistungen auf langfristigen öffentlich-rechtlichen Vereinbarungen beruhen,b)die Leistungen dem Erhalt der öffentlichen Infrastruktur und der Wahrnehmung einer allen Beteiligten obliegenden öffentlichen Aufgabe dienen,c)die Leistungen ausschließlich gegen Kostenerstattung erbracht werden undd)der Leistende gleichartige Leistungen im Wesentlichen an andere juristische Personen des öffentlichen Rechts erbringt."@de .

dtd:LA-id3910 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Leistungen auf langfristigen öffentlich-rechtlichen Vereinbarungen beruhen,"@de .

dtd:LA-id3911 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Leistungen dem Erhalt der öffentlichen Infrastruktur und der Wahrnehmung einer allen Beteiligten obliegenden öffentlichen Aufgabe dienen,"@de .

dtd:LA-id3912 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Leistungen ausschließlich gegen Kostenerstattung erbracht werden und"@de .

dtd:LA-id3913 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Leistende gleichartige Leistungen im Wesentlichen an andere juristische Personen des öffentlichen Rechts erbringt."@de .

dtd:LA-id3914 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id3915 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id3916 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Leistungen der Vermessungs- und Katasterbehörden bei der Wahrnehmung von Aufgaben der Landesvermessung und des Liegenschaftskatasters mit Ausnahme der Amtshilfe;"@de .

dtd:LA-id3917 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Tätigkeit der Bundesanstalt für Landwirtschaft und Ernährung, soweit Aufgaben der Marktordnung, der Vorratshaltung und der Nahrungsmittelhilfe wahrgenommen werden;"@de .

dtd:LA-id3918 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tätigkeiten, die in Anhang I der Richtlinie 2006/112/EG des Rates vom 28. November 2006 über das gemeinsame Mehrwertsteuersystem (ABl. L 347 vom 11.12.2006, S. 1) in der jeweils gültigen Fassung genannt sind, sofern der Umfang dieser Tätigkeiten nicht unbedeutend ist."@de .

dtd:LA-id3919 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Entnahme eines Gegenstands durch einen Unternehmer aus seinem Unternehmen für Zwecke, die außerhalb des Unternehmens liegen;"@de .

dtd:LA-id3920 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die unentgeltliche Zuwendung eines Gegenstands durch einen Unternehmer an sein Personal für dessen privaten Bedarf, sofern keine Aufmerksamkeiten vorliegen;"@de .

dtd:LA-id3921 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jede andere unentgeltliche Zuwendung eines Gegenstands, ausgenommen Geschenke von geringem Wert und Warenmuster für Zwecke des Unternehmens."@de .

dtd:LA-id3922 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Lieferungen, die der Beförderungs- oder Versendungslieferung vorangehen, gelten dort als ausgeführt, wo die Beförderung oder Versendung des Gegenstands beginnt."@de .

dtd:LA-id3923 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Lieferungen, die der Beförderungs- oder Versendungslieferung folgen, gelten dort als ausgeführt, wo die Beförderung oder Versendung des Gegenstands endet."@de .

dtd:LA-id3924 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verwendung eines dem Unternehmen zugeordneten Gegenstands, der zum vollen oder teilweisen Vorsteuerabzug berechtigt hat, durch einen Unternehmer für Zwecke, die außerhalb des Unternehmens liegen, oder für den privaten Bedarf seines Personals, sofern keine Aufmerksamkeiten vorliegen; dies gilt nicht, wenn der Vorsteuerabzug nach § 15 Absatz 1b ausgeschlossen oder wenn eine Vorsteuerberichtigung nach § 15a Absatz 6a durchzuführen ist;"@de .

dtd:LA-id3925 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die unentgeltliche Erbringung einer anderen sonstigen Leistung durch den Unternehmer für Zwecke, die außerhalb des Unternehmens liegen, oder für den privaten Bedarf seines Personals, sofern keine Aufmerksamkeiten vorliegen."@de .

dtd:LA-id3926 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den von jedem an der Erbringung beteiligten Unternehmer ausgestellten oder verfügbar gemachten Rechnungen die sonstige Leistung im Sinne des Satzes 2 und der Erbringer dieser Leistung angegeben sind;"@de .

dtd:LA-id3927 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den dem Leistungsempfänger ausgestellten oder verfügbar gemachten Rechnungen die sonstige Leistung im Sinne des Satzes 2 und der Erbringer dieser Leistung angegeben sind."@de .

dtd:LA-id3928 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Abrechnung gegenüber dem Leistungsempfänger autorisiert,"@de .

dtd:LA-id3929 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erbringung der sonstigen Leistung genehmigt oder"@de .

dtd:LA-id3930 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die allgemeinen Bedingungen der Leistungserbringung festlegt."@de .

dtd:LA-id3931 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verpflichtung besteht, es als vollständige oder teilweise Gegenleistung für eine Lieferung oder sonstige Leistung anzunehmen und"@de .

dtd:LA-id3932 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Liefergegenstand oder die sonstige Leistung oder die Identität des leistenden Unternehmers entweder auf dem Instrument selbst oder in damit zusammenhängenden Unterlagen, einschließlich der Bedingungen für die Nutzung dieses Instruments, angegeben sind."@de .

dtd:LA-id3933 a dtd:LA ;
    dtd:has_DL dtd:DL-id2212 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Eine sonstige Leistung im Zusammenhang mit einem Grundstück wird dort ausgeführt, wo das Grundstück liegt. Als sonstige Leistungen im Zusammenhang mit einem Grundstück sind insbesondere anzusehen: a)sonstige Leistungen der in § 4 Nr. 12 bezeichneten Art,b)sonstige Leistungen im Zusammenhang mit der Veräußerung oder dem Erwerb von Grundstücken,c)sonstige Leistungen, die der Erschließung von Grundstücken oder der Vorbereitung, Koordinierung oder Ausführung von Bauleistungen dienen."@de .

dtd:LA-id3934 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstige Leistungen der in § 4 Nr. 12 bezeichneten Art,"@de .

dtd:LA-id3935 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstige Leistungen im Zusammenhang mit der Veräußerung oder dem Erwerb von Grundstücken,"@de .

dtd:LA-id3936 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstige Leistungen, die der Erschließung von Grundstücken oder der Vorbereitung, Koordinierung oder Ausführung von Bauleistungen dienen."@de .

dtd:LA-id3937 a dtd:LA ;
    dtd:has_DL dtd:DL-id2219 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die kurzfristige Vermietung eines Beförderungsmittels wird an dem Ort ausgeführt, an dem dieses Beförderungsmittel dem Empfänger tatsächlich zur Verfügung gestellt wird. Als kurzfristig im Sinne des Satzes 1 gilt eine Vermietung über einen ununterbrochenen Zeitraum a)von nicht mehr als 90 Tagen bei Wasserfahrzeugen,b)von nicht mehr als 30 Tagen bei anderen Beförderungsmitteln.Die Vermietung eines Beförderungsmittels, die nicht als kurzfristig im Sinne des Satzes 2 anzusehen ist, an einen Empfänger, der weder ein Unternehmer ist, für dessen Unternehmen die Leistung bezogen wird, noch eine nicht unternehmerisch tätige juristische Person, der eine Umsatzsteuer-Identifikationsnummer erteilt worden ist, wird an dem Ort erbracht, an dem der Empfänger seinen Wohnsitz oder Sitz hat. Handelt es sich bei dem Beförderungsmittel um ein Sportboot, wird abweichend von Satz 3 die Vermietungsleistung an dem Ort ausgeführt, an dem das Sportboot dem Empfänger tatsächlich zur Verfügung gestellt wird, wenn sich auch der Sitz, die Geschäftsleitung oder eine Betriebsstätte des Unternehmers, von wo aus diese Leistung tatsächlich erbracht wird, an diesem Ort befindet."@de .

dtd:LA-id3938 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von nicht mehr als 90 Tagen bei Wasserfahrzeugen,"@de .

dtd:LA-id3939 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von nicht mehr als 30 Tagen bei anderen Beförderungsmitteln."@de .

dtd:LA-id3940 a dtd:LA ;
    dtd:has_DL dtd:DL-id2224 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die folgenden sonstigen Leistungen werden dort ausgeführt, wo sie vom Unternehmer tatsächlich erbracht werden: a)kulturelle, künstlerische, wissenschaftliche, unterrichtende, sportliche, unterhaltende oder ähnliche Leistungen, wie Leistungen im Zusammenhang mit Messen und Ausstellungen, einschließlich der Leistungen der jeweiligen Veranstalter sowie die damit zusammenhängenden Tätigkeiten, die für die Ausübung der Leistungen unerlässlich sind, an einen Empfänger, der weder ein Unternehmer ist, für dessen Unternehmen die Leistung bezogen wird, noch eine nicht unternehmerisch tätige juristische Person, der eine Umsatzsteuer-Identifikationsnummer erteilt worden ist,b)die Abgabe von Speisen und Getränken zum Verzehr an Ort und Stelle (Restaurationsleistung), wenn diese Abgabe nicht an Bord eines Schiffs, in einem Luftfahrzeug oder in einer Eisenbahn während einer Beförderung innerhalb des Gemeinschaftsgebiets erfolgt,c)Arbeiten an beweglichen körperlichen Gegenständen und die Begutachtung dieser Gegenstände für einen Empfänger, der weder ein Unternehmer ist, für dessen Unternehmen die Leistung ausgeführt wird, noch eine nicht unternehmerisch tätige juristische Person, der eine Umsatzsteuer-Identifikationsnummer erteilt worden ist."@de .

dtd:LA-id3941 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kulturelle, künstlerische, wissenschaftliche, unterrichtende, sportliche, unterhaltende oder ähnliche Leistungen, wie Leistungen im Zusammenhang mit Messen und Ausstellungen, einschließlich der Leistungen der jeweiligen Veranstalter sowie die damit zusammenhängenden Tätigkeiten, die für die Ausübung der Leistungen unerlässlich sind, an einen Empfänger, der weder ein Unternehmer ist, für dessen Unternehmen die Leistung bezogen wird, noch eine nicht unternehmerisch tätige juristische Person, der eine Umsatzsteuer-Identifikationsnummer erteilt worden ist,"@de .

dtd:LA-id3942 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Abgabe von Speisen und Getränken zum Verzehr an Ort und Stelle (Restaurationsleistung), wenn diese Abgabe nicht an Bord eines Schiffs, in einem Luftfahrzeug oder in einer Eisenbahn während einer Beförderung innerhalb des Gemeinschaftsgebiets erfolgt,"@de .

dtd:LA-id3943 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arbeiten an beweglichen körperlichen Gegenständen und die Begutachtung dieser Gegenstände für einen Empfänger, der weder ein Unternehmer ist, für dessen Unternehmen die Leistung ausgeführt wird, noch eine nicht unternehmerisch tätige juristische Person, der eine Umsatzsteuer-Identifikationsnummer erteilt worden ist."@de .

dtd:LA-id3944 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Eine Vermittlungsleistung an einen Empfänger, der weder ein Unternehmer ist, für dessen Unternehmen die Leistung bezogen wird, noch eine nicht unternehmerisch tätige juristische Person, der eine Umsatzsteuer-Identifikationsnummer erteilt worden ist, wird an dem Ort erbracht, an dem der vermittelte Umsatz als ausgeführt gilt."@de .

dtd:LA-id3945 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Einräumung der Eintrittsberechtigung zu kulturellen, künstlerischen, wissenschaftlichen, unterrichtenden, sportlichen, unterhaltenden oder ähnlichen Veranstaltungen, wie Messen und Ausstellungen, sowie die damit zusammenhängenden sonstigen Leistungen an einen Unternehmer für dessen Unternehmen oder an eine nicht unternehmerisch tätige juristische Person, der eine Umsatzsteuer-Identifikationsnummer erteilt worden ist, wird an dem Ort erbracht, an dem die Veranstaltung tatsächlich durchgeführt wird."@de .

dtd:LA-id3946 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einräumung, Übertragung und Wahrnehmung von Patenten, Urheberrechten, Markenrechten und ähnlichen Rechten;"@de .

dtd:LA-id3947 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die sonstigen Leistungen, die der Werbung oder der Öffentlichkeitsarbeit dienen, einschließlich der Leistungen der Werbungsmittler und der Werbeagenturen;"@de .

dtd:LA-id3948 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die sonstigen Leistungen aus der Tätigkeit als Rechtsanwalt, Patentanwalt, Steuerberater, Steuerbevollmächtigter, Wirtschaftsprüfer, vereidigter Buchprüfer, Sachverständiger, Ingenieur, Aufsichtsratsmitglied, Dolmetscher und Übersetzer sowie ähnliche Leistungen anderer Unternehmer, insbesondere die rechtliche, wirtschaftliche und technische Beratung;"@de .

dtd:LA-id3949 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Datenverarbeitung;"@de .

dtd:LA-id3950 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Überlassung von Informationen einschließlich gewerblicher Verfahren und Erfahrungen;"@de .

dtd:LA-id3951 a dtd:LA ;
    dtd:has_DL dtd:DL-id2260 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "a)Bank- und Finanzumsätze, insbesondere der in § 4 Nummer 8 Buchstabe a bis h bezeichneten Art und die Verwaltung von Krediten und Kreditsicherheiten, sowie Versicherungsumsätze der in § 4 Nummer 10 bezeichneten Art,b)die sonstigen Leistungen im Geschäft mit Gold, Silber und Platin. Das gilt nicht für Münzen und Medaillen aus diesen Edelmetallen;"@de .

dtd:LA-id3952 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bank- und Finanzumsätze, insbesondere der in § 4 Nummer 8 Buchstabe a bis h bezeichneten Art und die Verwaltung von Krediten und Kreditsicherheiten, sowie Versicherungsumsätze der in § 4 Nummer 10 bezeichneten Art,"@de .

dtd:LA-id3953 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die sonstigen Leistungen im Geschäft mit Gold, Silber und Platin. Das gilt nicht für Münzen und Medaillen aus diesen Edelmetallen;"@de .

dtd:LA-id3954 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gestellung von Personal;"@de .

dtd:LA-id3955 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Verzicht auf Ausübung eines der in Nummer 1 bezeichneten Rechte;"@de .

dtd:LA-id3956 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Verzicht, ganz oder teilweise eine gewerbliche oder berufliche Tätigkeit auszuüben;"@de .

dtd:LA-id3957 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vermietung beweglicher körperlicher Gegenstände, ausgenommen Beförderungsmittel;"@de .

dtd:LA-id3958 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id3959 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id3960 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id3961 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gewährung des Zugangs zum Erdgasnetz, zum Elektrizitätsnetz oder zu Wärme- oder Kältenetzen und die Fernleitung, die Übertragung oder Verteilung über diese Netze sowie die Erbringung anderer damit unmittelbar zusammenhängender sonstiger Leistungen."@de .

dtd:LA-id3962 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kein Unternehmer, für dessen Unternehmen die Leistung bezogen wird,"@de .

dtd:LA-id3963 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "keine ausschließlich nicht unternehmerisch tätige juristische Person, der eine Umsatzsteuer-Identifikationsnummer erteilt worden ist,"@de .

dtd:LA-id3964 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "keine juristische Person, die sowohl unternehmerisch als auch nicht unternehmerisch tätig ist, bei der die Leistung nicht ausschließlich für den privaten Bedarf des Personals oder eines Gesellschafters bestimmt ist,"@de .

dtd:LA-id3965 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die sonstigen Leistungen auf dem Gebiet der Telekommunikation;"@de .

dtd:LA-id3966 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Rundfunk- und Fernsehdienstleistungen;"@de .

dtd:LA-id3967 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die auf elektronischem Weg erbrachten sonstigen Leistungen."@de .

dtd:LA-id3968 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine in Absatz 3 Nr. 2 bezeichnete Leistung oder die langfristige Vermietung eines Beförderungsmittels,"@de .

dtd:LA-id3969 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine in Absatz 4 Satz 2 Nummer 1 bis 10 bezeichnete sonstige Leistung an eine im Inland ansässige juristische Person des öffentlichen Rechts oder"@de .

dtd:LA-id3970 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine in Absatz 5 Satz 2 Nummer 1 und 2 bezeichnete Leistung,"@de .

dtd:LA-id3971 a dtd:LA ;
    dtd:has_Value "kurze inländische Beförderungsstrecken als ausländische und kurze ausländische Beförderungsstrecken als inländische angesehen werden;"@de .

dtd:LA-id3972 a dtd:LA ;
    dtd:has_Value "Beförderungen über kurze Beförderungsstrecken in den in § 1 Abs. 3 bezeichneten Gebieten nicht wie Umsätze im Inland behandelt werden."@de .

dtd:LA-id3973 a dtd:LA ;

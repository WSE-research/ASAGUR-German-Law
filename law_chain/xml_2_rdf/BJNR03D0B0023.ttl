    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für den Leistungsbericht nach Artikel 134 der Verordnung (EU) 2021/2115 erforderlichen Angaben,"@de .

dtd:LA-id622 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Plausibilität des Prüfberichts einer gesetzlich zugelassenen Prüfstelle zu den getätigten Ausgaben,"@de .

dtd:LA-id623 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die eindeutige Zuordnung der geltend gemachten Ausgaben und erbrachten Dienstleistungen,"@de .

dtd:LA-id624 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Übereinstimmung der durchgeführten und geplanten Maßnahmen mit den im genehmigten operationellen Programm aufgeführten Maßnahmen,"@de .

dtd:LA-id625 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angaben zu den Mitgliedern der Erzeugerorganisationen im Hinblick auf Größe und Förderfähigkeit der Flächen gemäß § 11 dieser Verordnung,"@de .

dtd:LA-id626 a dtd:LA ;
    dtd:has_I dtd:I-id285 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ergebnisse der Flächenkontrollen der Länder gemäß § 27 Absatz 5 und § 29 Absatz 5 der GAPInVeKoS-Verordnung und der Kontrollen der Prüfer der Bundesanstalt,"@de .

dtd:LA-id627 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einhaltung der vorgeschriebenen finanziellen und sonstigen Begrenzungen und"@de .

dtd:LA-id628 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ob die Maßnahmen, für die eine Beihilfe beantragt worden ist, im Einklang stehen mit den geltenden nationalen und europäischen Rechtsvorschriften, insbesondere mit den verbindlichen Normen, die in nationalen Rechtsvorschriften oder im nationalen Strategieplan und den nationalen Förderleitlinien festgelegt sind."@de .

dtd:LA-id629 a dtd:LA ;
    dtd:has_Value "die Höhe der Beihilfe,"@de .

dtd:LA-id630 a dtd:LA ;
    dtd:has_Value "die Kontrollergebnisse der Vorjahre und"@de .

dtd:LA-id631 a dtd:LA ;
    dtd:has_Value "etwaige Hinweise auf Unregelmäßigkeiten."@de .

dtd:LA-id632 a dtd:LA ;
    dtd:has_Value "die geprüften Beihilferegelungen und Anträge,"@de .

dtd:LA-id633 a dtd:LA ;
    dtd:has_Value "die Namen und die Funktionen der anwesenden Personen,"@de .

dtd:LA-id634 a dtd:LA ;
    dtd:has_Value "die geprüften Maßnahmen und Unterlagen, einschließlich des dabei zugrunde gelegten Prüfpfads und der überprüften Nachweise, und"@de .


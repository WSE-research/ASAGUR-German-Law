dtd:LA-id878 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die beanstandeten Bestimmungen der Allgemeinen Geschäftsbedingungen im Wortlaut,"@de .

dtd:LA-id879 a dtd:LA ;
    dtd:has_Size dtd:normal ;

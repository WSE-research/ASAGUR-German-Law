    dtd:has_Value "die nach den §§ 307 bis 309 des Bürgerlichen Gesetzbuchs unwirksamen Bestimmungen in Allgemeinen Geschäftsbedingungen verwendet wurden,"@de .

dtd:LA-id860 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegen Verbraucherschutzgesetze verstoßen wurde oder"@de .

dtd:LA-id861 a dtd:LA ;
    dtd:has_Size dtd:normal ;

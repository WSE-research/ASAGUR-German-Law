dtd:LA-id862 a dtd:LA ;
    dtd:has_Value "die Bezeichnung des Antragstellers und des Antraggegners,"@de .

dtd:LA-id863 a dtd:LA ;
    dtd:has_Value "die Bezeichnung des Gerichts,"@de .

dtd:LA-id864 a dtd:LA ;
    dtd:has_Value "das Aktenzeichen des Verfahrens,"@de .

dtd:LA-id865 a dtd:LA ;
    dtd:has_Value "die Angabe der behaupteten Zuwiderhandlung, die Anlass des Antrags auf Erlass einer einstweiligen Verfügung ist,"@de .

dtd:LA-id866 a dtd:LA ;
    dtd:has_Value "das Datum des Eingangs des Antrags auf Erlass der einstweiligen Verfügung beim Gericht und"@de .

dtd:LA-id867 a dtd:LA ;

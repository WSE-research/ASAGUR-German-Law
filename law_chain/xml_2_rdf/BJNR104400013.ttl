dtd:Footnote-id2723 a dtd:Footnote ;
    dtd:has_BR dtd:BR-id373 ;
    dtd:has_FnZ "*"@de ;
    dtd:has_Group dtd:column ;
    dtd:has_ID "f779978_04_BJNR104400013BJNE000302130"@de ;
    dtd:has_Pos dtd:normal ;
    dtd:has_Value "Hinweis der Schriftleitung:Abfallverzeichnis-Verordnung vom 10. Dezember 2001 (BGBl. I S. 3379), die zuletzt durch Artikel 5 Absatz 22 des Gesetzes vom 24. Februar 2012 (BGBl. I S. 212) geändert worden ist."@de .

dtd:Footnote-id2725 a dtd:Footnote ;
    dtd:has_FnZ "1"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F829040_02_BJNR104400013BJNE003800130"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Der Energieeffizienzwert gilt nur, wenn ein Abhitzekessel anwendbar ist."@de .

dtd:Footnote-id2726 a dtd:Footnote ;
    dtd:has_FnZ "2"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F829040_03_BJNR104400013BJNE003800130"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Die Energieeffizienzwerte für den elektrischen Bruttowirkungsgrad gelten nur für Anlagen oder Teile von Anlagen, die mit einer Kondensationsturbine Strom erzeugen, unter Berücksichtigung möglicher Entnahmen vor Entnahmekondensationsturbinen."@de .

dtd:Footnote-id2727 a dtd:Footnote ;
    dtd:has_FnZ "3"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F829040_04_BJNR104400013BJNE003800130"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Ein Energieeffizienzwert von bis zu 35 Prozent kann durch höhere Dampfzustände erreicht werden."@de .

dtd:Footnote-id2728 a dtd:Footnote ;
    dtd:has_FnZ "4"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F829040_05_BJNR104400013BJNE003800130"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Die Energieeffizienzwerte für die Bruttoenergieeffizienz gelten nur für Anlagen oder Teile von Anlagen, die nur Wärme erzeugen oder die mit einer Gegendruckturbine Strom und aus dem Dampf aus der Turbine Wärme erzeugen."@de .

dtd:Footnote-id2729 a dtd:Footnote ;
    dtd:has_FnZ "5"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F829040_06_BJNR104400013BJNE003800130"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Eine höhere Bruttoenergieeffizienz, die sogar über 100 Prozent hinausgeht, kann erreicht werden, wenn ein Abgaskondensator verwendet wird."@de .

dtd:Footnote-id2730 a dtd:Footnote ;
    dtd:has_FnZ "6"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F829040_07_BJNR104400013BJNE003800130"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Bei der Verbrennung von Klärschlamm ist der Kesselwirkungsgrad stark abhängig vom Wassergehalt des Klärschlamms, der in die Feuerung eingeleitet wird."@de .

dtd:Footnote-id2731 a dtd:Footnote ;
    dtd:has_FnZ "*"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F829040_08_BJNR104400013BJNE003800130"@de ;
    dtd:has_Pos dtd:normal ;

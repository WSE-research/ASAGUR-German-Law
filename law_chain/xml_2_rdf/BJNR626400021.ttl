dtd:LA-id441 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei nach § 6 Absatz 1 Nummer 2 der Coronavirus-Testverordnung in der bis zum 28. Februar 2023 geltenden Fassung beauftragten Leistungserbringern der Nachweis der Beauftragung,"@de .

dtd:LA-id442 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Leistungen nach § 4a der Coronavirus-Testverordnung in der bis zum 28. Februar 2023 geltenden Fassung die Öffnungszeiten des Leistungserbringers je Tag und die Anzahl der Tests durchführenden Personen je Tag,"@de .

dtd:LA-id443 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Abrechnung von Leistungen nach § 12 Absatz 3 das einrichtungs- oder unternehmensbezogene Testkonzept und für jede abgerechnete Leistung die Unterschrift der die Testung durchführenden Person,"@de .

dtd:LA-id444 a dtd:LA ;

dtd:LA-id207 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben zu möglichen sonstigen Kosten, insbesondere zu Kosten, die einmalig oder aus besonderem Anlass entstehen können;"@de .

dtd:LA-id208 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben über die für die Überschussermittlung und Überschussbeteiligung geltenden Berechnungsgrundsätze und Maßstäbe;"@de .

dtd:LA-id209 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:LA-id4199 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Auswahl geeigneter Arbeitsmittel und deren sichere Verwendung,"@de .

dtd:LA-id4200 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für den vorgesehenen Verwendungszweck geeignete Gestaltung von Arbeits- und Fertigungsverfahren sowie"@de .

dtd:LA-id4201 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Qualifikation und Unterweisung der Beschäftigten."@de .

dtd:LA-id4202 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer, ohne Arbeitgeber zu sein, zu gewerblichen oder wirtschaftlichen Zwecken eine überwachungsbedürftige Anlage verwendet, sowie"@de .

dtd:LA-id4203 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Auftraggeber und der Zwischenmeister im Sinne des Heimarbeitsgesetzes."@de .

dtd:LA-id4204 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schülerinnen und Schüler sowie Studierende,"@de .

dtd:LA-id4205 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Heimarbeit Beschäftigte nach § 1 Absatz 1 des Heimarbeitsgesetzes sowie"@de .

dtd:LA-id4206 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstige Personen, insbesondere Personen, die in wissenschaftlichen Einrichtungen tätig sind."@de .

dtd:LA-id4207 a dtd:LA ;
    dtd:has_Value "den Arbeitsmitteln selbst,"@de .

dtd:LA-id4208 a dtd:LA ;
    dtd:has_Value "der Arbeitsumgebung und"@de .

dtd:LA-id4209 a dtd:LA ;
    dtd:has_Value "den Arbeitsgegenständen, an denen Tätigkeiten mit Arbeitsmitteln durchgeführt werden."@de .

dtd:LA-id4210 a dtd:LA ;
    dtd:has_Value "die Gebrauchstauglichkeit von Arbeitsmitteln einschließlich der ergonomischen, alters- und alternsgerechten Gestaltung,"@de .

dtd:LA-id4211 a dtd:LA ;
    dtd:has_Value "die sicherheitsrelevanten einschließlich der ergonomischen Zusammenhänge zwischen Arbeitsplatz, Arbeitsmittel, Arbeitsverfahren, Arbeitsorganisation, Arbeitsablauf, Arbeitszeit und Arbeitsaufgabe,"@de .

dtd:LA-id4212 a dtd:LA ;
    dtd:has_Value "die physischen und psychischen Belastungen der Beschäftigten, die bei der Verwendung von Arbeitsmitteln auftreten,"@de .

dtd:LA-id4213 a dtd:LA ;
    dtd:has_Value "vorhersehbare Betriebsstörungen und die Gefährdung bei Maßnahmen zu deren Beseitigung."@de .

dtd:LA-id4214 a dtd:LA ;
    dtd:has_Value "sicherheitsrelevante Veränderungen der Arbeitsbedingungen einschließlich der Änderung von Arbeitsmitteln dies erfordern,"@de .

dtd:LA-id4215 a dtd:LA ;
    dtd:has_Value "neue Informationen, insbesondere Erkenntnisse aus dem Unfallgeschehen oder aus der arbeitsmedizinischen Vorsorge, vorliegen oder"@de .

dtd:LA-id4216 a dtd:LA ;
    dtd:has_Value "die Überprüfung der Wirksamkeit der Schutzmaßnahmen nach § 4 Absatz 5 ergeben hat, dass die festgelegten Schutzmaßnahmen nicht wirksam oder nicht ausreichend sind."@de .

dtd:LA-id4217 a dtd:LA ;
    dtd:has_Value "die Gefährdungen, die bei der Verwendung der Arbeitsmittel auftreten,"@de .

dtd:LA-id4218 a dtd:LA ;
    dtd:has_Value "die zu ergreifenden Schutzmaßnahmen,"@de .

dtd:LA-id4219 a dtd:LA ;
    dtd:has_Value "wie die Anforderungen dieser Verordnung eingehalten werden, wenn von den nach § 21 Absatz 6 Nummer 1 bekannt gegebenen Regeln und Erkenntnissen abgewichen wird,"@de .

dtd:LA-id4220 a dtd:LA ;
    dtd:has_Value "Art und Umfang der erforderlichen Prüfungen sowie die Fristen der wiederkehrenden Prüfungen (Absatz 6 Satz 1) und"@de .

dtd:LA-id4221 a dtd:LA ;
    dtd:has_Value "das Ergebnis der Überprüfung der Wirksamkeit der Schutzmaßnahmen nach § 4 Absatz 5."@de .

dtd:LA-id4222 a dtd:LA ;
    dtd:has_Value "eine Gefährdungsbeurteilung durchgeführt hat,"@de .

dtd:LA-id4223 a dtd:LA ;
    dtd:has_Value "die dabei ermittelten Schutzmaßnahmen nach dem Stand der Technik getroffen hat und"@de .

dtd:LA-id4224 a dtd:LA ;
    dtd:has_Value "festgestellt hat, dass die Verwendung der Arbeitsmittel nach dem Stand der Technik sicher ist."@de .

dtd:LA-id4225 a dtd:LA ;
    dtd:has_Value "für die Art der auszuführenden Arbeiten geeignet sein,"@de .

dtd:LA-id4226 a dtd:LA ;
    dtd:has_Value "den gegebenen Einsatzbedingungen und den vorhersehbaren Beanspruchungen angepasst sein und"@de .

dtd:LA-id4227 a dtd:LA ;
    dtd:has_Value "über die erforderlichen sicherheitsrelevanten Ausrüstungen verfügen,"@de .

dtd:LA-id4228 a dtd:LA ;
    dtd:has_Value "die Arbeitsmittel einschließlich ihrer Schnittstelle zum Menschen müssen an die körperlichen Eigenschaften und die Kompetenz der Beschäftigten angepasst sein sowie biomechanische Belastungen bei der Verwendung vermieden sein. Zu berücksichtigen sind hierbei die Arbeitsumgebung, die Lage der Zugriffstellen und des Schwerpunktes des Arbeitsmittels, die erforderliche Körperhaltung, die Körperbewegung, die Entfernung zum Körper, die benötigte persönliche Schutzausrüstung sowie die psychische Belastung der Beschäftigten,"@de .

dtd:LA-id4229 a dtd:LA ;
    dtd:has_Value "die Beschäftigten müssen über einen ausreichenden Bewegungsfreiraum verfügen,"@de .

dtd:LA-id4230 a dtd:LA ;
    dtd:has_Value "es sind ein Arbeitstempo und ein Arbeitsrhythmus zu vermeiden, die zu Gefährdungen der Beschäftigten führen können,"@de .

dtd:LA-id4231 a dtd:LA ;
    dtd:has_Value "es sind Bedien- und Überwachungstätigkeiten zu vermeiden, die eine uneingeschränkte und dauernde Aufmerksamkeit erfordern."@de .

dtd:LA-id4232 a dtd:LA ;
    dtd:has_Value "die Errichtung von Arbeitsmitteln, der Auf- und Abbau, die Erprobung sowie die Instandhaltung und Prüfung von Arbeitsmitteln unter Berücksichtigung der sicherheitsrelevanten Aufstellungs- und Umgebungsbedingungen nach dem Stand der Technik erfolgen und sicher durchgeführt werden,"@de .

dtd:LA-id4233 a dtd:LA ;
    dtd:has_Value "erforderliche Sicherheits- und Schutzabstände eingehalten werden und"@de .

dtd:LA-id4234 a dtd:LA ;
    dtd:has_Value "alle verwendeten oder erzeugten Energieformen und Materialien sicher zu- und abgeführt werden können."@de .

dtd:LA-id4235 a dtd:LA ;
    dtd:has_Value "die Arbeitsmittel mindestens den sicherheitstechnischen Anforderungen der für sie zum Zeitpunkt der Verwendung geltenden Rechtsvorschriften zum Bereitstellen von Arbeitsmitteln auf dem Markt entsprechen,"@de .

dtd:LA-id4236 a dtd:LA ;
    dtd:has_Value "die Arbeitsmittel ausschließlich bestimmungsgemäß entsprechend den Vorgaben des Herstellers verwendet werden,"@de .

dtd:LA-id4237 a dtd:LA ;
    dtd:has_Value "keine zusätzlichen Gefährdungen der Beschäftigten unter Berücksichtigung der Arbeitsumgebung, der Arbeitsgegenstände, der Arbeitsabläufe sowie der Dauer und der zeitlichen Lage der Arbeitszeit auftreten und"@de .

dtd:LA-id4238 a dtd:LA ;
    dtd:has_Value "Instandhaltungsmaßnahmen nach § 10 getroffen und Prüfungen nach § 14 durchgeführt werden."@de .

dtd:LA-id4239 a dtd:LA ;
    dtd:has_Value "die von ihnen ausgehenden oder verwendeten Energien,"@de .

dtd:LA-id4240 a dtd:LA ;
    dtd:has_Value "direktes oder indirektes Berühren von Teilen, die unter elektrischer Spannung stehen, oder"@de .

dtd:LA-id4241 a dtd:LA ;
    dtd:has_Value "Störungen ihrer Energieversorgung."@de .

dtd:LA-id4242 a dtd:LA ;
    dtd:has_Value "als solche deutlich erkennbar, außerhalb des Gefahrenbereichs angeordnet und leicht und ohne Gefährdung erreichbar sein; ihre Betätigung darf zu keiner zusätzlichen Gefährdung führen,"@de .

dtd:LA-id4243 a dtd:LA ;
    dtd:has_Value "sicher beschaffen und auf vorhersehbare Störungen, Beanspruchungen und Zwänge ausgelegt sein,"@de .

dtd:LA-id4244 a dtd:LA ;
    dtd:has_Value "gegen unbeabsichtigtes oder unbefugtes Betätigen gesichert sein."@de .

dtd:LA-id4245 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arbeitsmittel ausreichend standsicher sein und, falls erforderlich, gegen unbeabsichtigte Positions- und Lageänderungen stabilisiert werden,"@de .

dtd:LA-id4246 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arbeitsmittel mit den erforderlichen sicherheitstechnischen Ausrüstungen versehen sein,"@de .

dtd:LA-id4247 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arbeitsmittel, ihre Teile und die Verbindungen untereinander den Belastungen aus inneren und äußeren Kräften standhalten,"@de .

dtd:LA-id4248 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schutzeinrichtungen bei Splitter- oder Bruchgefahr sowie gegen herabfallende oder herausschleudernde Gegenstände vorhanden sein,"@de .

dtd:LA-id4249 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sichere Zugänge zu Arbeitsplätzen an und in Arbeitsmitteln gewährleistet und ein gefahrloser Aufenthalt dort möglich sein,"@de .

dtd:LA-id4250 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schutzmaßnahmen getroffen werden, die sowohl einen Absturz von Beschäftigten als auch von Arbeitsmitteln sicher verhindern,"@de .

dtd:LA-id4251 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen getroffen werden, damit Personen nicht unbeabsichtigt in Arbeitsmitteln eingeschlossen werden; im Notfall müssen eingeschlossene Personen aus Arbeitsmitteln in angemessener Zeit befreit werden können,"@de .

dtd:LA-id4252 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schutzmaßnahmen gegen Gefährdungen durch bewegliche Teile von Arbeitsmitteln und gegen Blockaden solcher Teile getroffen werden; hierzu gehören auch Maßnahmen, die den unbeabsichtigten Zugang zum Gefahrenbereich von beweglichen Teilen von Arbeitsmitteln verhindern oder die bewegliche Teile vor dem Erreichen des Gefahrenbereichs stillsetzen,"@de .

dtd:LA-id4253 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen getroffen werden, die verhindern, dass die sichere Verwendung der Arbeitsmittel durch äußere Einwirkungen beeinträchtigt wird,"@de .

dtd:LA-id4254 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leitungen so verlegt sein, dass Gefährdungen vermieden werden, und"@de .

dtd:LA-id4255 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen getroffen werden, die verhindern, dass außer Betrieb gesetzte Arbeitsmittel zu Gefährdungen führen."@de .

dtd:LA-id4256 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen ausreichenden Schutz gegen Gefährdungen bieten,"@de .

dtd:LA-id4257 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "stabil gebaut sind,"@de .

dtd:LA-id4258 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sicher in Position gehalten werden,"@de .

dtd:LA-id4259 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Eingriffe, die für den Einbau oder den Austausch von Teilen sowie für Instandhaltungsarbeiten erforderlich sind, möglichst ohne Demontage der Schutzeinrichtungen zulassen,"@de .

dtd:LA-id4260 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "keine zusätzlichen Gefährdungen verursachen,"@de .

dtd:LA-id4261 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht auf einfache Weise umgangen oder unwirksam gemacht werden können und"@de .

dtd:LA-id4262 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beobachtung und Durchführung des Arbeitszyklus nicht mehr als notwendig einschränken."@de .

dtd:LA-id4263 a dtd:LA ;
    dtd:has_Value "die Verantwortlichkeiten für die Durchführung der erforderlichen Sicherungsmaßnahmen festzulegen,"@de .

dtd:LA-id4264 a dtd:LA ;
    dtd:has_Value "eine ausreichende Kommunikation zwischen Bedien- und Instandhaltungspersonal sicherzustellen,"@de .

dtd:LA-id4265 a dtd:LA ;
    dtd:has_Value "den Arbeitsbereich während der Instandhaltungsarbeiten abzusichern,"@de .

dtd:LA-id4266 a dtd:LA ;
    dtd:has_Value "das Betreten des Arbeitsbereichs durch Unbefugte zu verhindern, soweit das nach der Gefährdungsbeurteilung erforderlich ist,"@de .

dtd:LA-id4267 a dtd:LA ;
    dtd:has_Value "sichere Zugänge für das Instandhaltungspersonal vorzusehen,"@de .

dtd:LA-id4268 a dtd:LA ;
    dtd:has_Value "Gefährdungen durch bewegte oder angehobene Arbeitsmittel oder deren Teile sowie durch gefährliche Energien oder Stoffe zu vermeiden,"@de .

dtd:LA-id4269 a dtd:LA ;
    dtd:has_Value "dafür zu sorgen, dass Einrichtungen vorhanden sind, mit denen Energien beseitigt werden können, die nach einer Trennung des instand zu haltenden Arbeitsmittels von Energiequellen noch gespeichert sind; diese Einrichtungen sind entsprechend zu kennzeichnen,"@de .

dtd:LA-id4270 a dtd:LA ;
    dtd:has_Value "sichere Arbeitsverfahren für solche Arbeitsbedingungen festzulegen, die vom Normalzustand abweichen,"@de .

dtd:LA-id4271 a dtd:LA ;
    dtd:has_Value "erforderliche Warn- und Gefahrenhinweise bezogen auf Instandhaltungsarbeiten an den Arbeitsmitteln zur Verfügung zu stellen,"@de .

dtd:LA-id4272 a dtd:LA ;
    dtd:has_Value "dafür zu sorgen, dass nur geeignete Geräte und Werkzeuge und eine geeignete persönliche Schutzausrüstung verwendet werden,"@de .

dtd:LA-id4273 a dtd:LA ;
    dtd:has_Value "bei Auftreten oder Bildung gefährlicher explosionsfähiger Atmosphäre Schutzmaßnahmen entsprechend § 9 Absatz 4 Satz 1 zu treffen,"@de .

dtd:LA-id4274 a dtd:LA ;
    dtd:has_Value "Systeme für die Freigabe bestimmter Arbeiten anzuwenden."@de .

dtd:LA-id4275 a dtd:LA ;
    dtd:has_Value "eine Vorabmitteilung über einschlägige Gefährdungen bei der Arbeit, über Maßnahmen zur Feststellung von Gefährdungen sowie über Vorsichtsmaßregeln und Verfahren, damit die Rettungsdienste ihre eigenen Abhilfe- und Sicherheitsmaßnahmen vorbereiten können,"@de .

dtd:LA-id4276 a dtd:LA ;
    dtd:has_Value "Informationen über einschlägige und spezifische Gefährdungen, die bei einem Unfall oder Notfall auftreten können, einschließlich der Informationen über die Maßnahmen nach den Absätzen 1 und 2."@de .

dtd:LA-id4277 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorhandene Gefährdungen bei der Verwendung von Arbeitsmitteln einschließlich damit verbundener Gefährdungen durch die Arbeitsumgebung,"@de .

dtd:LA-id4278 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erforderliche Schutzmaßnahmen und Verhaltensregelungen und"@de .

dtd:LA-id4279 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen bei Betriebsstörungen, Unfällen und zur Ersten Hilfe bei Notfällen."@de .

dtd:LA-id4280 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kontrolle der vorschriftsmäßigen Montage oder Installation und der sicheren Funktion dieser Arbeitsmittel,"@de .

dtd:LA-id4281 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die rechtzeitige Feststellung von Schäden,"@de .

dtd:LA-id4282 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Feststellung, ob die getroffenen sicherheitstechnischen Maßnahmen geeignet und funktionsfähig sind."@de .

dtd:LA-id4283 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Art der Prüfung,"@de .

dtd:LA-id4284 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Prüfumfang,"@de .

dtd:LA-id4285 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ergebnis der Prüfung und"@de .

dtd:LA-id4286 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name und Unterschrift der zur Prüfung befähigten Person; bei ausschließlich elektronisch übermittelten Dokumenten elektronische Signatur."@de .

dtd:LA-id4287 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ob die für die Prüfung benötigten technischen Unterlagen, wie beispielsweise eine EG-Konformitätserklärung, vorhanden sind und ihr Inhalt plausibel ist und"@de .

dtd:LA-id4288 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ob die Anlage einschließlich der Anlagenteile entsprechend dieser Verordnung errichtet oder geändert worden ist und sich auch unter Berücksichtigung der Aufstellbedingungen in einem sicheren Zustand befindet."@de .

dtd:LA-id4289 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anlagenidentifikation,"@de .

dtd:LA-id4290 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Prüfdatum,"@de .

dtd:LA-id4291 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Art der Prüfung,"@de .

dtd:LA-id4292 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Prüfungsgrundlagen,"@de .

dtd:LA-id4293 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Prüfumfang,"@de .

dtd:LA-id4294 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Eignung und Funktionsfähigkeit der technischen Maßnahmen sowie Eignung der organisatorischen Maßnahmen,"@de .

dtd:LA-id4295 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ergebnis der Prüfung,"@de .

dtd:LA-id4296 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Fristen für die nächsten wiederkehrenden Prüfungen nach § 15 Absatz 2 und § 16 Absatz 2 sowie"@de .

dtd:LA-id4297 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name und Unterschrift des Prüfers, bei Prüfung durch zugelassene Überwachungsstellen zusätzlich Name der zugelassenen Überwachungsstelle; bei ausschließlich elektronisch übermittelten Dokumenten die elektronische Signatur."@de .

dtd:LA-id4298 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dampfkesselanlagen nach Anhang 2 Abschnitt 4 Nummer 2.1 Satz 1 Buchstabe a, die nach Artikel 13 in Verbindung mit Anhang II Diagramm 5 der Richtlinie 2014/68/EU des Europäischen Parlaments und des Rates vom 15. Mai 2014 zur Harmonisierung der Rechtsvorschriften der Mitgliedstaaten über die Bereitstellung von Druckgeräten auf dem Markt (ABl. L 189 vom 27.6.2014, S. 164) in die Kategorie IV einzustufen sind,"@de .

dtd:LA-id4299 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:Footnote-id1590 a dtd:Footnote ;
    dtd:has_FnZ "*"@de ;
    dtd:has_Group dtd:column ;
    dtd:has_ID "F775557_01_BJNR141200011"@de ;
    dtd:has_Pos dtd:normal ;
    dtd:has_Postfix ")"@de ;
    dtd:has_Value "Die §§ 5 und 6 dieser Verordnung dienen der Umsetzung der Richtlinie 2006/123/EG des Europäischen Parlaments und des Rates vom 12. Dezember 2006 über Dienstleistungen im Binnenmarkt (ABl. L 376 vom 27.12.2006, S. 36) sowie der Richtlinie 2005/36/EG des Europäischen Parlaments und des Rates vom 7. September 2005 über die Anerkennung von Berufsqualifikationen (ABl. L 255 vom 30.9.2005, S. 22, L 271 vom 16.10.2007, S. 18, L 93 vom 4.4.2008, S. 28, L 33 vom 3.2.2009, S. 49), die zuletzt durch die Verordnung (EG) Nr. 279/2009 (ABl. L 93 vom 7.4.2009, S. 11) geändert worden ist. Die Verpflichtungen aus der Richtlinie 98/34/EG des Europäischen Parlaments und des Rates vom 22. Juni 1998 über ein Informationsverfahren auf dem Gebiet der Normen und technischen Vorschriften und der Vorschriften für die Dienste der Informationsgesellschaft (ABl. L 204 vom 21.7.1998, S. 37), die zuletzt durch die Richtlinie 2006/96/EG (ABl. L 363 vom 20.12.2006, S. 1) geändert worden ist, sind beachtet worden."@de ;
    dtd:has_noindex dtd:noindex-id343 .

dtd:Footnote-id1592 a dtd:Footnote ;
    dtd:has_FnZ "*"@de ;
    dtd:has_Group dtd:table ;
    dtd:has_ID "f2_775557_BJNR141200011BJNE001400000"@de ;
    dtd:has_Pos dtd:normal ;
    dtd:has_Postfix ")"@de ;
    dtd:has_Value "In Zweifelsfällen soll der Nutzerin oder dem Nutzer empfohlen werden, den Hauttyp ärztlich bestimmen zu lassen."@de .

dtd:Footnote-id1593 a dtd:Footnote ;
    dtd:has_FnZ "**"@de ;
    dtd:has_Group dtd:table ;
    dtd:has_ID "f3_775557_BJNR141200011BJNE001400000"@de ;
    dtd:has_Pos dtd:normal ;
    dtd:has_Postfix ")"@de ;
    dtd:has_Value "Es wird davon abgeraten, UV-Bestrahlungsgeräte zu kosmetischen Zwecken und für sonstige Anwendungen außerhalb der Heil- oder Zahnheilkunde zu nutzen."@de .

dtd:Footnote-id1595 a dtd:Footnote ;

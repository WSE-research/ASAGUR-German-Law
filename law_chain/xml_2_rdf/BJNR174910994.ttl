    dtd:has_DL dtd:DL-id2611 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Handel a)an einem geregelten Markt,b)an einem multilateralen Handelssystem oderc)an einem organisierten Handelssystem"@de .

dtd:LA-id5088 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an einem geregelten Markt,"@de .


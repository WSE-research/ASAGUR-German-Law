    dtd:has_Value "eine Anordnung der Bundesanstalt, die sich auf eine der in den Nummern 1 bis 6a genannte Vorschrift bezieht,"@de .

dtd:LA-id5106 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufzeichnungen von Telefongesprächen,"@de .

dtd:LA-id5107 a dtd:LA ;

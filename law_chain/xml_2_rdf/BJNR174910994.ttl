dtd:LA-id5091 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu überwachen, ob die Verbote oder Gebote dieses Gesetzes oder der Verordnung (EU) Nr. 596/2014, der Verordnung (EU) Nr. 600/2014, der Verordnung (EU) Nr. 1286/2014, der Verordnung (EU) 2015/2365, der Verordnung (EU) 2016/1011, der Verordnung (EU) 2019/1238 eingehalten werden, oder"@de .

dtd:LA-id5092 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu prüfen, ob die Voraussetzungen für eine Maßnahme nach § 15 dieses Gesetzes, nach Artikel 42 der Verordnung (EU) 600/2014 oder nach Artikel 63 der Verordnung (EU) Nr. 2019/1238 vorliegen."@de .

dtd:LA-id5093 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über Veränderungen im Bestand in Finanzinstrumenten,"@de .

dtd:LA-id5094 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über die Identität weiterer Personen, insbesondere der Auftraggeber und der aus Geschäften berechtigten oder verpflichteten Personen,"@de .

dtd:LA-id5095 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über Volumen und Zweck einer mittels eines Warenderivats eingegangenen Position oder offenen Forderung sowie"@de .

dtd:LA-id5096 a dtd:LA ;

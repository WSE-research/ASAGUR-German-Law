dtd:LA-id1192 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine zweijährige schulische Ausbildung an einer staatlichen, staatlich genehmigten oder staatlich anerkannten Schule für pharmazeutisch-technische Assistentinnen und pharmazeutisch-technische Assistenten (Schule),"@de .

dtd:LA-id1193 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Praktikum von 160 Stunden in einer Apotheke,"@de .

dtd:LA-id1194 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Grundausbildung in Erster Hilfe von mindestens neun Unterrichtseinheiten zu je 45 Minuten außerhalb der schulischen Ausbildung,"@de .

dtd:LA-id1195 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine praktische Ausbildung von sechs Monaten in der Apotheke, davon mindestens drei Monate in einer öffentlichen Apotheke."@de .

dtd:LA-id1196 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Apothekerin oder einem Apotheker, die oder der bei der zuständigen Behörde beschäftigt ist oder von der zuständigen Behörde mit dieser Aufgabe betraut worden ist, als Vorsitzender oder Vorsitzendem,"@de .

dtd:LA-id1197 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer beauftragten Person der Schulverwaltung, wenn die Schule nach den Schulgesetzen des Landes der staatlichen Aufsicht durch die Schulverwaltung untersteht,"@de .

dtd:LA-id1198 a dtd:LA ;
    dtd:has_DL dtd:DL-id897 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "folgenden Fachprüferinnen oder Fachprüfern: a)mindestens einer Apothekerin oder einem Apotheker, die oder der Lehrkraft der Schule ist, sowie weiteren Lehrkräften der Schule entsprechend den zu prüfenden Fächern,b)in Apotheken tätigen Apothekerinnen oder Apothekern, die keine Lehrkräfte der Schule sind."@de .

dtd:LA-id1199 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens einer Apothekerin oder einem Apotheker, die oder der Lehrkraft der Schule ist, sowie weiteren Lehrkräften der Schule entsprechend den zu prüfenden Fächern,"@de .

dtd:LA-id1200 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Apotheken tätigen Apothekerinnen oder Apothekern, die keine Lehrkräfte der Schule sind."@de .

dtd:LA-id1201 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Personalausweis oder Reisepass in amtlich beglaubigter Abschrift,"@de .

dtd:LA-id1202 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bescheinigung nach § 1 Absatz 2 Satz 4 über die regelmäßige und erfolgreiche Teilnahme an den Ausbildungsveranstaltungen der schulischen Ausbildung,"@de .

dtd:LA-id1203 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bescheinigung über das Praktikum in einer Apotheke außerhalb der schulischen Ausbildung nach § 1 Abs. 3 Satz 3 und"@de .

dtd:LA-id1204 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Nachweis über eine Grundausbildung in Erster Hilfe nach § 1 Abs. 1 Satz 1 Nr. 3."@de .


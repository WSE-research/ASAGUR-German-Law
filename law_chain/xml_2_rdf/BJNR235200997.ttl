    dtd:has_Value "im Fach \"Chemisch-pharmazeutische Übungen\" sind zwei Arzneimittel nach den anerkannten pharmazeutischen Regeln zu prüfen;"@de .

dtd:LA-id1217 a dtd:LA ;
    dtd:has_Size dtd:normal ;

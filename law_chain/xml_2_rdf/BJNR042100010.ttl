dtd:LA-id530 a dtd:LA ;
    dtd:has_Value "Vorbereiten von Arbeitsabläufen, Arbeiten im Team, Organisation,"@de .

dtd:LA-id531 a dtd:LA ;
    dtd:has_Value "Qualitätssicherungssysteme anwenden,"@de .

dtd:LA-id532 a dtd:LA ;
    dtd:has_Value "Hygienemaßnahmen anwenden,"@de .

dtd:LA-id533 a dtd:LA ;
    dtd:has_Value "Produktionsverfahren zur Behandlung von Lebensmitteln und Rohstoffen durchführen,"@de .

dtd:LA-id534 a dtd:LA ;
    dtd:has_Value "Steuern und Regeln von Produktionsprozessen,"@de .

dtd:LA-id535 a dtd:LA ;
    dtd:has_Value "Annehmen, Lagern und Abgeben von Erzeugnissen, Produkten und Materialien,"@de .

dtd:LA-id536 a dtd:LA ;
    dtd:has_Value "Verpacken von Produkten,"@de .

dtd:LA-id537 a dtd:LA ;
    dtd:has_Value "Informations- und Kommunikationstechniken anwenden;"@de .

dtd:LA-id538 a dtd:LA ;
    dtd:has_Value "Aufbau und Organisation des Ausbildungsbetriebes,"@de .

dtd:LA-id539 a dtd:LA ;
    dtd:has_Value "Berufsbildung, Arbeits- und Tarifrecht,"@de .

dtd:LA-id540 a dtd:LA ;
    dtd:has_Value "Sicherheit und Gesundheitsschutz bei der Arbeit,"@de .

dtd:LA-id541 a dtd:LA ;
    dtd:has_Value "Umweltschutz."@de .

dtd:LA-id542 a dtd:LA ;
    dtd:has_Value "Milchbehandlung und"@de .

dtd:LA-id543 a dtd:LA ;
    dtd:has_Value "Produktionsabläufe"@de .

dtd:LA-id544 a dtd:LA ;
    dtd:has_Value "Der Prüfling soll nachweisen, dass er Verfahren zur Annahme und Reinigung, zur Erhitzung und Standardisierung, zur Kühlung und Lagerung von Milch unter Berücksichtigung der weiteren Verarbeitung beherrschen und dabei Maßnahmen zur Wirtschaftlichkeit und zur Arbeitsorganisation sowie Vorschriften zur Herstellung von Lebensmitteln, zur Hygiene, zum Umweltschutz, zur Sicherheit und zum Gesundheitsschutz bei der Arbeit berücksichtigen kann;"@de .

dtd:LA-id545 a dtd:LA ;
    dtd:has_Value "der Prüfling soll berufstypische Aufgaben schriftlich bearbeiten;"@de .

dtd:LA-id546 a dtd:LA ;
    dtd:has_Value "die Prüfungszeit beträgt 120 Minuten."@de .

dtd:LA-id547 a dtd:LA ;
    dtd:has_Value "Der Prüfling soll nachweisen, dass er Produkte aus Milch herstellen und dafür Roh-, Hilfs-, Zusatz- und Betriebsstoffe annehmen, kontrollieren und produktspezifisch vorbereiten, Anlagen bedienen, Prozesse überwachen sowie die dazu notwendigen Dokumentationen führen und dabei Vorschriften zur Herstellung von Lebensmitteln, Maßnahmen zur Hygienesicherung, zur Arbeitsorganisation, zum Umweltschutz, zur Sicherheit und zum Gesundheitsschutz bei der Arbeit sowie zur Wirtschaftlichkeit berücksichtigen und seine Vorgehensweise begründen kann;"@de .

dtd:LA-id548 a dtd:LA ;
    dtd:has_DL dtd:DL-id343 ;
    dtd:has_Value "hierfür sind aus folgenden Tätigkeiten mindestens zwei auszuwählen, wobei sich die Tätigkeit jeweils auf ein konkretes Produkt einschließlich Zwischenprodukte bezieht: a)Herstellen von Konsummilch,b)Herstellen von gesäuerten Milcherzeugnissen,c)Herstellen von Butter,d)Herstellen von Käse;"@de .

dtd:LA-id549 a dtd:LA ;
    dtd:has_Value "Herstellen von Konsummilch,"@de .

dtd:LA-id550 a dtd:LA ;
    dtd:has_Value "Herstellen von gesäuerten Milcherzeugnissen,"@de .

dtd:LA-id551 a dtd:LA ;
    dtd:has_Value "Herstellen von Butter,"@de .

dtd:LA-id552 a dtd:LA ;
    dtd:has_Value "Herstellen von Käse;"@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Value "der Prüfling soll zwei Arbeitsproben durchführen und hierüber jeweils ein situatives Fachgespräch führen;"@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_Value "die Prüfungszeit beträgt insgesamt 180 Minuten; innerhalb dieser Zeit sollen die Fachgespräche insgesamt in höchstens 20 Minuten durchgeführt werden."@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Produktherstellung,"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Milchtechnologie,"@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wirtschafts- und Sozialkunde."@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_DL dtd:DL-id368 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der Prüfling soll nachweisen, dass er Produkte aus Milch herstellen und dafür a)Roh-, Hilfs-, Zusatz- und Betriebsstoffe einsetzen,b)Anlagen vorbereiten und in Betrieb nehmen,c)Rezepturen umsetzen,d)produktspezifische Untersuchungen bewerten und Maßnahmen ergreifen,e)Prozesse überwachen und die dazu notwendigen Dokumentationen durchführen,f)Anlagen umrüsten, reinigen und desinfizieren,g)Qualitätssicherungssysteme anwendenund dabei Vorschriften zur Herstellung von Lebensmitteln, der Hygiene, zur Sicherheit und zum Gesundheitsschutz bei der Arbeit, zum Umweltschutz sowie Vorgaben zur Wirtschaftlichkeit umsetzen kann;"@de .


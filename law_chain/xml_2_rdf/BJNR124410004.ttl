dtd:LA-id247 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Entgegennahme, Bearbeitung und Weiterleitung von Anträgen nach Artikel 5, 6 Abs. 2, Artikel 9 Abs. 2, Artikel 17, 18 Abs. 2 oder Artikel 21 Abs. 2 der Verordnung (EG) Nr. 1829/2003 des Europäischen Parlaments und des Rates vom 22. September 2003 über genetisch veränderte Lebensmittel und Futtermittel (ABl. EU Nr. L 268 S. 1), soweit die Mitgliedstaaten im Rahmen des Zulassungsverfahrens zuständig sind,"@de .

dtd:LA-id248 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Stellungnahme nach Artikel 6 Abs. 3 Buchstabe b oder Artikel 18 Abs. 3 Buchstabe b der Verordnung (EG) Nr. 1829/2003,"@de .

dtd:LA-id249 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Stellungnahme nach Artikel 6 Abs. 3 Buchstabe c oder Artikel 18 Abs. 3 Buchstabe c der Verordnung (EG) Nr. 1829/2003,"@de .

dtd:LA-id250 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Stellungnahme nach Artikel 6 Abs. 4 Satz 3 oder Artikel 18 Abs. 4 Satz 3 der Verordnung (EG) Nr. 1829/2003 und"@de .

dtd:LA-id251 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Ersuchen nach Artikel 10 Abs. 1 Satz 1 oder Artikel 22 Abs. 1 Satz 1 der Verordnung (EG) Nr. 1829/2003 an die Europäische Behörde für Lebensmittelsicherheit."@de .

dtd:LA-id252 a dtd:LA ;
    dtd:has_Value "Artikel 12 und 13 der Verordnung (EG) Nr. 1829/2003 oder"@de .

dtd:LA-id253 a dtd:LA ;
    dtd:has_Value "Artikel 4 oder 5 der Verordnung (EG) Nr. 1830/2003"@de .

dtd:LA-id254 a dtd:LA ;
    dtd:has_Value "Artikel 24 und 25 der Verordnung (EG) Nr. 1829/2003 oder"@de .


dtd:LA-id1825 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Fahrzeug“ ein Schiff oder ein schwimmendes Gerät;"@de .

dtd:LA-id1826 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schiff“ ein Binnenschiff oder ein Seeschiff;"@de .

dtd:LA-id1827 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Binnenschiff“ ein Schiff, das ausschließlich oder vorwiegend für die Fahrt auf Binnenwasserstraßen bestimmt ist;"@de .

dtd:LA-id1828 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Seeschiff“ ein Schiff, das zur See- oder Küstenfahrt zugelassen und vorwiegend dafür bestimmt ist;"@de .

dtd:LA-id1829 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schleppboot“ ein eigens zum Schleppen gebautes Schiff;"@de .

dtd:LA-id1830 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schubboot“ ein eigens zur Fortbewegung eines Schubverbandes gebautes Schiff;"@de .

dtd:LA-id1831 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schubleichter“ ein zur Güterbeförderung bestimmtes und zur Fortbewegung durch Schieben gebautes oder eigens eingerichtetes Schiff ohne eigene Triebkraft oder mit eigener Triebkraft, die nur erlaubt, außerhalb eines Schubverbandes kleine Ortsveränderungen vorzunehmen;"@de .

dtd:LA-id1832 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Fahrgastschiff“ ein zur Beförderung von mehr als 12 Fahrgästen gebautes und eingerichtetes Tagesausflugs- oder Kabinenschiff;"@de .

dtd:LA-id1833 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Tagesausflugsschiff“ ein Fahrgastschiff ohne Kabinen für die Übernachtung von Fahrgästen;"@de .

dtd:LA-id1834 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Kabinenschiff“ ein Fahrgastschiff mit Kabinen für die Übernachtung von Fahrgästen;"@de .

dtd:LA-id1835 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Fahrgastboot“ ein Fahrzeug, das zur Beförderung von Fahrgästen zugelassen ist;"@de .

dtd:LA-id1836 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Barkasse“ ein zur Beförderung von Fahrgästen (Personenbarkasse) oder zum Schleppen (Schleppbarkasse) gebautes und eingerichtetes Binnenschiff bis 25 m Länge, mit Plicht mit versenktem Innenboden;"@de .

dtd:LA-id1837 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„schnelles Schiff“ ein Fahrzeug mit Maschinenantrieb, das eine Geschwindigkeit gegen Wasser von mehr als 40 km/h erreichen kann;"@de .

dtd:LA-id1838 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Fähre“ ein Fahrzeug, das dem Übersetzverkehr von einem Ufer zum anderen auf der Wasserstraße dient und von der zuständigen Behörde als Fähre behandelt wird;"@de .

dtd:LA-id1839 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„schwimmendes Gerät“ eine schwimmende Konstruktion mit auf ihr vorhandenen Arbeitseinrichtungen wie Krane, Bagger, Rammen, Elevatoren;"@de .

dtd:LA-id1840 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„schwimmende Anlage“ eine schwimmende Einrichtung, die in der Regel nicht zur Fortbewegung bestimmt ist, wie eine Badeanstalt, ein Dock, eine Landebrücke oder ein Bootshaus;"@de .

dtd:LA-id1841 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schwimmkörper“ ein Floß sowie andere einzeln oder in Verbindungen fahrtauglich gemachte Gegenstände, soweit es sich nicht um ein Schiff, ein schwimmendes Gerät oder eine schwimmende Anlage handelt;"@de .

dtd:LA-id1842 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Sportfahrzeug“ ein für Sport- oder Erholungszwecke bestimmtes Schiff, das kein Fahrgastschiff oder Fahrgastboot ist;"@de .

dtd:LA-id1843 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Verband“ ein starrer Verband oder ein Schleppverband;"@de .

dtd:LA-id1844 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Formation“ die Form der Zusammenstellung eines Verbandes;"@de .

dtd:LA-id1845 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„starrer Verband“ ein Schubverband oder gekuppelte Fahrzeuge;"@de .

dtd:LA-id1846 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schubverband“ eine starre Verbindung von Fahrzeugen, von denen sich mindestens eines vor dem oder den beiden Fahrzeugen mit Maschinenantrieb befindet, das oder die den Verband fortbewegt oder fortbewegen und als „schiebendes Fahrzeug“ oder „schiebende Fahrzeuge“ bezeichnet werden; als starr gilt auch ein Verband aus einem schiebenden und einem geschobenen Fahrzeug, deren Kupplungen ein gesteuertes Knicken ermöglichen;"@de .

dtd:LA-id1847 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„gekuppelte Fahrzeuge“ eine Zusammenstellung von längsseits starr gekuppelten Fahrzeugen, von denen sich keines vor dem Fahrzeug mit Maschinenantrieb befindet, das die Zusammenstellung fortbewegt;"@de .

dtd:LA-id1848 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schleppverband“ eine Zusammenstellung von einem oder mehreren Fahrzeugen, schwimmenden Anlagen oder Schwimmkörpern, die von einem oder mehreren zum Verband gehörenden Fahrzeugen mit Maschinenantrieb geschleppt wird;"@de .

dtd:LA-id1849 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Wasserverdrängung“ das eingetauchte Volumen des Schiffes in Kubikmetern;"@de .

dtd:LA-id1850 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Länge“ („L“) die größte Länge des Schiffskörpers in Metern, ohne Ruder und Bugspriet;"@de .

dtd:LA-id1851 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Breite“ („B“) die größte Breite des Schiffskörpers in Metern, gemessen an der Außenseite der Beplattung, ohne Schaufelräder, Scheuerleisten und Ähnliches;"@de .

dtd:LA-id1852 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Tiefgang“ („T“) der senkrechte Abstand vom tiefsten Punkt des Schiffskörpers, ohne Berücksichtigung des Kiels oder anderer fester Anbauten, bis zur Ebene der größten Einsenkung des Schiffskörpers, in Metern."@de .

dtd:LA-id1853 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Untersuchung von Fahrzeugen zum Verkehr auf Wasserstraßen, einschließlich der Ausstellung der Fahrtauglichkeitsbescheinigung,"@de .

dtd:LA-id1854 a dtd:LA ;

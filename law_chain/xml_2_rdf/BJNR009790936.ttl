    dtd:has_DL dtd:DL-id1113,
        dtd:DL-id1120,
        dtd:DL-id1129 ;
    dtd:has_SUP dtd:SUP-id510,
        dtd:SUP-id511,
        dtd:SUP-id512,
        dtd:SUP-id513,
        dtd:SUP-id514,
        dtd:SUP-id515,
        dtd:SUP-id516,
        dtd:SUP-id517,
        dtd:SUP-id518,
        dtd:SUP-id519,

    dtd:has_Value "Vorbereiten, Einrichten, Sichern und Räumen von Arbeitsplätzen,"@de .

dtd:LA-id693 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Handhaben und Warten von Werkzeugen, Geräten, Maschinen und technischen Einrichtungen,"@de .

dtd:LA-id694 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Be- und Verarbeiten von Werk- und Hilfsstoffen,"@de .


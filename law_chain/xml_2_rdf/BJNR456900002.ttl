dtd:LA-id689 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Familienname,"@de .

dtd:LA-id690 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geburtsname,"@de .

dtd:LA-id691 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vornamen,"@de .

dtd:LA-id692 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tag und Ort der Geburt,"@de .

dtd:LA-id693 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anschrift."@de .

dtd:LA-id694 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Kopie des gültigen Reisepasses oder Personalausweises,"@de .

dtd:LA-id695 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zwei gleiche Passbilder aus neuerer Zeit."@de .

dtd:LA-id696 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Mindestalter (§ 6) nachgewiesen und"@de .

dtd:LA-id697 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gebühren (§ 16) eingegangen sind."@de .

dtd:LA-id698 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über ausreichende Kenntnisse der für die Teilnahme am Binnenschifffahrtsfunk maßgebenden Vorschriften sowie die zur ordnungsgemäßen Bedienung der Funkanlage erforderlichen betrieblichen und technischen Kenntnisse verfügt (theoretischer Teil) und"@de .

dtd:LA-id699 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu ihrer praktischen Anwendung fähig ist (praktischer Teil)."@de .


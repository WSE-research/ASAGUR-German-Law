dtd:LA-id996 a dtd:LA ;
    dtd:has_Value "für ihren Arbeitgeber oder im Auftrag eines Unternehmens mit Sitz im Ausland vorübergehend im Inland tätig werden oder"@de .

dtd:LA-id997 a dtd:LA ;

dtd:LA-id962 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Aufenthaltstitel, der einer Ausländerin oder einem Ausländer die Ausübung einer Beschäftigung erlaubt, nach § 39 Absatz 1 Satz 1 des Aufenthaltsgesetzes ohne Zustimmung der Bundesagentur für Arbeit erteilt werden kann,"@de .

dtd:LA-id963 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bundesagentur für Arbeit nach § 39 Absatz 1 Satz 2 des Aufenthaltsgesetzes einem Aufenthaltstitel, der einer Ausländerin oder einem Ausländer die Ausübung einer Beschäftigung erlaubt, zustimmen kann,"@de .

dtd:LA-id964 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:LA-id440 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "anhand eines elektronischen Identitätsnachweises nach § 18 des Personalausweisgesetzes, nach § 12 des eID-Karte-Gesetzes oder nach § 78 Absatz 5 des Aufenthaltsgesetzes,"@de .

dtd:LA-id441 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "anhand einer qualifizierten elektronischen Signatur oder"@de .

dtd:LA-id442 a dtd:LA ;

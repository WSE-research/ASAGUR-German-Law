    dtd:has_Size dtd:normal ;
    dtd:has_Value "Jahresbericht"@de .

dtd:LA-id5053 a dtd:LA ;
    dtd:has_DL dtd:DL-id4006 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der Jahresbericht besteht aus: 1.Stammdaten (Nummer 2.1),2.Auswertung der Messungen und Kontrollen sowie Darstellung der Ergebnisse (Nummer 2.2),3.Erklärung zum Deponieverhalten (Nummer 2.3),4.Auswertung zu angenommenen und abgegebenen Abfällen (Nummer 2.4)."@de .


dtd:LA-id509 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Aufenthaltsgestattung nach dem Asylgesetz besitzen,"@de .

dtd:LA-id510 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Asylgesuch geäußert haben und nicht die in den Nummern 1, 2 bis 5 und 7 genannten Voraussetzungen erfüllen,"@de .

dtd:LA-id511 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über einen Flughafen einreisen wollen und denen die Einreise nicht oder noch nicht gestattet ist,"@de .

dtd:LA-id512 a dtd:LA ;
    dtd:has_DL dtd:DL-id284 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Aufenthaltserlaubnis besitzen a)wegen des Krieges in ihrem Heimatland nach § 23 Absatz 1 des Aufenthaltsgesetzes,b)nach § 25 Absatz 4 Satz 1 des Aufenthaltsgesetzes oderc)nach § 25 Absatz 5 des Aufenthaltsgesetzes, sofern die Entscheidung über die Aussetzung ihrer Abschiebung noch nicht 18 Monate zurückliegt,"@de .

dtd:LA-id513 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wegen des Krieges in ihrem Heimatland nach § 23 Absatz 1 des Aufenthaltsgesetzes,"@de .

dtd:LA-id514 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 25 Absatz 4 Satz 1 des Aufenthaltsgesetzes oder"@de .

dtd:LA-id515 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 25 Absatz 5 des Aufenthaltsgesetzes, sofern die Entscheidung über die Aussetzung ihrer Abschiebung noch nicht 18 Monate zurückliegt,"@de .

dtd:LA-id516 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Duldung nach § 60a des Aufenthaltsgesetzes besitzen,"@de .

dtd:LA-id517 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vollziehbar ausreisepflichtig sind, auch wenn eine Abschiebungsandrohung noch nicht oder nicht mehr vollziehbar ist,"@de .

dtd:LA-id518 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ehegatten, Lebenspartner oder minderjährige Kinder der in den Nummern 1 bis 5 genannten Personen sind, ohne daß sie selbst die dort genannten Voraussetzungen erfüllen,"@de .

dtd:LA-id519 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Folgeantrag nach § 71 des Asylgesetzes oder einen Zweitantrag nach § 71a des Asylgesetzes stellen oder"@de .

dtd:LA-id520 a dtd:LA ;
    dtd:has_Value "und bei denen weder eine erkennungsdienstliche Behandlung nach § 49 des Aufenthaltsgesetzes oder nach § 16 des Asylgesetzes durchgeführt worden ist, noch deren Daten nach § 3 Absatz 1 des AZR-Gesetzes gespeichert wurden; das Erfordernis einer erkennungsdienstlichen Behandlung gilt nicht, soweit eine erkennungsdienstliche Behandlung nach § 49 des Aufenthaltsgesetzes nicht vorgesehen ist."@de .

dtd:LA-id522 a dtd:LA ;
    dtd:has_Value "eine Aufenthaltserlaubnis nach § 24 Absatz 1 des Aufenthaltsgesetzes besitzen, die ihnen nach dem 24. Februar 2022 und vor dem 1. Juni 2022 erteilt wurde, oder"@de .

dtd:LA-id523 a dtd:LA ;
    dtd:has_Value "eine entsprechende Fiktionsbescheinigung nach § 81 Absatz 5 in Verbindung mit Absatz 3 oder Absatz 4 des Aufenthaltsgesetzes besitzen, die nach dem 24. Februar 2022 und vor dem 1. Juni 2022 ausgestellt wurde,"@de .

dtd:LA-id524 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "internationaler Schutz oder"@de .

dtd:LA-id525 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus anderen Gründen ein Aufenthaltsrecht gewährt worden ist,"@de .

dtd:LA-id526 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie ihrer Pflicht nach § 13 Absatz 3 Satz 3 des Asylgesetzes nicht nachkommen,"@de .

dtd:LA-id527 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie ihrer Mitwirkungspflicht nach § 15 Absatz 2 Nummer 4 des Asylgesetzes nicht nachkommen,"@de .

dtd:LA-id528 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Bundesamt für Migration und Flüchtlinge festgestellt hat, dass sie ihrer Mitwirkungspflicht nach § 15 Absatz 2 Nummer 5 des Asylgesetzes nicht nachkommen,"@de .

dtd:LA-id529 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Bundesamt für Migration und Flüchtlinge festgestellt hat, dass sie ihrer Mitwirkungspflicht nach § 15 Absatz 2 Nummer 6 des Asylgesetzes nicht nachkommen,"@de .

dtd:LA-id530 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie ihrer Mitwirkungspflicht nach § 15 Absatz 2 Nummer 7 des Asylgesetzes nicht nachkommen,"@de .

dtd:LA-id531 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie den gewährten Termin zur förmlichen Antragstellung bei der zuständigen Außenstelle des Bundesamtes für Migration und Flüchtlinge oder dem Bundesamt für Migration und Flüchtlinge nicht wahrgenommen haben oder"@de .

dtd:LA-id532 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie Angaben über ihre Identität oder Staatsangehörigkeit verweigern,"@de .

dtd:LA-id533 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 9 Absatz 3 dieses Gesetzes in Verbindung mit § 60 Absatz 1 Satz 1 Nummer 1 des Ersten Buches Sozialgesetzbuch nicht angeben oder"@de .

dtd:LA-id534 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 9 Absatz 3 dieses Gesetzes in Verbindung mit § 60 Absatz 1 Satz 1 Nummer 2 des Ersten Buches Sozialgesetzbuch nicht unverzüglich mitteilen"@de .

dtd:LA-id535 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leistungsberechtigte nach § 1 Absatz 1 Nummer 1, 3 und 4 in einer nach den §§ 51, 57 und 58 des Dritten Buches Sozialgesetzbuch dem Grunde nach förderungsfähigen Ausbildung sowie"@de .

dtd:LA-id536 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leistungsberechtigte nach § 1 Absatz 1 Nummer 3 und 4 in einer nach dem Bundesausbildungsförderungsgesetz dem Grunde nach förderungsfähigen Ausbildung, deren Bedarf sich nach den §§ 12, 13 Absatz 1 in Verbindung mit Absatz 2 Nummer 1 oder nach § 13 Absatz 1 Nummer 1 in Verbindung mit Absatz 2 Nummer 2 des Bundesausbildungsförderungsgesetzes bemisst und die Leistungen nach dem Bundesausbildungsförderungsgesetz erhalten."@de .

dtd:LA-id537 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Unterbringung in einer Gemeinschaftsunterkunft im Sinne von § 53 Absatz 1 des Asylgesetzes oder in einer Aufnahmeeinrichtung nach § 44 Absatz 1 des Asylgesetzes für jede erwachsene Person ein Regelbedarf in Höhe der Regelbedarfsstufe 2 anerkannt wird;"@de .

dtd:LA-id538 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für jede erwachsene Person, die das 25. Lebensjahr noch nicht vollendet hat, unverheiratet ist und mit mindestens einem Elternteil in einer Wohnung im Sinne von § 8 Absatz 1 Satz 2 des Regelbedarfs-Ermittlungsgesetzes zusammenlebt, ein Regelbedarf in Höhe der Regelbedarfsstufe 3 anerkannt wird."@de .

dtd:LA-id539 a dtd:LA ;
    dtd:has_Value "erwachsene Leistungsberechtigte, die in einer Wohnung im Sinne von § 8 Absatz 1 Satz 2 des Regelbedarfs-Ermittlungsgesetzes leben und für die nicht Nummer 2 Buchstabe a oder Nummer 3 Buchstabe a gelten, sowie für jugendliche Leistungsberechtigte, die nicht mit mindestens einem Elternteil in einer Wohnung leben, je 162 Euro;"@de .


dtd:LA-id292 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Donaufährenverordnung vom 4. Oktober 1965 (Verkehrsblatt S. 580), geändert durch Verordnung vom 20. März 1969 (Verkehrsblatt S. 184),"@de .

dtd:LA-id293 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verordnung über Fähren auf dem Edersee vom 22. April 1985 (Verkehrsblatt S. 317),"@de .

dtd:LA-id294 a dtd:LA ;

dtd:LA-id247 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des § 3 Abs. 1 Nr. 1 und 2 und Abs. 5 Satz 3 des Binnenschiffahrtsaufgabengesetzes in der Fassung der Bekanntmachung vom 4. August 1986 (BGBl. I S. 1270) verordnet das Bundesministerium für Verkehr im Einvernehmen mit dem Bundesministerium für Arbeit und Sozialordnung,"@de .

dtd:LA-id248 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des § 4 Abs. 2 Satz 1 und 3 des Binnenschiffahrtsaufgabengesetzes in Verbindung mit dem 2. Abschnitt des Verwaltungskostengesetzes vom 23. Juni 1970 (BGBl. I S. 821) verordnet das Bundesministerium für Verkehr im Einvernehmen mit dem Bundesministerium der Finanzen,"@de .

dtd:LA-id249 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des § 27 Abs. 1 des Bundeswasserstraßengesetzes in der Fassung der Bekanntmachung vom 23. August 1990 (BGBl. I S. 1818) verordnet das Bundesministerium für Verkehr:"@de .

dtd:LA-id250 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fähre:"@de .

dtd:LA-id251 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Wasserfahrzeug, das dem Übersetzverkehr von einem Ufer zum anderen dient und von der Strom- und Schiffahrtspolizeibehörde als Fähre behandelt wird,"@de .

dtd:LA-id252 a dtd:LA ;
    dtd:has_Value "Kahnfähre:"@de .

dtd:LA-id253 a dtd:LA ;
    dtd:has_Value "eine zur Beförderung von Personen gebaute, offene Fähre, die durch Muskelkraft fortbewegt wird,"@de .

dtd:LA-id254 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fährinhaber:"@de .

dtd:LA-id255 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der für den Betrieb und die Unterhaltung der Fähre verantwortliche Fährberechtigte oder Pächter der Fährberechtigung,"@de .

dtd:LA-id256 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fährführer:"@de .

dtd:LA-id257 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der für die Führung einer Fähre sowie für den Verkehr auf der Fähre Verantwortliche,"@de .

dtd:LA-id258 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fährpersonal:"@de .

dtd:LA-id259 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Fährführer, die sonstigen Besatzungsmitglieder und der vom Fährinhaber mit der Verkehrsregelung auf der Fähre oder an der Anlegestelle zusätzlich Beauftragte,"@de .

dtd:LA-id260 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anlegestelle:"@de .

dtd:LA-id261 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anlagen und Einrichtungen am Ufer zum An- und Ablegen der Fähre,"@de .

dtd:LA-id262 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufsichtsbehörde:"@de .

dtd:LA-id263 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das örtlich zuständige Wasserstraßen- und Schifffahrtsamt."@de .

dtd:LA-id264 a dtd:LA ;
    dtd:has_Value "den Betrieb und die Aufsicht über die Fähren auf den Bundeswasserstraßen der Zonen 2 bis 4 nach Anhang I der Binnenschiffsuntersuchungsordnung vom 21. September 2018 (BGBl. I S. 1398) in der jeweils geltenden und anzuwendenden Fassung,"@de .

dtd:LA-id265 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Verhalten des Fährpersonals, der Fährbenutzer an Bord und an den Anlegestellen."@de .

dtd:LA-id266 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bundeswehr,"@de .

dtd:LA-id267 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bundespolizei,"@de .

dtd:LA-id268 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bereitschaftspolizeien der Länder,"@de .

dtd:LA-id269 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Zivil- und Katastrophenschutzes,"@de .

dtd:LA-id270 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Wasserstraßen- und Schifffahrtsverwaltung des Bundes, die nicht im öffentlichen Verkehr verwendet werden; für die übrigen Fähren der Wasserstraßen- und Schifffahrtsverwaltung des Bundes gelten die §§ 4, 5 und 6 nicht,"@de .

dtd:LA-id271 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der deutsch-luxemburgischen Grenzstrecke der Mosel."@de .

dtd:LA-id272 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Landeklappen vor Beginn der Fahrt soweit wie nötig angehoben werden und gegen unbeabsichtigtes Absenken gesichert sind,"@de .

dtd:LA-id273 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorgeschriebene Absperrvorrichtungen der Fähre während der Fahrt geschlossen sind,"@de .

dtd:LA-id274 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach dem Festlegen der Fähre nur der landseitige Zugang geöffnet ist und daß dieser bei Dunkelheit oder unsichtigem Wetter ausreichend beleuchtet wird."@de .

dtd:LA-id275 a dtd:LA ;
    dtd:has_DL dtd:DL-id208 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Fährinhabera)entgegen § 4 Absatz 2 Satz 2 eine Probefahrt nicht oder nicht rechtzeitig durchführt oder nicht duldet,b)entgegen § 5 Abs. 1 eine Mitteilung nicht oder nicht rechtzeitig macht,c)entgegen § 6 den Fährbetrieb von einer anderen als den dort genannten Anlegestellen aus durchführt oder durchführen läßt,d)entgegen § 7 Abs. 5 Satz 2 oder § 14 Abs. 1 Satz 1 nicht dafür sorgt, daß die dort genannten Hinweistafeln angebracht werden, odere)entgegen § 14 Abs. 2 Satz 2 eine Hinweistafel entfernt, verändert oder unkenntlich macht,"@de .

dtd:LA-id276 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 4 Absatz 2 Satz 2 eine Probefahrt nicht oder nicht rechtzeitig durchführt oder nicht duldet,"@de .

dtd:LA-id277 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 5 Abs. 1 eine Mitteilung nicht oder nicht rechtzeitig macht,"@de .

dtd:LA-id278 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 6 den Fährbetrieb von einer anderen als den dort genannten Anlegestellen aus durchführt oder durchführen läßt,"@de .

dtd:LA-id279 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 7 Abs. 5 Satz 2 oder § 14 Abs. 1 Satz 1 nicht dafür sorgt, daß die dort genannten Hinweistafeln angebracht werden, oder"@de .


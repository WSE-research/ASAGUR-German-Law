    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 6 den Fährbetrieb von einer anderen als den dort genannten Anlegestellen aus durchführt,"@de .

dtd:LA-id284 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Vorschrift des § 7 Absatz 1 Satz 1, Absatz 2 bis 4, 6 oder 7 über die Sicherheit an Bord zuwiderhandelt,"@de .

dtd:LA-id285 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 8 Satz 1 das Betreten, Befahren oder Verlassen der Fähre zuläßt,"@de .

dtd:LA-id286 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 12 Absatz 1 eine Kahnfähre einsetzt,"@de .

dtd:LA-id287 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 12 Absatz 2 den Fährverkehr nicht einstellt,"@de .

dtd:LA-id288 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 13 die Fähre gegen unbefugte Benutzung nicht sichert oder"@de .


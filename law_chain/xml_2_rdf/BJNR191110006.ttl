dtd:LA-id592 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erscheint und gegen den Verschmelzungsbeschluss Widerspruch zu Protokoll erklärt oder"@de .

dtd:LA-id593 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht erscheint, sofern es zu der Versammlung zu Unrecht nicht zugelassen worden ist oder die Versammlung nicht ordnungsgemäß einberufen oder der Gegenstand der Beschlussfassung nicht ordnungsgemäß bekannt gemacht worden ist."@de .

dtd:LA-id594 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einer Europäischen Genossenschaft mit dualistischem System die Mitglieder des Leitungsorgans und bei einer Europäischen Genossenschaft mit monistischem System die geschäftsführenden Direktoren versichern, dass allen Gläubigern, die nach den Absätzen 1 und 2 einen Anspruch auf Sicherheitsleistung haben, eine angemessene Sicherheit geleistet wurde und"@de .

dtd:LA-id595 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vertretungsorgane der Europäischen Genossenschaft erklären, dass eine Klage gegen die Wirksamkeit des Verlegungsbeschlusses nicht oder nicht fristgemäß erhoben oder eine solche Klage rechtskräftig abgewiesen oder zurückgenommen worden ist."@de .

dtd:LA-id596 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die dem Vorstand zugewiesenen Rechte und Pflichten vom Vorsitzenden des Verwaltungsrats wahrzunehmen sind;"@de .

dtd:LA-id597 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auch der SCE-Betriebsrat entsprechend § 98 Abs. 2 und § 104 Abs. 1 des Aktiengesetzes antragsberechtigt ist."@de .

dtd:LA-id598 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mitglieder des Vorstands,"@de .

dtd:LA-id599 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mitglieder des Aufsichtsrats oder"@de .

dtd:LA-id600 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mitglieder des vertretungsberechtigten Organs"@de .

dtd:LA-id601 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Vorstandsmitglied entgegen § 9 Satz 2,"@de .

dtd:LA-id602 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Mitglied des Leitungsorgans einer Europäischen Genossenschaft mit dualistischem System oder als geschäftsführender Direktor einer Europäischen Genossenschaft mit monistischem System entgegen § 11 Abs. 3 Nr. 1 oder"@de .

dtd:LA-id603 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als geschäftsführender Direktor einer Europäischen Genossenschaft mit monistischem System entgegen § 17 Abs. 2 Satz 1, auch in Verbindung mit § 26 Satz 2,"@de .


dtd:LA-id154 a dtd:LA ;
    dtd:has_Value "die vorzulegenden Dokumente im Einzelnen genau bezeichnet sind,"@de .

dtd:LA-id155 a dtd:LA ;
    dtd:has_Value "die vorzulegenden Dokumente für das jeweilige Verfahren und dessen Ausgang von unmittelbarer und eindeutig zu erkennender Bedeutung sind,"@de .


dtd:LA-id112 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach den Vorschriften der Viehverkehrsverordnung über die Kennzeichnung und Registrierung von Rindern,"@de .

dtd:LA-id113 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach den Vorschriften der Rinder- und Schafprämien-Verordnung über die Schlachtnummer, das Schlacht- oder Lebendgewicht und die Kategorie"@de .

dtd:LA-id114 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Beitragserhebung,"@de .

dtd:LA-id115 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gewährung von Entschädigungen nach dem Tiergesundheitsgesetz oder einem der Verhütung oder Bekämpfung von Tierseuchen dienenden unmittelbar geltenden Rechtsakt der Europäischen Gemeinschaft oder der Europäischen Union oder"@de .

dtd:LA-id116 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gewährung von Leistungen, die nicht von Nummer 2 erfasst sind und die der Verhütung oder Bekämpfung einer anzeigepflichtigen Tierseuche oder einer meldepflichtigen Tierkrankheit dienen,"@de .

dtd:LA-id117 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Mitwirkung nach § 27 Absatz 3 Satz 1 Nummer 3 des Tiergesundheitsgesetzes erforderlich ist oder"@de .

dtd:LA-id118 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Durchführung wissenschaftlicher Forschung auf dem Gebiet der Tierseuchen erforderlich ist, das wissenschaftliche Interesse an der Durchführung des Forschungsvorhabens das Interesse des Betroffenen an dem Ausschluss der Zweckänderung erheblich überwiegt und der Zweck der Forschung auf andere Weise nicht oder nur mit unverhältnismäßigem Aufwand erreicht werden kann."@de .

dtd:LA-id119 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Geburtsdatum des Rindes, das in seinen Bestand übernommen worden ist,"@de .

dtd:LA-id120 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Geschlecht dieses Rindes,"@de .

dtd:LA-id121 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Rasse dieses Rindes nach dem Rasseschlüssel der Viehverkehrsverordnung,"@de .

dtd:LA-id122 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ohrmarkennummer des Muttertieres dieses Rindes,"@de .

dtd:LA-id123 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Registriernummer des Geburtsbetriebes dieses Rindes,"@de .

dtd:LA-id124 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Land, den Mitgliedstaat oder das Drittland, in dem dieses Rind geboren worden ist,"@de .

dtd:LA-id125 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Länder, die Mitgliedstaaten oder die Drittländer, in denen dieses Rind vor der Übernahme in den Bestand gehalten worden ist, und zwar unter Angabe der jeweiligen Haltungszeiträume,"@de .

dtd:LA-id126 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Namen, die Anschrift des Tierhalters und die Registriernummer des Betriebes, von dem dieses Rind übernommen worden ist, oder, im Falle des Abgangs eines Rindes, den Namen, die Anschrift des Tierhalters und die Registriernummer des Betriebes, an den dieses Rind abgegeben worden ist,"@de .

dtd:LA-id127 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Schlachtgewicht oder das Lebendgewicht eines geschlachteten Rindes, falls das Gemeinschaftsrecht diese Gewichtsangabe für eine Beihilfe vorsieht,"@de .

dtd:P-id129 a dtd:P ;
    dtd:has_BR dtd:BR-id68,

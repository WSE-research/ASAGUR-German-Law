dtd:LA-id613 a dtd:LA ;
    dtd:has_Value "prüft die Änderungsmitteilung und"@de .

dtd:LA-id614 a dtd:LA ;
    dtd:has_Value "bestätigt die Erfassung der mitgeteilten Änderung."@de .

dtd:LA-id615 a dtd:LA ;
    dtd:has_Value "Name, Anschrift und Kontaktdaten des Anspruchsberechtigten, insbesondere Postleitzahl und Ort, Straße und Hausnummer, Land, Telefonnummer sowie, sofern vorhanden, die europäische oder nationale Steuernummer,"@de .

dtd:LA-id616 a dtd:LA ;
    dtd:has_Value "eine vertretungsberechtigte natürliche Person,"@de .

dtd:LA-id617 a dtd:LA ;
    dtd:has_Value "Kontoverbindung,"@de .

dtd:LA-id618 a dtd:LA ;
    dtd:has_Value "sofern sich die Zuständigkeit aus dem Landesrecht ergibt, eine von einer zuständigen Landesbehörde ausgestellte Bestätigung der Anspruchsberechtigung unter Nennung der Rechtsgrundlagen und"@de .

dtd:LA-id619 a dtd:LA ;
    dtd:has_Value "örtlicher Zuständigkeitsbereich."@de .

dtd:LA-id620 a dtd:LA ;
    dtd:has_Value "prüft die Anspruchsberechtigung,"@de .

dtd:LA-id621 a dtd:LA ;
    dtd:has_Value "bestätigt die Registrierung,"@de .

dtd:LA-id622 a dtd:LA ;
    dtd:has_Value "teilt dem öffentlich-rechtlichen Entsorgungsträger oder der juristischen Person des öffentlichen Rechts die Registrierungsnummer mit,"@de .

dtd:LA-id623 a dtd:LA ;
    dtd:has_Value "prüft die Änderungsmitteilung und"@de .

dtd:LA-id624 a dtd:LA ;
    dtd:has_Value "bestätigt die Erfassung der mitgeteilten Änderung."@de .

dtd:LA-id625 a dtd:LA ;
    dtd:has_Value "ob ein Produkt ein Einwegkunststoffprodukt nach § 3 Nummer 1 und 2 ist,"@de .


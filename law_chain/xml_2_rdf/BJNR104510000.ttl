dtd:LA-id3246 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Krankheitserreger"@de .

dtd:LA-id3247 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein vermehrungsfähiges Agens (Virus, Bakterium, Pilz, Parasit) oder ein sonstiges biologisches transmissibles Agens, das bei Menschen eine Infektion oder übertragbare Krankheit verursachen kann,"@de .

dtd:LA-id3248 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Infektion"@de .

dtd:LA-id3249 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Aufnahme eines Krankheitserregers und seine nachfolgende Entwicklung oder Vermehrung im menschlichen Organismus,"@de .

dtd:LA-id3250 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "übertragbare Krankheit"@de .

dtd:LA-id3251 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine durch Krankheitserreger oder deren toxische Produkte, die unmittelbar oder mittelbar auf den Menschen übertragen werden, verursachte Krankheit,"@de .

dtd:LA-id3252 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bedrohliche übertragbare Krankheit"@de .

dtd:LA-id3253 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine übertragbare Krankheit, die auf Grund klinisch schwerer Verlaufsformen oder ihrer Ausbreitungsweise eine schwerwiegende Gefahr für die Allgemeinheit verursachen kann,"@de .

dtd:LA-id3254 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kranker"@de .

dtd:LA-id3255 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Person, die an einer übertragbaren Krankheit erkrankt ist,"@de .

dtd:LA-id3256 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Krankheitsverdächtiger"@de .

dtd:LA-id3257 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Person, bei der Symptome bestehen, welche das Vorliegen einer bestimmten übertragbaren Krankheit vermuten lassen,"@de .

dtd:LA-id3258 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ausscheider"@de .

dtd:LA-id3259 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Person, die Krankheitserreger ausscheidet und dadurch eine Ansteckungsquelle für die Allgemeinheit sein kann, ohne krank oder krankheitsverdächtig zu sein,"@de .

dtd:LA-id3260 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ansteckungsverdächtiger"@de .

dtd:LA-id3261 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Person, von der anzunehmen ist, dass sie Krankheitserreger aufgenommen hat, ohne krank, krankheitsverdächtig oder Ausscheider zu sein,"@de .

dtd:LA-id3262 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nosokomiale Infektion"@de .

dtd:LA-id3263 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Infektion mit lokalen oder systemischen Infektionszeichen als Reaktion auf das Vorhandensein von Erregern oder ihrer Toxine, die im zeitlichen Zusammenhang mit einer stationären oder einer ambulanten medizinischen Maßnahme steht, soweit die Infektion nicht bereits vorher bestand,"@de .

dtd:LA-id3264 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schutzimpfung"@de .

dtd:LA-id3265 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gabe eines Impfstoffes mit dem Ziel, vor einer übertragbaren Krankheit zu schützen,"@de .

dtd:LA-id3266 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "andere Maßnahme der spezifischen Prophylaxe"@de .

dtd:LA-id3267 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gabe von Antikörpern (passive Immunprophylaxe) oder die Gabe von Medikamenten (Chemoprophylaxe) zum Schutz vor Weiterverbreitung bestimmter übertragbarer Krankheiten,"@de .

dtd:LA-id3268 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id3269 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value ""@de .

dtd:LA-id3270 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gesundheitsschädling"@de .

dtd:LA-id3271 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Tier, durch das Krankheitserreger auf Menschen übertragen werden können,"@de .

dtd:LA-id3272 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sentinel-Erhebung"@de .

dtd:LA-id3273 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine epidemiologische Methode zur stichprobenartigen Erfassung der Verbreitung bestimmter übertragbarer Krankheiten und der Immunität gegen bestimmte übertragbare Krankheiten in ausgewählten Bevölkerungsgruppen,"@de .

dtd:LA-id3274 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gesundheitsamt"@de .

dtd:LA-id3275 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nach Landesrecht für die Durchführung dieses Gesetzes bestimmte und mit einem Amtsarzt besetzte Behörde,"@de .

dtd:LA-id3276 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einrichtung oder Unternehmen"@de .

dtd:LA-id3277 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine juristische Person, eine Personengesellschaft oder eine natürliche Person, in deren unmittelbarem Verantwortungsbereich natürliche Personen behandelt, betreut, gepflegt oder untergebracht werden,"@de .

dtd:LA-id3278 a dtd:LA ;
    dtd:has_DL dtd:DL-id1392 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leitung der Einrichtung a)die natürliche Person oder die natürlichen Personen, die im Verantwortungsbereich einer Einrichtung durch diese mit den Aufgaben nach diesem Gesetz betraut ist oder sind,b)sofern eine Aufgabenübertragung nach Buchstabe a nicht erfolgt ist, die natürliche Person oder die natürlichen Personen, die für die Geschäftsführung zuständig ist oder sind, oderc)sofern die Einrichtung von einer einzelnen natürlichen Person betrieben wird, diese selbst,"@de .

dtd:LA-id3279 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die natürliche Person oder die natürlichen Personen, die im Verantwortungsbereich einer Einrichtung durch diese mit den Aufgaben nach diesem Gesetz betraut ist oder sind,"@de .

dtd:LA-id3280 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sofern eine Aufgabenübertragung nach Buchstabe a nicht erfolgt ist, die natürliche Person oder die natürlichen Personen, die für die Geschäftsführung zuständig ist oder sind, oder"@de .

dtd:LA-id3281 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sofern die Einrichtung von einer einzelnen natürlichen Person betrieben wird, diese selbst,"@de .

dtd:LA-id3282 a dtd:LA ;
    dtd:has_DL dtd:DL-id1399 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leitung des Unternehmens a)die natürliche Person oder die natürlichen Personen, die im Verantwortungsbereich eines Unternehmens durch dieses mit den Aufgaben nach diesem Gesetz betraut ist oder sind,b)sofern eine Aufgabenübertragung nach Buchstabe a nicht erfolgt ist, die natürliche Person oder die natürlichen Personen, die für die Geschäftsführung zuständig ist oder sind, oderc)sofern das Unternehmen von einer einzelnen natürlichen Person betrieben wird, diese selbst,"@de .

dtd:LA-id3283 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die natürliche Person oder die natürlichen Personen, die im Verantwortungsbereich eines Unternehmens durch dieses mit den Aufgaben nach diesem Gesetz betraut ist oder sind,"@de .

dtd:LA-id3284 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sofern eine Aufgabenübertragung nach Buchstabe a nicht erfolgt ist, die natürliche Person oder die natürlichen Personen, die für die Geschäftsführung zuständig ist oder sind, oder"@de .

dtd:LA-id3285 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sofern das Unternehmen von einer einzelnen natürlichen Person betrieben wird, diese selbst,"@de .

dtd:LA-id3286 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "personenbezogene Angabe"@de .

dtd:LA-id3287 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name und Vorname, Geschlecht, Geburtsdatum, Anschrift der Hauptwohnung oder des gewöhnlichen Aufenthaltsortes und, falls abweichend, Anschrift des derzeitigen Aufenthaltsortes der betroffenen Person sowie, soweit vorliegend, Telefonnummer und E-Mail-Adresse,"@de .

dtd:LA-id3288 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Risikogebiet"@de .

dtd:LA-id3289 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Gebiet außerhalb der Bundesrepublik Deutschland, für das vom Bundesministerium für Gesundheit im Einvernehmen mit dem Auswärtigen Amt und dem Bundesministerium des Innern, für Bau und Heimat ein erhöhtes Risiko für eine Infektion mit einer bestimmten bedrohlichen übertragbaren Krankheit festgestellt wurde; die Einstufung als Risikogebiet erfolgt erst mit Ablauf des ersten Tages nach Veröffentlichung der Feststellung durch das Robert Koch-Institut im Internet unter der Adresse https://www.rki.de/risikogebiete."@de .

dtd:LA-id3290 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erstellt im Benehmen mit den jeweils zuständigen Bundesbehörden für Fachkreise als Maßnahme des vorbeugenden Gesundheitsschutzes Richtlinien, Empfehlungen, Merkblätter und sonstige Informationen zur Vorbeugung, Erkennung und Verhinderung der Weiterverbreitung übertragbarer Krankheiten,"@de .

dtd:LA-id3291 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wertet die Daten zu meldepflichtigen Krankheiten und meldepflichtigen Nachweisen von Krankheitserregern, die ihm nach diesem Gesetz und nach § 11 Absatz 5, § 16 Absatz 4 des IGV-Durchführungsgesetzes übermittelt worden sind, infektionsepidemiologisch aus,"@de .

dtd:LA-id3292 a dtd:LA ;
    dtd:has_DL dtd:DL-id1417 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "stellt die Ergebnisse der infektionsepidemiologischen Auswertungen den folgenden Behörden und Institutionen zur Verfügung: a)den jeweils zuständigen Bundesbehörden,b)dem Kommando Sanitätsdienst der Bundeswehr,c)den obersten Landesgesundheitsbehörden,d)den Gesundheitsämtern,e)den Landesärztekammern,f)dem Spitzenverband Bund der Krankenkassen,g)der Kassenärztlichen Bundesvereinigung,h)dem Institut für Arbeitsschutz der Deutschen Gesetzlichen Unfallversicherung undi)der Deutschen Krankenhausgesellschaft,"@de .

dtd:LA-id3293 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den jeweils zuständigen Bundesbehörden,"@de .

dtd:LA-id3294 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Kommando Sanitätsdienst der Bundeswehr,"@de .

dtd:LA-id3295 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den obersten Landesgesundheitsbehörden,"@de .

dtd:LA-id3296 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Gesundheitsämtern,"@de .

dtd:LA-id3297 a dtd:LA ;

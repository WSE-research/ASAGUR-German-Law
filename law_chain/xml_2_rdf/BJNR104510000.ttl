    dtd:has_Value "den Landesärztekammern,"@de .

dtd:LA-id3298 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Spitzenverband Bund der Krankenkassen,"@de .

dtd:LA-id3299 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Kassenärztlichen Bundesvereinigung,"@de .

dtd:LA-id3300 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Institut für Arbeitsschutz der Deutschen Gesetzlichen Unfallversicherung und"@de .

dtd:LA-id3301 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Deutschen Krankenhausgesellschaft,"@de .

dtd:LA-id3302 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "veröffentlicht die Ergebnisse der infektionsepidemiologischen Auswertungen periodisch und"@de .

dtd:LA-id3303 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "unterstützt die Länder und sonstigen Beteiligten bei ihren Aufgaben im Rahmen der epidemiologischen Überwachung nach diesem Gesetz."@de .

dtd:LA-id3304 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Weltgesundheitsorganisation eine gesundheitliche Notlage von internationaler Tragweite ausgerufen hat und die Einschleppung einer bedrohlichen übertragbaren Krankheit in die Bundesrepublik Deutschland droht oder"@de .

dtd:LA-id3305 a dtd:LA ;

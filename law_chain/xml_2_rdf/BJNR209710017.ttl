dtd:LA-id1885 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "öffentliche Stellen des Bundes,"@de .

dtd:LA-id1886 a dtd:LA ;
    dtd:has_DL dtd:DL-id1311 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "öffentliche Stellen der Länder, soweit der Datenschutz nicht durch Landesgesetz geregelt ist und soweit sie a)Bundesrecht ausführen oderb)als Organe der Rechtspflege tätig werden und es sich nicht um Verwaltungsangelegenheiten handelt."@de .

dtd:LA-id1887 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bundesrecht ausführen oder"@de .

dtd:LA-id1888 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Organe der Rechtspflege tätig werden und es sich nicht um Verwaltungsangelegenheiten handelt."@de .

dtd:LA-id1889 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Verantwortliche oder Auftragsverarbeiter personenbezogene Daten im Inland verarbeitet,"@de .

dtd:LA-id1890 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verarbeitung personenbezogener Daten im Rahmen der Tätigkeiten einer inländischen Niederlassung des Verantwortlichen oder Auftragsverarbeiters erfolgt oder"@de .

dtd:LA-id1891 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Verantwortliche oder Auftragsverarbeiter zwar keine Niederlassung in einem Mitgliedstaat der Europäischen Union oder in einem anderen Vertragsstaat des Abkommens über den Europäischen Wirtschaftsraum hat, er aber in den Anwendungsbereich der Verordnung (EU) 2016/679 des Europäischen Parlaments und des Rates vom 27. April 2016 zum Schutz natürlicher Personen bei der Verarbeitung personenbezogener Daten, zum freien Datenverkehr und zur Aufhebung der Richtlinie 95/46/EG (Datenschutz-Grundverordnung) (ABl. L 119 vom 4.5.2016, S. 1; L 314 vom 22.11.2016, S. 72; L 127 vom 23.5.2018, S. 2) in der jeweils geltenden Fassung fällt."@de .

dtd:LA-id1892 a dtd:LA ;
    dtd:has_Value "sie über den Bereich eines Landes hinaus tätig werden oder"@de .

dtd:LA-id1893 a dtd:LA ;
    dtd:has_Value "dem Bund die absolute Mehrheit der Anteile gehört oder die absolute Mehrheit der Stimmen zusteht."@de .

dtd:LA-id1894 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Aufgabenerfüllung öffentlicher Stellen,"@de .

dtd:LA-id1895 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Wahrnehmung des Hausrechts oder"@de .

dtd:LA-id1896 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Wahrnehmung berechtigter Interessen für konkret festgelegte Zwecke"@de .

dtd:LA-id1897 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "öffentlich zugänglichen großflächigen Anlagen, wie insbesondere Sport-, Versammlungs- und Vergnügungsstätten, Einkaufszentren oder Parkplätzen, oder"@de .

dtd:LA-id1898 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fahrzeugen und öffentlich zugänglichen großflächigen Einrichtungen des öffentlichen Schienen-, Schiffs- und Busverkehrs"@de .

dtd:LA-id1899 a dtd:LA ;
    dtd:has_Value "Unterrichtung und Beratung der öffentlichen Stelle und der Beschäftigten, die Verarbeitungen durchführen, hinsichtlich ihrer Pflichten nach diesem Gesetz und sonstigen Vorschriften über den Datenschutz, einschließlich der zur Umsetzung der Richtlinie (EU) 2016/680 erlassenen Rechtsvorschriften;"@de .

dtd:LA-id1900 a dtd:LA ;
    dtd:has_Value "Überwachung der Einhaltung dieses Gesetzes und sonstiger Vorschriften über den Datenschutz, einschließlich der zur Umsetzung der Richtlinie (EU) 2016/680 erlassenen Rechtsvorschriften, sowie der Strategien der öffentlichen Stelle für den Schutz personenbezogener Daten, einschließlich der Zuweisung von Zuständigkeiten, der Sensibilisierung und der Schulung der an den Verarbeitungsvorgängen beteiligten Beschäftigten und der diesbezüglichen Überprüfungen;"@de .

dtd:LA-id1901 a dtd:LA ;
    dtd:has_Value "Beratung im Zusammenhang mit der Datenschutz-Folgenabschätzung und Überwachung ihrer Durchführung gemäß § 67 dieses Gesetzes;"@de .

dtd:LA-id1902 a dtd:LA ;
    dtd:has_Value "Zusammenarbeit mit der Aufsichtsbehörde;"@de .

dtd:LA-id1903 a dtd:LA ;
    dtd:has_Value "Tätigkeit als Anlaufstelle für die Aufsichtsbehörde in mit der Verarbeitung zusammenhängenden Fragen, einschließlich der vorherigen Konsultation gemäß § 69 dieses Gesetzes, und gegebenenfalls Beratung zu allen sonstigen Fragen."@de .

dtd:LA-id1904 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den Monat Juni 2023 eine einmalige Sonderzahlung in Höhe von 1 240 Euro sowie"@de .

dtd:LA-id1905 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Monate Juli 2023 bis Februar 2024 eine monatliche Sonderzahlung in Höhe von jeweils 220 Euro."@de .

dtd:LA-id1906 a dtd:LA ;
    dtd:has_Value "dem Wohl des Bundes oder eines Landes Nachteile bereiten, insbesondere Nachteile für die Sicherheit der Bundesrepublik Deutschland oder ihre Beziehungen zu anderen Staaten, oder"@de .

dtd:LA-id1907 a dtd:LA ;
    dtd:has_Value "Grundrechte verletzen."@de .

dtd:LA-id1908 a dtd:LA ;
    dtd:has_Value "die Anwendung dieses Gesetzes und sonstiger Vorschriften über den Datenschutz, einschließlich der zur Umsetzung der Richtlinie (EU) 2016/680 erlassenen Rechtsvorschriften, zu überwachen und durchzusetzen,"@de .

dtd:LA-id1909 a dtd:LA ;
    dtd:has_Value "die Öffentlichkeit für die Risiken, Vorschriften, Garantien und Rechte im Zusammenhang mit der Verarbeitung personenbezogener Daten zu sensibilisieren und sie darüber aufzuklären, wobei spezifische Maßnahmen für Kinder besondere Beachtung finden,"@de .

dtd:LA-id1910 a dtd:LA ;
    dtd:has_Value "den Deutschen Bundestag und den Bundesrat, die Bundesregierung und andere Einrichtungen und Gremien über legislative und administrative Maßnahmen zum Schutz der Rechte und Freiheiten natürlicher Personen in Bezug auf die Verarbeitung personenbezogener Daten zu beraten,"@de .

dtd:LA-id1911 a dtd:LA ;
    dtd:has_Value "die Verantwortlichen und die Auftragsverarbeiter für die ihnen aus diesem Gesetz und sonstigen Vorschriften über den Datenschutz, einschließlich den zur Umsetzung der Richtlinie (EU) 2016/680 erlassenen Rechtsvorschriften, entstehenden Pflichten zu sensibilisieren,"@de .

dtd:LA-id1912 a dtd:LA ;
    dtd:has_Value "auf Anfrage jeder betroffenen Person Informationen über die Ausübung ihrer Rechte aufgrund dieses Gesetzes und sonstiger Vorschriften über den Datenschutz, einschließlich der zur Umsetzung der Richtlinie (EU) 2016/680 erlassenen Rechtsvorschriften, zur Verfügung zu stellen und gegebenenfalls zu diesem Zweck mit den Aufsichtsbehörden in anderen Mitgliedstaaten zusammenzuarbeiten,"@de .

dtd:LA-id1913 a dtd:LA ;
    dtd:has_Value "sich mit Beschwerden einer betroffenen Person oder Beschwerden einer Stelle, einer Organisation oder eines Verbandes gemäß Artikel 55 der Richtlinie (EU) 2016/680 zu befassen, den Gegenstand der Beschwerde in angemessenem Umfang zu untersuchen und den Beschwerdeführer innerhalb einer angemessenen Frist über den Fortgang und das Ergebnis der Untersuchung zu unterrichten, insbesondere, wenn eine weitere Untersuchung oder Koordinierung mit einer anderen Aufsichtsbehörde notwendig ist,"@de .

dtd:LA-id1914 a dtd:LA ;
    dtd:has_NB dtd:NB-id921 ;
    dtd:has_Value "mit anderen Aufsichtsbehörden zusammenzuarbeiten, auch durch Informationsaustausch, und ihnen Amtshilfe zu leisten, um die einheitliche Anwendung und Durchsetzung dieses Gesetzes und sonstiger Vorschriften über den Datenschutz, einschließlich der zur Umsetzung der Richtlinie (EU) 2016/680 erlassenen Rechtsvorschriften, zu gewährleisten,"@de .

dtd:LA-id1915 a dtd:LA ;
    dtd:has_Value "Untersuchungen über die Anwendung dieses Gesetzes und sonstiger Vorschriften über den Datenschutz, einschließlich der zur Umsetzung der Richtlinie (EU) 2016/680 erlassenen Rechtsvorschriften, durchzuführen, auch auf der Grundlage von Informationen einer anderen Aufsichtsbehörde oder einer anderen Behörde,"@de .

dtd:LA-id1916 a dtd:LA ;
    dtd:has_Value "maßgebliche Entwicklungen zu verfolgen, soweit sie sich auf den Schutz personenbezogener Daten auswirken, insbesondere die Entwicklung der Informations- und Kommunikationstechnologie und der Geschäftspraktiken,"@de .

dtd:LA-id1917 a dtd:LA ;
    dtd:has_Value "Beratung in Bezug auf die in § 69 genannten Verarbeitungsvorgänge zu leisten und"@de .

dtd:LA-id1918 a dtd:LA ;
    dtd:has_Value "Beiträge zur Tätigkeit des Europäischen Datenschutzausschusses zu leisten."@de .

dtd:LA-id1919 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von ihrer oder seiner Aufsicht unterliegenden Stellen erlangte personenbezogene Daten über den Inhalt und die näheren Umstände des Brief-, Post- und Fernmeldeverkehrs und"@de .

dtd:LA-id1920 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "personenbezogene Daten, die einem besonderen Amtsgeheimnis, insbesondere dem Steuergeheimnis nach § 30 der Abgabenordnung, unterliegen."@de .

dtd:LA-id1921 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jederzeit Zugang zu den Grundstücken und Diensträumen, einschließlich aller Datenverarbeitungsanlagen und -geräte, sowie zu allen personenbezogenen Daten und Informationen, die zur Erfüllung ihrer oder seiner Aufgaben notwendig sind, zu gewähren und"@de .

dtd:LA-id1922 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle Informationen, die für die Erfüllung ihrer oder seiner Aufgaben erforderlich sind, bereitzustellen."@de .

dtd:LA-id1923 a dtd:LA ;
    dtd:has_Value "die natürliche oder juristische Person als Klägerin oder Antragstellerin und"@de .

dtd:LA-id1924 a dtd:LA ;
    dtd:has_Value "die Aufsichtsbehörde als Beklagte oder Antragsgegnerin."@de .

dtd:LA-id1925 a dtd:LA ;
    dtd:has_DL dtd:DL-id1404 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch öffentliche und nichtöffentliche Stellen, wenn sie a)erforderlich ist, um die aus dem Recht der sozialen Sicherheit und des Sozialschutzes erwachsenden Rechte auszuüben und den diesbezüglichen Pflichten nachzukommen,b)zum Zweck der Gesundheitsvorsorge, für die Beurteilung der Arbeitsfähigkeit des Beschäftigten, für die medizinische Diagnostik, die Versorgung oder Behandlung im Gesundheits- oder Sozialbereich oder für die Verwaltung von Systemen und Diensten im Gesundheits- und Sozialbereich oder aufgrund eines Vertrags der betroffenen Person mit einem Angehörigen eines Gesundheitsberufs erforderlich ist und diese Daten von ärztlichem Personal oder durch sonstige Personen, die einer entsprechenden Geheimhaltungspflicht unterliegen, oder unter deren Verantwortung verarbeitet werden,c)aus Gründen des öffentlichen Interesses im Bereich der öffentlichen Gesundheit, wie des Schutzes vor schwerwiegenden grenzüberschreitenden Gesundheitsgefahren oder zur Gewährleistung hoher Qualitäts- und Sicherheitsstandards bei der Gesundheitsversorgung und bei Arzneimitteln und Medizinprodukten erforderlich ist; ergänzend zu den in Absatz 2 genannten Maßnahmen sind insbesondere die berufsrechtlichen und strafrechtlichen Vorgaben zur Wahrung des Berufsgeheimnisses einzuhalten, oderd)aus Gründen eines erheblichen öffentlichen Interesses zwingend erforderlich ist,"@de .

dtd:LA-id1926 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erforderlich ist, um die aus dem Recht der sozialen Sicherheit und des Sozialschutzes erwachsenden Rechte auszuüben und den diesbezüglichen Pflichten nachzukommen,"@de .

dtd:LA-id1927 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Zweck der Gesundheitsvorsorge, für die Beurteilung der Arbeitsfähigkeit des Beschäftigten, für die medizinische Diagnostik, die Versorgung oder Behandlung im Gesundheits- oder Sozialbereich oder für die Verwaltung von Systemen und Diensten im Gesundheits- und Sozialbereich oder aufgrund eines Vertrags der betroffenen Person mit einem Angehörigen eines Gesundheitsberufs erforderlich ist und diese Daten von ärztlichem Personal oder durch sonstige Personen, die einer entsprechenden Geheimhaltungspflicht unterliegen, oder unter deren Verantwortung verarbeitet werden,"@de .

dtd:LA-id1928 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus Gründen des öffentlichen Interesses im Bereich der öffentlichen Gesundheit, wie des Schutzes vor schwerwiegenden grenzüberschreitenden Gesundheitsgefahren oder zur Gewährleistung hoher Qualitäts- und Sicherheitsstandards bei der Gesundheitsversorgung und bei Arzneimitteln und Medizinprodukten erforderlich ist; ergänzend zu den in Absatz 2 genannten Maßnahmen sind insbesondere die berufsrechtlichen und strafrechtlichen Vorgaben zur Wahrung des Berufsgeheimnisses einzuhalten, oder"@de .

dtd:LA-id1929 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus Gründen eines erheblichen öffentlichen Interesses zwingend erforderlich ist,"@de .

dtd:LA-id1930 a dtd:LA ;
    dtd:has_DL dtd:DL-id1413 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch öffentliche Stellen, wenn sie a)zur Abwehr einer erheblichen Gefahr für die öffentliche Sicherheit erforderlich ist,b)zur Abwehr erheblicher Nachteile für das Gemeinwohl oder zur Wahrung erheblicher Belange des Gemeinwohls zwingend erforderlich ist oderc)aus zwingenden Gründen der Verteidigung oder der Erfüllung über- oder zwischenstaatlicher Verpflichtungen einer öffentlichen Stelle des Bundes auf dem Gebiet der Krisenbewältigung oder Konfliktverhinderung oder für humanitäre Maßnahmen erforderlich ist"@de .

dtd:LA-id1931 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Abwehr einer erheblichen Gefahr für die öffentliche Sicherheit erforderlich ist,"@de .

dtd:LA-id1932 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Abwehr erheblicher Nachteile für das Gemeinwohl oder zur Wahrung erheblicher Belange des Gemeinwohls zwingend erforderlich ist oder"@de .

dtd:LA-id1933 a dtd:LA ;

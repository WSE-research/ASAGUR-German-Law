dtd:LA-id739 a dtd:LA ;
    dtd:has_Value "eine Auflistung der vom Bewachungsunternehmen eingesetzten Ausrüstung nach § 6 Absatz 1 Satz 1,"@de .

dtd:LA-id740 a dtd:LA ;

dtd:LA-id127 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Umsetzung innerhalb eines Betriebes der Deutsche Bahn Aktiengesellschaft, wenn sie mit einem Wechsel des Dienstortes verbunden ist,"@de .

dtd:LA-id128 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zuweisung einer Tätigkeit auf Dauer in einem anderen Betrieb der Deutsche Bahn Aktiengesellschaft, Versetzung,"@de .

dtd:LA-id129 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorübergehende Zuweisung einer Tätigkeit bei einem anderen Betrieb der Deutsche Bahn Aktiengesellschaft, Abordnung,"@de .

dtd:LA-id130 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anordnungen, welche die Freiheit in der Wahl der Wohnung beschränken,"@de .

dtd:LA-id131 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Regelung der Ordnung im Betrieb und des Verhaltens der Beschäftigten,"@de .

dtd:LA-id132 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beginn und Ende der täglichen Arbeitszeit und der Pausen bei der Deutsche Bahn Aktiengesellschaft sowie die Verteilung der Arbeitszeit auf die einzelnen Wochentage,"@de .

dtd:LA-id133 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anordnung von Mehrarbeit,"@de .

dtd:LA-id134 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufstellung des Urlaubsplanes, Festsetzung der zeitlichen Lage des Erholungsurlaubs für einzelne Beschäftigte, wenn zwischen dem Arbeitgeber und den beteiligten Beschäftigten kein Einverständnis erzielt wird,"@de .

dtd:LA-id135 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einführung und Anwendung technischer Einrichtungen, die dazu bestimmt sind, das Verhalten oder die Leistung der Beschäftigten zu überwachen,"@de .

dtd:LA-id136 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen des Arbeitsschutzes zur Verhütung von Dienst- und Arbeitsunfällen sowie von arbeitsbedingten Gesundheitsgefahren,"@de .

dtd:LA-id137 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit es sich um anderweitige Bezüge für zugewiesene Beamte handelt, Fragen der betrieblichen Lohngestaltung, insbesondere die Aufstellung von Entlohnungsmethoden sowie deren Änderung, Festsetzung der Akkord- und Prämiensätze und vergleichbarer leistungsbezogener Entgelte, einschließlich der Geldfaktoren,"@de .

dtd:LA-id138 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundsätze über die Bewertung von anerkannten Vorschlägen im Rahmen des betrieblichen Vorschlagwesens,"@de .

dtd:LA-id139 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gestaltung der Arbeitsplätze,"@de .

dtd:LA-id140 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "grundlegende Änderungen von Arbeitsverfahren und Arbeitsabläufen,"@de .

dtd:LA-id141 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Hebung der Arbeitsleistung und Erleichterung des Arbeitsablaufs,"@de .

dtd:LA-id142 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Absehen von der Ausschreibung von Dienstposten, die besetzt werden sollen,"@de .

dtd:LA-id143 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erstellen von Personalfragebogen, soweit der Fragebogen Fragen zur Tätigkeit bei der Deutsche Bahn Aktiengesellschaft zum Inhalt hat,"@de .

dtd:LA-id144 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beurteilungsrichtlinien für eine Tätigkeit bei der Deutsche Bahn Aktiengesellschaft,"@de .

dtd:LA-id145 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erlaß von Richtlinien über die personelle Auswahl bei Versetzungen,"@de .

dtd:LA-id146 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auswahl der Teilnehmer an Fortbildungsveranstaltungen,"@de .

dtd:LA-id147 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "allgemeine Fragen der Fortbildung der Beschäftigten,"@de .

dtd:LA-id148 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einführung grundlegend neuer Arbeitsmethoden,"@de .

dtd:LA-id149 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Entscheidung über Anträge auf Genehmigung einer Nebentätigkeit; Widerruf einer Nebentätigkeitsgenehmigung; Entscheidung über die Annahme von Belohnungen, Geschenken und sonstigen Vorteilen,"@de .

dtd:LA-id150 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Entscheidung über Anträge nach den § 91, 92 oder nach § 92a des Bundesbeamtengesetzes auf Teilzeitbeschäftigung, auf Ermäßigung der regelmäßigen Arbeitszeit,"@de .

dtd:LA-id151 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geltendmachung von Schadensersatzansprüchen nach Maßgabe des § 75 des Bundesbeamtengesetzes und Geltendmachung von Herausgabeansprüchen nach § 71 Abs. 2 des Bundesbeamtengesetzes,"@de .

dtd:LA-id152 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Stellenausschreibung nach § 22 Abs. 1 in Verbindung mit § 8 Abs. 1 Satz 1 und § 9 des Bundesbeamtengesetzes und § 4 Abs. 2 der Bundeslaufbahnverordnung in der Fassung der Bekanntmachung vom 2. Juli 2002 (BGBl. I S. 2459, 2671), die zuletzt durch Artikel 15 Absatz 28 des Gesetzes vom 5. Februar 2009 (BGBl. I S. 160) geändert worden ist, zur Übertragung von höher bewerteten Tätigkeiten bei der Deutsche Bahn Aktiengesellschaft,"@de .

dtd:LA-id153 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gewährung von Urlaub nach der Erholungsurlaubsverordnung, der Sonderurlaubsverordnung und der Elternzeitverordnung, soweit eine Entscheidung nicht der obersten Dienstbehörde vorbehalten ist; Dienstbefreiung,"@de .

dtd:LA-id154 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gewähren von Freizeitausgleich oder Vergütung für Mehrarbeit,"@de .

dtd:LA-id155 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anordnung und Genehmigung von Dienstreisen und Dienstgängen,"@de .

dtd:LA-id156 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorübergehende Untersagung der Dienstausübung,"@de .

dtd:LA-id157 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Genehmigung nach § 67 Abs. 3 sowie die §§ 68 und 69 des Bundesbeamtengesetzes in Angelegenheiten der Deutsche Bahn Aktiengesellschaft,"@de .

dtd:LA-id158 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auskünfte an die Presse in Angelegenheiten der Deutsche Bahn Aktiengesellschaft (§ 70 des Bundesbeamtengesetzes),"@de .

dtd:LA-id159 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Entgegennahme von Anzeigen zum Nachweis der Dienstunfähigkeit bei Erkrankung,"@de .

dtd:LA-id160 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verlangen des Nachweises der vorübergehenden Dienstunfähigkeit bei Erkrankung,"@de .

dtd:LA-id161 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anordnung zu ärztlichen Untersuchungen,"@de .

dtd:LA-id162 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Veranlassen von Gesundheitsmaßnahmen zur Wiederherstellung oder Erhaltung der Dienstfähigkeit,"@de .

dtd:LA-id163 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Begründung der Notwendigkeit einer Unabkömmlichstellung bei Grundwehrdienst und Wehrübung,"@de .

dtd:LA-id164 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erstattung von Auslagen auf Grund des Bundesreisekostengesetzes, des Bundesumzugskostengesetzes sowie ergänzender Bestimmungen des Bundesministeriums für Verkehr und digitale Infrastruktur nach § 7 Abs. 5 des Gesetzes zur Zusammenführung und Neugliederung der Bundeseisenbahnen,"@de .

dtd:LA-id165 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zusage der Umzugskostenvergütung,"@de .

dtd:LA-id166 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Führen von Teilakten nach § 106 Abs. 2 des Bundesbeamtengesetzes, wenn und soweit es sich um Entscheidungen und Maßnahmen handelt, die der Deutsche Bahn Aktiengesellschaft zur Ausübung übertragen sind,"@de .

dtd:LA-id167 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einschätzungen der Leistungen nach § 27 Abs. 4, 5 und 7 des Bundesbesoldungsgesetzes."@de .


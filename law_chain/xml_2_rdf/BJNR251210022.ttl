dtd:entry-id1342 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Ergebnis aus Absicherungsgeschäften im Sinn dieser Anlage ist das Fair Value Ergebnis von Absicherungsgeschäften für die geplante Einspeisung im Abrechnungszeitraum der Stromerzeugungsanlage aus Verträgen, die vor dem 1. November 2022 geschlossen wurden."@de .

dtd:entry-id1343 a dtd:entry ;

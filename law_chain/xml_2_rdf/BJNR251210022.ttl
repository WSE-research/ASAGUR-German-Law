dtd:entry-id1344 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Als geplant gilt die Planungsperspektive zum Zeitpunkt des Abschlusses des Absicherungsgeschäftes."@de .

dtd:entry-id1345 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.3"@de .

dtd:entry-id1346 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Als Fair Value gilt der beizulegende Zeitwert, der als der Preis definiert ist, der in einem geordneten Geschäftsvorfall zwischen Marktteilnehmern am Bemessungsstichtag für den Verkauf eines Vermögenswerts eingenommen bzw. für die Übertragung einer Schuld gezahlt würde bzw. wird."@de .

dtd:entry-id1347 a dtd:entry ;
    dtd:has_B dtd:B-id587 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "2."@de .

dtd:entry-id1348 a dtd:entry ;
    dtd:has_B dtd:B-id588 ;

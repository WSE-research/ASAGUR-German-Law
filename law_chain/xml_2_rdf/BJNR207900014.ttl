dtd:entry-id136 a dtd:entry ;
    dtd:has_B dtd:B-id65 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Einreise, Aufenthaltstitel, Zugang zum Arbeitsmarkt"@de ;
    dtd:has_align dtd:center ;
    dtd:has_colsep "0"@de ;
    dtd:has_rowsep "0"@de .

dtd:entry-id137 a dtd:entry ;
    dtd:has_B dtd:B-id66 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Entry, residence permit, access to the labour market"@de ;

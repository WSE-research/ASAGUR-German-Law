    dtd:has_Size dtd:normal ;
    dtd:has_Value "Genossenschaften,"@de .

dtd:LA-id156 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "andere juristische Personen, die mit dem Genossenschaftswesen verbunden sind."@de .

dtd:LA-id157 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Vertreter des Ministeriums der Finanzen,"@de .

dtd:LA-id158 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Vertreter des Ministeriums für Wirtschaft,"@de .

dtd:LA-id159 a dtd:LA ;

    dtd:has_Value "entgegen  19 Absatz 2 die zustndige Behrde oder den Zulassungsinhaber nicht oder nicht rechtzeitig informiert,"@de .

dtd:LA-id1510 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  30 Abs. 1 ein Mischfuttermittel verwendet,"@de .

dtd:LA-id1511 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  30 Abs. 4 ein Ftterungsarzneimittel in Verkehr bringt,"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei eingefhrten Geweben und Gewebezubereitungen der Staat, in dem sie entnommen wurden, sowie der Ausfuhrstaat, sofern dieser nicht der Staat der Entnahme ist."@de .

dtd:LA-id1484 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorsicht und Gewebe und Zellen,"@de .

dtd:LA-id1485 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kennung der Gewebeeinrichtung, die die Gewebe oder Gewebezubereitungen be- oder verarbeitet hat, sowie der Einrichtung, die die Gewebe oder Gewebezubereitungen erhalten soll, einschlielich ihrer Anschriften und Telefonnummern, sowie"@de .


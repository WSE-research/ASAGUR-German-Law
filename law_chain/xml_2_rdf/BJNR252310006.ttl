    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ergebnisse der fr die Gewinnung erforderlichen Laboruntersuchungen,"@de .

dtd:LA-id1478 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Datum der Abgabe,"@de .

dtd:LA-id1479 a dtd:LA ;

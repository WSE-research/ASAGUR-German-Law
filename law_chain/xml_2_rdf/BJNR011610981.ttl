dtd:LA-id137 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mitteilungen nach Artikel 15a Unterabsatz 2 und Artikel 21 Absatz 2 der Verordnung (EG) Nr. 952/2006 sowie"@de .


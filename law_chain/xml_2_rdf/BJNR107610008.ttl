dtd:P-id61 a dtd:P ;
    dtd:has_BR dtd:BR-id47 ;
    dtd:has_Value " (+++ Textnachweis ab: 1.7.2008 +++) Das G ist als Artikel 2 des G v. 26.6.2008 I 1076 vom Bundestag beschlossen worden. Es tritt gem. Art. 3 dieses G am 1.7.2008 in Kraft"@de ;
    dtd:has_pre dtd:pre-id76 .

dtd:P-id63 a dtd:P ;
    dtd:has_Value "(1) Der aktuelle Rentenwert beträgt ab dem 1. Juli 2008 26,56 Euro."@de .

dtd:P-id64 a dtd:P ;
    dtd:has_Value "(2) Der aktuelle Rentenwert (Ost) beträgt ab dem 1. Juli 2008 23,34 Euro."@de .

dtd:P-id66 a dtd:P ;
    dtd:has_Value "(1) Der allgemeine Rentenwert in der Alterssicherung der Landwirte beträgt ab dem 1. Juli 2008 12,26 Euro."@de .

dtd:P-id67 a dtd:P ;
    dtd:has_Value "(2) Der allgemeine Rentenwert (Ost) in der Alterssicherung der Landwirte beträgt ab dem 1. Juli 2008 10,78 Euro."@de .

dtd:P-id69 a dtd:P ;
    dtd:has_Value "(1) Der Ausgleichsbedarf beträgt zum 30. Juni 2009 0,9825."@de .

dtd:P-id70 a dtd:P ;
    dtd:has_Value "(2) Der Ausgleichsbedarf (Ost) beträgt zum 30. Juni 2009 0,9870."@de .

dtd:P-id72 a dtd:P ;
    dtd:has_Value "(1) Der Anpassungsfaktor für die zum 1. Juli 2008 anzupassenden Geldleistungen der gesetzlichen Unfallversicherung im Sinne des § 44 Abs. 4 und des § 95 des Siebten Buches Sozialgesetzbuch beträgt 1,0110."@de .

dtd:P-id73 a dtd:P ;
    dtd:has_Value "(2) Die vom Jahresarbeitsverdienst abhängigen Geldleistungen und das Pflegegeld der gesetzlichen Unfallversicherung im Sinne des § 215 Abs. 5 des Siebten Buches Sozialgesetzbuch für Versicherungsfälle, die vor dem 1. Juli 2008 eingetreten sind, werden zum 1. Juli 2008 angepasst. Der Anpassungsfaktor beträgt 1,0110."@de .

dtd:P-id75 a dtd:P ;
    dtd:has_DL dtd:DL-id53 ;
    dtd:has_Value "Das Pflegegeld der gesetzlichen Unfallversicherung beträgt ab dem 1. Juli 2008 1.für Versicherungsfälle, auf die § 44 Abs. 2 des Siebten Buches Sozialgesetzbuch anzuwenden ist, zwischen 300 Euro und 1.199 Euro monatlich,2.für Versicherungsfälle, auf die § 215 Abs. 5 des Siebten Buches Sozialgesetzbuch anzuwenden ist, zwischen 260 Euro und 1.040 Euro."@de .


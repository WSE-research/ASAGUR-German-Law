dtd:LA-id537 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jederzeit Zutritt in alle Diensträume zu gewähren."@de .

dtd:LA-id538 a dtd:LA ;
    dtd:has_Value "der Vollzug der Eilanordnung auszusetzen und"@de .

dtd:LA-id539 a dtd:LA ;
    dtd:has_Value "die Eilanordnung durch das zuständige Bundesministerium aufzuheben."@de .

dtd:LA-id540 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Anordnung nach § 2 Abs. 1 Satz 1 oder Absatz 1a Satz 1 zuwiderhandelt,"@de .

dtd:LA-id541 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 2 Abs. 2 Satz 2 eine Person betraut oder"@de .


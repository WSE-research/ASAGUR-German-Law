dtd:LA-id219 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sind Auslandsvermittlungsstellen die zentralen Adoptionsstellen und die in Absatz 2 genannten Adoptionsvermittlungsstellen;"@de .

dtd:LA-id220 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ist zentrale Behörde des Heimatstaates (Artikel 2 Absatz 1 des Übereinkommens) die Stelle, die nach dem Recht dieses Staates die jeweils in Betracht kommende Aufgabe einer zentralen Behörde wahrnimmt."@de .

dtd:LA-id221 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "anzugeben, aus welchem Heimatstaat sie ein Kind annehmen möchten,"@de .

dtd:LA-id222 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an den Voraussetzungen für die Vorlage der Berichte nach § 7b Absatz 2 Satz 1 und § 7c Absatz 2 Satz 2 des Adoptionsvermittlungsgesetzes mitzuwirken und"@de .

dtd:LA-id223 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu versichern, dass eine weitere Bewerbung um die Vermittlung eines Kindes aus dem Ausland nicht anhängig ist."@de .

dtd:LA-id224 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Annahme dem Wohl des Kindes dient und"@de .

dtd:LA-id225 a dtd:LA ;
    dtd:has_DL dtd:DL-id204 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "a)mit der Begründung eines Annahmeverhältnisses im Inland zu rechnen ist oder, b)sofern die Annahme im Ausland vollzogen werden soll, diese nicht zu einem Ergebnis führt, das unter Berücksichtigung des Kindeswohls mit wesentlichen Grundsätzen des deutschen Rechts offensichtlich unvereinbar, insbesondere mit den Grundrechten unvereinbar ist. "@de .

dtd:LA-id226 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit der Begründung eines Annahmeverhältnisses im Inland zu rechnen ist oder,"@de .

dtd:LA-id227 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sofern die Annahme im Ausland vollzogen werden soll, diese nicht zu einem Ergebnis führt, das unter Berücksichtigung des Kindeswohls mit wesentlichen Grundsätzen des deutschen Rechts offensichtlich unvereinbar, insbesondere mit den Grundrechten unvereinbar ist."@de .

dtd:LA-id228 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Auslandsvermittlungsstelle den Vermittlungsvorschlag der zentralen Behörde des Heimatstaates nach § 5 Abs. 1 Satz 1 gebilligt hat und"@de .

dtd:LA-id229 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Adoptionsbewerber sich gemäß § 7 Abs. 1 mit dem Vermittlungsvorschlag einverstanden erklärt haben."@de .

dtd:LA-id230 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aufgewandt wurden, während sich das Kind rechtmäßig in der Obhut der Adoptionsbewerber befand, und"@de .

dtd:LA-id231 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auch dann aufzuwenden gewesen wären, wenn zu diesem Zeitpunkt ein Annahmeverhältnis zwischen den Adoptionsbewerbern und dem Kind bestanden hätte."@de .


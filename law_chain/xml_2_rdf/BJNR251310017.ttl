dtd:LA-id233 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Auskunftsanspruch einer durch heterologe Verwendung von Samen gezeugten Person nach § 10 und die Bedeutung, die die Kenntnis der Abstammung für die Entwicklung eines Menschen hat, sowie über die Möglichkeit, sich über die Folgen einer Samenspende beraten zu lassen,"@de .

dtd:LA-id234 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Pflicht der Entnahmeeinrichtung, die in Absatz 2 Satz 1 aufgeführten personenbezogenen Daten zu erheben und diese sowie die Spendenkennungssequenz oder die eindeutige Spendennummer nach Absatz 2 Satz 2 für die in Absatz 4 Satz 2 bis 4 genannte Dauer zu speichern,"@de .

dtd:LA-id235 a dtd:LA ;
    dtd:has_DL dtd:DL-id133 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Umfang der Verarbeitung der personenbezogenen Daten des Samenspenders bei der Entnahmeeinrichtung sowie a)die Übermittlungsverpflichtung bezüglich seiner personenbezogenen Daten an das Bundesinstitut für Arzneimittel und Medizinprodukte,b)die Speicherung seiner personenbezogenen Daten in dem Samenspenderregister sowie deren Speicherungsdauer undc)das Recht, über Absatz 2 hinausgehende freiwillige Angaben jederzeit gemäß Absatz 3 Satz 2 widerrufen zu können,"@de .

dtd:LA-id236 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Übermittlungsverpflichtung bezüglich seiner personenbezogenen Daten an das Bundesinstitut für Arzneimittel und Medizinprodukte,"@de .

dtd:LA-id237 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Speicherung seiner personenbezogenen Daten in dem Samenspenderregister sowie deren Speicherungsdauer und"@de .

dtd:LA-id238 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Recht, über Absatz 2 hinausgehende freiwillige Angaben jederzeit gemäß Absatz 3 Satz 2 widerrufen zu können,"@de .

dtd:LA-id239 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Tatsache, dass das Bundesinstitut für Arzneimittel und Medizinprodukte den Samenspender von der Speicherung seiner personenbezogenen Daten in Kenntnis setzen und dass es dazu bei Bedarf eine Anfrage zu seinen Anschriftsdaten bei der Meldebehörde durchführen wird,"@de .

dtd:LA-id240 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verpflichtung des Bundesinstituts für Arzneimittel und Medizinprodukte nach § 10 Absatz 4 Satz 1, auf Antrag einer nach § 10 Absatz 1 anspruchsberechtigten Person Auskunft über die Identität des Samenspenders aus dem Samenspenderregister zu erteilen,"@de .

dtd:LA-id241 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Verfahren der Auskunftserteilung und die Verpflichtung des Bundesinstituts für Arzneimittel und Medizinprodukte, den Samenspender über eine ihn betreffende Auskunftserteilung und darüber zu informieren, dass das Bundesinstitut für Arzneimittel und Medizinprodukte dazu eine Anfrage zu seinen Anschriftsdaten bei der Meldebehörde durchführen wird, sowie"@de .

dtd:LA-id242 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Ausschluss der Feststellung der rechtlichen Vaterschaft des Samenspenders gemäß § 1600d Absatz 4 des Bürgerlichen Gesetzbuchs."@de .

dtd:LA-id243 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Familienname und, sofern abweichend, Geburtsname,"@de .

dtd:LA-id244 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vornamen,"@de .

dtd:LA-id245 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geburtstag und Geburtsort,"@de .

dtd:LA-id246 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Staatsangehörigkeit und"@de .

dtd:LA-id247 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anschrift."@de .

dtd:LA-id248 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Auskunftsanspruch einer durch heterologe Verwendung von Samen gezeugten Person nach § 10 und die Bedeutung, die die Kenntnis der Abstammung für die Entwicklung eines Menschen hat, sowie die Möglichkeit, sich über die Folgen einer künstlichen Befruchtung durch heterologe Verwendung von Samen beraten zu lassen,"@de .

dtd:LA-id249 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Pflicht der Einrichtung der medizinischen Versorgung, die in § 5 Absatz 2 Satz 1 aufgeführten personenbezogenen Daten zu erheben und für die in § 5 Absatz 6 Satz 2 genannte Dauer zu speichern,"@de .

dtd:LA-id250 a dtd:LA ;
    dtd:has_DL dtd:DL-id164 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Umfang der Verarbeitung der personenbezogenen Daten der Empfängerin der Samenspende bei der Einrichtung der medizinischen Versorgung sowie a)die Übermittlungsverpflichtung bezüglich ihrer personenbezogenen Daten an das Bundesinstitut für Arzneimittel und Medizinprodukte undb)die Speicherung ihrer personenbezogenen Daten im Samenspenderregister sowie deren Speicherungsdauer,"@de .


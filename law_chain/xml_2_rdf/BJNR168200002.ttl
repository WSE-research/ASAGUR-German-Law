dtd:LA-id2565 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der voraussichtliche Beginn der Amtszeit der zu wählenden Aufsichtsratsmitglieder der Arbeitnehmer;"@de .

dtd:LA-id2566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der zu wählenden Aufsichtsratsmitglieder der Arbeitnehmer;"@de .

dtd:LA-id2567 a dtd:LA ;
    dtd:has_Value "bei einem börsennotierten Unternehmen der Anteil, mit dem Frauen und Männer nach § 96 Absatz 2 Satz 1 des Aktiengesetzes jeweils mindestens im Aufsichtsrat vertreten sein müssen;"@de .

dtd:LA-id2568 a dtd:LA ;
    dtd:has_Value "bei einem börsennotierten Unternehmen, ob der Geschlechteranteil nach § 96 Absatz 2 Satz 1 des Aktiengesetzes gemäß § 96 Absatz 2 Satz 2 des Aktiengesetzes vom Aufsichtsrat insgesamt zu erfüllen ist (Gesamterfüllung) oder ob der Gesamterfüllung nach § 96 Absatz 2 Satz 3 des Aktiengesetzes widersprochen wurde, mit der Folge, dass der Geschlechteranteil für diese Wahl von der Anteilseignerseite und der Seite der Arbeitnehmer getrennt zu erfüllen ist (Getrennterfüllung);"@de .

dtd:LA-id2569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der in dem Unternehmen in der Regel beschäftigten Arbeitnehmerinnen und Arbeitnehmer."@de .

dtd:LA-id2570 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Betriebsrat und dem Sprecherausschuss,"@de .

dtd:LA-id2571 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den in dem Unternehmen vertretenen Gewerkschaften,"@de .

dtd:LA-id2572 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den nach § 117 Abs. 2 Satz 1 des Betriebsverfassungsgesetzes durch Tarifvertrag errichteten Vertretungen für im Flugbetrieb beschäftigte Arbeitnehmerinnen und Arbeitnehmer des Unternehmens."@de .

dtd:LA-id2573 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Betrieb eintritt oder aus ihm ausscheidet,"@de .

dtd:LA-id2574 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das 18. Lebensjahr vollendet oder"@de .

dtd:LA-id2575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Eigenschaft als in § 3 Abs. 1 Nr. 1 des Gesetzes bezeichneter Arbeitnehmer oder leitender Angestellter wechselt,"@de .

dtd:LA-id2576 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Datum ihres Erlasses;"@de .

dtd:LA-id2577 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wo und wie die Wahlberechtigten in die Wählerliste, das Gesetz und diese Verordnung Einsicht nehmen können;"@de .

dtd:LA-id2578 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass Einsprüche gegen die Richtigkeit der Wählerliste nur innerhalb von einer Woche seit Erlass der Bekanntmachung schriftlich beim Betriebswahlvorstand eingelegt werden können; der letzte Tag der Frist ist anzugeben;"@de .

dtd:LA-id2579 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass Einsprüche gegen Berichtigungen und Ergänzungen der Wählerliste nur innerhalb von einer Woche seit der Berichtigung oder der Ergänzung eingelegt werden können;"@de .

dtd:LA-id2580 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass an Wahlen und Abstimmungen nur Arbeitnehmerinnen und Arbeitnehmer teilnehmen können, die in der Wählerliste eingetragen sind."@de .

dtd:LA-id2581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass jede Arbeitnehmerin und jeder Arbeitnehmer innerhalb von einer Woche seit Erlass der Bekanntmachung schriftlich vom Betriebswahlvorstand die Änderung der eigenen Eintragung als in § 3 Abs. 1 Nr. 1 des Gesetzes bezeichneter Arbeitnehmer oder leitender Angestellter in der Wählerliste verlangen kann; der letzte Tag der Frist ist anzugeben;"@de .

dtd:LA-id2582 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass dem Änderungsverlangen nach Nummer 1 zu entsprechen ist, wenn ein Mitglied des Betriebswahlvorstands dem Verlangen zustimmt;"@de .

dtd:LA-id2583 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass gegen die Richtigkeit der Wählerliste nur Einspruch eingelegt werden kann, soweit nicht nach Nummer 1 eine Änderung der Wählerliste verlangt werden kann."@de .

dtd:LA-id2584 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Datum ihres Erlasses;"@de .

dtd:LA-id2585 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass die Aufsichtsratsmitglieder der Arbeitnehmer in unmittelbarer Wahl gewählt werden, wenn nicht die Wahlberechtigten die Wahl durch Delegierte beschließen;"@de .

dtd:LA-id2586 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mindestzahl der Wahlberechtigten, von denen ein Antrag auf Abstimmung darüber, dass die Wahl der Aufsichtsratsmitglieder der Arbeitnehmer durch Delegierte erfolgen soll, unterzeichnet sein muss;"@de .

dtd:LA-id2587 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass ein Antrag nur innerhalb von zwei Wochen seit Erlass der Bekanntmachung schriftlich beim Betriebswahlvorstand eingereicht werden kann; der letzte Tag der Frist ist anzugeben;"@de .

dtd:LA-id2588 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mindestzahl der Wahlberechtigten, deren Beteiligung an der Abstimmung erforderlich ist;"@de .

dtd:LA-id2589 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass ein Beschluss über die Wahl der Aufsichtsratsmitglieder der Arbeitnehmer durch Delegierte nur mit der Mehrheit der abgegebenen Stimmen gefasst werden kann;"@de .

dtd:LA-id2590 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anschrift des Betriebswahlvorstands."@de .

dtd:LA-id2591 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Datum ihres Erlasses;"@de .

dtd:LA-id2592 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass die Aufsichtsratsmitglieder der Arbeitnehmer durch Delegierte gewählt werden, wenn nicht die Wahlberechtigten die unmittelbare Wahl beschließen;"@de .

dtd:LA-id2593 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mindestzahl der Wahlberechtigten, von denen ein Antrag auf Abstimmung darüber, dass die Aufsichtsratsmitglieder der Arbeitnehmer in unmittelbarer Wahl gewählt werden sollen, unterzeichnet sein muss;"@de .


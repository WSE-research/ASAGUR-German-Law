dtd:LA-id468 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Würde sowie die Interessen und Bedürfnisse der Bewohnerinnen und Bewohner von Heimen vor Beeinträchtigungen zu schützen,"@de .

dtd:LA-id469 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Selbständigkeit, die Selbstbestimmung und die Selbstverantwortung der Bewohnerinnen und Bewohner zu wahren und zu fördern,"@de .

dtd:LA-id470 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einhaltung der dem Träger des Heims (Träger) gegenüber den Bewohnerinnen und Bewohnern obliegenden Pflichten zu sichern,"@de .

dtd:LA-id471 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mitwirkung der Bewohnerinnen und Bewohner zu sichern,"@de .

dtd:LA-id472 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine dem allgemein anerkannten Stand der fachlichen Erkenntnisse entsprechende Qualität des Wohnens und der Betreuung zu sichern,"@de .

dtd:LA-id473 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beratung in Heimangelegenheiten zu fördern sowie"@de .

dtd:LA-id474 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zusammenarbeit der für die Durchführung dieses Gesetzes zuständigen Behörden mit den Trägern und deren Verbänden, den Pflegekassen, dem Medizinischen Dienst der Krankenversicherung sowie den Trägern der Sozialhilfe zu fördern."@de .

dtd:LA-id475 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Räume, insbesondere die Wohn-, Aufenthalts-, Therapie- und Wirtschaftsräume sowie die Verkehrsflächen, sanitären Anlagen und die technischen Einrichtungen,"@de .

dtd:LA-id476 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Eignung der Leitung des Heims (Leitung) und der Beschäftigten."@de .

dtd:LA-id477 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bewohnerinnen und Bewohner sowie die Heimbeiräte und Heimfürsprecher über ihre Rechte und Pflichten,"@de .

dtd:LA-id478 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, die ein berechtigtes Interesse haben, über Heime im Sinne des § 1 und über die Rechte und Pflichten der Träger und der Bewohnerinnen und Bewohner solcher Heime und"@de .

dtd:LA-id479 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Antrag Personen und Träger, die die Schaffung von Heimen im Sinne des § 1 anstreben oder derartige Heime betreiben, bei der Planung und dem Betrieb der Heime."@de .

dtd:LA-id480 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Würde sowie die Interessen und Bedürfnisse der Bewohnerinnen und Bewohner vor Beeinträchtigungen schützen,"@de .

dtd:LA-id481 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Selbständigkeit, die Selbstbestimmung und die Selbstverantwortung der Bewohnerinnen und Bewohner wahren und fördern, insbesondere bei behinderten Menschen die sozialpädagogische Betreuung und heilpädagogische Förderung sowie bei Pflegebedürftigen eine humane und aktivierende Pflege unter Achtung der Menschenwürde gewährleisten,"@de .

dtd:LA-id482 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine angemessene Qualität der Betreuung der Bewohnerinnen und Bewohner, auch soweit sie pflegebedürftig sind, in dem Heim selbst oder in angemessener anderer Weise einschließlich der Pflege nach dem allgemein anerkannten Stand medizinisch-pflegerischer Erkenntnisse sowie die ärztliche und gesundheitliche Betreuung sichern,"@de .

dtd:LA-id483 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Eingliederung behinderter Menschen fördern,"@de .

dtd:LA-id484 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Bewohnerinnen und Bewohnern eine nach Art und Umfang ihrer Betreuungsbedürftigkeit angemessene Lebensgestaltung ermöglichen und die erforderlichen Hilfen gewähren,"@de .

dtd:LA-id485 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die hauswirtschaftliche Versorgung sowie eine angemessene Qualität des Wohnens erbringen,"@de .

dtd:LA-id486 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sicherstellen, dass für pflegebedürftige Bewohnerinnen und Bewohner Pflegeplanungen aufgestellt und deren Umsetzung aufgezeichnet werden,"@de .

dtd:LA-id487 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gewährleisten, dass in Einrichtungen der Behindertenhilfe für die Bewohnerinnen und Bewohner Förder- und Hilfepläne aufgestellt und deren Umsetzung aufgezeichnet werden,"@de .

dtd:LA-id488 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen ausreichenden Schutz der Bewohnerinnen und Bewohner vor Infektionen gewährleisten und sicherstellen, dass von den Beschäftigten die für ihren Aufgabenbereich einschlägigen Anforderungen der Hygiene eingehalten werden, und"@de .

dtd:LA-id489 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sicherstellen, dass die Arzneimittel bewohnerbezogen und ordnungsgemäß aufbewahrt und die in der Pflege tätigen Mitarbeiterinnen und Mitarbeiter mindestens einmal im Jahr über den sachgerechten Umgang mit Arzneimitteln beraten werden."@de .

dtd:LA-id490 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die notwendige Zuverlässigkeit, insbesondere die wirtschaftliche Leistungsfähigkeit zum Betrieb des Heims, besitzt,"@de .

dtd:LA-id491 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sicherstellt, dass die Zahl der Beschäftigten und ihre persönliche und fachliche Eignung für die von ihnen zu leistende Tätigkeit ausreicht,"@de .

dtd:LA-id492 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "angemessene Entgelte verlangt und"@de .

dtd:LA-id493 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Qualitätsmanagement betreibt."@de .

dtd:LA-id494 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einhaltung der in den Rechtsverordnungen nach § 3 enthaltenen Regelungen gewährleistet ist,"@de .

dtd:LA-id495 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die vertraglichen Leistungen erbracht werden und"@de .

dtd:LA-id496 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einhaltung der nach § 14 Abs. 7 erlassenen Vorschriften gewährleistet ist."@de .

dtd:LA-id497 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den vorgesehenen Zeitpunkt der Betriebsaufnahme,"@de .

dtd:LA-id498 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Namen und die Anschriften des Trägers und des Heims,"@de .

dtd:LA-id499 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Nutzungsart des Heims und der Räume sowie deren Lage, Zahl und Größe und die vorgesehene Belegung der Wohnräume,"@de .

dtd:LA-id500 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die vorgesehene Zahl der Mitarbeiterstellen,"@de .

dtd:LA-id501 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Namen, die berufliche Ausbildung und den Werdegang der Heimleitung und bei Pflegeheimen auch der Pflegedienstleitung sowie die Namen und die berufliche Ausbildung der Betreuungskräfte,"@de .

dtd:LA-id502 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die allgemeine Leistungsbeschreibung sowie die Konzeption des Heims,"@de .

dtd:LA-id503 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Versorgungsvertrag nach § 72 sowie eine Leistungs- und Qualitätsvereinbarung nach § 80a des Elften Buches Sozialgesetzbuch oder die Erklärung, ob ein solcher Versorgungsvertrag oder eine solche Leistungs- und Qualitätsvereinbarung angestrebt werden,"@de .

dtd:LA-id504 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vereinbarungen nach § 75 Abs. 3 des Zwölften Buches Sozialgesetzbuch oder die Erklärung, ob solche Vereinbarungen angestrebt werden,"@de .

dtd:LA-id505 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einzelvereinbarungen aufgrund § 39a des Fünften Buches Sozialgesetzbuch oder die Erklärung, ob solche Vereinbarungen angestrebt werden,"@de .

dtd:LA-id506 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Unterlagen zur Finanzierung der Investitionskosten,"@de .

dtd:LA-id507 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Muster der Heimverträge sowie sonstiger verwendeter Verträge,"@de .

dtd:LA-id508 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Satzung oder einen Gesellschaftsvertrag des Trägers sowie"@de .

dtd:LA-id509 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Heimordnung, soweit eine solche vorhanden ist."@de .

dtd:LA-id510 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die wirtschaftliche und finanzielle Lage des Heims,"@de .

dtd:LA-id511 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Nutzungsart, die Lage, die Zahl und die Größe der Räume sowie die Belegung der Wohnräume,"@de .

dtd:LA-id512 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Name, der Vorname, das Geburtsdatum, die Anschrift und die Ausbildung der Beschäftigten, deren regelmäßige Arbeitszeit, die von ihnen in dem Heim ausgeübte Tätigkeit und die Dauer des Beschäftigungsverhältnisses sowie die Dienstpläne,"@de .

dtd:LA-id513 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Name, der Vorname, das Geburtsdatum, das Geschlecht, der Betreuungsbedarf der Bewohnerinnen und Bewohner sowie bei pflegebedürftigen Bewohnerinnen und Bewohnern die Pflegestufe,"@de .

dtd:LA-id514 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Erhalt, die Aufbewahrung und die Verabreichung von Arzneimitteln einschließlich der pharmazeutischen Überprüfung der Arzneimittelvorräte und der Unterweisung der Mitarbeiterinnen und Mitarbeiter über den sachgerechten Umgang mit Arzneimitteln,"@de .

dtd:LA-id515 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Pflegeplanungen und die Pflegeverläufe für pflegebedürftige Bewohnerinnen und Bewohner,"@de .

dtd:LA-id516 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Bewohnerinnen und Bewohner von Einrichtungen der Behindertenhilfe Förder- und Hilfepläne einschließlich deren Umsetzung,"@de .

dtd:LA-id517 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Maßnahmen zur Qualitätsentwicklung sowie zur Qualitätssicherung,"@de .

dtd:LA-id518 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die freiheitsbeschränkenden und die freiheitsentziehenden Maßnahmen bei Bewohnerinnen und Bewohnern sowie der Angabe des für die Anordnung der Maßnahme Verantwortlichen,"@de .

dtd:LA-id519 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für die Bewohnerinnen und Bewohner verwalteten Gelder oder Wertsachen."@de .

dtd:LA-id520 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "andere als die in § 5 aufgeführten Leistungen des Trägers abgegolten werden,"@de .

dtd:LA-id521 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "geringwertige Aufmerksamkeiten versprochen oder gewährt werden,"@de .

dtd:LA-id522 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leistungen im Hinblick auf die Überlassung eines Heimplatzes zum Bau, zum Erwerb, zur Instandsetzung, zur Ausstattung oder zum Betrieb des Heims versprochen oder gewährt werden,"@de .

dtd:LA-id523 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id524 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für das Heim genutzten Grundstücke und Räume zu betreten; soweit diese einem Hausrecht der Bewohnerinnen und Bewohner unterliegen, nur mit deren Zustimmung,"@de .

dtd:LA-id525 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Prüfungen und Besichtigungen vorzunehmen,"@de .

dtd:LA-id526 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einsicht in die Aufzeichnungen nach § 13 des Auskunftspflichtigen im jeweiligen Heim zu nehmen,"@de .

dtd:LA-id527 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich mit den Bewohnerinnen und Bewohnern sowie dem Heimbeirat oder dem Heimfürsprecher in Verbindung zu setzen,"@de .

dtd:LA-id528 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei pflegebedürftigen Bewohnerinnen und Bewohnern mit deren Zustimmung den Pflegezustand in Augenschein zu nehmen,"@de .

dtd:LA-id529 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beschäftigten zu befragen."@de .

dtd:LA-id530 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anzeige nach § 12 unterlassen oder unvollständige Angaben gemacht hat,"@de .

dtd:LA-id531 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anordnungen nach § 17 Abs. 1 nicht innerhalb der gesetzten Frist befolgt,"@de .

dtd:LA-id532 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen entgegen einem nach § 18 ergangenen Verbot beschäftigt,"@de .

dtd:LA-id533 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegen § 14 Abs. 1, 3 oder Abs. 4 oder eine nach § 14 Abs. 7 erlassene Rechtsverordnung verstößt."@de .

dtd:LA-id534 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 12 Abs. 1 Satz 2 eine Anzeige nicht, nicht richtig oder nicht rechtzeitig erstattet,"@de .

dtd:LA-id535 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Heim betreibt, obwohl ihm dies durch vollziehbare Verfügung nach § 19 Abs. 1 oder 2 untersagt worden ist,"@de .

dtd:LA-id536 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 14 Abs. 1 sich Geld- oder geldwerte Leistungen versprechen oder gewähren lässt oder einer nach § 14 Abs. 7 erlassenen Rechtsverordnung zuwiderhandelt, soweit diese für einen bestimmten Tatbestand auf diese Bußgeldvorschrift verweist."@de .

dtd:LA-id537 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Rechtsverordnung nach § 3 oder § 10 Abs. 5 zuwiderhandelt, soweit sie für einen bestimmten Tatbestand auf diese Bußgeldvorschrift verweist,"@de .

dtd:LA-id538 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 12 Abs. 4 Satz 1 eine Anzeige nicht, nicht richtig oder nicht rechtzeitig erstattet,"@de .

dtd:LA-id539 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 14 Abs. 5 Satz 1 sich Geld- oder geldwerte Leistungen versprechen oder gewähren lässt,"@de .

dtd:LA-id540 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 15 Abs. 1 Satz 5 eine Auskunft nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig erteilt oder entgegen § 15 Abs. 2 Satz 2 oder Abs. 3 Satz 2 eine Maßnahme nicht duldet oder"@de .

dtd:LA-id541 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Anordnung nach § 17 Abs. 1 oder § 18 zuwiderhandelt."@de .

dtd:LA-id542 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Heimgesetz in der Fassung der Bekanntmachung vom 23. April 1990 (BGBl. I S. 763, 1069)"@de .

dtd:LA-id543 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit folgender Maßgabe:"@de .

dtd:LA-id544 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Heimverhältnisse, die beim Wirksamwerden des Beitritts bestehen, richten sich von diesem Zeitpunkt an nach dem neuen Recht."@de .


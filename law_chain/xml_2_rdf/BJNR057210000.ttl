dtd:LA-id1655 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebsstätte:"@de .

dtd:LA-id1656 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geschäftsstelle des Unternehmens, das an einer Binnenschifffahrtsstraße oder an einer Wasserstraße, die mit einer Binnenschifffahrtsstraße verbunden ist, liegt, und an der das Unternehmen Sportboote zur Vermietung anbietet,"@de .

dtd:LA-id1657 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Binnenschifffahrtsstraßen:"@de .

dtd:LA-id1658 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bundeswasserstraßen nach § 1 Abs. 1 Nr. 2 des Binnenschifffahrtsaufgabengesetzes mit Ausnahme der Elbe im Hamburger Hafen und den Seeschifffahrtsstraßen,"@de .

dtd:LA-id1659 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sportboot:"@de .

dtd:LA-id1660 a dtd:LA ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id1863 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Sport- oder Erholungszwecke verwendetes Wasserfahrzeug mit einer Länge von weniger als 20 m ohne Ruder und Bugspriet, dessen Produkt aus Länge x Breite x Tiefgang ein Volumen von 100 cbm nicht erreicht, ausgenommen Segelsurfbretter,"@de .

dtd:LA-id1661 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unternehmen:"@de .

dtd:LA-id1662 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "natürliche oder juristische Personen sowie rechtsfähige Personengesellschaften und deren Bevollmächtigte, die Sportboote zum Einsatz auf Binnenschifffahrtsstraßen vermieten,"@de .

dtd:LA-id1663 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vermietung:"@de .

dtd:LA-id1664 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die gegen Entgelt erfolgende Überlassung eines Sportbootes zum Gebrauch an wechselnde Mieter; wird ein Sportboot ausschließlich zu Testzwecken einem Kaufinteressenten überlassen, liegt keine Vermietung im Sinne dieser Verordnung vor, wenn die Testfahrt den Zeitraum von 48 Stunden nicht überschreitet,"@de .

dtd:LA-id1665 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gelegenheitsverkehr:"@de .

dtd:LA-id1666 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beförderung von Personen gegen Entgelt (Fahrgäste) unter Gestellung eines Bootsführers durch das Unternehmen mit einem in Gänze angemieteten Sportboot zur Ausführung einer Fahrt, deren Zweck, Ziel und Ablauf ausschließlich der Mieter bestimmt und die keine regelmäßige Verkehrsverbindung zwischen bestimmten Ausgangs- und Endpunkten darstellt."@de .

dtd:LA-id1667 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Binnenschiffsuntersuchungsordnung:"@de .

dtd:LA-id1668 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Binnenschiffsuntersuchungsordnung vom 21. September 2018 (BGBl. I S. 1398) in der jeweils geltenden Fassung,"@de .

dtd:LA-id1669 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Binnenschifffahrt-Kennzeichnungsverordnung:"@de .

dtd:LA-id1670 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Binnenschifffahrt-Kennzeichnungsverordnung vom 21. Februar 1995 (BGBl. I S. 226), die zuletzt durch Artikel 30 des Gesetzes vom 8. November 2011 (BGBl. I S. 2178) geändert worden ist, in der jeweils geltenden und anzuwendenden Fassung,"@de .

dtd:LA-id1671 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "See-Sportbootverordnung:"@de .

dtd:LA-id1672 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "See-Sportbootverordnung vom 29. August 2002 (BGBl. I S. 3457), die zuletzt durch Artikel 34 des Gesetzes vom 8. November 2011 (BGBl. I S. 2178) geändert worden ist, in der jeweils geltenden Fassung,"@de .

dtd:LA-id1673 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ES-TRIN:"@de .

dtd:LA-id1674 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Europäischer Standard der technischen Vorschriften für Binnenschiffe in der Ausgabe 2019/1, der vom Europäischen Ausschuss für die Ausarbeitung von Standards im Bereich der Binnenschifffahrt (CESNI) angenommen wurde (Bekanntmachung des Bundesministeriums für Verkehr und digitale Infrastruktur vom 9. Dezember 2019, BAnz AT 09.12.2019 B2). Bei der Anwendung des ES-TRIN ist unter Mitgliedstaat ein Mitgliedstaat der Europäischen Union oder der Zentralkommission für die Rheinschifffahrt zu verstehen,"@de .

dtd:LA-id1675 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Binnenschifffahrtsstraßen-Ordnung:"@de .

dtd:LA-id1676 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anlage zu § 1 Absatz 1 der Verordnung zur Einführung der Binnenschifffahrtsstraßen-Ordnung vom 16. Dezember 2011 (BGBl. 2012 I S. 2) in der jeweils geltenden und anzuwendenden Fassung."@de .

dtd:LA-id1677 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in dessen Amtsbezirk das Sportboot seinen ständigen Liegeplatz hat oder sich die Betriebsstätte befindet oder"@de .

dtd:LA-id1678 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das dem Sitz des Unternehmens am nächsten liegt."@de .

dtd:LA-id1679 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Fahrtauglichkeitsbescheinigung nach der Binnenschiffsuntersuchungsordnung,"@de .

dtd:LA-id1680 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein gültiges Abnahmeprotokoll des Germanischen Lloyds oder einer anderen benannten Stelle nach Artikel 9 der Richtlinie 94/25/EG oder eines öffentlich bestellten und vereidigten Sachverständigen oder eines gemäß Norm EN 45013 von einer akkreditierten Stelle zertifizierten Boots- und Yachtsachverständigen mit dem Inhalt der Anlage 2 oder"@de .

dtd:LA-id1681 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine gültige Konformitätserklärung nach dem Muster des Anhangs XV der Richtlinie 94/25/EG zur Angleichung der Rechts- und Verwaltungsvorschriften der Mitgliedstaaten über Sportboote."@de .


dtd:LA-id732 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Teilnahme an den Auswertungen vorgeschriebener Aufzeichnungen,"@de .

dtd:LA-id733 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Teilnahme an weiterführenden Untersuchungen einschließlich der Beratungen über die Ergebnisse, Ursachen und Sicherheitsempfehlungen,"@de .

dtd:LA-id734 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anregungen zum Untersuchungsumfang."@de .

dtd:LA-id735 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auskünfte einholen,"@de .

dtd:LA-id736 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zeugen, Sachverständige und andere für die Ermittlungen wichtige Personen befragen und schriftliche Äußerungen von ihnen einholen,"@de .

dtd:LA-id737 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Urkunden und Akten beiziehen und einsehen, soweit nicht besondere Verwendungsbeschränkungen entgegenstehen."@de .

dtd:LA-id738 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die in § 17 Abs. 1 genannten Adressaten,"@de .

dtd:LA-id739 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Internationale Zivilluftfahrt-Organisation bei Luftfahrzeugen mit einer Höchstmasse über 5 700 kg,"@de .

dtd:LA-id740 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kommission der Europäischen Gemeinschaft."@de .

dtd:LA-id741 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vertraulichen Erklärungen und Angaben, die im Zusammenhang mit der Untersuchung auf Befragen der Bundesstelle abgegeben wurden; als vertraulich sind Erklärungen zu werten, die als solche abgegeben wurden und als deren Urheber die erklärende Person nicht in Erscheinung treten will oder darf,"@de .

dtd:LA-id742 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufnahmen von persönlichen Gesprächen auf Tonaufzeichnungsgeräten und deren Umschrift,"@de .

dtd:LA-id743 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "medizinischer Daten einschließlich bildlicher Darstellungen von Personen,"@de .

dtd:LA-id744 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die beteiligten Luftfahrzeuge nach Staatsangehörigkeitszeichen, Baumuster, Hersteller, Art der Beschädigung des Luftfahrzeugs, Art der Drittschäden, bei der Beförderung gefährlicher Güter die Art des Gefahrguts,"@de .

dtd:LA-id745 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der Luftfahrzeuginsassen,"@de .

dtd:LA-id746 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der verunglückten Insassen und die Unfallfolgen (tödliche, schwere, andere Verletzungen),"@de .

dtd:LA-id747 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unfallort, Datum, Hergang und Umstände des Unfalls (Betriebsphase, Art der Störung) sowie ermittelte Unfallursachen."@de .

dtd:LA-id748 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 12 Abs. 1 Satz 2 die Unfallstelle betritt,"@de .

dtd:LA-id749 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 12 Abs. 2 Satz 1 die Unfallstelle, Unfallspuren, Wrackteile, Trümmerstücke oder sonstigen Inhalt des Luftfahrzeugs vor der Freigabe verändert,"@de .

dtd:LA-id750 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ohne Zustimmung nach § 14 Abs. 7 Satz 1 sich zum Stand der Untersuchung oder zu einzelnen Ergebnissen öffentlich äußert oder"@de .

dtd:LA-id751 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 16 Abs. 3 Satz 1 der Pflicht zu wahrheitsgemäßen Aussage oder zur Erstattung von Gutachten nicht nachkommt."@de .

dtd:LA-id752 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fastzusammenstoß/gefährliche Begegnung; gefährliche Annäherung von zwei Luftfahrzeugen, bei der mindestens ein Luftfahrzeug nach Instrumentenflugregeln betrieben wurde und ein Ausweichsmanöver erforderlich war oder angemessen gewesen wäre, um einen Zusammenstoß oder eine gefährliche Situation zu vermeiden;"@de .

dtd:LA-id753 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nur knapp vermiedene Bodenberührung mit einem nicht außer Kontrolle geratenen Luftfahrzeug (CFIT);"@de .

dtd:LA-id754 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "abgebrochener Start auf einer gesperrten oder belegten Startbahn oder Start von einer solchen Bahn mit kritischem Hindernisabstand;"@de .

dtd:LA-id755 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Landung oder Landeversuch auf einer gesperrten oder belegten Landebahn;"@de .

dtd:LA-id756 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erhebliches Unterschreiten der vorausberechneten Flugleistungen beim Start oder im Anfangssteigflug;"@de .

dtd:LA-id757 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Brände oder Rauch in der Fluggastkabine oder im Laderaum und Triebwerksbrände, auch wenn diese Brände mit Hilfe von Löschmitteln gelöscht wurden;"@de .

dtd:LA-id758 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Umstände, die die Flugbesatzung zur Benutzung von Sauerstoff zwangen;"@de .

dtd:LA-id759 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Strukturversagen an der Luftfahrzeugzelle oder eine Triebwerkszerlegung, die nicht als Unfall eingestuft werden;"@de .

dtd:LA-id760 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mehrfaches Versagen eines oder mehrerer Luftfahrzeugsysteme, wodurch der Betrieb des Luftfahrzeugs ernsthaft beeinträchtigt wurde;"@de .

dtd:LA-id761 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jeder Ausfall von Flugbesatzungsmitgliedern während des Flugs;"@de .

dtd:LA-id762 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jeder Kraftstoffmangel, bei dem der Luftfahrzeugführer eine Notlage erklären mußte;"@de .

dtd:LA-id763 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Störungen bei Start oder Landung; Störungen wie zu frühes oder zu spätes Aufsetzen, Überschießen oder seitliches Abkommen von der Start- oder Landebahn;"@de .

dtd:LA-id764 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ausfall von Systemen, meteorologische Erscheinungen, Betrieb außerhalb des zulässigen Flugbereichs oder sonstige Ergebnisse, die Schwierigkeiten bei der Steuerung des Luftfahrzeugs hätten hervorrufen können;"@de .

dtd:LA-id765 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Versagen von mehr als einem System in einem redundanten System, das für die Flugführung und -navigation unverzichtbar ist."@de .

dtd:P-id767 a dtd:P ;
    dtd:has_Value "Dieses Gesetz dient der Umsetzung der Richtlinie 94/56/EWG des Ratesvom 21. November 1994 über Grundsätze für die Untersuchung vonUnfällen und Störungen in der Zivilluftfahrt (ABl. EG Nr. L 319S. 14).Das G wurde als Artikel 1 G 96-1-39/1 v. 26.8.1998 I 2470 (FlUUG/LuftRAnpG) vomBundestag beschlossen und ist gem. Art. 6 dieses G mWv 1.9.1998in Kraft getreten."@de ;

dtd:LA-id1763 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Beamten während ihres Beamtenverhältnisses begangenen Dienstvergehen (§ 77 Abs. 1 des Bundesbeamtengesetzes) und"@de .

dtd:LA-id1764 a dtd:LA ;
    dtd:has_DL dtd:DL-id1537 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Ruhestandsbeamtena)während ihres Beamtenverhältnisses begangenen Dienstvergehen (§ 77 Abs. 1 des Bundesbeamtengesetzes) undb)nach Eintritt in den Ruhestand begangenen als Dienstvergehen geltenden Handlungen (§ 77 Abs. 2 des Bundesbeamtengesetzes)."@de .

dtd:LA-id1765 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "während ihres Beamtenverhältnisses begangenen Dienstvergehen (§ 77 Abs. 1 des Bundesbeamtengesetzes) und"@de .

dtd:LA-id1766 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach Eintritt in den Ruhestand begangenen als Dienstvergehen geltenden Handlungen (§ 77 Abs. 2 des Bundesbeamtengesetzes)."@de .

dtd:LA-id1767 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verweis (§ 6)"@de .

dtd:LA-id1768 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geldbuße (§ 7)"@de .

dtd:LA-id1769 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kürzung der Dienstbezüge (§ 8)"@de .

dtd:LA-id1770 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zurückstufung (§ 9) und"@de .

dtd:LA-id1771 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Entfernung aus dem Beamtenverhältnis (§ 10)."@de .

dtd:LA-id1772 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kürzung des Ruhegehalts (§ 11) und"@de .

dtd:LA-id1773 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aberkennung des Ruhegehalts (§ 12)."@de .

dtd:LA-id1774 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn der Beamte ihrer nicht würdig ist,"@de .

dtd:LA-id1775 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn die Entfernung aus dem Beamtenverhältnis zumindest auch auf der Verletzung der Pflicht des Beamten beruht, sich durch sein gesamtes Verhalten zu der freiheitlichen demokratischen Grundordnung zu bekennen und für deren Erhaltung einzutreten, oder"@de .

dtd:LA-id1776 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit der Beamte den erkennbaren Umständen nach nicht bedürftig ist."@de .

dtd:LA-id1777 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Verweis, wenn der Beamte durch ein leichtes Dienstvergehen das Vertrauen des Dienstherrn oder der Allgemeinheit in die pflichtgemäße Amtsführung geringfügig beeinträchtigt hat,"@de .

dtd:LA-id1778 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Geldbuße, wenn der Beamte durch ein leichtes bis mittelschweres Dienstvergehen das Vertrauen des Dienstherrn oder der Allgemeinheit in die pflichtgemäße Amtsführung nicht nur geringfügig beeinträchtigt hat,"@de .

dtd:LA-id1779 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Kürzung der Dienstbezüge, wenn der Beamte durch ein mittelschweres Dienstvergehen das Vertrauen des Dienstherrn oder der Allgemeinheit in die pflichtgemäße Amtsführung erheblich beeinträchtigt hat,"@de .

dtd:LA-id1780 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Kürzung des Ruhegehalts, wenn der Ruhestandsbeamte ein mittelschweres Dienstvergehen begangen hat, das geeignet ist, das Ansehen des öffentlichen Dienstes oder des Berufsbeamtentums erheblich zu beeinträchtigen,"@de .

dtd:LA-id1781 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Zurückstufung, wenn der Beamte durch ein mittelschweres bis schweres Dienstvergehen das Vertrauen des Dienstherrn oder der Allgemeinheit in die pflichtgemäße Amtsführung nachhaltig erschüttert hat."@de .

dtd:LA-id1782 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Verweis, eine Geldbuße oder eine Kürzung des Ruhegehalts nicht ausgesprochen werden,"@de .

dtd:LA-id1783 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Kürzung der Dienstbezüge nur ausgesprochen werden, wenn dies zusätzlich erforderlich ist, um den Beamten zur Pflichterfüllung anzuhalten."@de .

dtd:LA-id1784 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Verweis, wenn seit der Vollendung eines Dienstvergehens mehr als zwei Jahre vergangen sind,"@de .

dtd:LA-id1785 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Geldbuße oder eine Kürzung der Dienstbezüge oder des Ruhegehalts, wenn seit der Vollendung eines Dienstvergehens mehr als drei Jahre vergangen sind, und"@de .

dtd:LA-id1786 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Zurückstufung, wenn seit der Vollendung eines Dienstvergehens mehr als sieben Jahre vergangen sind."@de .

dtd:LA-id1787 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "schriftliche dienstliche Auskünfte eingeholt werden,"@de .

dtd:LA-id1788 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zeugen und Sachverständige vernommen oder ihre schriftliche Äußerung eingeholt werden,"@de .

dtd:LA-id1789 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Urkunden und Akten beigezogen sowie"@de .

dtd:LA-id1790 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Augenschein eingenommen werden."@de .

dtd:LA-id1791 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Entfernung aus dem Beamtenverhältnis nach § 5 Absatz 1 Nummer 5 in Verbindung mit § 10 Absatz 1,"@de .

dtd:LA-id1792 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einstellung eines Disziplinarverfahrens, wenn das Disziplinarverfahren wegen Beendigung des Beamtenverhältnisses nach § 41 Absatz 1 des Bundesbeamtengesetzes nicht zu Ende geführt wird, und"@de .

dtd:LA-id1793 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einstellung eines Disziplinarverfahrens, wenn die Beamtin oder der Beamte auf Verlangen nach § 33 des Bundesbeamtengesetzes aus dem Beamtenverhältnis entlassen wird und das Disziplinarverfahren voraussichtlich zur Entfernung aus dem Beamtenverhältnis geführt hätte."@de .

dtd:LA-id1794 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Dienstvergehen nicht erwiesen ist,"@de .

dtd:LA-id1795 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Dienstvergehen zwar erwiesen ist, eine Disziplinarmaßnahme jedoch nicht angezeigt erscheint,"@de .

dtd:LA-id1796 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 14 oder § 15 eine Disziplinarmaßnahme nicht ausgesprochen werden darf oder"@de .

dtd:LA-id1797 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Disziplinarverfahren oder eine Disziplinarmaßnahme aus sonstigen Gründen unzulässig ist."@de .

dtd:LA-id1798 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Beamte stirbt,"@de .

dtd:LA-id1799 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Beamtenverhältnis durch Entlassung, Verlust der Beamtenrechte oder Entfernung endet oder"@de .

dtd:LA-id1800 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einem Ruhestandsbeamten die Folgen einer gerichtlichen Entscheidung nach § 59 Abs. 1 des Beamtenversorgungsgesetzes eintreten."@de .

dtd:LA-id1801 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Tatsachen, die ein Dienstvergehen begründen,"@de .

dtd:LA-id1802 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die anderen Tatsachen, die für die Entscheidung bedeutsam sind, und"@de .

dtd:LA-id1803 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beweismittel, die für die Entscheidung bedeutsam sind."@de .

dtd:LA-id1804 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der persönliche und berufliche Werdegang des Beamten und"@de .

dtd:LA-id1805 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gang des Disziplinarverfahrens."@de .

dtd:LA-id1806 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die oberste Dienstbehörde bis zum Höchstmaß und"@de .

dtd:LA-id1807 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die der obersten Dienstbehörde unmittelbar nachgeordneten Dienstvorgesetzten bis zu einer Kürzung um 20 Prozent auf zwei Jahre."@de .

dtd:LA-id1808 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Disziplinarverfahren voraussichtlich die Entfernung aus dem Beamtenverhältnis oder die Aberkennung des Ruhegehalts erfolgen wird,"@de .

dtd:LA-id1809 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in einem wegen desselben Sachverhalts eingeleiteten Strafverfahren voraussichtlich eine Strafe verhängt wird, die den Verlust der Rechte als Beamter oder Ruhestandsbeamter zur Folge hat,"@de .

dtd:LA-id1810 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einem Beamten auf Probe oder einem Beamten auf Widerruf voraussichtlich eine Entlassung nach § 5 Absatz 3 Satz 2 dieses Gesetzes in Verbindung mit § 34 Absatz 1 Satz 1 Nummer 1 oder § 37 Absatz 1 Satz 1 des Bundesbeamtengesetzes erfolgen wird oder"@de .

dtd:LA-id1811 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch sein Verbleiben im Dienst der Dienstbetrieb oder die Ermittlungen wesentlich beeinträchtigt würden und die vorläufige Dienstenthebung zu der Bedeutung der Sache und der zu erwartenden Disziplinarmaßnahme nicht außer Verhältnis steht."@de .

dtd:LA-id1812 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Disziplinarverfahren voraussichtlich die Aberkennung des Ruhegehalts erfolgen wird oder"@de .

dtd:LA-id1813 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in einem wegen desselben Sachverhalts eingeleiteten Strafverfahren voraussichtlich eine Strafe verhängt wird, die den Verlust der Rechte als Ruhestandsbeamter zur Folge hat."@de .

dtd:LA-id1814 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Disziplinarverfahren unanfechtbar die Entfernung aus dem Beamtenverhältnis, die Aberkennung des Ruhegehalts oder eine Entlassung nach § 5 Absatz 3 Satz 2 dieses Gesetzes in Verbindung mit § 34 Absatz 1 Satz 1 Nummer 1 oder § 37 Absatz 1 Satz 1 des Bundesbeamtengesetzes erfolgt ist,"@de .

dtd:LA-id1815 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in einem wegen desselben Sachverhalts eingeleiteten Strafverfahren rechtskräftig eine Strafe verhängt worden ist, die den Verlust der Rechte als Beamter oder Ruhestandsbeamter zur Folge hat,"@de .

dtd:LA-id1816 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Disziplinarverfahren auf Grund des § 32 Absatz 1 Nummer 3 eingestellt worden ist und ein neues Disziplinarverfahren, das innerhalb von drei Monaten nach der Einstellung wegen desselben Sachverhalts eingeleitet worden ist, zur Entfernung aus dem Beamtenverhältnis oder zur Aberkennung des Ruhegehalts geführt hat oder"@de .

dtd:LA-id1817 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Disziplinarverfahren aus den Gründen des § 32 Absatz 2 eingestellt worden ist und die für den Erlass der Disziplinarverfügung zuständige Behörde festgestellt hat, dass die Entfernung aus dem Beamtenverhältnis oder die Aberkennung des Ruhegehalts gerechtfertigt gewesen wäre."@de .

dtd:LA-id1818 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Zurücknahme der Klage, des Antrags oder eines Rechtsmittels,"@de .

dtd:LA-id1819 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Erledigung des gerichtlichen Disziplinarverfahrens in der Hauptsache und"@de .

dtd:LA-id1820 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über die Kosten."@de .

dtd:LA-id1821 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch das Dienstvergehen verletzt ist,"@de .

dtd:LA-id1822 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ehegatte, Lebenspartner oder gesetzlicher Vertreter des Beamten oder des Verletzten ist oder war,"@de .

dtd:LA-id1823 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit dem Beamten oder dem Verletzten in gerader Linie verwandt oder verschwägert oder in der Seitenlinie bis zum dritten Grad verwandt oder bis zum zweiten Grad verschwägert ist oder war,"@de .

dtd:LA-id1824 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in dem Disziplinarverfahren gegen den Beamten tätig war oder als Zeuge gehört wurde oder als Sachverständiger ein Gutachten erstattet hat,"@de .

dtd:LA-id1825 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in einem wegen desselben Sachverhalts eingeleiteten Straf- oder Bußgeldverfahren gegen den Beamten beteiligt war,"@de .

dtd:LA-id1826 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dienstvorgesetzter des Beamten ist oder war oder bei einem Dienstvorgesetzten des Beamten mit der Bearbeitung von Personalangelegenheiten des Beamten befasst ist oder"@de .

dtd:LA-id1827 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Mitglied einer Personalvertretung in dem Disziplinarverfahren gegen den Beamten mitgewirkt hat."@de .

dtd:LA-id1828 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "er im Strafverfahren rechtskräftig zu einer Freiheits-strafe verurteilt worden ist,"@de .

dtd:LA-id1829 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Disziplinarverfahren gegen ihn unanfechtbar eine Disziplinarmaßnahme mit Ausnahme eines Verweises ausgesprochen worden ist,"@de .

dtd:LA-id1830 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "er in ein Amt außerhalb der Bezirke, für die das Gericht zuständig ist, versetzt wird,"@de .

dtd:LA-id1831 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Beamtenverhältnis endet oder"@de .

dtd:LA-id1832 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Voraussetzungen für das Amt des Beamtenbeisitzers nach § 47 Abs. 1 bei ihrer Auswahl oder Bestellung nicht vorlagen."@de .

dtd:LA-id1833 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in dem Urteil eine Disziplinarmaßnahme ausgesprochen worden ist, die nach Art oder Höhe im Gesetz nicht vorgesehen ist,"@de .

dtd:LA-id1834 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tatsachen oder Beweismittel beigebracht werden, die erheblich und neu sind,"@de .

dtd:LA-id1835 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Urteil auf dem Inhalt einer unechten oder verfälschten Urkunde oder auf einem vorsätzlich oder fahrlässig falsch abgegebenen Zeugnis oder Gutachten beruht,"@de .

dtd:LA-id1836 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Urteil, auf dessen tatsächlichen Feststellungen das Urteil im Disziplinarverfahren beruht, durch ein anderes rechtskräftiges Urteil aufgehoben worden ist,"@de .

dtd:LA-id1837 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an dem Urteil ein Richter oder Beamtenbeisitzer mitgewirkt hat, der sich in dieser Sache der strafbaren Verletzung einer Amtspflicht schuldig gemacht hat,"@de .

dtd:LA-id1838 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an dem Urteil ein Richter oder Beamtenbeisitzer mitgewirkt hat, der von der Ausübung des Richteramts kraft Gesetzes ausgeschlossen war, es sei denn, dass die Gründe für den gesetzlichen Ausschluss bereits erfolglos geltend gemacht worden waren, oder"@de .

dtd:LA-id1839 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Beamte nachträglich glaubhaft ein Dienstvergehen eingesteht, das in dem Disziplinarverfahren nicht hat festgestellt werden können."@de .

dtd:LA-id1840 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Urteil im Straf- oder Bußgeldverfahren ergangen ist, das sich auf denselben Sachverhalt gründet und diesen ebenso würdigt, solange dieses Urteil nicht rechtskräftig aufgehoben worden ist, oder"@de .

dtd:LA-id1841 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Urteil im Strafverfahren ergangen ist, durch das der Verurteilte sein Amt oder seinen Anspruch auf Ruhegehalt verloren hat oder ihn verloren hätte, wenn er noch im Dienst gewesen wäre oder Ruhegehalt bezogen hätte."@de .

dtd:LA-id1842 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Unterhaltsleistung darf die Höhe der Rentenanwartschaft aus der Nachversicherung nicht erreichen;"@de .

dtd:LA-id1843 a dtd:LA ;

dtd:LA-id3270 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Name und die Anschrift des Reeders; im Falle eines anderen Arbeitgebers der Name und die Anschrift des Arbeitgebers und des Reeders,"@de .


dtd:LA-id3162 a dtd:LA ;
    dtd:has_Value "Lotsinnen oder Lotsen sowie Personen, die im Auftrag des Bundes, eines Landes oder einer anderen öffentlich-rechtlichen Körperschaft Beratungs- oder Kontrolltätigkeit an Bord ausüben,"@de .

dtd:LA-id3163 a dtd:LA ;
    dtd:has_Value "Personen, die im Auftrag einer Werft oder eines Anlagenherstellers zur Ausführung von Gewährleistungsarbeiten oder Garantiearbeiten oder anderen an Bord notwendigen Arbeiten oder zur Einweisung der Besatzung in der Regel nicht länger als 96 Stunden an Bord tätig sind,"@de .

dtd:LA-id3164 a dtd:LA ;
    dtd:has_Value "Personen, die zur Ausführung von unaufschiebbaren Reparaturen oder Wartungsarbeiten, die von den Besatzungsmitgliedern nicht selbst ausgeführt werden können oder dürfen, in der Regel nicht länger als 96 Stunden an Bord tätig sind,"@de .

dtd:LA-id3165 a dtd:LA ;
    dtd:has_Value "Reederei- und Ladungsinspektorinnen oder -inspektoren, die auf der Grundlage der Reiseplanung in der Regel nicht länger als 72 Stunden an Bord tätig sein sollen,"@de .

dtd:LA-id3166 a dtd:LA ;
    dtd:has_Value "Künstlerinnen oder Künstler, die zur Unterhaltung der Fahrgäste nicht länger als 72 Stunden an Bord tätig sind,"@de .

dtd:LA-id3167 a dtd:LA ;
    dtd:has_Value "Wissenschaftlerinnen oder Wissenschaftler, die vorübergehend an Bord von Schiffen tätig sind,"@de .

dtd:LA-id3168 a dtd:LA ;
    dtd:has_Value "Personen, die sich auf einem Schiff befinden, um von dort aus besondere Tätigkeiten zur Errichtung, zur Änderung oder zum Betrieb von Bauwerken, künstlichen Inseln oder sonstigen Anlagen auf See durchzuführen,"@de .

dtd:LA-id3169 a dtd:LA ;
    dtd:has_Value "Fachschülerinnen oder -schüler und Hochschul- oder Fachhochschulstudentinnen oder -studenten, die an nach Landesrecht eingerichteten Ausbildungsstätten ausgebildet werden und zu diesem Zweck eine praktische Ausbildung und Seefahrtszeit auf einem Schiff durchführen,"@de .

dtd:LA-id3170 a dtd:LA ;
    dtd:has_Value "Schülerinnen oder Schüler, die im Rahmen von landesrechtlichen Vorschriften ein Praktikum an Bord leisten,"@de .

dtd:LA-id3171 a dtd:LA ;
    dtd:has_Value "Schülerinnen oder Schüler, denen durch Vermittlung des Verbandes Deutscher Reeder auf vertraglicher Grundlage während der Schulferien Einblick in die Praxis der Seefahrtberufe gewährt wird, ohne dass diese Personen an Bord tätig sind,"@de .

dtd:LA-id3172 a dtd:LA ;
    dtd:has_Value "Kanalsteurer auf dem Nord-Ostsee-Kanal und"@de .

dtd:LA-id3173 a dtd:LA ;
    dtd:has_Value "Sicherheitskräfte privater nach der Gewerbeordnung zugelassener Bewachungsunternehmen."@de .

dtd:LA-id3174 a dtd:LA ;
    dtd:has_Value "die Tätigkeit auf einer bestimmten Schiffsreise erfolgt oder erfolgen soll,"@de .

dtd:LA-id3175 a dtd:LA ;
    dtd:has_Value "eine über den in Satz 1 Nummer 2 oder 3 genannten Zeitraum hinausgehende Tätigkeit an Bord für die Erfüllung einer bestimmten Aufgabe erforderlich ist, die von den nach den schiffssicherheitsrechtlichen Vorschriften an Bord tätigen Besatzungsmitgliedern nicht selbst ausgeführt werden kann oder darf, und"@de .

dtd:LA-id3176 a dtd:LA ;
    dtd:has_Value "der vorgesehene Einsatz drei Wochen nicht überschreitet."@de .

dtd:LA-id3177 a dtd:LA ;
    dtd:has_Value "der Eigentümer des Schiffes oder"@de .

dtd:LA-id3178 a dtd:LA ;
    dtd:has_Value "jede andere Organisation oder Person, die vom Eigentümer des Schiffes die Verantwortung für den Betrieb des Schiffes übernommen und die sich mit der Übernahme dieser Verantwortung in dem Vertrag mit dem Eigentümer verpflichtet hat, die Aufgaben und Pflichten zu erfüllen, die dem Reeder nach diesem Gesetz und den anderen Rechtsvorschriften zur Umsetzung des Seearbeitsübereinkommens auferlegt werden."@de .

dtd:LA-id3179 a dtd:LA ;
    dtd:has_Value "eine andere Organisation oder Person bestimmte Aufgaben und Pflichten im Auftrag des Reeders erfüllt oder"@de .

dtd:LA-id3180 a dtd:LA ;
    dtd:has_Value "eine andere Organisation oder Person Arbeitgeber oder Ausbildender eines Besatzungsmitglieds ist (anderer Arbeitgeber)."@de .

dtd:LA-id3181 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach dem Ergebnis der Untersuchung die Seediensttauglichkeit nur bis zu diesem Zeitpunkt voraussehbar ist,"@de .

dtd:LA-id3182 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nur eine befristete Tätigkeit vorliegt oder"@de .

dtd:LA-id3183 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Grund der Funktion an Bord eine kürzere Gültigkeitsdauer angezeigt ist."@de .

dtd:LA-id3184 a dtd:LA ;
    dtd:has_Value "nach Aktenlage auf der Grundlage der Ergebnisse vorangegangener ärztlicher Untersuchungen oder anderer medizinischer Befunde,"@de .

dtd:LA-id3185 a dtd:LA ;
    dtd:has_Value "auf der Grundlage einer Untersuchung eines Arztes des seeärztlichen Dienstes der Berufsgenossenschaft oder"@de .

dtd:LA-id3186 a dtd:LA ;
    dtd:has_Value "auf der Grundlage eines Gutachtens einer Fachärztin oder eines Facharztes."@de .

dtd:LA-id3187 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mehrfachuntersuchungen zu vermeiden,"@de .

dtd:LA-id3188 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Notwendigkeit besonderer ärztlicher Beurteilung Rechnung zu tragen,"@de .

dtd:LA-id3189 a dtd:LA ;
    dtd:has_Size dtd:normal ;

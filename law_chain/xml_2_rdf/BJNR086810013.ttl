    dtd:has_Value "Abschluss der Untersuchung und Abschlusstag,"@de .

dtd:LA-id3217 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kostenträger der Seediensttauglichkeitsuntersuchung,"@de .

dtd:LA-id3218 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Seediensttauglichkeit für Dienstzweige,"@de .

dtd:LA-id3219 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gültigkeit des Seediensttauglichkeitszeugnisses,"@de .

dtd:LA-id3220 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nummer des Seediensttauglichkeitszeugnisses,"@de .

dtd:LA-id3221 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Diagnosegruppen in anonymisierter Form,"@de .

dtd:LA-id3222 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sperrvermerke der Berufsgenossenschaft."@de .

dtd:LA-id3223 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die näheren Anforderungen an die Seediensttauglichkeit, einschließlich der erforderlichen Nachuntersuchungen durch die Berufsgenossenschaft,"@de .

dtd:LA-id3224 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Durchführung der vorgeschriebenen Untersuchungen,"@de .

dtd:LA-id3225 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ausgestaltung des Seediensttauglichkeitszeugnisses,"@de .

dtd:LA-id3226 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Voraussetzungen für die Zulassung der Ärzte, insbesondere die Anforderungen an die Befähigung und die persönliche Eignung, sowie die erforderlichen Nachweise,"@de .

dtd:LA-id3227 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Erfordernis einer Fortbildung der zugelassenen Ärzte,"@de .

dtd:LA-id3228 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die näheren Einzelheiten über die Verarbeitung der Daten des Seediensttauglichkeitsverzeichnisses"@de .

dtd:LA-id3229 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die erforderlichen technischen und organisatorischen Maßnahmen nach den Artikeln 24, 25 und 32 der Verordnung (EU) 2016/679 des Europäischen Parlaments und des Rates vom 27. April 2016 zum Schutz natürlicher Personen bei der Verarbeitung personenbezogener Daten, zum freien Datenverkehr und zur Aufhebung der Richtlinie 95/46/EG (Datenschutz-Grundverordnung) (ABl. L 119 vom 4.5.2016, S. 1; L 314 vom 22.11.2016, S. 72; L 127 vom 23.5.2018, S. 2) in der jeweils geltenden Fassung zur Sicherstellung des Datenschutzes und der Datensicherheit getroffen,"@de .

dtd:LA-id3230 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Nutzung allgemein zugänglicher Netze Verschlüsselungsverfahren angewendet und"@de .

dtd:LA-id3231 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zulässigkeit der Direkteinstellung oder der Abrufe kontrolliert"@de .

dtd:LA-id3232 a dtd:LA ;
    dtd:has_Value "die Besatzungsliste,"@de .

dtd:LA-id3233 a dtd:LA ;
    dtd:has_Value "eine Kopie der Besatzungsliste oder"@de .

dtd:LA-id3234 a dtd:LA ;
    dtd:has_Value "ein die Besatzung des Schiffes wiedergebender Auszug aus dem Seetagebuch"@de .

dtd:LA-id3235 a dtd:LA ;
    dtd:has_Value "der zu schließende Heuervertrag die Anforderungen der §§ 28 und 29 erfüllt,"@de .

dtd:LA-id3236 a dtd:LA ;
    dtd:has_Value "er seinen Verpflichtungen nach den §§ 73 bis 76 nachkommt und"@de .

dtd:LA-id3237 a dtd:LA ;
    dtd:has_Value "er eine Versicherung abgeschlossen hat, um Personen, die an Bord eines Schiffes vermittelt worden sind, für finanzielle Verluste zu entschädigen, die ihnen infolge einer vom Reeder zu vertretenden Pflichtverletzung aus dem Beschäftigungsvertrag entstehen."@de .

dtd:LA-id3238 a dtd:LA ;
    dtd:has_Value "keine Mittel, Verfahren oder Listen verwendet, um sie an der Aufnahme einer Beschäftigung zu hindern, die ihrer Qualifikation entspricht,"@de .

dtd:LA-id3239 a dtd:LA ;
    dtd:has_Value "von ihnen weder unmittelbar noch mittelbar eine Vergütung für die Vermittlung verlangt,"@de .

dtd:LA-id3240 a dtd:LA ;

dtd:LA-id292 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Abgase:"@de .

dtd:LA-id293 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Trägergase mit den festen, flüssigen oder gasförmigen Emissionen;"@de .

dtd:LA-id294 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Altanlagen:"@de .

dtd:LA-id295 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anlagen, die vor dem Inkrafttreten dieser Verordnung errichtet oder genehmigt worden sind;"@de .

dtd:LA-id296 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anlagen zur Feuerbestattung (Anlagen):"@de .

dtd:LA-id297 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle technischen Einrichtungen, die der Einäscherung des menschlichen Leichnams dienen;"@de .

dtd:LA-id298 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Emissionen:"@de .

dtd:LA-id299 a dtd:LA ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id328,
        dtd:ABWFORMAT-id329 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die von der Anlage ausgehenden Luftverunreinigungen; sie werden angegeben als Massenkonzentration in den Einheiten Nanogramm je Kubikmeter (ng/cbm) oder Milligramm je Kubikmeter (mg/cbm), bezogen auf das Abgasvolumen im Normzustand (273 K, 1013 hPa) nach Abzug des Feuchtegehaltes an Wasserdampf; sie beziehen sich auf einen Volumengehalt an Sauerstoff im Abgas von 11 vom Hundert, bei elektrisch betriebenen Anlagen auf einen Volumengehalt an Sauerstoff im Abgas von 15 vom Hundert."@de .

dtd:LA-id300 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Emissionen von Kohlenmonoxid einen Stundenmittelwert von 50 mg je Kubikmeter Abgas,"@de .

dtd:LA-id301 a dtd:LA ;
    dtd:has_DL dtd:DL-id235 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Emissionen von Gesamtstaub und organischen Stoffen, gebildet als Stundenmittelwert und in Übereinstimmung mit dem im Anhang 1 festgelegten Verfahren, die folgenden Emissionsgrenzwertea)Gesamtstaub 10 mg/cbm, b)organische Stoffe, angegeben als Gesamtkohlenstoff, 20 mg/cbm und "@de .

dtd:LA-id302 a dtd:LA ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id330 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gesamtstaub 10 mg/cbm,"@de .

dtd:LA-id303 a dtd:LA ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id331 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "organische Stoffe, angegeben als Gesamtkohlenstoff, 20 mg/cbm und"@de .

dtd:LA-id304 a dtd:LA ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id332 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Emissionen von den im Anhang 2 genannten Dioxinen und Furanen, angegeben als Summenwert und gebildet als Mittelwert über die jeweilige Probenahmezeit, jeweils in Übereinstimmung mit dem im Anhang 2 festgelegten Verfahren, den Emissionsgrenzwert von 0,1 ng/cbm"@de .

dtd:LA-id305 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die höchste Kante des Dachfirstes der Anlage um mindestens 3 Meter überragt und"@de .

dtd:LA-id306 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens 10 Meter über Flur liegt."@de .

dtd:LA-id307 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Volumengehalt an Sauerstoff im Abgas,"@de .

dtd:LA-id308 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Massenkonzentration von Kohlenmonoxid im Abgas und"@de .

dtd:LA-id309 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mindesttemperatur nach § 3 Abs. 2"@de .

dtd:LA-id310 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einzelne Anforderungen der Verordnung nicht oder nur mit unverhältnismäßig hohem Aufwand erfüllbar sind,"@de .

dtd:LA-id311 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "schädliche Umwelteinwirkungen nicht zu befürchten sind und"@de .

dtd:LA-id312 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im übrigen die dem Stand der Technik entsprechenden Maßnahmen zur Emissionsbegrenzung angewandt werden."@de .

dtd:LA-id313 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 4 eine Anlage errichtet oder betreibt,"@de .

dtd:LA-id314 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 5 Satz 1 ein Abgas nicht oder nicht in der vorgeschriebenen Weise ableitet,"@de .

dtd:LA-id315 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 6 eine Anzeige nicht, nicht richtig oder nicht rechtzeitig erstattet,"@de .

dtd:LA-id316 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 7 Abs. 1 oder Abs. 2 eine Anlage betreibt,"@de .

dtd:LA-id317 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 7 Abs. 3 Satz 1 oder 2 eine Meßeinrichtung nicht oder nicht rechtzeitig kalibrieren oder nicht oder nicht rechtzeitig prüfen oder die Kalibrierung nicht oder nicht rechtzeitig wiederholen läßt oder"@de .

dtd:LA-id318 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 9 Satz 1 oder 2 die Einhaltung der Anforderungen nicht, nicht richtig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig prüfen läßt oder eine Prüfung nicht, nicht richtig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig wiederholen läßt."@de .

dtd:LA-id319 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zur Bestimmung der Massenkonzentration an Gesamtstaub und Gesamtkohlenstoff nach § 4 Nr. 2 beträgt die Probenahmezeit eine Stunde; die Probenahme erfolgt ab Beginn einer Einäscherung."@de .

dtd:LA-id320 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Es sind Proben während fünf aufeinanderfolgender Einäscherungsvorgänge zu ziehen."@de .

dtd:LA-id321 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pausenzeiten zwischen jeweils zwei Einäscherungen bleiben unberücksichtigt."@de .

dtd:LA-id322 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Messung der organischen Stoffe, angegeben als Gesamtkohlenstoff, erfolgt nach der FID-Methode. Die Kalibrierung des Gerätes erfolgt mit Propan oder Butan. Es sind geeignete Meßgeräte zu verwenden, die eine Bekanntgabe für Meßaufgaben nach der 17. BImSchV besitzen."@de .

dtd:LA-id323 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zur Bestimmung der Massenkonzentration von Dioxinen und Furanen nach § 4 Nr. 3 beträgt die Probenahmezeit mindestens sechs Stunden."@de .

dtd:LA-id324 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Probenahme erfolgt ab Beginn einer Einäscherung."@de .

dtd:LA-id325 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pausenzeiten zwischen jeweils zwei Einäscherungen bleiben unberücksichtigt."@de .

dtd:LA-id326 a dtd:LA ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id333 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Für die in diesem Anhang genannten Dioxine und Furane soll die Nachweisgrenze des eingesetzten Analyseverfahrens nicht über 0,005 ng/cbm Abgas liegen."@de .

dtd:LA-id327 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Für den nach § 4 Nr. 3 zu bildenden Summenwert sind die im Abgas ermittelten Konzentrationen der nachstehend genannten Dioxine und Furane mit den angegebenen Äquivalenzfaktoren zu multiplizieren und zu summieren."@de .

dtd:P-id335 a dtd:P ;
    dtd:has_Value "Die Verpflichtungen aus der Richtlinie 83/189/EWG des Ratesvom 28. März 1983 über ein Informationsverfahren auf demGebiet der Normen und technischen Vorschriften(ABl. EG Nr. L 109 S. 8), zuletzt geändert durchdie Richtlinie 94/10/EG des Europäischen Parlamentsund des Rates vom 23. März 1994 (ABl. EG Nr. L 100S. 30), sind beachtet worden."@de ;
    dtd:has_noindex dtd:noindex-id140 .

dtd:P-id337 a dtd:P ;
    dtd:has_BR dtd:BR-id134 ;
    dtd:has_Value " (+++ Textnachweis ab: 1.5.1997 +++)(+++ Amtlicher Hinweis des Normgebers auf EG-Recht:     Beachtung der       EWGRL 189/83 (CELEX Nr: 383L0189) +++)Die V wurde von der Bundesregierung nach Anhörung der beteiligten Kreise mit Zustimmung des Bundesrates erlassen. Sie tritt gem. Art. 3 V v. 19.3.1997 I 545 mWv 1.5.1997 in Kraft."@de ;
    dtd:has_pre dtd:pre-id381 .

dtd:P-id339 a dtd:P ;
    dtd:has_Value "Diese Verordnung gilt für die Errichtung, die Beschaffenheit und den Betrieb von Anlagen zur Feuerbestattung."@de .

dtd:P-id341 a dtd:P ;
    dtd:has_DL dtd:DL-id219 ;
    dtd:has_Value "Im Sinne dieser Verordnung sind: 1.Abgase:die Trägergase mit den festen, flüssigen oder gasförmigen Emissionen;  2.Altanlagen:Anlagen, die vor dem Inkrafttreten dieser Verordnung errichtet oder genehmigt worden sind;  3.Anlagen zur Feuerbestattung (Anlagen):alle technischen Einrichtungen, die der Einäscherung des menschlichen Leichnams dienen;  4.Emissionen:die von der Anlage ausgehenden Luftverunreinigungen; sie werden angegeben als Massenkonzentration in den Einheiten Nanogramm je Kubikmeter (ng/cbm) oder Milligramm je Kubikmeter (mg/cbm), bezogen auf das Abgasvolumen im Normzustand (273 K, 1013 hPa) nach Abzug des Feuchtegehaltes an Wasserdampf; sie beziehen sich auf einen Volumengehalt an Sauerstoff im Abgas von 11 vom Hundert, bei elektrisch betriebenen Anlagen auf einen Volumengehalt an Sauerstoff im Abgas von 15 vom Hundert. "@de .

dtd:P-id343 a dtd:P ;
    dtd:has_Value "(1) Soweit die Anlagen mit Brennern ausgerüstet sind, dürfen diese nur mit Gasen der öffentlichen Gasversorgung, Flüssiggas, Wasserstoff oder Heizöl EL betrieben werden."@de .

dtd:P-id344 a dtd:P ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id382 ;
    dtd:has_Value "(2) Die Temperatur nach der letzten Verbrennungsluftzuführung muß mindestens 850 Grad C, ermittelt als Zehnminutenmittelwert, betragen."@de .

dtd:P-id345 a dtd:P ;
    dtd:has_Value "(3) Durch geeignete Vorrichtungen ist sicherzustellen, daß ein Sarg nicht eingefahren werden kann, wenn die Mindesttemperatur nach § 3 Abs. 2 unterschritten ist oder die kontinuierlich ermittelte Konzentration von Kohlenmonoxid oder die Anzeige für die Rauchgasdichte auf eine Störung des ordnungsgemäßen Betriebes hinweist. Eine bereits begonnene Einäscherung ist zu Ende zu führen."@de .

dtd:P-id347 a dtd:P ;
    dtd:has_DL dtd:DL-id228 ;
    dtd:has_Value "Anlagen dürfen nur so errichtet und betrieben werden, daß 1.die Emissionen von Kohlenmonoxid einen Stundenmittelwert von 50 mg je Kubikmeter Abgas, 2.die Emissionen von Gesamtstaub und organischen Stoffen, gebildet als Stundenmittelwert und in Übereinstimmung mit dem im Anhang 1 festgelegten Verfahren, die folgenden Emissionsgrenzwertea)Gesamtstaub 10 mg/cbm, b)organische Stoffe, angegeben als Gesamtkohlenstoff, 20 mg/cbm und  3.die Emissionen von den im Anhang 2 genannten Dioxinen und Furanen, angegeben als Summenwert und gebildet als Mittelwert über die jeweilige Probenahmezeit, jeweils in Übereinstimmung mit dem im Anhang 2 festgelegten Verfahren, den Emissionsgrenzwert von 0,1 ng/cbm nicht überschreiten. 11121"@de .

dtd:P-id349 a dtd:P ;
    dtd:has_DL dtd:DL-id240 ;
    dtd:has_Value "Abgase sind über einen oder mehrere Schornsteine in die freie Luftströmung so abzuleiten, daß die Höhe der Austrittsöffnung für die Abgase 1.die höchste Kante des Dachfirstes der Anlage um mindestens 3 Meter überragt und 2.mindestens 10 Meter über Flur liegt. Bei einer Dachneigung von weniger als 20 Grad ist die Höhe der Austrittsöffnung auf einen fiktiven Dachfirst zu beziehen, dessen Höhe unter Zugrundelegung einer Dachneigung von 20 Grad zu berechnen ist."@de .

dtd:P-id351 a dtd:P ;
    dtd:has_Value "Der Betreiber einer Anlage hat diese der zuständigen Behörde spätestens einen Monat vor der Inbetriebnahme anzuzeigen."@de .

dtd:P-id353 a dtd:P ;
    dtd:has_DL dtd:DL-id245 ;
    dtd:has_Value "(1) Die Anlagen sind mit Meßeinrichtungen auszurüsten, die 1.den Volumengehalt an Sauerstoff im Abgas, 2.die Massenkonzentration von Kohlenmonoxid im Abgas und 3.die Mindesttemperatur nach § 3 Abs. 2 fortlaufend messen und registrieren. Die Anlagen dürfen nur mit hierzu geeigneten und funktionsfähigen Meßeinrichtungen betrieben werden."@de .

dtd:P-id354 a dtd:P ;
    dtd:has_Value "(2) Die Anlagen sind zur Überwachung der Funktionstüchtigkeit der Staubabscheideeinrichtungen mit Meßgeräten auszurüsten, die die Rauchgasdichte kontinuierlich messen. Die Anlagen dürfen nur mit hierzu geeigneten und funktionsfähigen Rauchgasdichtemeßgeräten, die Rückschlüsse auf die ständige Einhaltung des Emissionsgrenzwertes für Gesamtstaub nach § 4 Nr. 2 Buchstabe a ermöglichen, betrieben werden."@de .

dtd:P-id355 a dtd:P ;
    dtd:has_Value "(3) Der Betreiber hat durch eine von der zuständigen obersten Landesbehörde oder der nach Landesrecht bestimmten Behörde für Kalibrierungen bekanntgegebene Stelle den ordnungsgemäßen Einbau der Meßeinrichtungen zur kontinuierlichen Überwachung von Kohlenmonoxid, Sauerstoff, Rauchgasdichte und Temperatur bescheinigen zu lassen sowie die Meßeinrichtungen vor Inbetriebnahme kalibrieren und jeweils spätestens nach Ablauf eines Jahres auf Funktionsfähigkeit prüfen zu lassen. Der Betreiber hat die Kalibrierung spätestens fünf Jahre nach der letzten Kalibrierung wiederholen zu lassen. Der Betreiber hat die Bescheinigung über den ordnungsgemäßen Einbau, die Berichte über das Ergebnis der Kalibrierung und der Prüfung der Funktionsfähigkeit der zuständigen Behörde jeweils innerhalb von drei Monaten nach Durchführung vorzulegen."@de .

dtd:P-id357 a dtd:P ;
    dtd:has_Value "(1) Während des Betriebes der Anlage ist für den Kohlenmonoxidmeßwert für jede aufeinanderfolgende Stunde der Mittelwert zu bilden."@de .

dtd:P-id358 a dtd:P ;
    dtd:has_Value "(2) Über die Auswertung der kontinuierlichen Messungen hat der Betreiber einen Meßbericht zu erstellen oder erstellen zu lassen und innerhalb von drei Monaten nach Ablauf eines jeden Kalenderjahres der zuständigen Behörde vorzulegen. Der Betreiber muß die Aufzeichnungen fünf Jahre aufbewahren."@de .

dtd:P-id359 a dtd:P ;
    dtd:has_Value "(3) Der Grenzwert für Kohlenmonoxid ist eingehalten, wenn kein Stundenmittelwert nach § 7 Abs. 1 Nr. 2 in Verbindung mit Absatz 1 den Grenzwert nach § 4 Nr. 1 überschreitet."@de .

dtd:P-id361 a dtd:P ;
    dtd:has_Value "Der Betreiber einer nach Inkrafttreten dieser Verordnung errichteten Anlage hat die Einhaltung der Anforderungen für Gesamtstaub, Gesamtkohlenstoff und Dioxine und Furane nach § 4 frühestens drei Monate und spätestens sechs Monate nach der Inbetriebnahme von einer nach § 29b Absatz 2 in Verbindung mit § 26 des Bundes-Immissionsschutzgesetzes bekanntgegebenen Stelle nach Maßgabe von Anhang 1 und Anhang 2 prüfen zu lassen. Der Betreiber hat die Prüfung nach Satz 1 im Abstand von drei Jahren wiederholen zu lassen."@de .

dtd:P-id363 a dtd:P ;
    dtd:has_Value "(1) Über die Messungen nach § 9 ist ein Meßbericht zu erstellen und der zuständigen Behörde innerhalb von drei Monaten nach Durchführung der Messung vorzulegen. Der Meßbericht muß Angaben über die Meßplanung, das Ergebnis, die verwendeten Meßverfahren und die Betriebsbedingungen, die für die Beurteilung der Meßergebnisse von Bedeutung sind, enthalten. Der Betreiber muß die Berichte fünf Jahre aufbewahren."@de .

dtd:P-id364 a dtd:P ;
    dtd:has_Value "(2) Die Emissionsgrenzwerte gelten als eingehalten, wenn kein Ergebnis einer Einzelmessung des Stundenmittelwertes den jeweiligen Emissionsgrenzwert nach § 4 Nr. 2 oder den Mittelwert über die Probenahmezeit nach § 4 Nr. 3 überschreitet."@de .

dtd:P-id366 a dtd:P ;
    dtd:has_Value "Altanlagen müssen die Anforderungen dieser Verordnung spätestens nach Ablauf von drei Jahren nach Inkrafttreten dieser Verordnung einhalten."@de .

dtd:P-id368 a dtd:P ;
    dtd:has_DL dtd:DL-id252 ;
    dtd:has_Value "(1) Die zuständige Behörde kann auf Antrag des Betreibers Ausnahmen von Vorschriften dieser Verordnung zulassen, soweit unter Berücksichtigung der besonderen Umstände des Einzelfalles 1.einzelne Anforderungen der Verordnung nicht oder nur mit unverhältnismäßig hohem Aufwand erfüllbar sind, 2.schädliche Umwelteinwirkungen nicht zu befürchten sind und 3.im übrigen die dem Stand der Technik entsprechenden Maßnahmen zur Emissionsbegrenzung angewandt werden.  "@de .

dtd:P-id369 a dtd:P ;
    dtd:has_Value "(2) Eine Ausnahme von § 5 kommt insbesondere in Betracht, wenn Belange des Denkmalschutzes berührt sind."@de .

dtd:P-id370 a dtd:P ;
    dtd:has_Value "(3) Die zuständige Behörde kann die Ausnahme unter Bedingungen erteilen, mit Auflagen verbinden oder befristen."@de .

dtd:P-id372 a dtd:P ;
    dtd:has_Value "Die Befugnis der zuständigen Behörde, auf der Grundlage des Bundes-Immissionsschutzgesetzes andere oder weitergehende Anordnungen zu treffen, bleibt unberührt."@de .

dtd:P-id374 a dtd:P ;
    dtd:has_DL dtd:DL-id259 ;
    dtd:has_Value "Ordnungswidrig im Sinne des § 62 Abs. 1 Nr. 7 des Bundes-Immissionsschutzgesetzes handelt, wer vorsätzlich oder fahrlässig 1.entgegen § 4 eine Anlage errichtet oder betreibt, 2.entgegen § 5 Satz 1 ein Abgas nicht oder nicht in der vorgeschriebenen Weise ableitet, 3.entgegen § 6 eine Anzeige nicht, nicht richtig oder nicht rechtzeitig erstattet, 4.entgegen § 7 Abs. 1 oder Abs. 2 eine Anlage betreibt, 5.entgegen § 7 Abs. 3 Satz 1 oder 2 eine Meßeinrichtung nicht oder nicht rechtzeitig kalibrieren oder nicht oder nicht rechtzeitig prüfen oder die Kalibrierung nicht oder nicht rechtzeitig wiederholen läßt oder 6.entgegen § 9 Satz 1 oder 2 die Einhaltung der Anforderungen nicht, nicht richtig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig prüfen läßt oder eine Prüfung nicht, nicht richtig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig wiederholen läßt. "@de .

dtd:P-id376 a dtd:P ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id384 ;
    dtd:has_Value " Fundstelle des Originaltextes: BGBl. I 1997, 547 "@de ;
    dtd:has_kommentar dtd:kommentar-id383 .

dtd:P-id377 a dtd:P ;
    dtd:has_DL dtd:DL-id272 ;
    dtd:has_Value " 1.Zur Bestimmung der Massenkonzentration an Gesamtstaub und Gesamtkohlenstoff nach § 4 Nr. 2 beträgt die Probenahmezeit eine Stunde; die Probenahme erfolgt ab Beginn einer Einäscherung. 2.Es sind Proben während fünf aufeinanderfolgender Einäscherungsvorgänge zu ziehen. 3.Pausenzeiten zwischen jeweils zwei Einäscherungen bleiben unberücksichtigt. 4.Die Messung der organischen Stoffe, angegeben als Gesamtkohlenstoff, erfolgt nach der FID-Methode. Die Kalibrierung des Gerätes erfolgt mit Propan oder Butan. Es sind geeignete Meßgeräte zu verwenden, die eine Bekanntgabe für Meßaufgaben nach der 17. BImSchV besitzen. "@de .

dtd:P-id379 a dtd:P ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id386 ;
    dtd:has_Value " Fundstelle des Originaltextes: BGBl. I 1997, 547 "@de ;
    dtd:has_kommentar dtd:kommentar-id385 .

dtd:P-id380 a dtd:P ;
    dtd:has_DL dtd:DL-id281 ;
    dtd:has_Value """ 1.Zur Bestimmung der Massenkonzentration von Dioxinen und Furanen nach § 4 Nr. 3 beträgt die Probenahmezeit mindestens sechs Stunden. 2.Die Probenahme erfolgt ab Beginn einer Einäscherung. 3.Pausenzeiten zwischen jeweils zwei Einäscherungen bleiben unberücksichtigt. 4.Für die in diesem Anhang genannten Dioxine und Furane soll die Nachweisgrenze des eingesetzten Analyseverfahrens nicht über 0,005 ng/cbm Abgas liegen. 5.Für den nach § 4 Nr. 3 zu bildenden Summenwert sind die im Abgas ermittelten Konzentrationen der nachstehend genannten Dioxine und Furane mit den angegebenen Äquivalenzfaktoren zu multiplizieren und zu summieren.  
		
			
			
			
			
				
					 
					 
					Äquivalenzfaktor
				
				
					2,3,7,8
					- Tetrachlordibenzodioxin (TCDD)
					1
				
				
					1,2,3,7,8
					- Pentachlordibenzodioxin (PeCDD)
					0,5
				
				
					1,2,3,4,7,8
					- Hexachlordibenzodioxin (HxCDD)
					0,1
				
				
					1,2,3,7,8,9
					- Hexachlordibenzodioxin (HxCDD)
					0,1
				
				
					1,2,3,6,7,8
					- Hexachlordibenzodioxin (HxCDD)
					0,1
				
				
					1,2,3,4,6,7,8
					- Heptachlordibenzodioxin (HpCDD)
					0,01
				
				
					Octachlordibenzodioxin (OCDD)
					 
					0,001
				
				
					2,3,7,8
					- Tetrachlordibenzofuran (TCDF)
					0,1
				
				
					2,3,4,7,8
					- Pentachlordibenzofuran (PeCDF)
					0,5
				
				
					1,2,3,7,8
					- Pentachlordibenzofuran (PeCDF)
					0,05
				
				
					1,2,3,4,7,8
					- Hexachlordibenzofuran (HxCDF)
					0,1
				
				
					1,2,3,7,8,9
					- Hexachlordibenzofuran (HxCDF)
					0,1
				
				
					1,2,3,6,7,8
					- Hexachlordibenzofuran (HxCDF)
					0,1
				
				
					2,3,4,6,7,8
					- Hexachlordibenzofuran (HxCDF)
					0,1
				
				
					1,2,3,4,6,7,8
					- Heptachlordibenzofuran (HpCDF)
					0,01
				
				
					1,2,3,4,7,8,9
					- Heptachlordibenzofuran (HpCDF)
					0,01
				
				
					Octachlordibenzofuran (OCDF)
					 
					0,001
				
			
		
	 """@de ;

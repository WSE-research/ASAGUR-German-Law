dtd:LA-id589 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verringerung der durch die Opioidabhängigkeit bedingten Risiken während einer Schwangerschaft sowie während und nach der Geburt."@de .

dtd:LA-id590 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich zu Beginn der Behandlung mit einem suchtmedizinisch qualifizierten Arzt abstimmen sowie"@de .

dtd:LA-id591 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sicherstellen, dass sich sein Patient zu Beginn der Behandlung und mindestens einmal in jedem Quartal dem suchtmedizinisch qualifizierten Arzt nach Nummer 1 im Rahmen einer Konsiliarbehandlung vorstellt."@de .

dtd:LA-id592 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein zur Substitution zugelassenes Arzneimittel, das nicht den Stoff Diamorphin enthält,"@de .


    dtd:has_Value "finanzielle Vorgaben"@de .

dtd:LA-id2373 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorgaben zu Abrechnungswesen, Zahlungs- und Fakturierungsgrundsätzen"@de .

dtd:LA-id2374 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "technisch-organisatorische Vorgaben"@de .


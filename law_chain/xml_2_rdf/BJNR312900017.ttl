dtd:LA-id663 a dtd:LA ;
    dtd:has_Value "„Gesamtvorhaben KONSENS“ das Zusammenwirken des Bundes und der Länder nach § 1,"@de .

dtd:LA-id664 a dtd:LA ;
    dtd:has_Value "„IT-Verfahren“ die Zusammenfassung mehrerer Software-Entwicklungen,"@de .

dtd:LA-id665 a dtd:LA ;
    dtd:has_Value "„Hauptversion“ eine neue Version einer Software mit signifikant erweiterter Funktionalität,"@de .

dtd:LA-id666 a dtd:LA ;
    dtd:has_Value "„Vorhabensplan“ der jährlich fortzuschreibende Plan der zu entwickelnden IT-Verfahren und Software,"@de .

dtd:LA-id667 a dtd:LA ;
    dtd:has_Value "„Sourcingstrategie“ die Entwicklung, Anpassung und Planung einer Beschaffungsstrategie zum Einsatz interner und externer Unterstützung,"@de .

dtd:LA-id668 a dtd:LA ;
    dtd:has_Value "„Architektur“ eine Beschreibung von IT-, Fach- und Betriebsarchitektur einschließlich der technischen Basis, auf der IT-Verfahren oder Software zur Umsetzung der festgelegten Anforderungen bereitgestellt werden müssen."@de .

dtd:LA-id669 a dtd:LA ;
    dtd:has_Value "Bereinigung von Fehlern der eingesetzten Software,"@de .

dtd:LA-id670 a dtd:LA ;
    dtd:has_Value "geringfügige Anpassung der Schnittstellen,"@de .

dtd:LA-id671 a dtd:LA ;
    dtd:has_Value "geringfügige Änderungen in der Architektur,"@de .

dtd:LA-id672 a dtd:LA ;
    dtd:has_Value "geringfügige Funktionserweiterungen oder Funktionsänderungen und"@de .

dtd:LA-id673 a dtd:LA ;
    dtd:has_Value "Performanceverbesserungsmaßnahmen."@de .

dtd:LA-id674 a dtd:LA ;
    dtd:has_Value "einem Beschlussvorschlag des Bundes nicht mehr als zehn Länder widersprechen oder"@de .

dtd:LA-id675 a dtd:LA ;
    dtd:has_Value "einem Beschlussvorschlag eines oder mehrerer Länder die Länder mit einfacher Mehrheit zustimmen und der Bund nicht widerspricht."@de .

dtd:LA-id676 a dtd:LA ;
    dtd:has_Value "die Vorlage des Vorhabensplans zur Genehmigung an die Finanzminister des Bundes und der Länder,"@de .

dtd:LA-id677 a dtd:LA ;
    dtd:has_Value "die Vorlage des Gesamtbudgetplans (die jährlichen Finanzbedarfe und die Finanzplanung) und des Berichts über die Ergebnisse des Finanzcontrollings für das Vorjahr zur Genehmigung an die Finanzminister des Bundes und der Länder,"@de .

dtd:LA-id678 a dtd:LA ;
    dtd:has_Value "die länderübergreifende verbindliche Release- und Einsatzplanung für die IT-Verfahren und die Software sowie"@de .

dtd:LA-id679 a dtd:LA ;
    dtd:has_Value "die Übertragung von Produktions- und Serviceaufgaben auf zentrale Produktions- und Servicestellen."@de .

dtd:LA-id680 a dtd:LA ;
    dtd:has_Value "einem Beschlussvorschlag des Bundes nicht mehr als zwei Länder widersprechen oder"@de .

dtd:LA-id681 a dtd:LA ;
    dtd:has_Value "einem Beschlussvorschlag eines oder mehrerer Länder die Länder mit einfacher Mehrheit zustimmen und der Bund nicht widerspricht."@de .


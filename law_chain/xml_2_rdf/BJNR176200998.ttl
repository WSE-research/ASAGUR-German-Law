    dtd:has_Size dtd:normal ;
    dtd:has_Value "Herzklappen"@de .

dtd:LA-id626 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht resorbierbare Gefäßprothesen und -stützen"@de .

dtd:LA-id627 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gelenkersatz für Hüfte oder Knie"@de .

dtd:LA-id628 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wirbelkörperersatzsysteme und Bandscheibenprothesen"@de .


dtd:LA-id536 a dtd:LA ;
    dtd:has_Value "zur klinischen Prüfung,"@de .

dtd:LA-id537 a dtd:LA ;
    dtd:has_Value "zur Verwendung in einer Leistungsstudie oder"@de .

dtd:LA-id538 a dtd:LA ;
    dtd:has_Value "die in ausschließlich eigener Verantwortung für persönliche Zwecke erworben und angewendet werden."@de .

dtd:LA-id539 a dtd:LA ;
    dtd:has_Value "das Errichten,"@de .

dtd:LA-id540 a dtd:LA ;
    dtd:has_Value "das Bereithalten,"@de .

dtd:LA-id541 a dtd:LA ;
    dtd:has_Value "die Instandhaltung,"@de .

dtd:LA-id542 a dtd:LA ;
    dtd:has_Value "die Aufbereitung sowie"@de .

dtd:LA-id543 a dtd:LA ;
    dtd:has_Value "sicherheits- und messtechnische Kontrollen."@de .

dtd:LA-id544 a dtd:LA ;
    dtd:has_Value "hinsichtlich der jeweiligen Tätigkeit über aktuelle Kenntnisse aufgrund einer geeigneten Ausbildung und einer einschlägigen beruflichen Tätigkeit verfügt,"@de .

dtd:LA-id545 a dtd:LA ;
    dtd:has_Value "hinsichtlich der fachlichen Beurteilung keiner Weisung unterliegt und"@de .

dtd:LA-id546 a dtd:LA ;
    dtd:has_Value "über die Mittel, insbesondere Räume, Geräte und sonstige Arbeitsmittel, wie geeignete Mess- und Prüfeinrichtungen, verfügt, die erforderlich sind, die jeweilige Tätigkeit ordnungsgemäß und nachvollziehbar durchzuführen."@de .

dtd:LA-id547 a dtd:LA ;
    dtd:has_Value "die Aufgaben einer Kontaktperson für Behörden, Hersteller und Vertreiber im Zusammenhang mit Meldungen über Risiken von Medizinprodukten sowie bei der Umsetzung von Sicherheitskorrekturmaßnahmen im Feld und sonstigen notwendigen Korrekturmaßnahmen,"@de .

dtd:LA-id548 a dtd:LA ;
    dtd:has_Value "die Koordinierung interner Prozesse der Gesundheitseinrichtung zur Erfüllung der Melde- und Mitwirkungspflichten der Anwender und Betreiber und"@de .

dtd:LA-id549 a dtd:LA ;
    dtd:has_Value "die Koordinierung der Umsetzung der Korrekturmaßnahmen und der Sicherheitskorrekturmaßnahmen im Feld in den Gesundheitseinrichtungen."@de .

dtd:LA-id550 a dtd:LA ;
    dtd:has_Value "der Vorgaben des Artikels 17 der Verordnung (EU) 2017/745 in Verbindung mit der Durchführungsverordnung (EU) 2020/1207 der Kommission vom 19. August 2020 zur Festlegung von Vorschriften zur Anwendung der Verordnung (EU) 2017/745 des Europäischen Parlaments und des Rates hinsichtlich gemeinsamer Spezifikationen für die Aufbereitung von Einmalprodukten (ABl. L 273 vom 20.8.2020, S. 3) in der jeweils geltenden Fassung und"@de .

dtd:LA-id551 a dtd:LA ;
    dtd:has_Value "der Empfehlung nach Absatz 2."@de .

dtd:LA-id552 a dtd:LA ;
    dtd:has_Value "zur Durchführung eines Konformitätsbewertungsverfahrens nach Artikel 52 der Verordnung (EU) 2017/745,"@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Value "zur Aushändigung der Informationen und Unterlagen für den Nachweis der Konformität des Produktes nach Artikel 10 Absatz 14 der Verordnung (EU) 2017/745,"@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_Value "zur Abgabe einer EU-Konformitätserklärung nach Artikel 10 Absatz 6 und Artikel 19 der Verordnung (EU) 2017/745,"@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_Value "zur Anbringung einer CE-Kennzeichnung auf das Produkt nach Artikel 10 Absatz 6 und Artikel 20 der Verordnung (EU) 2017/745 und"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Value "zur Durchführung einer klinischen Bewertung nach Artikel 10 Absatz 3 und Artikel 61 der Verordnung (EU) 2017/745."@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dieses Medizinprodukt am Betriebsort einer Funktionsprüfung unterzogen hat und"@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die vom Betreiber beauftragte Person anhand der Gebrauchsanweisung sowie beigefügter sicherheitsbezogener Informationen und Instandhaltungshinweise in die sachgerechte Handhabung, Anwendung und den Betrieb des Medizinproduktes sowie in die zulässige Verbindung mit anderen Medizinprodukten, Gegenständen und Zubehör eingewiesen hat."@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Value "erforderliche Angaben zur eindeutigen Identifikation des Medizinproduktes,"@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Value "Beleg über die Funktionsprüfung und Einweisung nach § 10 Absatz 1,"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Value "Name der nach § 10 Absatz 1 Satz 1 Nummer 2 beauftragten Person, Zeitpunkt der Einweisung sowie Namen der eingewiesenen Personen,"@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Value "Fristen und Datum der Durchführung sowie das Ergebnis von vorgeschriebenen sicherheits- und messtechnischen Kontrollen und Datum von Instandhaltungen sowie der Name der verantwortlichen Person oder der Firma, die diese Maßnahme durchgeführt hat,"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Value "Datum, Art und Folgen von Funktionsstörungen und wiederholten gleichartigen Bedienungsfehlern sowie"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Value "Angaben zu Vorkommnismeldungen an Behörden und Hersteller."@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bezeichnung, Art und Typ, Loscode oder die Seriennummer, Anschaffungsjahr des Medizinproduktes,"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name oder Firma und die Anschrift des Herstellers oder des Bevollmächtigten oder, sofern der Hersteller keinen Unternehmenssitz in der Europäischen Union und keinen Bevollmächtigten beauftragt hat, des Importeurs,"@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die der CE-Kennzeichnung hinzugefügte Kennnummer der Benannten Stelle, soweit diese nach den Vorschriften des Medizinproduktegesetzes in der bis einschließlich 25. Mai 2021 geltenden Fassung oder nach der Verordnung (EU) 2017/745 angegeben ist,"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit vorhanden, betriebliche Identifikationsnummer,"@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Standort und betriebliche Zuordnung,"@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nach § 11 Absatz 1 Satz 2 und 3 festgelegte Frist für sicherheitstechnische Kontrollen."@de .

dtd:LA-id571 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anzeichen dafür vorliegen, dass das Medizinprodukt die Fehlergrenzen nach Absatz 2 nicht einhält oder"@de .

dtd:LA-id572 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die messtechnischen Eigenschaften des Medizinproduktes durch einen Eingriff oder auf andere Weise beeinflusst worden sein könnten."@de .

dtd:LA-id573 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für das Messwesen zuständige Behörden oder"@de .

dtd:LA-id574 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, Betriebe oder Einrichtungen, die selbst oder deren Beschäftigte, die die messtechnischen Kontrollen durchführen, die Voraussetzungen von § 5 hinsichtlich der messtechnischen Kontrollen des jeweiligen Medizinproduktes erfüllen."@de .

dtd:LA-id575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über die messtechnische Kontrolle ein Protokoll anzufertigen, das das Datum der Durchführung und die Ergebnisse der messtechnischen Kontrolle unter Angabe der ermittelten Messwerte, der Messverfahren und sonstiger Beurteilungsergebnisse enthält, und"@de .

dtd:LA-id576 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Medizinprodukt nach erfolgreicher messtechnischer Kontrolle mit einem Zeichen zu kennzeichnen; aus dem Zeichen müssen das Jahr der nächsten messtechnischen Kontrolle und die Behörde oder Person, die die messtechnische Kontrolle durchgeführt hat, eindeutig und rückverfolgbar hervorgehen."@de .

dtd:LA-id577 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Informationen im Sinne des Artikels 18 Absatz 1 Unterabsatz 1 der Verordnung (EU) 2017/745 in einer Form bereitzustellen, die einen schnellen Zugang zu den Informationen ermöglicht und"@de .

dtd:LA-id578 a dtd:LA ;
    dtd:has_DL dtd:DL-id426 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Implantationsausweis im Sinne des Artikels 18 Absatz 1 Unterabsatz 3 der Verordnung (EU) 2017/745 zur Verfügung zu stellen, der neben den Angaben nach Artikel 18 Absatz 1 Unterabsatz 1 Buchstabe a der Verordnung (EU) 2017/745 folgende zusätzliche Angaben enthält: a)den Vor- und Zuname der Patientin oder des Patienten,b)den Namen und die Adresse der Einrichtung, in der die Implantation durchgeführt wurde undc)das Datum der Implantation."@de .

dtd:LA-id579 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Vor- und Zuname der Patientin oder des Patienten,"@de .

dtd:LA-id580 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Namen und die Adresse der Einrichtung, in der die Implantation durchgeführt wurde und"@de .

dtd:LA-id581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Datum der Implantation."@de .

dtd:LA-id582 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dies zur Durchführung der besonderen Aufgaben gerechtfertigt ist oder"@de .

dtd:LA-id583 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Besonderheiten eingelagerter Medizinprodukte dies erfordern oder"@de .

dtd:LA-id584 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erfüllung zwischenstaatlicher Verpflichtungen der Bundesrepublik Deutschland dies erfordern"@de .

dtd:LA-id585 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 4 Absatz 8 oder § 10 Absatz 1 Satz 1 oder Absatz 2 ein dort genanntes Medizinprodukt betreibt oder anwendet,"@de .

dtd:LA-id586 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 6 Absatz 1 nicht sicherstellt, dass ein Beauftragter für Medizinproduktesicherheit bestimmt ist,"@de .

dtd:LA-id587 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 6 Absatz 4 nicht sicherstellt, dass eine Funktions-E-Mail-Adresse bekannt gemacht ist,"@de .

dtd:LA-id588 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 7 Absatz 2, § 8 Absatz 4 Satz 1, § 11 Absatz 4 oder § 14 Absatz 5 Nummer 2 eine Person, einen Betrieb oder eine Einrichtung beauftragt,"@de .

dtd:LA-id589 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 8 Absatz 1 Satz 1, auch in Verbindung mit Satz 2, die Aufbereitung eines dort genannten Medizinproduktes nicht richtig durchführt,"@de .

dtd:LA-id590 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ohne Zertifizierung nach § 8 Absatz 3 ein dort genanntes Medizinprodukt aufbereitet,"@de .

dtd:LA-id591 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 9 Absatz 1 Satz 1 ein Qualitätssicherungssystem nicht, nicht richtig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig einrichtet,"@de .

dtd:LA-id592 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 11 Absatz 1 Satz 1, auch in Verbindung mit Satz 5, oder § 14 Absatz 1 Satz 1 eine Kontrolle nicht, nicht richtig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig durchführt oder nicht, nicht richtig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig durchführen lässt,"@de .

dtd:LA-id593 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 11 Absatz 3 Satz 2 oder § 14 Absatz 7 Satz 2 ein dort genanntes Protokoll nicht oder nicht für die vorgeschriebene Dauer aufbewahrt,"@de .

dtd:LA-id594 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 12 Absatz 1 Satz 1 oder § 13 Absatz 1 Satz 1 ein Medizinproduktebuch oder ein Bestandsverzeichnis nicht, nicht richtig oder nicht vollständig führt,"@de .

dtd:LA-id595 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 14 Absatz 6 eine dort genannte Tätigkeit nicht, nicht richtig oder nicht rechtzeitig anzeigt,"@de .

dtd:LA-id596 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id597 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 14 Absatz 7 Satz 1 Nummer 2 ein Medizinprodukt nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig kennzeichnet,"@de .

dtd:LA-id598 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 15 Absatz 1 ein dort genanntes Dokument nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig aushändigt,"@de .

dtd:LA-id599 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 15 Absatz 2 Satz 1 oder Satz 2 erster Halbsatz eine dort genannte Dokumentation oder Aufzeichnung nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht mindestens 20 Jahre aufbewahrt oder"@de .

dtd:LA-id600 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 19 Nummer 1 oder Nummer 2 Satz 1, auch in Verbindung mit Satz 2, ein Medizinprodukt betreibt oder weiterbetreibt."@de .

dtd:LA-id601 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Medizinprodukte nach § 2 Nr. 1 der Medizingeräteverordnung dürfen außer in den Fällen des § 5 Abs. 10 der Medizingeräteverordnung nur betrieben werden, wenn sie der Bauart nach zugelassen sind."@de .

dtd:LA-id602 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ist die Bauartzulassung zurückgenommen oder widerrufen worden, dürfen vor der Bekanntmachung der Rücknahme oder des Widerrufs im Bundesanzeiger in Betrieb genommene Medizinprodukte nur weiterbetrieben werden, wenn sie der zurückgenommenen oder widerrufenen Zulassung entsprechen und in der Bekanntmachung nach § 5 Abs. 9 der Medizingeräteverordnung nicht festgestellt wird, dass Gefahren für Patienten, Beschäftigte oder Dritte zu befürchten sind. Dies gilt auch, wenn eine Bauartzulassung nach § 5 Abs. 8 Nr. 2 der Medizingeräteverordnung erloschen ist."@de .

dtd:LA-id603 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Medizinprodukte, für die dem Betreiber vor Inkrafttreten des Medizinproduktegesetzes eine Ausnahme nach § 8 Abs. 1 der Medizingeräteverordnung erteilt wurde, dürfen nach den in der Ausnahmezulassung festgelegten Maßnahmen weiterbetrieben werden."@de .

dtd:LA-id604 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der Betreiber eines Medizinproduktes, der gemäß § 8 Abs. 2 der Medizingeräteverordnung von den allgemein anerkannten Regeln der Technik, soweit sie sich auf den Betrieb des Medizinproduktes beziehen, abweichen durfte, darf dieses Produkt in der bisherigen Form weiterbetreiben, wenn er eine ebenso wirksame Maßnahme trifft. Auf Verlangen der zuständigen Behörde hat der Betreiber nachzuweisen, dass die andere Maßnahme ebenso wirksam ist."@de .

dtd:LA-id605 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Medizinprodukte nach § 2 Nr. 1 und 3 der Medizingeräteverordnung dürfen nur von Personen angewendet werden, die am Medizinprodukt unter Berücksichtigung der Gebrauchsanweisung in die sachgerechte Handhabung eingewiesen worden sind. Werden solche Medizinprodukte mit Zusatzgeräten zu Gerätekombinationen erweitert, ist die Einweisung auf die Kombination und deren Besonderheiten zu erstrecken. Nur solche Personen dürfen einweisen, die auf Grund ihrer Kenntnisse und praktischen Erfahrungen für die Einweisung und die Handhabung dieser Medizinprodukte geeignet sind."@de .

dtd:LA-id606 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der Betreiber eines Medizinproduktes nach § 2 Nr. 1 der Medizingeräteverordnung hat die in der Bauartzulassung festgelegten sicherheitstechnischen Kontrollen im dort vorgeschriebenen Umfang fristgerecht durchzuführen oder durchführen zu lassen. Bei Dialysegeräten, die mit ortsfesten Versorgungs- und Aufbereitungseinrichtungen verbunden sind, ist die sicherheitstechnische Kontrolle auch auf diese Einrichtungen zu erstrecken."@de .

dtd:LA-id607 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bei Medizinprodukten nach § 2 Nr. 1 der Medizingeräteverordnung, für die nach § 28 Abs. 1 der Medizingeräteverordnung Bauartzulassungen nicht erforderlich waren oder die nach § 28 Abs. 2 der Medizingeräteverordnung betrieben werden dürfen, gelten für Umfang und Fristen der sicherheitstechnischen Kontrollen die Angaben in den Prüfbescheinigungen nach § 28 Abs. 1 oder 2 der Medizingeräteverordnung."@de .

dtd:LA-id608 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bestandsverzeichnisse und Gerätebücher nach den §§ 12 und 13 der Medizingeräteverordnung dürfen weitergeführt werden und gelten als Bestandsverzeichnis und Medizinproduktebuch entsprechend den §§ 12 und 13 dieser Verordnung."@de .

dtd:LA-id609 a dtd:LA ;
    dtd:has_DL dtd:DL-id492 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unbeschadet, ob Medizinprodukte die Anforderungen nach § 6 Abs. 1 Satz 1 der Medizingeräteverordnung im Einzelfall erfüllen, dürfen Medizinprodukte weiterbetrieben werden, wenn siea)vor dem Wirksamwerden des Beitritts zulässigerweise in dem in Artikel 3 des Einigungsvertrages genannten Gebiet betrieben wurden,b)bis zum 31. Dezember 1991 errichtet und in Betrieb genommen wurden und den Vorschriften entsprechen, die am Tage vor dem Wirksamwerden des Beitritts in dem in Artikel 3 des Einigungsvertrages genannten Gebiet gegolten haben."@de .

dtd:LA-id610 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vor dem Wirksamwerden des Beitritts zulässigerweise in dem in Artikel 3 des Einigungsvertrages genannten Gebiet betrieben wurden,"@de .

dtd:LA-id611 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bis zum 31. Dezember 1991 errichtet und in Betrieb genommen wurden und den Vorschriften entsprechen, die am Tage vor dem Wirksamwerden des Beitritts in dem in Artikel 3 des Einigungsvertrages genannten Gebiet gegolten haben."@de .

dtd:LA-id612 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nichtimplantierbare aktive Medizinprodukte zur"@de .

dtd:LA-id613 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erzeugung und Anwendung elektrischer Energie zur unmittelbaren Beeinflussung der Funktion von Nerven und/oder Muskeln beziehungsweise der Herztätigkeit einschließlich Defibrillatoren,"@de .

dtd:LA-id614 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "intrakardialen Messung elektrischer Größen oder Messung anderer Größen unter Verwendung elektrisch betriebener Messsonden in Blutgefäßen beziehungsweise an freigelegten Blutgefäßen,"@de .

dtd:LA-id615 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erzeugung und Anwendung jeglicher Energie zur unmittelbaren Koagulation, Gewebezerstörung oder Zertrümmerung von Ablagerungen in Organen,"@de .

dtd:LA-id616 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "unmittelbare Einbringung von Substanzen und Flüssigkeiten in den Blutkreislauf unter potentiellem Druckaufbau, wobei die Substanzen und Flüssigkeiten auch aufbereitete oder speziell behandelte körpereigene sein können, deren Einbringen mit einer Entnahmefunktion direkt gekoppelt ist,"@de .

dtd:LA-id617 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "maschinellen Beatmung mit oder ohne Anästhesie,"@de .

dtd:LA-id618 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Diagnose mit bildgebenden Verfahren nach dem Prinzip der Kernspinresonanz,"@de .

dtd:LA-id619 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Therapie mit Druckkammern,"@de .

dtd:LA-id620 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Therapie mittels Hypothermie"@de .

dtd:LA-id621 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Säuglingsinkubatoren sowie"@de .

dtd:LA-id622 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "externe aktive Komponenten aktiver Implantate."@de .

dtd:LA-id623 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aktive implantierbare Medizinprodukte"@de .

dtd:LA-id624 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nachfolgende implantierbare Produkte:"@de .


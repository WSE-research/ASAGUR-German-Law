    dtd:has_Size dtd:normal ;
    dtd:has_Value "die letzten drei Jahresabschlüsse einschließlich der Prüfungsberichte (gegebenenfalls die Konzernabschlüsse und Einzelabschlüsse der Beteiligungsgesellschaften);"@de .

dtd:LA-id202 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Beurteilung der voraussichtlichen Ertragsentwicklung der nächsten fünf Jahre;"@de .

dtd:LA-id203 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Investitionsübersicht;"@de .

dtd:LA-id204 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Übersicht zur aktuellen Finanz- und Liquiditätslage;"@de .

dtd:LA-id205 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Übersicht über sämtliche Vermögenswerte, die einen durch die Rechtsverordnung gemäß § 9 Abs. 4 noch festzulegenden Einzelwert übersteigen, aufgeteilt nach betriebsnotwendigen und nicht betriebsnotwendigen Vermögenswerten; bei nicht betriebsnotwendigen Vermögenswerten ist zusätzlich der geschätzte aktuelle Verkehrswert anzugeben;"@de .

dtd:LA-id206 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Auflistung aller seit dem Abschluss der Rangrücktrittsvereinbarung erfolgten Veräußerungen von Anlagegütern, soweit der Veräußerungserlös einen durch die Rechtsverordnung gemäß § 9 Abs. 4 noch festzulegenden Betrag übersteigt, mit Angaben darüber, ob eine Wiederbeschaffung erfolgt ist;"@de .

dtd:LA-id207 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit in der Anlage 2 der Rangrücktrittsvereinbarung enthaltene nicht betriebsnotwendige Vermögens- und Betriebsteile noch nicht veräußert wurden, ist dem Antrag ein unabhängiges Sachverständigengutachten zum aktuellen Verkehrswert dieser Vermögens- und Betriebsteile beizufügen."@de .

dtd:LA-id208 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu den gemäß § 2 Abs. 1 Satz 1 nicht zu berücksichtigenden Bewertungswahlrechten nach § 2 Abs. 1 Satz 2,"@de .

dtd:LA-id209 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Angemessenheit der Vergütungen nach § 2 Abs. 5,"@de .

dtd:LA-id210 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über Umfang und Einzelheiten der nach § 8 Abs. 2 vorzulegenden Unterlagen sowie"@de .

dtd:LA-id211 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Ermittlung des Ablösebetrages gemäß Absatz 2 und § 7 Abs. 1 Satz 2 bis 4"@de .

dtd:P-id213 a dtd:P ;
    dtd:has_BR dtd:BR-id139 ;
    dtd:has_Value " (+++ Textnachweis ab: 1.7.2004 +++)Das G wurde als Artikel 1 d. G v. 25.6.2004 I 1383 vom Bundestag beschlossen. Es ist gem. Art. 7 Abs. 1 dieses G am 1.7.2004 in Kraft getreten."@de ;

    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Ansatz des niedrigeren Teilwerts nach § 6 Abs. 1 Nr. 1 Satz 2 und Nr. 2 Satz 2 des Einkommensteuergesetzes;"@de .

dtd:LA-id193 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bewertung von Verbindlichkeiten nach § 6 Abs. 1 Nr. 3 Satz 1 in Verbindung mit Nr. 2 Satz 2 des Einkommensteuergesetzes;"@de .


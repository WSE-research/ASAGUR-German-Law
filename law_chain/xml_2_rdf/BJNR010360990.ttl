dtd:LA-id4490 a dtd:LA ;
    dtd:has_B dtd:B-id369 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fahrbahnarten"@de .

dtd:LA-id4491 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die in Beiblatt 2 aufgeführten akustischen Kenndaten gelten für Fahrzeugarten auf Schwellengleisen im Schotterbett und für ein durchschnittlich gepflegtes Rad-Schiene-System. Für andere Fahrbahnarten sind aufgrund der Schienen- und Radrauheit für die Teilquellen 1 und 2 Pegelkorrekturen nach Tabelle 15 vorzunehmen."@de .

dtd:LA-id4492 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anmerkung: Schwellengleise im Schotterbett schließen Beton-, Holz- und Stahlschwellen ein."@de .


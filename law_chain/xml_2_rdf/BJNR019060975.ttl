dtd:LA-id318 a dtd:LA ;
    dtd:has_DL dtd:DL-id269 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Lohnsteuerhilfevereine,a)wenn die Anerkennung als Lohnsteuerhilfeverein erloschen oder unanfechtbar zurückgenommen oder widerrufen ist,b)wenn der Sitz und die Geschäftsleitung aus dem Bezirk der Aufsichtsbehörde verlegt wird;"@de .

dtd:LA-id319 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn die Anerkennung als Lohnsteuerhilfeverein erloschen oder unanfechtbar zurückgenommen oder widerrufen ist,"@de .

dtd:LA-id320 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn der Sitz und die Geschäftsleitung aus dem Bezirk der Aufsichtsbehörde verlegt wird;"@de .

dtd:LA-id321 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beratungsstellen, wenn die Beratungsstelle geschlossen ist;"@de .

dtd:LA-id322 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beratungsstellen, wenn deren Sitz in den Bezirk einer anderen Aufsichtsbehörde verlegt wird."@de .

dtd:LA-id323 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegenüber mehreren entschädigungspflichtigen Personen, auf welche sich der Versicherungsschutz erstreckt,"@de .

dtd:LA-id324 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bezüglich eines aus mehreren Verstößen stammenden einheitlichen Schadens,"@de .

dtd:LA-id325 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bezüglich sämtlicher Folgen eines Verstoßes. Dabei gilt mehrfaches, auf gleicher oder gleichartiger Fehlerquelle beruhendes Tun oder Unterlassen als einheitlicher Verstoß, wenn die betreffenden Angelegenheiten miteinander in rechtlichem oder wirtschaftlichem Zusammenhang stehen. In diesem Fall kann die Leistung des Versicherers auf das Fünffache der Mindestversicherungssumme begrenzt werden."@de .

dtd:LA-id326 a dtd:LA ;

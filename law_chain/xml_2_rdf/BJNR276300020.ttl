dtd:LA-id81 a dtd:LA ;
    dtd:has_Value "das Grundgehalt,"@de .

dtd:LA-id82 a dtd:LA ;
    dtd:has_Value "der Familienzuschlag,"@de .

dtd:LA-id83 a dtd:LA ;
    dtd:has_Value "die Amtszulagen,"@de .

dtd:LA-id84 a dtd:LA ;
    dtd:has_Value "die Stellenzulagen,"@de .

dtd:LA-id85 a dtd:LA ;
    dtd:has_Value "die Überleitungszulagen,"@de .

dtd:LA-id86 a dtd:LA ;
    dtd:has_Value "die Ausgleichszulagen, die wegen des Wegfalls oder der Verminderung der Bezüge nach den Nummern 1 bis 5 zustehen,"@de .


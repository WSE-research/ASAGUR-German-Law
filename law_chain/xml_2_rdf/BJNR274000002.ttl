dtd:LA-id670 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bereitstellen von Werkzeugen sowie von Werk-, Betriebs- und Hilfsstoffen,"@de .

dtd:LA-id671 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Warten und Pflegen von Betriebsmitteln,"@de .

dtd:LA-id672 a dtd:LA ;

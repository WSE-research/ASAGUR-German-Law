    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundlagen der Metallbearbeitung,"@de .

dtd:LA-id674 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bedienen von Produktionsanlagen, Überwachen von Produktionsabläufen,"@de .

dtd:LA-id675 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bearbeiten von Brillengläsern,"@de .

dtd:LA-id676 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Reinigen von Gläsern,"@de .

dtd:LA-id677 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Oberflächenveredlung,"@de .

dtd:LA-id678 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kundenberatung."@de .

dtd:LA-id679 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Manuelles und maschinelles Bearbeiten von Brillengläsern unter Berücksichtigung des Gesundheitsschutzes bei der Arbeit sowie"@de .

dtd:LA-id680 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Messen, Prüfen und Kontrollieren einschließlich Anfertigen eines Arbeitsplanes und eines Prüf- und Messprotokolls."@de .

dtd:LA-id681 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Herstellen von Brillengläsern einschließlich Oberflächenveredlung und Konfektionierung sowie"@de .

dtd:LA-id682 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Messen, Prüfen und Kontrollieren auf geometrische Anforderungen, optische Eigenschaften und kosmetische Abweichungen einschließlich Arbeitsplanung, Ändern und Optimieren von Programmen für numerisch gesteuerte Geräte, Maschinen oder Anlagen."@de .

dtd:LA-id683 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fertigungstechnik,"@de .

dtd:LA-id684 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mess- und Prüftechnik sowie"@de .

dtd:LA-id685 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wirtschafts- und Sozialkunde."@de .

dtd:LA-id686 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Für den Prüfungsbereich Fertigungstechnik:"@de .

dtd:LA-id687 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beschreiben der Vorgehensweise bei der Herstellung von Brillengläsern aus Glas und Kunststoff mit verschiedenen Fertigungsverfahren, Erstellen von Fertigungsunterlagen, Planen, Steuern und Optimieren von Arbeitsabläufen unter Berücksichtigung des Qualitätsmanagements."@de .

dtd:LA-id688 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dabei soll der Prüfling zeigen, dass er die Arbeitssicherheits- und Umweltschutzbestimmungen berücksichtigen, die Verwendung von Werk- und Hilfsstoffen planen sowie Werkzeuge, Maschinen und Verfahren zuordnen kann. Weiter soll der Prüfling zeigen, dass er Problemanalysen durchführen kann, die für die Bearbeitung der Brillengläser erforderlich sind. Er soll zeigen, dass er Werkzeuge und Hilfsmittel unter Beachtung von technischen Vorgaben auswählen und Arbeitsschritte planen kann."@de .

dtd:LA-id689 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Für den Prüfungsbereich Mess- und Prüftechnik:"@de .

dtd:LA-id690 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beschreiben der Vorgehensweise beim Messen, Prüfen und Kontrollieren sowie bei der systematischen Eingrenzung von Fehlern im technischen System nach vorgegebenen Anforderungen im Rahmen des Qualitätsmanagements."@de .

dtd:LA-id691 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dabei soll der Prüfling zeigen, dass er Probleme bei der Bearbeitung von Brillengläsern analysieren und Maßnahmen zur Behebung einleiten kann. Weiter soll der Prüfling zeigen, dass er Arbeitsschritte, Werkzeuge und Hilfsmittel unter Beachtung technischer Vorgaben, der Arbeitssicherheit, des Gesundheits- und Umweltschutzes auswählen und anwenden kann. Er soll zeigen, dass er funktionale Zusammenhänge von Geräten, Maschinen, Anlagen und deren Systeme erläutern, Mess- und Prüfverfahren auswählen und anwenden, Mess- und Prüfmittel einsetzen sowie Mess- und Prüfergebnisse dokumentieren und interpretieren kann."@de .

dtd:LA-id692 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Für den Prüfungsbereich Wirtschafts- und Sozialkunde:"@de .

dtd:LA-id693 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufgaben, die sich auf praxisbezogene Fälle beziehen sollen, insbesondere aus folgenden Gebieten:"@de .

dtd:LA-id694 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Allgemeine wirtschaftliche und gesellschaftliche Zusammenhänge der Berufs- und Arbeitswelt."@de .

dtd:LA-id695 a dtd:LA ;
    dtd:has_Value "Bedeutung des Ausbildungsvertrages, insbesondere Abschluss, Dauer und Beendigung, erklären"@de .

dtd:LA-id696 a dtd:LA ;
    dtd:has_Value "gegenseitige Rechte und Pflichten aus dem Ausbildungsvertrag nennen"@de .

dtd:LA-id697 a dtd:LA ;
    dtd:has_Value "Möglichkeiten der beruflichen Fortbildung nennen"@de .

dtd:LA-id698 a dtd:LA ;
    dtd:has_Value "wesentliche Teile des Arbeitsvertrages nennen"@de .

dtd:LA-id699 a dtd:LA ;
    dtd:has_Value "wesentliche Bestimmungen der für den ausbildenden Betrieb geltenden Tarifverträge nennen"@de .

dtd:LA-id700 a dtd:LA ;
    dtd:has_Value "Aufbau und Aufgaben des ausbildenden Betriebes erläutern"@de .

dtd:LA-id701 a dtd:LA ;
    dtd:has_Value "Grundfunktionen des ausbildenden Betriebes, wie Beschaffung, Fertigung, Absatz und Verwaltung erklären"@de .

dtd:LA-id702 a dtd:LA ;
    dtd:has_Value "Beziehungen des ausbildenden Betriebes und seiner Beschäftigten zu Wirtschaftsorganisationen, Berufsvertretungen und Gewerkschaften nennen"@de .

dtd:LA-id703 a dtd:LA ;
    dtd:has_Value "Grundlagen, Aufgaben und Arbeitsweise der betriebsverfassungs- oder personalvertretungsrechtlichen Organe des ausbildenden Betriebes beschreiben"@de .

dtd:LA-id704 a dtd:LA ;
    dtd:has_Value "Gefährdung von Sicherheit und Gesundheit am Arbeitsplatz feststellen und Maßnahmen zu ihrer Vermeidung ergreifen"@de .

dtd:LA-id705 a dtd:LA ;
    dtd:has_Value "berufsbezogene Arbeitsschutz- und Unfallverhütungsvorschriften anwenden"@de .

dtd:LA-id706 a dtd:LA ;
    dtd:has_Value "Verhaltensweisen bei Unfällen beschreiben sowie erste Maßnahmen einleiten"@de .

dtd:LA-id707 a dtd:LA ;
    dtd:has_Value "Vorschriften des vorbeugenden Brandschutzes anwenden; Verhaltensweisen bei Bränden beschreiben und Maßnahmen zur Brandbekämpfung ergreifen"@de .

dtd:LA-id708 a dtd:LA ;
    dtd:has_Value "mögliche Umweltbelastungen durch den Ausbildungsbetrieb und seinen Beitrag zum Umweltschutz an Beispielen erklären"@de .

dtd:LA-id709 a dtd:LA ;
    dtd:has_Value "für den Ausbildungsbetrieb geltende Regelungen des Umweltschutzes anwenden"@de .

dtd:LA-id710 a dtd:LA ;
    dtd:has_Value "Möglichkeiten der wirtschaftlichen und umweltschonenden Energie- und Materialverwendung nutzen"@de .

dtd:LA-id711 a dtd:LA ;
    dtd:has_Value "Abfälle vermeiden; Stoffe und Materialien einer umweltschonenden Entsorgung zuführen"@de .

dtd:LA-id712 a dtd:LA ;
    dtd:has_Value "Arbeitsplatz unter Berücksichtigung der betrieblichen Vorgaben mitgestalten, insbesondere Verbesserungen der Arbeitsumgebung unter Berücksichtigung gesundheitlicher Aspekte anregen"@de .

dtd:LA-id713 a dtd:LA ;
    dtd:has_Value "Arbeitsabläufe unter Beachtung wirtschaftlicher und terminlicher Vorgaben planen und abstimmen"@de .

dtd:LA-id714 a dtd:LA ;
    dtd:has_Value "Material, Werkzeuge, Prüf- und Messmittel bereitstellen und betriebsbereit machen"@de .

dtd:LA-id715 a dtd:LA ;
    dtd:has_Value "Arbeitsplatz unter Berücksichtigung des Arbeitsauftrages vorbereiten"@de .

dtd:LA-id716 a dtd:LA ;
    dtd:has_Value "Bearbeitungsmaschinen und technische Einrichtungen betriebsbereit machen und überprüfen sowie Maßnahmen zur Fehlerbeseitigung ergreifen"@de .

dtd:LA-id717 a dtd:LA ;
    dtd:has_Value "Arbeitsschritte unter Berücksichtigung funktionaler, konstruktiver, fertigungstechnischer und wirtschaftlicher Gesichtspunkte festlegen"@de .

dtd:LA-id718 a dtd:LA ;
    dtd:has_Value "Arbeitsergebnisse zusammenführen, erbrachte Leistungen kontrollieren und anhand von Vorgaben bewerten sowie dokumentieren"@de .

dtd:LA-id719 a dtd:LA ;
    dtd:has_Value "technische Zeichnungen sowie Skizzen und Stücklisten anfertigen und anwenden"@de .

dtd:LA-id720 a dtd:LA ;
    dtd:has_Value "Normen, insbesondere Toleranznormen, anwenden"@de .

dtd:LA-id721 a dtd:LA ;
    dtd:has_Value "Mess- und Prüfdaten lesen und dokumentieren"@de .

dtd:LA-id722 a dtd:LA ;
    dtd:has_Value "Informationen beschaffen und auswerten; Informations- und Kommunikationstechniken nutzen; Daten sichern und schützen"@de .


    dtd:has_Value "entgegen Artikel 19 Satz 1 verpackte Eier der Klasse A umpackt,"@de .

dtd:LA-id255 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen Artikel 20 Absatz 1 oder Absatz 2, jeweils auch in Verbindung mit Absatz 3, oder Artikel 22 Absatz 2 nicht, nicht richtig oder nicht vollständig Buch führt,"@de .

dtd:LA-id256 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen Artikel 21 Absatz 1 oder Artikel 22 Absatz 1 Satz 1 eine Aufzeichnung nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig fertigt,"@de .

dtd:LA-id257 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen Artikel 22 Absatz 1 Satz 2 die Bestandsbuchführung nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig aktualisiert,"@de .

dtd:LA-id258 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen Artikel 23 Aufzeichnungen oder Unterlagen nicht aufbewahrt oder"@de .

dtd:LA-id259 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen Artikel 24 Absatz 5 Satz 2 eine Aufzeichnung nicht oder nicht rechtzeitig zur Verfügung stellt."@de .

dtd:LA-id260 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1b Eier zum Verkauf vorrätig hält, anbietet, feilhält, liefert, verkauft oder sonst in den Verkehr bringt,"@de .

dtd:LA-id261 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 2 in Rechnungen, Lieferscheinen oder sonstigen Transportbegleitpapieren nicht die jeweilige Güte- und Gewichtsklasse angibt,"@de .

dtd:LA-id262 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 für Eier ohne Angabe der jeweiligen Güte- und Gewichtsklasse wirbt,"@de .

dtd:LA-id263 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id264 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 4 Preisnotierungen oder Preisfeststellungen für Eier nicht die vorgeschriebene Güte- und Gewichtsklasse zugrunde legt oder"@de .

dtd:LA-id265 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 6 Absatz 2 Satz 1 eine Anzeige nicht, nicht vollständig oder nicht rechtzeitig macht."@de .

dtd:LA-id266 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geschäftsräume und Grundstücke, Verkaufseinrichtungen und Transportmittel betreten und dort Besichtigungen vornehmen,"@de .

dtd:LA-id267 a dtd:LA ;
    dtd:has_Size dtd:normal ;

    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name und Vorname, Geburtstag, Wohnort und Wohnung des Verpfänders sowie Art des Ausweises, aus dem diese Angaben entnommen sind, und ausstellende Behörde,"@de .

dtd:LA-id323 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "schriftliche Vollmacht des Verpfänders, falls der Überbringer des Pfandes nicht der Verpfänder ist,"@de .

dtd:LA-id324 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betrag und Fälligkeit des Darlehens,"@de .

dtd:LA-id325 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vereinbarte Leistungen, soweit diese nicht in den allgemeinen Geschäftsbedingungen des Pfandleihers festgelegt sind,"@de .

dtd:LA-id326 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tag der Einlösung,"@de .

dtd:LA-id327 a dtd:LA ;
    dtd:has_DL dtd:DL-id261 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bezeichnung des Pfandes nach Zahl und Art sowie die zur Unterscheidung geeigneten Angaben, wie Maß, Fabrikmarke und -nummer, bei Gold- und Silbersachen Gewicht und etwaiger Feingehaltsstempel, bei Kraftfahrzeugen und Kraftfahrzeuganhängerna)Art, Hersteller und Typ,b)amtliches Kennzeichen,c)Fabriknummer des Fahrgestells und des Motors,d)Anzahl der Ersatzreifen,e)Nutzlast (nur für Lastkraftwagen und Kraftfahrzeuganhänger),"@de .

dtd:LA-id328 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Art, Hersteller und Typ,"@de .

dtd:LA-id329 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "amtliches Kennzeichen,"@de .

dtd:LA-id330 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fabriknummer des Fahrgestells und des Motors,"@de .

dtd:LA-id331 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anzahl der Ersatzreifen,"@de .

dtd:LA-id332 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nutzlast (nur für Lastkraftwagen und Kraftfahrzeuganhänger),"@de .

dtd:LA-id333 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zahlungen des Verpfänders,"@de .

dtd:LA-id334 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tag der Verwertung,"@de .

dtd:LA-id335 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Höhe und Verbleib des Verwertungserlöses und"@de .

dtd:LA-id336 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Verlust eines Pfandscheins Tag der Mitteilung des Verlustes."@de .

dtd:LA-id337 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "er sich wegen seiner Forderungen auf Rückzahlung des Darlehens sowie auf Zahlung von Zinsen, Vergütungen und Kosten nur aus dem Pfand befriedigen darf,"@de .

dtd:LA-id338 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "er berechtigt ist, drei Jahre nach Ablauf des Jahres, in dem das Pfand verwertet worden ist, den Teil des Erlöses, der ihm nicht zu seiner Befriedigung gebührt und nicht an den Verpfänder ausgezahlt worden ist, an die zuständige Behörde abzuführen oder sich daraus nach Maßgabe des § 11 Abs. 2 zu befriedigen, und daß damit dieser Teil des Erlöses verfällt."@de .

dtd:LA-id339 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Hingabe des Darlehens einen monatlichen Zins von eins vom Hundert des Darlehnsbetrags,"@de .

dtd:LA-id340 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Kosten des Geschäftsbetriebs Vergütungen gemäß der Anlage zu dieser Verordnung,"@de .

dtd:LA-id341 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die notwendigen Kosten der Verwertung."@de .

dtd:LA-id342 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Prämien für eine auf Verlangen des Verpfänders abgeschlossene besondere Versicherung,"@de .

dtd:LA-id343 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kosten eines Gutachtens über den Wert des Pfandes."@de .

dtd:LA-id344 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der Tag der Hingabe des Darlehens darf nur mitgerechnet werden, wenn das Darlehen an diesem Tag zurückgezahlt wird,"@de .

dtd:LA-id345 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein angefangener Monat darf als voller Monat gerechnet werden."@de .

dtd:LA-id346 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 2 die für den Geschäftsbetrieb benutzten Räume oder einen Wechsel der Räume nicht oder nicht rechtzeitig anzeigt,"@de .

dtd:LA-id347 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Vorschrift des § 3 Abs. 1, 2 oder 3 über Aufzeichnungen, Unterlagen und Belege zuwiderhandelt,"@de .

dtd:LA-id348 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .


dtd:I-id134 a dtd:I ;
    dtd:has_Value "Hinweis: Es können für ein Profil, Standard, Leitfaden mehrere Versionen in die Anlage aufgenommen werden."@de .

dtd:LA-id276 a dtd:LA ;
    dtd:has_DL dtd:DL-id175 ;
    dtd:has_Value "„Interoperabilität“ die Fähigkeit zweier oder mehrerer informationstechnischer Anwendungen, a)Informationen auszutauschen und diese für die korrekte Ausführung einer konkreten Funktion ohne Änderung des Inhalts der Daten zu nutzen,b)miteinander zu kommunizieren,c)bestimmungsgemäß zusammenzuarbeiten;"@de .

dtd:LA-id277 a dtd:LA ;
    dtd:has_Value "Informationen auszutauschen und diese für die korrekte Ausführung einer konkreten Funktion ohne Änderung des Inhalts der Daten zu nutzen,"@de .

dtd:LA-id278 a dtd:LA ;
    dtd:has_Value "miteinander zu kommunizieren,"@de .

dtd:LA-id279 a dtd:LA ;
    dtd:has_Value "bestimmungsgemäß zusammenzuarbeiten;"@de .

dtd:LA-id280 a dtd:LA ;
    dtd:has_Value "„Standard“ diejenigen Dokumente, die den aktuellen Stand der Technik mit Anforderungs- und Lösungsdefinitionen enthalten, wobei der Entstehungsprozess des Dokuments öffentlich bekannt und dokumentiert ist, inklusive der Prozesse der Veröffentlichung, Nutzung und Versionierung;"@de .

dtd:LA-id281 a dtd:LA ;
    dtd:has_Value "„Profil“ diejenigen Dokumente, die aus einem oder mehreren Standards bestehen, die für eine spezifische Anwendung zusammengestellt sind;"@de .

dtd:LA-id282 a dtd:LA ;
    dtd:has_Value "„Leitfaden“ diejenigen Dokumente, die mindestens eine Anforderung an die Informationsübertragung enthalten; sie erläutern oder dokumentieren die Nutzung einer oder mehrerer Standards oder Profile."@de .

dtd:LA-id283 a dtd:LA ;
    dtd:has_Value "Identifikation der Bedarfe an Anforderungen, Richtlinien und Leitlinien von technischen, semantischen und syntaktischen Standards, Profilen und Leitfäden unter Berücksichtigung europäischer Anforderungen und internationaler Standards,"@de .

dtd:LA-id284 a dtd:LA ;
    dtd:has_Value "Priorisierung der Bedarfe nach Nummer 1,"@de .

dtd:LA-id285 a dtd:LA ;
    dtd:has_Value "Entwicklung und anlassbezogene und turnusmäßige, in der Regel zweijährige, Revision und Fortschreibung von Anforderungen, Richtlinien und Leitlinien von technischen, semantischen und syntaktischen Standards, Profilen und Leitfäden unter Berücksichtigung der Priorisierung nach Nummer 2,"@de .

dtd:LA-id286 a dtd:LA ;
    dtd:has_Value "Empfehlung und anlassbezogene und turnusmäßige, in der Regel zweijährige, Revision und Fortschreibung von Empfehlungen technischer, semantischer und syntaktischer Standards, Profile und Leitfäden und deren Veröffentlichung auf der Wissensplattform nach § 7,"@de .

dtd:LA-id287 a dtd:LA ;
    dtd:has_Value "initiale Ernennung eines Expertengremiums nach § 4,"@de .

dtd:LA-id288 a dtd:LA ;
    dtd:has_Value "Benennung von Experten nach § 5,"@de .

dtd:LA-id289 a dtd:LA ;
    dtd:has_Value "Einrichtung von IOP-Arbeitskreisen nach § 6,"@de .

dtd:LA-id290 a dtd:LA ;
    dtd:has_Value "jährliche Vorlage eines Berichts an das Bundesministerium für Gesundheit nach § 11 sowie monatliche Berichterstattung zum Stand der Arbeiten und geplanten Weiterentwicklungen,"@de .

dtd:LA-id291 a dtd:LA ;
    dtd:has_Value "Einholung und Bewertung von Stellungnahmen zur Erfüllung der Aufgaben nach den Nummern 1, 2, 3 und 4, insbesondere der Stellungnahmen des Bundesamtes für Sicherheit in der Informationstechnik und der oder des Bundesbeauftragten für den Datenschutz und die Informationsfreiheit, binnen vier Wochen nach Beschluss durch die Koordinierungsstelle,"@de .

dtd:LA-id292 a dtd:LA ;
    dtd:has_Value "Betrieb der Wissensplattform nach § 7,"@de .

dtd:LA-id293 a dtd:LA ;
    dtd:has_Value "Festlegung und Fortschreibung der Geschäfts- und Verfahrensordnung nach § 12,"@de .

dtd:LA-id294 a dtd:LA ;
    dtd:has_Value "Organisation und Koordination der Aufgaben nach den Nummern 1 bis 11."@de .

dtd:LA-id295 a dtd:LA ;
    dtd:has_Value "Anwender informationstechnischer Systeme, insbesondere die gematik GmbH und die Kassenärztlichen Bundesvereinigungen,"@de .

dtd:LA-id296 a dtd:LA ;
    dtd:has_Value "für die Wahrnehmung der Interessen der Industrie maßgebliche Bundesverbände aus dem Bereich innovativer Technologien im Gesundheitswesen,"@de .

dtd:LA-id297 a dtd:LA ;
    dtd:has_Value "Bundesländer,"@de .

dtd:LA-id298 a dtd:LA ;
    dtd:has_Value "fachlich betroffene nationale und internationale Standardisierungs- und Normungsorganisationen,"@de .

dtd:LA-id299 a dtd:LA ;
    dtd:has_Value "Verbände, insbesondere der Spitzenverband Bund der Krankenkassen,"@de .

dtd:LA-id300 a dtd:LA ;
    dtd:has_Value "fachlich betroffene Fachgesellschaften des Gesundheitswesens sowie"@de .


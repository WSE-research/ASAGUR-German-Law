dtd:LA-id168 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gültige Einheitenzertifikate der zertifizierungspflichtigen Erzeugungseinheiten,"@de .

dtd:LA-id169 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die mit dem Netzbetreiber vereinbarten Leistungsangaben der Anschluss-Scheinleistung, der Wirkleistung jeweils für Einspeisung und Bezug sowie der installierten Wirkleistung,"@de .

dtd:LA-id170 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Schutzkonzept, bestehend aus übergeordnetem Entkupplungsschutz, Entkupplungsschutz der Erzeugungseinheit, Eigenschutz der Erzeugungseinheit, und die Erfüllung der Vorgaben des Netzbetreibers und"@de .

dtd:LA-id171 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Konzept zur Wirkleistungssteuerung des Netzsicherheitsmanagements und zur Blindleistungsregelung sowie deren Eignung zur Umsetzung der Vorgaben des Netzbetreibers."@de .

dtd:LA-id172 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine maximale Einspeiseleistung von 270 Kilowatt am Verknüpfungspunkt mit dem Netz der allgemeinen Versorgung erbringen,"@de .

dtd:LA-id173 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine kumulierte installierte Leistung von bis zu 270 Kilowatt hinter demselben Verknüpfungspunkt mit dem Netz der allgemeinen Versorgung aufweisen und"@de .

dtd:LA-id174 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über gültige Einheiten- und Komponentenzertifikate für alle zertifizierungspflichtigen Einheiten und Komponenten nach den technischen Anschlussregeln für Erzeugungsanlagen mit Anschluss an ein Niederspannungsnetz des in § 49 Absatz 2 Satz 1 Nummer 1 des Energiewirtschaftsgesetzes bezeichneten Verbandes verfügen."@de .

dtd:LA-id175 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erzeugungsanlagen mit einer kumulierten installierten Leistung von über 950 Kilowatt sowie"@de .

dtd:LA-id176 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Berechnung der maximalen Einspeiseleistung nach Absatz 4 Satz 1 Nummer 1 sowie die Berechnung der kumulierten installierten Leistung nach Absatz 4 Satz 1 Nummer 2 und Satz 2."@de .

dtd:LA-id177 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die technische Regel, nach der die Einheit oder Komponente zertifiziert worden ist,"@de .

dtd:LA-id178 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihre Klasse, beispielsweise Photovoltaik, Windenergie, Speicher, Verbrennungskraftmaschine, Wechselrichter,"@de .

dtd:LA-id179 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihre Hersteller, einschließlich einer Service-Kontaktadresse zur Fehlerbehebung,"@de .

dtd:LA-id180 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihre Typenbezeichnung und"@de .

dtd:LA-id181 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihre Leistung."@de .

dtd:LA-id182 a dtd:LA ;
    dtd:has_Value "bisher keine solche Änderung oder Erweiterung erfolgt ist,"@de .

dtd:LA-id183 a dtd:LA ;
    dtd:has_Value "die Änderung oder Erweiterung von einer Verordnung nach § 12 Absatz 3a des Energiewirtschaftsgesetzes oder den technischen Regeln des in § 49 Absatz 2 Satz 1 Nummer 1 des Energiewirtschaftsgesetzes bezeichneten Verbandes vorgeschrieben ist und"@de .

dtd:LA-id184 a dtd:LA ;
    dtd:has_DL dtd:DL-id144 ;
    dtd:has_Value "der Hersteller von Einheiten und Komponenten für Erzeugungsanlagen in Ergänzung zu dem bestehenden Einheiten- oder Komponentenzertifikat einen von einer im Sinne von § 2 Absatz 2 Satz 2 akkreditierten Zertifizierungsstelle ausgestellten Nachweis an den Betreiber des Registers übermittelt, aus dem sich folgendes ergibt: a)die geänderten oder hinzugefügten elektrotechnischen Eigenschaften sowieb)die Tatsache, dass die Einheit oder Komponente auch nach der Änderung oder Erweiterung die allgemeinen technischen Mindestanforderungen nach § 19 des Energiewirtschaftsgesetzes einhält."@de .

dtd:LA-id185 a dtd:LA ;
    dtd:has_Value "die geänderten oder hinzugefügten elektrotechnischen Eigenschaften sowie"@de .

dtd:LA-id186 a dtd:LA ;
    dtd:has_Value "die Tatsache, dass die Einheit oder Komponente auch nach der Änderung oder Erweiterung die allgemeinen technischen Mindestanforderungen nach § 19 des Energiewirtschaftsgesetzes einhält."@de .

dtd:LA-id187 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen den Pflichten nach § 2 oder nach § 3 in Betrieb genommen wurde oder die Auflage nach § 2 Absatz 2b nicht erfüllt hat oder über ein ungültiges Einheiten- oder Komponentenzertifikat verfügt,"@de .

dtd:LA-id188 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über Einheiten- oder Komponentenzertifikate der zertifizierungspflichtigen Einheiten oder Komponenten verfügt, die in dem Register nach § 49 des Energiewirtschaftsgesetzes als ungültig gekennzeichnet sind, oder"@de .

dtd:LA-id189 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht den technischen Mindestanforderungen entspricht, die im Einheiten- oder Komponentenzertifikat ausgewiesen sind."@de .

dtd:LA-id190 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Räume und Grundstücke, in oder auf denen sich die Erzeugungsanlage befindet, während der üblichen Geschäftszeiten betreten, wobei der Betreiber der Erzeugungsanlage, bei juristischen Personen, rechtsfähigen Personengesellschaften und nichtrechtsfähigen Vereinen die nach Gesetz oder Satzung zur Vertretung berufenen Personen, verpflichtet ist oder sind, das Betreten von Geschäftsräumen und Geschäftsgrundstücken während der üblichen Geschäftszeiten zu dulden, und"@de .

dtd:LA-id191 a dtd:LA ;
    dtd:has_DL dtd:DL-id161 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erzeugungsanlage und, soweit erforderlich, die Kundenanlage oder die Kundenanlage zur betrieblichen Eigenversorgung hinter der Anschlusssicherung ändern, wobei a)die berechtigten Interessen des Anlagenbetreibers und des Anschlussnehmers zu beachten sind,b)durch die Änderung der Leitungs- und Messaufbau in der Kundenanlage nicht verändert werden darf undc)der Betreiber der Erzeugungsanlage, bei juristischen Personen, rechtsfähigen Personengesellschaften und nichtrechtsfähigen Vereinen die nach Gesetz oder Satzung zur Vertretung berufenen Personen, verpflichtet ist oder sind, die Änderung zu dulden."@de .

dtd:LA-id192 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die berechtigten Interessen des Anlagenbetreibers und des Anschlussnehmers zu beachten sind,"@de .

dtd:LA-id193 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch die Änderung der Leitungs- und Messaufbau in der Kundenanlage nicht verändert werden darf und"@de .

dtd:LA-id194 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Betreiber der Erzeugungsanlage, bei juristischen Personen, rechtsfähigen Personengesellschaften und nichtrechtsfähigen Vereinen die nach Gesetz oder Satzung zur Vertretung berufenen Personen, verpflichtet ist oder sind, die Änderung zu dulden."@de .


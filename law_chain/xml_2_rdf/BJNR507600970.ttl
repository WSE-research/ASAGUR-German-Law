    dtd:has_SP dtd:SP-id19 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vertretung in Verwaltungsverfahren"@de .

dtd:LA-id56 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Für die Vertretung des Bundes in Verwaltungsverfahren gilt Nr. 1 entsprechend. § 174 des Bundesbeamtengesetzes und § 126 Abs. 3 des Beamtenrechtsrahmengesetzes bleiben jedoch unberührt."@de .

dtd:LA-id57 a dtd:LA ;
    dtd:has_SP dtd:SP-id20 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rechtsgeschäftliche Vertretung"@de .

dtd:LA-id58 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bei Rechtsgeschäften wird der Bund - unbeschadet besonders angeordneter Beschränkungen - durch den Präsidenten der Bundesanstalt für gesamtdeutsche Aufgaben insoweit vertreten, als der Bundesanstalt nach § 27 Abs. 1 der Wirtschaftsbestimmungen für die Reichsbehörden die Befugnis zur Erteilung von Annahme- und Auszahlungsanordnungen übertragen ist. Im übrigen vertritt der Bundesminister für innerdeutsche Beziehungen den Bund. Dies gilt auch für Rechtsgeschäfte, die den Erwerb oder die Veräußerung eines Grundstückes zum Gegenstand haben."@de .

dtd:LA-id59 a dtd:LA ;
    dtd:has_SP dtd:SP-id21 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Abweichende Regelungen"@de .

dtd:LA-id60 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der Bundesminister für innerdeutsche Beziehungen kann im Einzelfall die Vertretung abweichend von dieser Anordnung regeln oder sie selbst übernehmen."@de .

dtd:LA-id61 a dtd:LA ;
    dtd:has_SP dtd:SP-id22 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Inkrafttreten"@de .

dtd:LA-id62 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Diese Anordnung tritt am Tage nach ihrer Veröffentlichung im Bundesanzeiger in Kraft."@de .


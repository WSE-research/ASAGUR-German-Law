dtd:LA-id528 a dtd:LA ;
    dtd:has_Value "wesentliche Barrieren"@de .

dtd:LA-id529 a dtd:LA ;
    dtd:has_Value "die Barrieren, auf denen der sichere Einschluss der radioaktiven Abfälle beruht;"@de .

dtd:LA-id530 a dtd:LA ;
    dtd:has_Value "weitere Barrieren"@de .

dtd:LA-id531 a dtd:LA ;
    dtd:has_Value "die Barrieren, die zusätzlich zu den wesentlichen Barrieren und im Zusammenwirken mit ihnen eine Ausbreitung von Radionukliden be- oder verhindern;"@de .

dtd:LA-id532 a dtd:LA ;
    dtd:has_Value "Bewertungszeitraum"@de .

dtd:LA-id533 a dtd:LA ;
    dtd:has_Value "der Zeitraum, für den die Langzeitsicherheit des Endlagers zu prüfen und darzustellen ist;"@de .

dtd:LA-id534 a dtd:LA ;
    dtd:has_Value "Endlagergebinde"@de .

dtd:LA-id535 a dtd:LA ;
    dtd:has_Value "die zur Endlagerung vorgesehenen Behälter mit radioaktiven Abfällen;"@de .

dtd:LA-id536 a dtd:LA ;
    dtd:has_Value "Integrität"@de .

dtd:LA-id537 a dtd:LA ;
    dtd:has_Value "der Erhalt der für den sicheren Einschluss der radioaktiven Abfälle relevanten Eigenschaften der Barrieren des Endlagersystems;"@de .

dtd:LA-id538 a dtd:LA ;
    dtd:has_Value "Langzeitsicherheit"@de .

dtd:LA-id539 a dtd:LA ;
    dtd:has_Value "der dauerhafte Schutz des Menschen und, soweit es um den langfristigen Schutz der menschlichen Gesundheit geht, der Umwelt vor der schädlichen Wirkung ionisierender Strahlung radioaktiver Abfälle;"@de .

dtd:LA-id540 a dtd:LA ;
    dtd:has_Value "Sicherheitsbericht"@de .

dtd:LA-id541 a dtd:LA ;
    dtd:has_Value "der Sicherheitsbericht nach § 3 Absatz 1 Nummer 1 der Atomrechtlichen Verfahrensverordnung;"@de .

dtd:LA-id542 a dtd:LA ;
    dtd:has_Value "Sicherheitsfunktion"@de .

dtd:LA-id543 a dtd:LA ;
    dtd:has_Value "eine Eigenschaft einer Komponente des Endlagersystems oder ein im Endlagersystem ablaufender Prozess, die oder der sicherheitsrelevante Anforderungen an ein sicherheitsbezogenes System oder Teilsystem oder an eine Einzelkomponente erfüllt;"@de .

dtd:LA-id544 a dtd:LA ;
    dtd:has_Value "Robustheit"@de .

dtd:LA-id545 a dtd:LA ;
    dtd:has_Value "die Unempfindlichkeit der Sicherheitsfunktionen des Endlagersystems und seiner Barrieren gegenüber inneren und äußeren Einflüssen und Störungen."@de .

dtd:LA-id546 a dtd:LA ;
    dtd:has_Value "zu erwartende Entwicklungen oder"@de .

dtd:LA-id547 a dtd:LA ;
    dtd:has_Value "abweichende Entwicklungen."@de .

dtd:LA-id548 a dtd:LA ;
    dtd:has_Value "ein oder mehrere einschlusswirksame Gebirgsbereiche oder"@de .

dtd:LA-id549 a dtd:LA ;
    dtd:has_Value "im Fall des Wirtsgesteins Kristallingestein, sofern kein einschlusswirksamer Gebirgsbereich ausgewiesen werden kann, für die jeweilige geologische Umgebung geeignete technische und geotechnische Barrieren."@de .

dtd:LA-id550 a dtd:LA ;
    dtd:has_SUP dtd:SUP-id222 ;
    dtd:has_Value "insgesamt höchstens ein Anteil von 10–4 und"@de .

dtd:LA-id551 a dtd:LA ;
    dtd:has_SUP dtd:SUP-id223 ;
    dtd:has_Value "jährlich höchstens ein Anteil von 10–9"@de .

dtd:LA-id552 a dtd:LA ;
    dtd:has_DL dtd:DL-id376 ;
    dtd:has_Value "die Ausbildung von Fluidwegsamkeiten, die zum Eindringen oder Austreten von erheblichen Mengen an Flüssigkeiten oder Gasen führen können, innerhalb des einschlusswirksamen Gebirgsbereichs ausgeschlossen ist; dafür dürfen a)die Dilatanzfestigkeiten der Gesteinsformationen des einschlusswirksamen Gebirgsbereichs außerhalb der auffahrungsbedingten Auflockerungszonen auf Grund von zu erwartenden Beanspruchungen nicht überschritten werden undb)die zu erwartenden Fluiddrücke die Fluiddruckbelastbarkeiten der Gesteinsformationen des einschlusswirksamen Gebirgsbereichs nicht in einer Weise überschreiten, die zu einer erheblichen Zunahme von Fluidwegsamkeiten im einschlusswirksamen Gebirgsbereich führt;"@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Value "die Dilatanzfestigkeiten der Gesteinsformationen des einschlusswirksamen Gebirgsbereichs außerhalb der auffahrungsbedingten Auflockerungszonen auf Grund von zu erwartenden Beanspruchungen nicht überschritten werden und"@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_Value "die zu erwartenden Fluiddrücke die Fluiddruckbelastbarkeiten der Gesteinsformationen des einschlusswirksamen Gebirgsbereichs nicht in einer Weise überschreiten, die zu einer erheblichen Zunahme von Fluidwegsamkeiten im einschlusswirksamen Gebirgsbereich führt;"@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_Value "durch die Temperaturentwicklung die Barrierewirkung des einschlusswirksamen Gebirgsbereichs nicht erheblich beeinträchtigt wird und"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Value "die möglichen Änderungen der chemischen Verhältnisse im Einlagerungsbereich, insbesondere auf Grund der in das Endlagerbergwerk eingebrachten Materialien, die Barrierewirkung des einschlusswirksamen Gebirgsbereichs nicht erheblich beeinträchtigen."@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Value "die im Einlagerungsbereich möglicherweise ablaufenden hydraulischen, chemischen und physikalischen Prozesse, insbesondere Korrosion und Erosion,"@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Value "im umgebenden Gebirge auftretende Spannungen, Drücke und mögliche Gebirgsbewegungen und"@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Value "die Temperaturentwicklung."@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Value "für die zu erwartenden Entwicklungen die abgeschätzte zusätzliche effektive Dosis für Einzelpersonen der Bevölkerung höchstens im Bereich von 10 Mikrosievert pro Kalenderjahr liegt und"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Value "für die abweichenden Entwicklungen die abgeschätzte zusätzliche effektive Dosis für Einzelpersonen der Bevölkerung 100 Mikrosievert pro Kalenderjahr nicht überschreitet."@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Value "0,95 im Zeitraum von 500 Jahren nach dem geplanten Verschluss des Endlagers und"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Value "0,98 während des übrigen Bewertungszeitraumes."@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Value "einen Ablaufplan für die Errichtung, den Betrieb und die Stilllegung des Endlagers, der darlegt, wie die Sicherheit des Endlagers nach § 17 sichergestellt werden kann und wie die radioaktiven Abfälle in einem sicheren Zustand gehalten werden können,"@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Value "eine Darstellung der Maßnahmen, mit denen die Rückholbarkeit der eingelagerten radioaktiven Abfälle nach § 13 bis zum Beginn der Stilllegung gewährleistet wird, und"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Value "eine Darstellung der Vorkehrungen, die zur Ermöglichung einer Bergung der eingelagerten radioaktiven Abfälle nach § 14 getroffen werden."@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Value "zur Gewährleistung des erforderlichen Schutzes des Endlagers vor Störmaßnahmen und sonstigen Einwirkungen Dritter und"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Value "zur Überwachung von Kernmaterial."@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_DL dtd:DL-id427 ;
    dtd:has_Value "die Definition der wesentlichen Barrieren nach § 4 Absatz 3 unter Berücksichtigung der Endlagergebinde, der Einlagerungstechnik und der Einlagerungsgeometrie, dabei a)im Fall des § 4 Absatz 3 Nummer 1 Lage und Abmessungen des einschlusswirksamen Gebirgsbereichs oderb)im Fall des § 4 Absatz 3 Nummer 2 Spezifikationen der wesentlichen technischen und geotechnischen Barrieren,"@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_Value "im Fall des § 4 Absatz 3 Nummer 1 Lage und Abmessungen des einschlusswirksamen Gebirgsbereichs oder"@de .

dtd:LA-id571 a dtd:LA ;
    dtd:has_Value "im Fall des § 4 Absatz 3 Nummer 2 Spezifikationen der wesentlichen technischen und geotechnischen Barrieren,"@de .

dtd:LA-id572 a dtd:LA ;
    dtd:has_Value "die Definition der weiteren Barrieren des Endlagersystems unter Berücksichtigung der Endlagergebinde, der Einlagerungstechnik und der Einlagerungsgeometrie,"@de .

dtd:LA-id573 a dtd:LA ;
    dtd:has_Value "die Positionierung und technische Ausführung aller untertägigen Hohlräume, insbesondere der für die Einlagerung von Endlagergebinden bestimmten Bereiche, sowie aller Tageszugänge,"@de .

dtd:LA-id574 a dtd:LA ;
    dtd:has_Value "die Spezifikation der Einbauten und Geräte, die der Handhabung von Endlagergebinden dienen,"@de .

dtd:LA-id575 a dtd:LA ;
    dtd:has_Value "die sicherheitstechnischen Anforderungen an die Endlagergebinde sowie die Vorgaben für die Behandlung der darin enthaltenen Abfälle nach § 3 Absatz 1 Satz 2 der Atomrechtlichen Entsorgungsverordnung vom 29. November 2018 (BGBl. I S. 2034, 2172) in der jeweils geltenden Fassung,"@de .

dtd:LA-id576 a dtd:LA ;
    dtd:has_Value "das Einlagerungskonzept, insbesondere Anordnung sowie Handhabung und Kontrolle der Endlagergebinde,"@de .

dtd:LA-id577 a dtd:LA ;
    dtd:has_Value "die Maßnahmen zur Gewährleistung der Rückholbarkeit bereits eingelagerter Endlagergebinde und"@de .

dtd:LA-id578 a dtd:LA ;
    dtd:has_Value "die Stilllegungsmaßnahmen einschließlich der Verschlussmaßnahmen."@de .

dtd:LA-id579 a dtd:LA ;
    dtd:has_Value "die Langzeitsicherheit des Endlagers, insbesondere die Qualität des sicheren Einschlusses der radioaktiven Abfälle und Robustheit des Endlagersystems, sowie"@de .

dtd:LA-id580 a dtd:LA ;
    dtd:has_Value "die Betriebssicherheit des Endlagers."@de .

dtd:LA-id581 a dtd:LA ;
    dtd:has_DL dtd:DL-id442 ;
    dtd:has_Value "für die zu erwartenden Entwicklungen des Endlagersystems die eingelagerten Endlagergebinde für einen Zeitraum von 500 Jahren nach dem vorgesehenen Verschluss des Endlagers a)individuell aufgefunden und identifiziert werden können,b)mechanisch so stabil sind, dass eine Handhabung ganzer Endlagergebinde möglich ist, undc)bei ihrer Handhabung keine Freisetzung von radioaktiven Aerosolen erwarten lassen und"@de .

dtd:LA-id582 a dtd:LA ;
    dtd:has_Value "individuell aufgefunden und identifiziert werden können,"@de .

dtd:LA-id583 a dtd:LA ;
    dtd:has_Value "mechanisch so stabil sind, dass eine Handhabung ganzer Endlagergebinde möglich ist, und"@de .

dtd:LA-id584 a dtd:LA ;
    dtd:has_Value "bei ihrer Handhabung keine Freisetzung von radioaktiven Aerosolen erwarten lassen und"@de .

dtd:LA-id585 a dtd:LA ;
    dtd:has_DL dtd:DL-id449 ;
    dtd:has_Value "eine umfassende Dokumentation angelegt wird über a)das aufgefahrene Endlagerbergwerk einschließlich seiner Stilllegung,b)sämtliche eingelagerten Endlagergebinde einschließlich ihrer jeweiligen Beladung und Position im Endlagerbergwerk undc)die zu erwartenden und die abweichenden Entwicklungen des Endlagersystems.Die Dokumentation muss an mindestens zwei räumlich und organisatorisch voneinander getrennten Stellen möglichst langfristig verfügbar und lesbar gehalten werden."@de .

dtd:LA-id586 a dtd:LA ;
    dtd:has_Value "das aufgefahrene Endlagerbergwerk einschließlich seiner Stilllegung,"@de .

dtd:LA-id587 a dtd:LA ;
    dtd:has_Value "sämtliche eingelagerten Endlagergebinde einschließlich ihrer jeweiligen Beladung und Position im Endlagerbergwerk und"@de .

dtd:LA-id588 a dtd:LA ;
    dtd:has_Value "die zu erwartenden und die abweichenden Entwicklungen des Endlagersystems."@de .

dtd:LA-id589 a dtd:LA ;
    dtd:has_Value "die Errichtung der übertägigen Betriebs- und Infrastrukturgebäude sowie die Errichtung der Einrichtungen zur zeitweiligen Lagerung und Handhabung von Endlagergebinden,"@de .

dtd:LA-id590 a dtd:LA ;
    dtd:has_Value "die Errichtung der Zugangs- und Bewetterungsbauwerke,"@de .

dtd:LA-id591 a dtd:LA ;
    dtd:has_Value "das Auffahren der untertägigen Infrastrukturbereiche und das Auffahren der Ansatzpunkte für Zugangsstrecken zu den Bereichen des Endlagerbergwerks, die für die Einlagerung von radioaktiven Abfällen vorgesehen sind,"@de .

dtd:LA-id592 a dtd:LA ;
    dtd:has_Value "die Installation aller technischen Einrichtungen, die für die untertägige Handhabung und Einlagerung von Endlagergebinden erforderlich sind, und"@de .

dtd:LA-id593 a dtd:LA ;
    dtd:has_Value "die Bereitstellung aller technischen Einrichtungen, die für eine mögliche Rückholung von eingelagerten Endlagergebinden erforderlich sind."@de .

dtd:LA-id594 a dtd:LA ;
    dtd:has_Value "muss die Errichtung des Endlagers nach § 15 abgeschlossen sein,"@de .

dtd:LA-id595 a dtd:LA ;
    dtd:has_Value "muss die Handhabung und Einlagerung von Endlagergebinden ohne radioaktive Beladung durchgeführt werden,"@de .

dtd:LA-id596 a dtd:LA ;
    dtd:has_Value "muss die Funktionsfähigkeit aller technischen Einrichtungen, die für eine mögliche Rückholung von eingelagerten Endlagergebinden erforderlich sind, sichergestellt werden und"@de .

dtd:LA-id597 a dtd:LA ;
    dtd:has_Value "müssen die vorgesehenen technischen Stilllegungs- und Verschlussmaßnahmen nach § 11 Absatz 1 Satz 2 Nummer 8 verfügbar sein."@de .

dtd:LA-id598 a dtd:LA ;
    dtd:has_Value "Normalbetrieb,"@de .

dtd:LA-id599 a dtd:LA ;
    dtd:has_Value "anomaler Betrieb,"@de .

dtd:LA-id600 a dtd:LA ;
    dtd:has_Value "Auslegungsstörfälle oder"@de .

dtd:LA-id601 a dtd:LA ;
    dtd:has_Value "auslegungsüberschreitende Unfälle und Ereignisse."@de .

dtd:LA-id602 a dtd:LA ;
    dtd:has_Value "für den Normalbetrieb Maßnahmen vorzusehen, die den bestimmungsgemäßen Betrieb des Endlagers gewährleisten und das Eintreten anderer Anlagenzustände vermeiden,"@de .

dtd:LA-id603 a dtd:LA ;
    dtd:has_Value "für den anomalen Betrieb Maßnahmen vorzusehen, die das Eintreten von Störfällen verhindern und das Endlager in den Normalbetrieb zurückführen,"@de .

dtd:LA-id604 a dtd:LA ;
    dtd:has_Value "für Auslegungsstörfälle Maßnahmen entsprechend § 104 Absatz 1 Satz 1 und 2 der Strahlenschutzverordnung vom 29. November 2018 (BGBl. I S. 2034, 2036), die durch Artikel 1 der Verordnung vom 27. März 2020 (BGBl. I S. 748) geändert worden ist, in der jeweils geltenden Fassung vorzusehen, die den Störfall beherrschen und das Endlager in einen sichereren Anlagenzustand zurückführen,"@de .

dtd:LA-id605 a dtd:LA ;
    dtd:has_Value "für auslegungsüberschreitende Unfälle und Ereignisse Maßnahmen vorzusehen, die die Auswirkungen des Ereignisses auf die Umgebung soweit wie möglich begrenzen."@de .

dtd:LA-id606 a dtd:LA ;
    dtd:has_Value "darf die Robustheit des Endlagersystems für hochradioaktive Abfälle für zu erwartende Entwicklungen nicht beeinträchtigt werden und"@de .

dtd:LA-id607 a dtd:LA ;
    dtd:has_Value "dürfen sich mögliche Austragungen von Radionukliden nach § 4 Absatz 5 und 6 für die zu erwartenden und die abweichenden Entwicklungen nicht erhöhen."@de .

dtd:LA-id608 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der element- und isotopenweisen Zusammensetzungen,"@de .

dtd:LA-id609 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der physikalischen und chemischen Beschaffenheiten der hochradioaktiven Abfälle und"@de .

dtd:LA-id610 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der in Teil B genannten reaktivitätsrelevanten Gebindeparameter"@de .

dtd:LA-id611 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Menge der Spaltstoffe sowie ihre element- und isotopenweise Zusammensetzung,"@de .

dtd:LA-id612 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Neutronen moderierende oder reflektierende Stoffe in den Spaltstoffanordnungen, zwischen den Spaltstoffanordnungen und um die Spaltstoffanordnungen,"@de .

dtd:LA-id613 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die geometrische Anordnung aller beteiligten Materialien und"@de .

dtd:LA-id614 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Temperatur der Anordnung."@de .

dtd:LA-id615 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ungewissheiten bei den eingesetzten Wirkungsquerschnitten,"@de .

dtd:LA-id616 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ungewissheiten hinsichtlich der Spaltstoffmenge,"@de .

dtd:LA-id617 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ungewissheiten hinsichtlich der element- und isotopenweisen Zusammensetzung der Spaltstoffe,"@de .

dtd:LA-id618 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ungewissheiten hinsichtlich der Art, Zusammensetzung und Konzentration der übrigen Materialien, insbesondere der Neutronen moderierenden, reflektierenden oder absorbierenden Stoffe,"@de .

dtd:LA-id619 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auswirkungen von Ungewissheiten in der Berechnung der für den Ausschluss von Kritikalität wesentlichen geologischen, geophysikalischen und geochemischen Prozesse im Endlagersystem und"@de .

dtd:LA-id620 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bekannte systematische Abweichungen in den verwendeten Berechnungsprogrammen."@de .


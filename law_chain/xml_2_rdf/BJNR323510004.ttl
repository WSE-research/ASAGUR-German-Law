dtd:LA-id189 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Empfehlung zur Höhe der Abführung,"@de .

dtd:LA-id190 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Entlastung des Vorstands und"@de .

dtd:LA-id191 a dtd:LA ;

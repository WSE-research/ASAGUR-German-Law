    dtd:has_Value "die Verwendung des Bilanzgewinns."@de .

dtd:LA-id192 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der oder dem Vorsitzenden, die oder der vom Bundesministerium der Finanzen entsandt wird,"@de .


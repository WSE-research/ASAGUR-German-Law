dtd:LA-id635 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Soldatinnen und Soldaten,"@de .

dtd:LA-id636 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beamtinnen und Beamte des Bundes,"@de .

dtd:LA-id637 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Richterinnen und Richter des Bundes,"@de .

dtd:LA-id638 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arbeitnehmerinnen und Arbeitnehmer des Bundes, mit Ausnahme der bei deutschen Dienststellen im Ausland eingestellten Ortskräfte, sowie"@de .

dtd:LA-id639 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Helferinnen und Helfer des Technischen Hilfswerks nach § 2 Absatz 1 des THW-Gesetzes,"@de .

dtd:LA-id640 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Hilfen zur Erhaltung oder Erlangung eines Arbeitsplatzes einschließlich der Beratung und Vermittlung,"@de .

dtd:LA-id641 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Berufsvorbereitung einschließlich einer erforderlichen Grundausbildung,"@de .

dtd:LA-id642 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die berufliche Anpassung und Weiterbildung, auch soweit sie einen zur Teilnahme erforderlichen schulischen Abschluss einschließen,"@de .

dtd:LA-id643 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die berufliche Ausbildung, auch soweit sie schulisch durchgeführt wird, und"@de .

dtd:LA-id644 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Schulausbildung, wenn der in Aussicht genommene Beruf dies erfordert."@de .

dtd:LA-id645 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "medizinische Leistungen zur Behandlung der gesundheitlichen Schädigung oder"@de .

dtd:LA-id646 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leistungen zur beruflichen Qualifizierung nach § 3 oder anderen Gesetzen"@de .

dtd:LA-id647 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einsatzgeschädigte nach § 1 Nummer 1 bis 3 oder 5, die in einem öffentlich-rechtlichen Dienstverhältnis zum Bund stehen, wegen durch den Einsatzunfall bedingter Dienstunfähigkeit nur auf eigenen Antrag in den Ruhestand versetzt oder entlassen werden, wobei § 34 Absatz 1 Satz 1 Nummer 2 des Bundesbeamtengesetzes bei mangelnder Bewährung wegen allein fehlender gesundheitlicher Eignung, die auf dem Einsatzunfall beruht, nicht anzuwenden ist, und"@de .

dtd:LA-id648 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Arbeitsverhältnisse von Einsatzgeschädigten nach § 1 Nummer 4 oder 5 mit dem Bund nicht wegen der durch den Einsatzunfall bedingten Arbeitsunfähigkeit gekündigt werden."@de .

dtd:LA-id649 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erreicht sind oder"@de .

dtd:LA-id650 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "voraussichtlich nicht mehr erreicht werden können."@de .

dtd:LA-id651 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch eine Berufung in das Dienstverhältnis einer Berufssoldatin oder eines Berufssoldaten nach § 7 Absatz 1,"@de .

dtd:LA-id652 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch eine Berufung in das Dienstverhältnis einer Beamtin oder eines Beamten nach § 8 Absatz 1 Satz 1 Nummer 1,"@de .

dtd:LA-id653 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch eine Einstellung als Arbeitnehmerin oder Arbeitnehmer nach § 8 Absatz 1 Satz 1 Nummer 2 mit dem Beginn des Arbeitsverhältnisses oder"@de .

dtd:LA-id654 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Ablauf des Monats, in dem die oder der Einsatzgeschädigte das 65. Lebensjahr vollendet."@de .

dtd:LA-id655 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Ende der Schutzzeit, wenn kein Antrag auf Weiterverwendung nach § 7 gestellt wird,"@de .

dtd:LA-id656 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Nichtbewährung in der Probezeit nach § 7 Absatz 1 Satz 1 oder"@de .

dtd:LA-id657 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Entlassung auf entsprechenden schriftlichen Antrag der Soldatin oder des Soldaten."@de .

dtd:LA-id658 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die gesundheitliche Schädigung nicht ausschlaggebend für die Nichteingliederung in das Arbeitsleben ist,"@de .

dtd:LA-id659 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die gesundheitliche Schädigung eine bereits erfolgte Eingliederung in das Arbeitsleben nicht behindert,"@de .

dtd:LA-id660 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einstellung nicht das Erreichen eines der Ziele des § 4 Absatz 1 erwarten lässt,"@de .

dtd:LA-id661 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einsatzversorgung nach § 63f des Soldatenversorgungsgesetzes gewährt worden ist oder"@de .

dtd:LA-id662 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Einstellung nach Satz 1 bereits zu einem inzwischen wieder beendeten Wehrdienstverhältnis geführt hat."@de .

dtd:LA-id663 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus einem früheren öffentlich-rechtlichen Dienstverhältnis bereits in den Ruhestand getreten waren oder versetzt worden waren oder"@de .

dtd:LA-id664 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für sie jeweils festgesetzte soldatische Altersgrenze erreicht oder überschritten haben."@de .

dtd:LA-id665 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in ein Beamtenverhältnis auf Probe mit einer Probezeit von sechs Monaten zu berufen, sofern sie nicht wegen ihres körperlichen Zustandes oder aus gesundheitlichen Gründen zur Erfüllung der mit dem neuen Amt verbundenen Dienstpflichten dauernd unfähig sind, oder"@de .

dtd:LA-id666 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Arbeitnehmerin oder Arbeitnehmer mit einer Probezeit von sechs Monaten einzustellen, wenn sie in Bezug auf die künftige Tätigkeit arbeitsfähig sind und keine Beeinträchtigung entsprechend Nummer 1 vorliegt."@de .

dtd:LA-id667 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wer aus einem Dienstverhältnis als Soldatin auf Zeit oder Soldat auf Zeit nach § 6 in ein Wehrdienstverhältnis besonderer Art eintritt, erhält die Leistungen der Berufsförderung und Dienstzeitversorgung nach Abschnitt I des Zweiten Teils des Soldatenversorgungsgesetzes, die bei Beendigung des Wehrdienstverhältnisses durch Zeitablauf zustehen, erst, wenn auch das Wehrdienstverhältnis besonderer Art endet."@de .

dtd:LA-id668 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wer nach § 6 in ein Wehrdienstverhältnis besonderer Art eintritt, erwirbt dadurch keine Ansprüche auf Berufsförderung und Dienstzeitversorgung. Zeiten in einem Wehrdienstverhältnis besonderer Art führen nicht zur Eingliederungsberechtigung nach den §§ 9 und 10 des Soldatenversorgungsgesetzes."@de .

dtd:LA-id669 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Durch ein Wehrdienstverhältnis besonderer Art nach § 6 ist der Anspruch auf Freistellung vom militärischen Dienst nach § 5 Absatz 5 des Soldatenversorgungsgesetzes in der bis einschließlich 25. Juli 2012 geltenden Fassung unabhängig von seiner Dauer abgegolten und die Gesamtförderungsdauer nach § 5 Absatz 4 des Soldatenversorgungsgesetzes entsprechend herabgesetzt."@de .

dtd:LA-id670 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Endet die Schutzzeit nach § 4 bei einer Soldatin auf Zeit oder einem Soldaten auf Zeit vor Ablauf der Zeit, für die sie oder er in das Dienstverhältnis berufen ist, und wurden während der Schutzzeit berufliche Qualifikationen im Sinne des § 5 Absatz 6 bis 9 des Soldatenversorgungsgesetzes erworben, vermindern sich der Anspruch auf Freistellung vom militärischen Dienst nach § 5 Absatz 5 des Soldatenversorgungsgesetzes in der bis einschließlich 25. Juli 2012 geltenden Fassung und die Gesamtförderungsdauer nach § 5 Absatz 4 des Soldatenversorgungsgesetzes entsprechend."@de .

dtd:LA-id671 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 42 des Soldatenversorgungsgesetzes ist auf die Hinterbliebenen der Einsatzgeschädigten nach § 1 Nummer 1, die während der Schutzzeit nach § 4 verstorben sind, unbeachtlich einer Wehrdienstzeit von mindestens sechs Jahren und hinsichtlich der Dauer unbeachtlich des Anspruchs auf Übergangsgebührnisse entsprechend anzuwenden."@de .

dtd:LA-id672 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 62 Absatz 1 des Soldatenversorgungsgesetzes gilt entsprechend für Einsatzgeschädigte nach § 1 Nummer 1, deren Wehrdienstverhältnis besonderer Art anders als durch eine Berufung nach § 7 Absatz 1 oder § 8 Absatz 1 Satz 1 Nummer 1 oder durch eine Einstellung nach § 8 Absatz 1 Satz 1 Nummer 2 geendet hat, und für die Hinterbliebenen der Einsatzgeschädigten nach § 1 Nummer 1, die während des Wehrdienstverhältnisses besonderer Art verstorben sind."@de .

dtd:LA-id673 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die gesundheitliche Schädigung nicht ausschlaggebend für die Nichteingliederung in das Arbeitsleben ist,"@de .

dtd:LA-id674 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die gesundheitliche Schädigung eine bereits erfolgte Eingliederung in das Arbeitsleben nicht behindert,"@de .

dtd:LA-id675 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einstellung nicht das Erreichen eines der Ziele des § 4 Absatz 1 Satz 1 erwarten lässt,"@de .

dtd:LA-id676 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einsatzversorgung nach § 37 Absatz 3 des Beamtenversorgungsgesetzes oder nach § 63f des Soldatenversorgungsgesetzes gewährt wird oder wurde oder"@de .

dtd:LA-id677 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Einstellung nach Satz 1 bereits zu einem inzwischen wieder beendeten Beamtenverhältnis geführt hat."@de .

dtd:LA-id678 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch eine Berufung in ein Beamtenverhältnis auf Probe nach § 11 Absatz 3 Satz 1,"@de .

dtd:LA-id679 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch eine Einstellung als Arbeitnehmerin oder Arbeitnehmer nach § 11 Absatz 3 Satz 6 mit dem Beginn des Arbeitsverhältnisses oder"@de .

dtd:LA-id680 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Ablauf des Monats, in dem die oder der Einsatzgeschädigte das 65. Lebensjahr vollendet."@de .

dtd:LA-id681 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kein Antrag auf Weiterverwendung nach § 11 Absatz 3 gestellt wird, mit dem Ende der Schutzzeit,"@de .

dtd:LA-id682 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beamtin oder der Beamte schriftlich die Entlassung aus dem Beamtenverhältnis verlangt oder"@de .

dtd:LA-id683 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Fall des § 34 Absatz 1 Satz 1 Nummer 1 des Bundesbeamtengesetzes vorliegt."@de .

dtd:LA-id684 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die gesundheitliche Schädigung nicht ausschlaggebend für die Nichteingliederung in das Arbeitsleben ist,"@de .

dtd:LA-id685 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die gesundheitliche Schädigung eine bereits erfolgte Eingliederung in das Arbeitsleben nicht behindert,"@de .

dtd:LA-id686 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einstellung nicht das Erreichen eines der Ziele des § 4 Absatz 1 erwarten lässt,"@de .

dtd:LA-id687 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einsatzversorgung nach § 63f des Soldatenversorgungsgesetzes gewährt worden ist oder die altersmäßigen Voraussetzungen des Bezugs einer ungekürzten Vollrente wegen Alters nach dem Sechsten Buch Sozialgesetzbuch erfüllt sind oder"@de .

dtd:LA-id688 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Einstellung nach Satz 1 bereits zu einem inzwischen wieder beendeten Arbeitsverhältnis geführt hat."@de .

dtd:LA-id689 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in ein Beamtenverhältnis auf Probe mit einer Probezeit von sechs Monaten zu berufen, sofern sie nicht wegen ihres körperlichen Zustandes oder aus gesundheitlichen Gründen zur Erfüllung der mit dem neuen Amt verbundenen Dienstpflichten dauernd unfähig sind, oder"@de .

dtd:LA-id690 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Arbeitnehmerin oder Arbeitnehmer mit einer Probezeit von sechs Monaten einzustellen, wenn sie in Bezug auf die künftige Tätigkeit arbeitsfähig sind und keine Beeinträchtigung entsprechend Nummer 1 vorliegt."@de .

dtd:LA-id691 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Verletztengeldes nach § 47 des Siebten Buches Sozialgesetzbuch, wenn ihr Dienstherr oder Arbeitgeber das Dienst- oder Arbeitsverhältnis aufgrund des Einsatzunfalls beendet,"@de .

dtd:LA-id692 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Verdienstausfalls nach § 3 Absatz 3 Satz 2 und 3 des THW-Gesetzes, der ihnen als beruflich selbstständigen Helferinnen oder Helfern infolge des Einsatzunfalls entsteht, oder"@de .

dtd:LA-id693 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der nach § 3 Absatz 4 des THW-Gesetzes fortzugewährenden Leistungen, soweit ihnen Leistungen der Bundesagentur für Arbeit, Sozialhilfe sowie sonstige Unterstützungen oder Bezüge aus öffentlichen Mitteln infolge des Einsatzunfalls nicht fortgewährt werden."@de .

dtd:LA-id694 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fahrtkosten für die Hin- und Rückreise in entsprechender Anwendung der §§ 4 und 5 des Bundesreisekostengesetzes,"@de .

dtd:LA-id695 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unterbringungskosten einschließlich Kurtaxe,"@de .


dtd:LA-id148 a dtd:LA ;
    dtd:has_Value "aus dem Ausschuss für Menschenrechte und humanitäre Hilfe des Deutschen Bundestages zwei Mitglieder,"@de .

dtd:LA-id149 a dtd:LA ;
    dtd:has_Value "drei vom Bundestag zu benennende Vertreter oder Vertreterinnen wissenschaftlicher Einrichtungen mit menschenrechtlichem Bezug,"@de .

dtd:LA-id150 a dtd:LA ;
    dtd:has_Value "drei vom Bundestag zu benennende Mitglieder der Zivilgesellschaft,"@de .

dtd:LA-id151 a dtd:LA ;
    dtd:has_Value "vom Forum Menschenrechte drei Vertreter oder Vertreterinnen."@de .

dtd:LA-id152 a dtd:LA ;
    dtd:has_Value "von der Beauftragten der Bundesregierung für Migration, Flüchtlinge und Integration,"@de .

dtd:LA-id153 a dtd:LA ;
    dtd:has_Value "von dem Beauftragten der Bundesregierung für Menschenrechte und Humanitäre Hilfe,"@de .

dtd:LA-id154 a dtd:LA ;
    dtd:has_Value "von der Beauftragten der Bundesregierung für Menschenrechtsfragen,"@de .

dtd:LA-id155 a dtd:LA ;
    dtd:has_Value "von der Beauftragten der Bundesregierung für Aussiedlerfragen und nationale Minderheiten,"@de .

dtd:LA-id156 a dtd:LA ;
    dtd:has_Value "von der Beauftragten der Bundesregierung für die Belange behinderter Menschen,"@de .

dtd:LA-id157 a dtd:LA ;
    dtd:has_Value "des Bundesministeriums für wirtschaftliche Zusammenarbeit und Entwicklung,"@de .

dtd:LA-id158 a dtd:LA ;
    dtd:has_Value "des Bundesministeriums für Familie, Senioren, Frauen und Jugend,"@de .

dtd:LA-id159 a dtd:LA ;
    dtd:has_Value "des Bundesministeriums der Verteidigung,"@de .


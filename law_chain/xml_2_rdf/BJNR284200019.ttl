dtd:LA-id556 a dtd:LA ;
    dtd:has_Value "Maßnahmen, insbesondere zur Unfallverhütung, zum Arbeitsschutz, zur Gefahrgutlagerung und zur Ressourceneffizienz sowie zum Umweltschutz planen und begründen,"@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Value "Instandhaltung von Werkzeugen und Maschinen planen sowie"@de .


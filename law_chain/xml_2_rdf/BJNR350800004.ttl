dtd:LA-id569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "\"Absperrende Grundbeschichtungsstoffe\" sind Beschichtungsstoffe mit Versiegelungs- oder absperrenden Eigenschaften zur Anwendung auf Holz, Wänden oder Decken."@de .


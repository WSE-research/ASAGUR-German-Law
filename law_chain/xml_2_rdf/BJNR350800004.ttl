dtd:LA-id530 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gebäude:"@de .

dtd:LA-id531 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "selbständig benutzbare, überdeckte bauliche Anlagen, die von Menschen betreten werden können und geeignet oder bestimmt sind, dem Schutz von Menschen, Tieren oder Sachen zu dienen;"@de .

dtd:LA-id532 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bauteile:"@de .

dtd:LA-id533 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hierzu zählen u. a. Fertigteile, Fenster, Türen, Zargen, Fußböden und Treppen, nicht hingegen Möbel;"@de .

dtd:LA-id534 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dekorative Bauelemente:"@de .

dtd:LA-id535 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Stuck, Vertäfelungen, nichttragende dekorative Säulen und andere Bauelemente, die der Dekoration von Gebäuden dienen;"@de .

dtd:LA-id536 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beschichtungsstoffe:"@de .

dtd:LA-id537 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gemische, die dazu verwendet werden, auf einer Oberfläche einen Film mit dekorativer, schützender oder sonstiger funktionaler Wirkung zu erzielen; in einem Gemisch enthaltene organische Lösemittel sowie vor Gebrauch zuzugebende organische Lösemittel sind Teil der Beschichtungsstoffe;"@de .

dtd:LA-id538 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beschichtungsstoffe auf Lösemittelbasis (Lb):"@de .

dtd:LA-id539 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beschichtungsstoffe, deren Viskosität mit Hilfe von Lösemitteln eingestellt wird;"@de .

dtd:LA-id540 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beschichtungsstoffe auf Wasserbasis (Wb):"@de .

dtd:LA-id541 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beschichtungsstoffe, deren Viskosität mit Hilfe von Wasser eingestellt wird;"@de .

dtd:LA-id542 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Farben und Lacke:"@de .

dtd:LA-id543 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die in Anhang I Ziffer 1 aufgeführten gebrauchsfertigen Produkte, mit Ausnahme von Aerosolen, zur Beschichtung von Gebäuden, ihren Bauteilen und dekorativen Bauelementen zu dekorativen, schützenden oder sonstigen funktionalen Zwecken;"@de .

dtd:LA-id544 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Film:"@de .

dtd:LA-id545 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine zusammenhängende Beschichtung, die durch die Aufbringung einer oder mehrerer Schichten auf ein Substrat entsteht;"@de .

dtd:LA-id546 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "flüchtige organische Verbindung (VOC):"@de .

dtd:LA-id547 a dtd:LA ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id591 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine organische Verbindung mit einem Anfangssiedepunkt von höchstens 250 Grad C bei einem Standarddruck von 101,3 kPa.;"@de .

dtd:LA-id548 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "organisches Lösemittel:"@de .

dtd:LA-id549 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine flüchtige organische Verbindung, die allein oder in Verbindung mit anderen Stoffen zur Auflösung oder Verdünnung von Rohstoffen, Stoffen und Gemischen oder Abfallstoffen, als Reinigungsmittel zur Auflösung von Verschmutzungen, als Dispersionsmittel, als Mittel zur Einstellung der Viskosität oder der Oberflächenspannung oder als Weichmacher oder Konservierungsstoff verwendet wird;"@de .

dtd:LA-id550 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "organische Verbindung:"@de .

dtd:LA-id551 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Verbindung, die zumindest das Element Kohlenstoff und eines oder mehrere der Elemente Wasserstoff, Halogene, Sauerstoff, Schwefel, Phosphor, Silizium oder Stickstoff enthält, ausgenommen Kohlenstoffoxide sowie anorganische Karbonate und Bikarbonate;"@de .

dtd:LA-id552 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Produkte für die Fahrzeugreparaturlackierung:"@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die in Anhang I Ziffer 2 aufgeführten Produkte zur Behandlung von Kraftfahrzeugen im Sinne der Richtlinie 70/156/EWG oder eines Teils dieser Kraftfahrzeuge im Zuge einer Reparatur, Konservierung oder Verschönerung außerhalb der Fertigungsanlagen;"@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "VOC-Gehalt:"@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:LA-id286 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leistungen abnehmen und dokumentieren sowie Nachkalkulation durchführen."@de .

dtd:LA-id287 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Meisterprüfungsprojekt und ein darauf bezogenes Fachgespräch,"@de .

dtd:LA-id288 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Situationsaufgabe."@de .

dtd:LA-id289 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Es sind ein Ensemble aus mindestens fünf unterschiedlichen Gefäßen, das sowohl Flachzeug als auch Deckellösungen enthält, ein Gefäß mit einer Mindesthöhe von 45 Zentimetern und einem Mindestdurchmesser von 30 Zentimetern sowie eine Schale mit einem Mindestdurchmesser von 50 Zentimetern frei gedreht herzustellen oder"@de .

dtd:LA-id290 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "es sind ein Ensemble aus mindestens fünf unterschiedlichen baukeramischen Teilen und eine körperhafte Baukeramik oder ein dreidimensionales, mehrteiliges Keramikgroßprojekt herzustellen. Bei der körperhaften Baukeramik sowie bei dem Keramikgroßprojekt müssen die Längen-, Breiten- und Höhenmaße in der Summe mindestens 130 Zentimeter ergeben, wobei eine Seite eine Mindestlänge von 60 Zentimeter aufweisen muss und keine Seitenlänge weniger als 10 Zentimeter betragen darf, oder"@de .

dtd:LA-id291 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "es sind die Oberflächen eines Ensembles aus sieben unterschiedlichen, sowohl flächigen als auch kubischen vorgefertigten Teilen zu gestalten. Zusätzlich sind entweder ein vorgefertigtes bauchiges Gefäß mit einer Mindesthöhe von 45 Zentimetern und einem Mindestdurchmesser von 30 Zentimetern sowie eine vorgefertigte Schale mit einem Mindestdurchmesser von 50 Zentimetern oder eine vorgefertigte Baukeramik mit zwei Seitenlängen von jeweils mindestens 60 Zentimetern zu gestalten."@de .

dtd:LA-id292 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zwei baukeramische Elemente nach Zeichnung zuschneiden, zusammenbauen, verstegen und verputzen,"@de .

dtd:LA-id293 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Serie von drei Gefäßen mit einer Mindesthöhe von 25 Zentimetern oder Tellern oder Schalen mit einem Mindestdurchmesser von 30 Zentimetern nach vorgegebenem Muster frei drehen,"@de .

dtd:LA-id294 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen mitgebrachten Teller mit einem Durchmesser von 40 Zentimetern nach Vorlage in Pinselmalerei dekorieren,"@de .

dtd:LA-id295 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Teller oder eine Schale mit einem Mindestdurchmesser von 40 Zentimetern oder ein bauchiges Gefäß, in frei gewählter Form, mit einer Mindesthöhe von 40 Zentimetern frei drehen,"@de .

dtd:LA-id296 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Serie von drei Deckelgefäßen mit jeweils mindestens einem Liter Volumen frei drehen,"@de .

dtd:LA-id297 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Zylinder mit einer Mindesthöhe von 40 Zentimetern oder eine Kugel mit einem Mindestdurchmesser von 30 Zentimetern frei drehen,"@de .

dtd:LA-id298 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine quadratische Platte mit einer Kantenlänge von 50 Zentimetern anfertigen und verstegen,"@de .

dtd:LA-id299 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Grundlage für einen Gipsabdruck eine quadratische Platte mit einer Kantenlänge von 50 Zentimetern anfertigen und nach Themenvorgabe plastisch dekorieren,"@de .

dtd:LA-id300 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Lüftungsgitter oder ein Verzierteil mit ausgeschnittenem, durchbrochenem Motiv anfertigen,"@de .

dtd:LA-id301 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zwei unterschiedliche Profilteile herstellen und montieren,"@de .

dtd:LA-id302 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen frei gewählten Körper mit einer Mindesthöhe von 40 Zentimetern in Überschlagstechnik aufbauen,"@de .

dtd:LA-id303 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein mitgebrachtes, lederhart getrocknetes oder geschrühtes bauchiges Gefäß mit einer Mindesthöhe von 40 Zentimetern in einer frei gewählten Dekortechnik dekorieren,"@de .

dtd:LA-id304 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Dekor als Rapport für ein Gefäß eigener Wahl entwickeln und ausführen,"@de .

dtd:LA-id305 a dtd:LA ;

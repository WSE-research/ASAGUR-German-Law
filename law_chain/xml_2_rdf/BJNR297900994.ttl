dtd:LA-id2078 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verwertung aus anderen Gründen nicht möglich ist."@de .

dtd:LA-id2079 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "infolge einer rechtmäßigen Inanspruchnahme nach § 20 Abs. 1 oder"@de .

dtd:LA-id2080 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch eine Maßnahme auf Grund des § 62 Abs. 1"@de .

dtd:LA-id2081 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "infolge einer rechtswidrigen Maßnahme oder"@de .

dtd:LA-id2082 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als unbeteiligter Dritter"@de .

dtd:LA-id2083 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die mit Zustimmung der zuständigen Behörde freiwillig bei der Erfüllung von Aufgaben der Bundespolizei mitgewirkt oder Sachen zur Verfügung gestellt haben,"@de .

dtd:LA-id2084 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nach § 63 Abs. 2 zu Hilfspolizeibeamten bestellt worden sind"@de .

dtd:LA-id2085 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundstücke mit Ausnahme von Gebäuden betreten und befahren,"@de .

dtd:LA-id2086 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "verlangen, daß Grundstückseigentümer und -besitzer einen Grenzpfad freilassen, an Einfriedungen Durchlässe oder Übergänge einrichten, Wassergräben überbrücken oder das Anbringen von Grenzmarkierungen oder Warnhinweisen dulden,"@de .

dtd:LA-id2087 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf eigene Kosten Grenzmarkierungen oder Warnhinweise setzen, Grenzpfade, Durchlässe, Übergänge oder Brücken einrichten oder verbessern."@de .

dtd:LA-id2088 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den mit der Wahrnehmung dieser Aufgaben betrauten Beamten den Zutritt zu ihren Anlagen und Beförderungsmitteln unentgeltlich zu gestatten,"@de .

dtd:LA-id2089 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie bei der Wahrnehmung dieser Aufgaben unentgeltlich zu befördern,"@de .

dtd:LA-id2090 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den für die Wahrnehmung dieser Aufgaben zuständigen Dienststellen Fahr- und Flugpläne sowie die tatsächlichen Verkehrsbewegungen rechtzeitig und unentgeltlich mitzuteilen."@de .

dtd:LA-id2091 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Überwachung der Grenzen und bei der Kontrolle des grenzüberschreitenden Verkehrs (§ 2 Abs. 2 Nr. 1 und 2),"@de .

dtd:LA-id2092 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Abwehr von Gefahren auf dem Gebiet der Bahnanlagen der Eisenbahnen des Bundes (§ 3),"@de .

dtd:LA-id2093 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Schutz vor Angriffen auf die Sicherheit des Luftverkehrs (§ 4) oder"@de .

dtd:LA-id2094 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Schutz von Verfassungsorganen des Bundes und Bundesministerien (§ 5) sowie zur Sicherung von Einrichtungen der Bundespolizei (§ 1 Abs. 3)"@de .

dtd:LA-id2095 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Anforderung oder mit Zustimmung der zuständigen Bundespolizeibehörde,"@de .

dtd:LA-id2096 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Abwehr einer gegenwärtigen Gefahr, zur Verfolgung von Straftaten im Sinne des § 12 Abs. 1 auf frischer Tat sowie zur Verfolgung und Wiederergreifung von aus dem Gewahrsam der Bundespolizei Entwichenen, wenn die zuständige Bundespolizeibehörde die erforderlichen Maßnahmen nicht rechtzeitig treffen kann."@de .

dtd:LA-id2097 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die polizeiliche Kontrolle des grenzüberschreitenden Verkehrs (§ 2 Abs. 2 Nr. 2) an einzelnen Grenzübergangsstellen,"@de .

dtd:LA-id2098 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstige Aufgaben nach § 2."@de .


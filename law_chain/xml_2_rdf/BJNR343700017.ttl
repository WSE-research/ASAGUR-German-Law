dtd:LA-id1485 a dtd:LA ;
    dtd:has_NB dtd:NB-id740 ;
    dtd:has_Value "des § 12 der Patentanwaltsordnung, der zuletzt durch Artikel 4 Nummer 7 des Gesetzes vom 12. Mai 2017 (BGBl. I S. 1121) geändert worden ist, das Bundesministerium der Justiz und für Verbraucherschutz im Einvernehmen mit dem Bundesministerium der Finanzen und"@de .

dtd:LA-id1486 a dtd:LA ;
    dtd:has_Value "des § 10 des Gesetzes über die Tätigkeit europäischer Patentanwälte in Deutschland vom 12. Mai 2017 (BGBl. I S. 1121, 1137) das Bundesministerium der Justiz und für Verbraucherschutz:"@de .

dtd:LA-id1487 a dtd:LA ;
    dtd:has_Value "die Geburtsurkunde im Original oder in amtlich beglaubigter Kopie,"@de .

dtd:LA-id1488 a dtd:LA ;
    dtd:has_Value "ein tabellarischer Lebenslauf,"@de .

dtd:LA-id1489 a dtd:LA ;
    dtd:has_Value "ein aktuelles Lichtbild,"@de .

dtd:LA-id1490 a dtd:LA ;
    dtd:has_Value "eine amtlich beglaubigte Kopie des Personalausweises oder Reisepasses sowie gegebenenfalls eine amtlich beglaubigte Kopie des Aufenthaltstitels,"@de .

dtd:LA-id1491 a dtd:LA ;
    dtd:has_Value "Zeugnisse über die staatlichen oder akademischen Studienabschlussprüfungen sowie Urkunden über die erlangten Hochschulgrade, jeweils im Original oder in amtlich beglaubigter Kopie,"@de .

dtd:LA-id1492 a dtd:LA ;
    dtd:has_Value "eine Bescheinigung über ein Jahr praktischer technischer Tätigkeit oder, falls gemäß § 6 Absatz 1 Satz 2 der Patentanwaltsordnung Befreiung hiervon beantragt wird, Nachweise darüber, auf welche andere Weise die praktische technische Erfahrung erworben wurde,"@de .

dtd:LA-id1493 a dtd:LA ;
    dtd:has_Value "die schriftlich gegenüber dem Deutschen Patent- und Markenamt abgegebene Erklärung eines Patentanwalts, dass und ab wann die Bereitschaft besteht, die Ausbildung in der eigenen Kanzlei (§ 26 Absatz 1 und 2, § 41d Absatz 4 der Patentanwaltsordnung) zu übernehmen, oder die entsprechende schriftliche Erklärung eines Unternehmens, dass und ab wann die Bereitschaft besteht, die Ausbildung in der Patentabteilung des Unternehmens durch einen Patentassessor (§ 11 Absatz 1 der Patentanwaltsordnung), der in einem ständigen Dienstverhältnis beschäftigt ist, zu übernehmen (Ausbildungserklärung), und"@de .

dtd:LA-id1494 a dtd:LA ;
    dtd:has_Value "im Fall der Ausbildung in der Patentabteilung eines Unternehmens die schriftliche Erklärung des Unternehmens, die Bewerberin oder den Bewerber während der Zeit der Ausbildung nicht zu Tätigkeiten heranzuziehen, die nicht dem Erreichen des Ausbildungsziels dienen."@de .

dtd:LA-id1495 a dtd:LA ;
    dtd:has_Value "während der Ausbildung im ersten Ausbildungsabschnitt seit mehr als sechs Monaten ohne eine Ausbildende oder einen Ausbildenden ist, die oder der entsprechend § 2 Absatz 2 Nummer 7 oder Absatz 4 zur Ausbildung bereit ist,"@de .

dtd:LA-id1496 a dtd:LA ;
    dtd:has_Value "den zweiten Ausbildungsabschnitt nicht innerhalb von einem Jahr nach dem erfolgreichen Abschluss des ersten Ausbildungsabschnitts antritt oder"@de .

dtd:LA-id1497 a dtd:LA ;
    dtd:has_Value "schuldhaft Pflichten nach § 23 Absatz 1 Satz 1 oder Absatz 3 schwerwiegend oder dauerhaft verletzt."@de .

dtd:LA-id1498 a dtd:LA ;
    dtd:has_Value "umfassende Kenntnisse auf dem Gebiet des gewerblichen Rechtsschutzes erlangen,"@de .

dtd:LA-id1499 a dtd:LA ;
    dtd:has_Value "die für den Beruf des Patentanwalts oder Patentassessors erforderlichen allgemeinen Rechtskenntnisse erwerben und"@de .

dtd:LA-id1500 a dtd:LA ;
    dtd:has_Value "mit der praktischen Tätigkeit des Patentanwalts oder Patentassessors vertraut gemacht werden."@de .

dtd:LA-id1501 a dtd:LA ;
    dtd:has_Value "mindestens zwei Jahre und zwei Monate und höchstens drei Jahre im Geltungsbereich dieser Verordnung in einer Patentanwaltskanzlei oder in der Patentabteilung eines Unternehmens (erster Ausbildungsabschnitt),"@de .

dtd:LA-id1502 a dtd:LA ;
    dtd:has_Value "zwei Monate beim Deutschen Patent- und Markenamt (zweiter Ausbildungsabschnitt) und"@de .

dtd:LA-id1503 a dtd:LA ;
    dtd:has_Value "sechs Monate beim Bundespatentgericht (dritter Ausbildungsabschnitt)."@de .

dtd:LA-id1504 a dtd:LA ;
    dtd:has_Value "wenn eine Bewerberin oder ein Bewerber schwerbehindert oder einer schwerbehinderten Person gleichgestellt ist (§ 2 Absatz 2 und 3 des Neunten Buches Sozialgesetzbuch),"@de .

dtd:LA-id1505 a dtd:LA ;
    dtd:has_Value "während einer Elternzeit nach dem Bundeselterngeld- und Elternzeitgesetz oder"@de .

dtd:LA-id1506 a dtd:LA ;
    dtd:has_Value "während einer Pflegezeit nach dem Pflegezeitgesetz oder dem Familienpflegezeitgesetz."@de .

dtd:LA-id1507 a dtd:LA ;
    dtd:has_Value "eine mit einer Note und Punktzahl nach § 46 versehene Bewertung der Leistungen der Bewerberin oder des Bewerbers,"@de .

dtd:LA-id1508 a dtd:LA ;
    dtd:has_Value "eine Äußerung zu der Eignung, den Fähigkeiten, den Kenntnissen, den praktischen Leistungen und der Führung der Bewerberin oder des Bewerbers sowie"@de .

dtd:LA-id1509 a dtd:LA ;
    dtd:has_Value "die Angabe der Tätigkeiten, zu denen die Bewerberin oder der Bewerber herangezogen worden ist."@de .

dtd:LA-id1510 a dtd:LA ;
    dtd:has_Value "als Patentanwalt in einer Kanzlei im Sinne der Patentanwaltsordnung tätig gewesen sind oder"@de .

dtd:LA-id1511 a dtd:LA ;
    dtd:has_Value "als Patentassessor auf dem Gebiet des deutschen gewerblichen Rechtsschutzes in einem ständigen Dienstverhältnis in einem Unternehmen tätig gewesen sind."@de .

dtd:LA-id1512 a dtd:LA ;
    dtd:has_Value "mit der Bestandskraft oder der Anordnung der sofortigen Vollziehbarkeit der Rücknahme oder des Widerrufs der Zulassung zur Patentanwaltschaft nach § 21 der Patentanwaltsordnung,"@de .

dtd:LA-id1513 a dtd:LA ;
    dtd:has_Value "mit der Rechtskraft eines berufsgerichtlichen Urteils auf Ausschließung aus der Patentanwaltschaft oder"@de .

dtd:LA-id1514 a dtd:LA ;
    dtd:has_Value "mit der Anordnung eines Berufsverbots nach § 132 der Patentanwaltsordnung."@de .

dtd:LA-id1515 a dtd:LA ;
    dtd:has_Value "eine Tätigkeit ausüben, die mit den Pflichten ordentlich Ausbildender unvereinbar ist, oder"@de .

dtd:LA-id1516 a dtd:LA ;
    dtd:has_Value "die Pflicht zur gewissenhaften Ausbildung grob vernachlässigen und eine zweimalige Ermahnung durch die Patentanwaltskammer erfolglos geblieben ist."@de .

dtd:LA-id1517 a dtd:LA ;
    dtd:has_Value "in ihrer Person Gründe vorliegen, die nach § 21 Absatz 2 Nummer 1 bis 3 der Patentanwaltsordnung den Widerruf der Zulassung zur Patentanwaltschaft rechtfertigen würden,"@de .

dtd:LA-id1518 a dtd:LA ;
    dtd:has_Value "in ihrer Person die Voraussetzungen nach Absatz 2 entsprechend vorliegen oder"@de .

dtd:LA-id1519 a dtd:LA ;
    dtd:has_Value "sie Berichts- oder Vorlagepflichten nach § 14 Absatz 2 trotz Ermahnung und ohne wichtigen Grund nicht nachgekommen sind."@de .

dtd:LA-id1520 a dtd:LA ;
    dtd:has_Value "mit dem Tag, den die oder der Ausbildende und die Bewerberin oder der Bewerber einvernehmlich als Ende der Ausbildung bestimmen,"@de .

dtd:LA-id1521 a dtd:LA ;
    dtd:has_Value "mit einem freiwilligen Ausscheiden nach § 5,"@de .

dtd:LA-id1522 a dtd:LA ;
    dtd:has_Value "mit dem Tag, zu dem die oder der Ausbildende die Ausbildungserklärung gegenüber dem Deutschen Patent- und Markenamt schriftlich widerruft,"@de .

dtd:LA-id1523 a dtd:LA ;
    dtd:has_Value "mit dem Tag, ab dem die oder der Ausbildende nach § 15 Absatz 1 bis 3 nicht mehr zur Ausbildung befugt ist, oder"@de .

dtd:LA-id1524 a dtd:LA ;
    dtd:has_Value "mit dem Ablauf der Höchstausbildungsdauer nach § 7 Nummer 1."@de .

dtd:LA-id1525 a dtd:LA ;
    dtd:has_Value "umfassende Kenntnisse auf dem Gebiet des deutschen gewerblichen Rechtsschutzes, insbesondere des Patent-, Gebrauchsmuster-, Marken- und Designrechts sowie des Rechts der Arbeitnehmererfindungen,"@de .

dtd:LA-id1526 a dtd:LA ;
    dtd:has_Value "Kenntnisse des Unionsrechts auf dem Gebiet des gewerblichen Rechtsschutzes und des Inhalts zwischenstaatlicher Vereinbarungen auf diesem Gebiet,"@de .

dtd:LA-id1527 a dtd:LA ;
    dtd:has_Value "Grundzüge des ausländischen gewerblichen Rechtsschutzes, insbesondere demjenigen in den Vereinigten Staaten von Amerika, in der Volksrepublik China und in Japan,"@de .

dtd:LA-id1528 a dtd:LA ;
    dtd:has_Value "Kenntnisse der Patentanwaltsordnung und der Berufsordnung der Patentanwälte sowie"@de .

dtd:LA-id1529 a dtd:LA ;
    dtd:has_Value "ergänzend zum Studium im allgemeinen Recht an einer Universität Grundzüge der in § 32 Absatz 2 genannten Rechtsgebiete, soweit diese Rechtskenntnisse für die Tätigkeit eines Patentanwalts oder Patentassessors von Bedeutung sind."@de .

dtd:LA-id1530 a dtd:LA ;
    dtd:has_Value "die erworbenen Rechtskenntnisse praktisch anzuwenden,"@de .

dtd:LA-id1531 a dtd:LA ;
    dtd:has_Value "die von einem Patentanwalt oder Patentassessor auszuführenden Tätigkeiten selbständig zu erledigen und"@de .

dtd:LA-id1532 a dtd:LA ;
    dtd:has_Value "mit den Auftraggeberinnen und Auftraggebern zu kommunizieren."@de .

dtd:LA-id1533 a dtd:LA ;
    dtd:has_Value "der Antrag vor Beginn der gerichtlichen Ausbildung schriftlich beim Deutschen Patent- und Markenamt eingereicht wurde,"@de .

dtd:LA-id1534 a dtd:LA ;
    dtd:has_Value "mit dem Antrag nachgewiesen wurde, dass die nach Landesrecht zuständige Behörde die Übernahme der Ausbildung genehmigt hat und"@de .

dtd:LA-id1535 a dtd:LA ;
    dtd:has_Value "die Ausbildung erfolgreich absolviert wurde."@de .

dtd:LA-id1536 a dtd:LA ;
    dtd:has_Value "der Antrag vor Beginn der Ausbildung im Ausland schriftlich beim Deutschen Patent- und Markenamt eingereicht wurde,"@de .

dtd:LA-id1537 a dtd:LA ;
    dtd:has_Value "die Ausbildenden und die Ausbildungsinhalte schriftlich mitgeteilt wurden und"@de .

dtd:LA-id1538 a dtd:LA ;
    dtd:has_Value "die Ausbildung erfolgreich absolviert wurde."@de .

dtd:LA-id1539 a dtd:LA ;
    dtd:has_Value "eine frühestens vier Monate vor dem Ende der Mindestausbildungszeit des ersten Ausbildungsabschnitts ausgestellte Bescheinigung der oder des Ausbildenden darüber, dass das Ziel des Ausbildungsabschnitts voraussichtlich erreicht wird oder bereits erreicht ist,"@de .

dtd:LA-id1540 a dtd:LA ;
    dtd:has_Value "eine Bescheinigung der oder des die Arbeitsgemeinschaft Leitenden über die regelmäßige Teilnahme an der Arbeitsgemeinschaft im ersten Ausbildungsabschnitt und"@de .

dtd:LA-id1541 a dtd:LA ;
    dtd:has_Value "eine Erklärung der Bewerberin oder des Bewerbers, auf welche Patentklassen nach dem Straßburger Abkommen vom 24. März 1971 über die internationale Patentklassifikation (BGBl. 1975 II S. 283, 284), das durch den Beschluss vom 2. Oktober 1979 (BGBl. 1984 II S. 799, 801) geändert worden ist, sich die Ausbildung im ersten Ausbildungsabschnitt und gegebenenfalls eine bisherige berufliche Tätigkeit erstreckt haben."@de .

dtd:LA-id1542 a dtd:LA ;
    dtd:has_Value "die Zuweisung der Bewerberinnen und Bewerber zu mindestens je einer oder einem Ausbildenden auf den Gebieten der technischen und der nichttechnischen Schutzrechte und die Dauer der Zuweisung,"@de .

dtd:LA-id1543 a dtd:LA ;
    dtd:has_Value "die Anzahl und den Inhalt der Lehr- und Informationsveranstaltungen nach § 26 sowie"@de .

dtd:LA-id1544 a dtd:LA ;
    dtd:has_Value "im dritten Ausbildungsabschnitt die Anzahl und den Gegenstand der zu Übungszwecken anzufertigenden schriftlichen Arbeiten (Übungsklausuren) und deren Bearbeitungsdauer."@de .

dtd:LA-id1545 a dtd:LA ;
    dtd:has_Value "nach Art und Umfang die Ausbildung beeinträchtigen können,"@de .

dtd:LA-id1546 a dtd:LA ;
    dtd:has_Value "das Ansehen des Deutschen Patent- und Markenamts oder des Bundespatentgerichts oder das Vertrauen der Allgemeinheit in die Unparteilichkeit oder Unbefangenheit des Deutschen Patent- und Markenamts oder des Bundespatentgerichts beeinträchtigen können oder"@de .

dtd:LA-id1547 a dtd:LA ;
    dtd:has_Value "Bewerberinnen und Bewerber in einen Widerstreit mit ihrer Pflicht nach § 23 Absatz 1 Satz 1 oder in einen Interessenwiderstreit im Sinne des § 39a Absatz 4 bis 6 und des § 41 der Patentanwaltsordnung bringen können."@de .

dtd:LA-id1548 a dtd:LA ;
    dtd:has_Value "eines an einer Universität für die Ausbildung zum Patentanwalt oder Patentassessor besonders eingerichteten Studiengangs oder"@de .

dtd:LA-id1549 a dtd:LA ;
    dtd:has_Value "eines rechtswissenschaftlichen Studiums an einer Universität, das mit der ersten Prüfung nach § 5 Absatz 1 des Deutschen Richtergesetzes oder dem Bachelor of Laws abschließt."@de .

dtd:LA-id1550 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer oder einem Vorsitzenden mit Befähigung zum Richteramt nach § 5 Absatz 1 des Deutschen Richtergesetzes, die oder der möglichst am Bundespatentgericht tätig sein soll, jedoch auch beim Deutschen Patent- und Markenamt tätig sein kann,"@de .

dtd:LA-id1551 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens vier stellvertretenden Vorsitzenden mit Befähigung zum Richteramt nach § 5 Absatz 1 des Deutschen Richtergesetzes,"@de .

dtd:LA-id1552 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens 20 Personen, die rechtskundige oder technische Mitglieder des Deutschen Patent- und Markenamts nach § 26 Absatz 2 des Patentgesetzes oder Richterin oder Richter am Bundespatentgericht sind, und"@de .

dtd:LA-id1553 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens 60 gemäß § 12 zur Ausbildung befugten Patentanwälten oder Patentassessoren."@de .

dtd:LA-id1554 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf seinen Antrag hin vom Deutschen Patent- und Markenamt abberufen wird,"@de .

dtd:LA-id1555 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus dem Deutschen Patent- und Markenamt oder dem Bundespatentgericht ausscheidet, es sei denn, dass es unmittelbar in das jeweils andere Organ eintritt,"@de .

dtd:LA-id1556 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Tätigkeit als Patentanwalt oder Patentassessor aufgibt, es sei denn, dass es eine andere Tätigkeit als Patentanwalt oder Patentassessor ausübt oder unmittelbar aufnimmt, oder"@de .

dtd:LA-id1557 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vom Deutschen Patent- und Markenamt aus wichtigem Grund gemäß § 86 des Verwaltungsverfahrensgesetzes abberufen wird."@de .

dtd:LA-id1558 a dtd:LA ;
    dtd:has_Value "Entwürfe für die Klausuren zu erstellen,"@de .

dtd:LA-id1559 a dtd:LA ;
    dtd:has_Value "bei den Klausurterminen Aufsicht zu führen,"@de .

dtd:LA-id1560 a dtd:LA ;
    dtd:has_Value "die Klausuren zu bewerten und"@de .

dtd:LA-id1561 a dtd:LA ;
    dtd:has_Value "die mündliche Prüfung abzunehmen."@de .

dtd:LA-id1562 a dtd:LA ;

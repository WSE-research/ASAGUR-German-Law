dtd:LA-id524 a dtd:LA ;
    dtd:has_Value "des § 4d des Unterlassungsklagengesetzes, der durch Artikel 2 Nummer 3 des Gesetzes vom 26. November 2020 (BGBl. I S. 2568) eingefügt worden ist, sowie"@de .

dtd:LA-id525 a dtd:LA ;
    dtd:has_Value "des § 8b Absatz 3 des Gesetzes gegen den unlauteren Wettbewerb und des § 4d des Unterlassungsklagengesetzes, von denen § 8b Absatz 3 des Gesetzes gegen den unlauteren Wettbewerb durch Artikel 1 Nummer 2 und § 4d des Unterlassungsklagengesetzes durch Artikel 2 Nummer 3 des Gesetzes vom 26. November 2020 (BGBl. I S. 2568) eingefügt worden sind,"@de .

dtd:LA-id526 a dtd:LA ;
    dtd:has_Value "den Namen, eine ladungsfähige Anschrift, eine Telefonnummer und soweit vorhanden eine E-Mail-Adresse des Vereins sowie die Adressen der Webseiten, die der Verein eingerichtet hat,"@de .

dtd:LA-id527 a dtd:LA ;
    dtd:has_Value "das Gründungsdatum des Vereins und das Eintragungsdatum im Vereinsregister sowie die Registernummer des Vereins und das zuständige Registergericht,"@de .

dtd:LA-id528 a dtd:LA ;
    dtd:has_Value "Angaben zum Zweck und zu den satzungsmäßigen Aufgaben des Vereins sowie die Angabe, ob der verfolgte Zweck steuerbegünstigt ist,"@de .

dtd:LA-id529 a dtd:LA ;
    dtd:has_Value "das Datum, zu dem der Verein mit der Wahrnehmung seiner satzungsmäßigen Aufgaben der nicht gewerbsmäßigen Aufklärung und Beratung im Interesse der Verbraucher begonnen hat,"@de .

dtd:LA-id530 a dtd:LA ;
    dtd:has_Value "die Angaben zu den Mitgliedern des Vereins nach § 2 Absatz 1 und 3 Satz 1,"@de .

dtd:LA-id531 a dtd:LA ;
    dtd:has_Value "die Angaben zu den Organmitgliedern nach § 3 Absatz 1 Satz 1 und 2, Absatz 2 und 3 Satz 1,"@de .

dtd:LA-id532 a dtd:LA ;
    dtd:has_Value "einen Bericht nach § 4 Absatz 1 über die Tätigkeiten des Vereins,"@de .

dtd:LA-id533 a dtd:LA ;
    dtd:has_Value "die Angaben zur sachlichen und personellen Ausstattung des Vereins nach § 5 Absatz 1 und"@de .

dtd:LA-id534 a dtd:LA ;
    dtd:has_Value "die Angaben zur finanziellen Ausstattung des Vereins nach § 6 Absatz 1."@de .

dtd:LA-id535 a dtd:LA ;
    dtd:has_Value "zum Zweck und zur Tätigkeit des Vereins, aus denen sich ergibt, dass er als Verbraucherverband anzusehen ist, und"@de .

dtd:LA-id536 a dtd:LA ;
    dtd:has_Value "zur finanziellen Ausstattung des Vereins und zu den öffentlichen Fördermitteln im Kalenderjahr der Antragstellung, anhand derer geprüft werden kann, ob der Verein überwiegend mit öffentlichen Mitteln gefördert wird."@de .

dtd:LA-id537 a dtd:LA ;
    dtd:has_Value "zu jeder natürlichen Person, deren Vorname und Nachname, deren Geburtsdatum sowie eine ladungsfähige Anschrift"@de .

dtd:LA-id538 a dtd:LA ;
    dtd:has_DL dtd:DL-id299 ;
    dtd:has_Value "zu jedem Verband a)dessen Name oder Firma, eine ladungsfähige Anschrift, dessen Rechtsform, dessen satzungsmäßige Zwecke und Tätigkeiten sowieb)die Vornamen, Nachnamen und die ladungsfähigen Anschriften der Mitglieder von dessen Vertretungsorgan."@de .

dtd:LA-id539 a dtd:LA ;
    dtd:has_Value "dessen Name oder Firma, eine ladungsfähige Anschrift, dessen Rechtsform, dessen satzungsmäßige Zwecke und Tätigkeiten sowie"@de .

dtd:LA-id540 a dtd:LA ;
    dtd:has_Value "die Vornamen, Nachnamen und die ladungsfähigen Anschriften der Mitglieder von dessen Vertretungsorgan."@de .

dtd:LA-id541 a dtd:LA ;
    dtd:has_Value "aufgrund ihrer Mitgliedschaft im Verein und"@de .

dtd:LA-id542 a dtd:LA ;
    dtd:has_Value "aufgrund von Rechtsgeschäften mit dem Verein soweit diese Zuwendungen nicht unter § 3 Absatz 2 oder § 5 Absatz 1 Satz 1 Nummer 3 und 4 fallen."@de .

dtd:LA-id543 a dtd:LA ;
    dtd:has_Value "der Vorname, der Nachname und eine ladungsfähige Anschrift sowie"@de .

dtd:LA-id544 a dtd:LA ;
    dtd:has_Value "besondere Qualifikationen für dessen Tätigkeiten für den Verein."@de .

dtd:LA-id545 a dtd:LA ;
    dtd:has_Value "die jeweilige Höhe der einem Organmitglied gewährten Vergütungen oder gewährten Aufwendungspauschalen unter Angabe des Umfangs und der Art der vergüteten Tätigkeit und"@de .

dtd:LA-id546 a dtd:LA ;
    dtd:has_Value "die Gesamthöhe der anderen, nicht von Nummer 1 oder § 2 Absatz 3 Satz 1 Nummer 1 erfassten Zuwendungen an die Mitglieder der einzelnen Vereinsorgane."@de .

dtd:LA-id547 a dtd:LA ;
    dtd:has_Value "das Datum, an dem die Beratung stattgefunden hat,"@de .

dtd:LA-id548 a dtd:LA ;
    dtd:has_Value "den Vornamen und den Nachnamen des Beraters, der die Beratung durchgeführt hat,"@de .

dtd:LA-id549 a dtd:LA ;
    dtd:has_Value "die Angaben über den Ort, die Art und die Dauer der Beratung und"@de .

dtd:LA-id550 a dtd:LA ;
    dtd:has_Value "den Gegenstand und das Ergebnis der Beratung, insbesondere die vorgeschlagene Problemlösung."@de .

dtd:LA-id551 a dtd:LA ;
    dtd:has_Value "den Ort, an dem er seine satzungsmäßigen Tätigkeiten nach § 4 Absatz 1 betreibt, unter Angabe der Beratungszeiten,"@de .

dtd:LA-id552 a dtd:LA ;
    dtd:has_Value "die Anzahl und die berufliche Qualifikation der Personen, die für den Verein tätig sind und nicht unter § 3 fallen,"@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Value "die jeweilige Höhe und den Rechtsgrund der Vergütungen oder Aufwendungspauschalen, die die einzelnen Personen, die in Nummer 2 genannt sind, vom Beginn des Kalenderjahres vor der Antragstellung bis zum Zeitpunkt der Antragstellung vom Verein erhalten haben, unter Angabe des Umfangs und der Art der Tätigkeit, für die die Vergütung oder der Aufwendungsersatz gewährt wurde, und"@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_Value "die Gesamthöhe sonstiger, nicht von Nummer 3 oder § 2 Absatz 3 Satz 1 Nummer 1 erfassten Zuwendungen, die Personen, die in Nummer 2 genannt sind, vom Beginn des Kalenderjahres vor der Antragstellung bis zum Zeitpunkt der Antragstellung vom Verein erhalten haben."@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_Value "Vornamen und Nachnamen der einzelnen, in Absatz 1 Satz 1 Nummer 2 genannten Personen angibt und"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Value "Zuwendungen nach Absatz 1 Satz 1 Nummer 4 einzeln auflistet und die Zuwendungsempfänger und den jeweiligen Rechtsgrund der Zuwendung benennt."@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_DL dtd:DL-id347 ;
    dtd:has_Value "die jeweilige Höhe der Einnahmen des Vereins durch a)die Summe aller Mitgliedsbeiträge,b)staatliche Zuwendungen,c)Zuwendungen sonstiger Dritter undd)seine Tätigkeiten sowie"@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Value "die Summe aller Mitgliedsbeiträge,"@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Value "staatliche Zuwendungen,"@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Value "Zuwendungen sonstiger Dritter und"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Value "seine Tätigkeiten sowie"@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Value "die Höhe der Ausgaben des Vereins für die Aufklärungs- und Beratungstätigkeit des Vereins."@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Value "jede Änderung seiner Angaben, die nach § 4 Absatz 3 Satz 2 des Unterlassungsklagengesetzes in der Liste der qualifizierten Verbraucherverbände eingetragen sind, und"@de .


dtd:LA-id1090 a dtd:LA ;
    dtd:has_Value "Verdunstungskühlanlagen,"@de .

dtd:LA-id1091 a dtd:LA ;
    dtd:has_Value "Kühltürme und"@de .

dtd:LA-id1092 a dtd:LA ;
    dtd:has_Value "Nassabscheider."@de .

dtd:LA-id1093 a dtd:LA ;
    dtd:has_Value "Verdunstungskühlanlagen, bei denen Kondenswasserbildung durch Taupunktunterschreitung möglich ist, insbesondere Anlagen mit Kaltwassersätzen,"@de .

dtd:LA-id1094 a dtd:LA ;
    dtd:has_DL dtd:DL-id806 ;
    dtd:has_Value "Wärmeübertrager, in denen a)das die Prozesswärme aufnehmende Fluid ausschließlich in einem geschlossenen Kreislauf geführt wird undb)die Prozesswärme ausschließlich direkt über Luftwärmeübertragung an die zur Kühlung herangeführte Luft übertragen wird,"@de .

dtd:LA-id1095 a dtd:LA ;
    dtd:has_Value "das die Prozesswärme aufnehmende Fluid ausschließlich in einem geschlossenen Kreislauf geführt wird und"@de .

dtd:LA-id1096 a dtd:LA ;
    dtd:has_Value "die Prozesswärme ausschließlich direkt über Luftwärmeübertragung an die zur Kühlung herangeführte Luft übertragen wird,"@de .

dtd:LA-id1097 a dtd:LA ;
    dtd:has_Value "Befeuchtungseinrichtungen in Raumlufttechnischen Anlagen, die integrierter Bestandteil der luftführenden Bereiche dieser Anlagen sind und die bei Bedarf auch zur adiabaten Kühlung eingesetzt werden,"@de .

dtd:LA-id1098 a dtd:LA ;
    dtd:has_Value "Anlagen, in denen das Nutzwasser und die Verrieselungsflächen eine dauerhaft konstante Temperatur von 60 Grad Celsius oder mehr haben,"@de .

dtd:LA-id1099 a dtd:LA ;
    dtd:has_Value "Nassabscheider, in denen das Nutzwasser dauerhaft einen pH-Wert von 4 oder weniger oder einen pH-Wert von 10 oder mehr hat,"@de .

dtd:LA-id1100 a dtd:LA ;
    dtd:has_Value "Nassabscheider, bei denen das Abgas nach Verlassen des Abscheiders für mindestens 10 Sekunden auf mindestens 72 Grad Celsius erhitzt wird, wodurch sichergestellt ist, dass trockenes Abgas abgeleitet wird,"@de .

dtd:LA-id1101 a dtd:LA ;
    dtd:has_Value "Anlagen, in denen das Nutzwasser dauerhaft eine Salzkonzentration von mehr als 100 Gramm Halogenide je Liter hat,"@de .

dtd:LA-id1102 a dtd:LA ;
    dtd:has_Value "Nassabscheider, die ausschließlich mit Frischwasser im Durchlaufbetrieb betrieben werden, und"@de .

dtd:LA-id1103 a dtd:LA ;
    dtd:has_Value "Anlagen, die in einer Halle stehen und in diese emittieren."@de .

dtd:LA-id1104 a dtd:LA ;
    dtd:has_Value "„Änderung einer Anlage“:"@de .

dtd:LA-id1105 a dtd:LA ;
    dtd:has_Value "die Änderung der Lage, der Beschaffenheit oder des Betriebs einer Anlage, die sich auf die Vermehrung oder die Ausbreitung von Legionellen auswirken kann;"@de .

dtd:LA-id1106 a dtd:LA ;
    dtd:has_Value "„Bestandsanlage“:"@de .

dtd:LA-id1107 a dtd:LA ;
    dtd:has_Value "eine Anlage, die vor dem 19. August 2017 errichtet und vor dem 19. Februar 2018 in Betrieb genommen worden ist;"@de .

dtd:LA-id1108 a dtd:LA ;
    dtd:has_Value "„Inbetriebnahme“:"@de .

dtd:LA-id1109 a dtd:LA ;
    dtd:has_Value "die erstmalige Aufnahme des Betriebs einer neu errichteten Anlage;"@de .

dtd:LA-id1110 a dtd:LA ;
    dtd:has_Value "„Koloniebildende Einheit“ (KBE):"@de .

dtd:LA-id1111 a dtd:LA ;
    dtd:has_Value "die Einheit, in der die Anzahl anzüchtbarer und auszählbarer Mikroorganismen ausgedrückt wird;"@de .

dtd:LA-id1112 a dtd:LA ;
    dtd:has_Value "„Kühlturm“:"@de .

dtd:LA-id1113 a dtd:LA ;
    dtd:has_Value "eine Anlage, bei der durch Verdunstung von Wasser Wärme an die Umgebungsluft abgeführt wird, insbesondere bestehend aus einer Verrieselungs- oder Verregnungseinrichtung für Kühlwasser und einem Wärmeübertrager, in der die Luft im Wesentlichen durch den natürlichen Zug, der im Kaminbauwerk des Kühlturms erzeugt wird, durch den Kühlturm gefördert wird und einer Kühlleistung von mehr als 200 Megawatt je Luftaustritt einschließlich der Nassabscheider, deren gereinigte Rauchgase über den Kühlturm abgeleitet werden; der Einsatz drückend angeordneter Ventilatoren zur Unterstützung der Luftzufuhr ist unschädlich, soweit diese das Charakteristikum des Kühlturms nur unwesentlich beeinflussen;"@de .

dtd:LA-id1114 a dtd:LA ;
    dtd:has_Value "„Legionellen“:"@de .

dtd:LA-id1115 a dtd:LA ;
    dtd:has_Value "ein Parameter zur Beurteilung der hygienischen Qualität des Nutzwassers; er umfasst alle Legionellenarten (Legionella spp.), die nach genormten Verfahren auf einem definierten Nährmedium anzüchtbar sind und Kolonien bilden;"@de .

dtd:LA-id1116 a dtd:LA ;
    dtd:has_Value "„Nassabscheider“:"@de .

dtd:LA-id1117 a dtd:LA ;
    dtd:has_Value "ein Abscheider, der dem Entfernen fester, flüssiger und gasförmiger Verunreinigungen aus einem Abgas mit Hilfe einer Waschflüssigkeit dient, wobei die Verunreinigungen an die in die Abgasströmung eingebrachte Waschflüssigkeit gebunden und mit dieser zusammen abgeschieden werden; nicht erfasst sind insbesondere Abscheider, bei denen die Reinigungsleistung durch Mikroorganismen bewirkt wird, wie Biofilter oder Rieselbettfilter, unbeschadet einer gegebenenfalls vorhandenen Berieselung des Filters zur Lebenserhaltung der die Abscheideleistung erbringenden Mikroorganismen;"@de .

dtd:LA-id1118 a dtd:LA ;
    dtd:has_Value "„Neuanlage“:"@de .

dtd:LA-id1119 a dtd:LA ;
    dtd:has_Value "eine Anlage, die keine Bestandsanlage ist;"@de .

dtd:LA-id1120 a dtd:LA ;
    dtd:has_DL dtd:DL-id850 ;
    dtd:has_Value "„Nutzwasser“: a)das Wasser, das in einer Verdunstungskühlanlage oder einem Kühlturm zum Zweck der Wärmeabfuhr eingesetzt wird und dabei im Kontakt mit der Atmosphäre steht (Kühlwasser) undb)das Wasser, das in einem Nassabscheider zum Zwecke der Reinigung eingesetzt wird und dabei im Kontakt mit der Atmosphäre steht (Waschflüssigkeit);"@de .

dtd:LA-id1121 a dtd:LA ;
    dtd:has_Value "das Wasser, das in einer Verdunstungskühlanlage oder einem Kühlturm zum Zweck der Wärmeabfuhr eingesetzt wird und dabei im Kontakt mit der Atmosphäre steht (Kühlwasser) und"@de .

dtd:LA-id1122 a dtd:LA ;
    dtd:has_Value "das Wasser, das in einem Nassabscheider zum Zwecke der Reinigung eingesetzt wird und dabei im Kontakt mit der Atmosphäre steht (Waschflüssigkeit);"@de .

dtd:LA-id1123 a dtd:LA ;
    dtd:has_Value "„Referenzwert“:"@de .

dtd:LA-id1124 a dtd:LA ;
    dtd:has_Value "die sich bei ordnungsgemäßem Betrieb einstellende anlagentypische allgemeine Koloniezahl im Nutzwasser;"@de .

dtd:LA-id1125 a dtd:LA ;
    dtd:has_Value "„Verdunstungskühlanlage“:"@de .

dtd:LA-id1126 a dtd:LA ;
    dtd:has_Value "eine Anlage, bei der durch Verdunstung von Wasser Wärme an die Umgebungsluft abgeführt wird, insbesondere bestehend aus einer Verrieselungs- oder Verregnungseinrichtung für Kühlwasser und einem Wärmeübertrager, ausgenommen Kühltürme;"@de .

dtd:LA-id1127 a dtd:LA ;
    dtd:has_Value "„Wiederinbetriebnahme“:"@de .

dtd:LA-id1128 a dtd:LA ;
    dtd:has_Value "die erneute Aufnahme des Betriebs einer Anlage nach einer Änderung gemäß Nummer 1;"@de .

dtd:LA-id1129 a dtd:LA ;
    dtd:has_Value "„Zusatzwasser“:"@de .

dtd:LA-id1130 a dtd:LA ;
    dtd:has_Value "das Wasser, das dem Nutzwasser zugesetzt wird, insbesondere zum Ausgleich von Verdunstungsverlusten oder zur Begrenzung der Eindickung;"@de .

dtd:LA-id1131 a dtd:LA ;
    dtd:has_Value "„akkreditierte Inspektionsstelle Typ A“:"@de .

dtd:LA-id1132 a dtd:LA ;
    dtd:has_Value "von einer nationalen Akkreditierungsstelle im Sinne der Verordnung (EG) Nr. 765/2008 des Europäischen Parlaments und des Rates vom 9. Juli 2008 über die Vorschriften für die Akkreditierung und Marktüberwachung im Zusammenhang mit der Vermarktung von Produkten und zur Aufhebung der Verordnung (EWG) Nr. 339/93 des Rates (ABl. L 218 vom 13.8.2008, S. 30) in der jeweils geltenden Fassung für die Durchführung der erforderlichen Inspektionen akkreditierte Inspektionsstelle die Inspektionen gemäß DIN EN ISO/IEC 17020, Ausgabe Juli 2012, Absatz 4.1.6 Buchstabe a in Verbindung mit Abschnitt A.1 des Anhangs A als unabhängige Dritte anbietet;"@de .

dtd:LA-id1133 a dtd:LA ;
    dtd:has_Value "„akkreditiertes Prüflaboratorium“:"@de .

dtd:LA-id1134 a dtd:LA ;
    dtd:has_Value "von einer nationalen Akkreditierungsstelle im Sinne der Verordnung (EG) Nr. 765/2008 des Europäischen Parlaments und des Rates vom 9. Juli 2008 über die Vorschriften für die Akkreditierung und Marktüberwachung im Zusammenhang mit der Vermarktung von Produkten und zur Aufhebung der Verordnung (EWG) Nr. 339/93 des Rates (ABl. L 218 vom 13.8.2008, S. 30) in der jeweils geltenden Fassung für die Durchführung der erforderlichen Prüfverfahren in der Matrix Kühl- und Waschwasser akkreditiertes Labor;"@de .

dtd:LA-id1135 a dtd:LA ;
    dtd:has_Value "„allgemeine Koloniezahl“:"@de .

dtd:LA-id1136 a dtd:LA ;
    dtd:has_Value "ein Parameter zur Beurteilung der hygienischen Qualität des Nutzwassers; er umfasst alle Mikroorganismen, die nach genormten Verfahren auf oder in einem definierten Nähragarmedium anzüchtbar sind und Kolonien bilden;"@de .

dtd:LA-id1137 a dtd:LA ;
    dtd:has_DL dtd:DL-id855 ;
    dtd:has_Value "„mikrobiologische Untersuchung“: a)die Untersuchung des Nutzwassers nach genormten Prüfverfahren durch ein dafür akkreditiertes Prüflaboratorium (Laboruntersuchung) undb)die Untersuchung zur Differenzierung der Legionellen durch ein dafür akkreditiertes Prüflaboratorium;"@de .

dtd:LA-id1138 a dtd:LA ;
    dtd:has_Value "die Untersuchung des Nutzwassers nach genormten Prüfverfahren durch ein dafür akkreditiertes Prüflaboratorium (Laboruntersuchung) und"@de .

dtd:LA-id1139 a dtd:LA ;
    dtd:has_Value "die Untersuchung zur Differenzierung der Legionellen durch ein dafür akkreditiertes Prüflaboratorium;"@de .

dtd:LA-id1140 a dtd:LA ;
    dtd:has_Value "„öffentlich bestellter und vereidigter Sachverständiger“:"@de .

dtd:LA-id1141 a dtd:LA ;
    dtd:has_Value "ein nach § 36, gegebenenfalls in Verbindung mit § 36a, der Gewerbeordnung vom 22. Februar 1999 (BGBl. I S. 202), die zuletzt durch Artikel 626 Absatz 3 der Verordnung vom 31. August 2015 (BGBl. I S. 1474) geändert worden ist, öffentlich bestellter und vereidigter Sachverständiger;"@de .

dtd:LA-id1142 a dtd:LA ;
    dtd:has_Value "„hygienisch fachkundige Person“:"@de .

dtd:LA-id1143 a dtd:LA ;
    dtd:has_Value "Person, die an einer Schulung entsprechend der Richtlinie VDI 2047 Blatt 2, Ausgabe Januar 2015, oder der Richtlinie VDI 6022 Blatt 4, Ausgabe August 2012, oder vergleichbarer Art und vergleichbaren Umfangs teilgenommen hat."@de .

dtd:LA-id1144 a dtd:LA ;
    dtd:has_Value "die eingesetzten Werkstoffe für die Wasserqualität und die einzusetzenden Betriebsstoffe, einschließlich Desinfektions- und Reinigungsmittel, geeignet sind,"@de .

dtd:LA-id1145 a dtd:LA ;
    dtd:has_Value "Tropfenauswurf durch geeignete Tropfenabscheider oder gleichwertige Maßnahmen effektiv minimiert wird,"@de .

dtd:LA-id1146 a dtd:LA ;
    dtd:has_Value "Totzonen, in denen das Wasser während des bestimmungsgemäßen Betriebs stagniert, möglichst vermieden werden,"@de .

dtd:LA-id1147 a dtd:LA ;
    dtd:has_Value "wasserführende Bauteile möglichst vollständig entleert werden können,"@de .

dtd:LA-id1148 a dtd:LA ;
    dtd:has_Value "Biozide dem Nutzwasser dosiert zugesetzt werden können,"@de .

dtd:LA-id1149 a dtd:LA ;
    dtd:has_Value "Vorkehrungen für die regelmäßige Überprüfung relevanter chemischer, physikalischer oder mikrobiologischer Parameter getroffen werden,"@de .

dtd:LA-id1150 a dtd:LA ;
    dtd:has_Value "Vorkehrungen für die regelmäßige Probenahme für mikrobiologische Untersuchungen getroffen werden und"@de .

dtd:LA-id1151 a dtd:LA ;
    dtd:has_Value "Vorkehrungen für die Durchführung regelmäßiger Instandhaltungen getroffen werden."@de .

dtd:LA-id1152 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bis zur Bestimmung des Referenzwertes nach Satz 1 oder 2,"@de .

dtd:LA-id1153 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Anlagen, die bestimmungsgemäß an nicht mehr als 90 aufeinanderfolgenden Tagen im Jahr in Betrieb sind, oder"@de .

dtd:LA-id1154 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Anlagen, für die der Betreiber erklärt, auf die Bestimmung des Referenzwertes nach Satz 1 oder 2 zu verzichten."@de .

dtd:LA-id1155 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Sicherstellung der hygienischen Beschaffenheit des Nutzwassers regelmäßig mindestens zweiwöchentliche betriebsinterne Überprüfungen chemischer, physikalischer oder mikrobiologischer Kenngrößen des Nutzwassers durchzuführen,"@de .

dtd:LA-id1156 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Überprüfung der Einhaltung des Referenzwertes regelmäßig mindestens alle drei Monate Laboruntersuchungen des Nutzwassers auf den Parameter allgemeine Koloniezahl durchführen zu lassen."@de .

dtd:LA-id1157 a dtd:LA ;
    dtd:has_Value "Untersuchungen zur Aufklärung der Ursachen durchzuführen und"@de .

dtd:LA-id1158 a dtd:LA ;
    dtd:has_Value "die erforderlichen Maßnahmen für einen ordnungsgemäßen Betrieb, insbesondere Sofortmaßnahmen zur Verminderung der mikrobiellen Belastung, zu ergreifen."@de .

dtd:LA-id1159 a dtd:LA ;
    dtd:has_Value "Untersuchungen zur Aufklärung der Ursachen durchzuführen,"@de .

dtd:LA-id1160 a dtd:LA ;
    dtd:has_Value "die erforderlichen Maßnahmen für einen ordnungsgemäßen Betrieb zu ergreifen,"@de .

dtd:LA-id1161 a dtd:LA ;
    dtd:has_Value "betriebsinterne Überprüfungen wöchentlich durchzuführen und"@de .

dtd:LA-id1162 a dtd:LA ;
    dtd:has_Value "Laboruntersuchungen auf die Parameter allgemeine Koloniezahl und Legionellen monatlich durchführen zu lassen."@de .

dtd:LA-id1163 a dtd:LA ;
    dtd:has_Value "die Pflichten nach Absatz 2 zu erfüllen und"@de .

dtd:LA-id1164 a dtd:LA ;
    dtd:has_Value "technische Maßnahmen nach dem Stand der Technik, insbesondere Sofortmaßnahmen zur Verminderung der mikrobiellen Belastung, zu ergreifen, um die Legionellenkonzentration im Nutzwasser unter den in Anlage 1 genannten Prüfwert 2 zu reduzieren."@de .

dtd:LA-id1165 a dtd:LA ;
    dtd:has_Value "Untersuchungen zur Aufklärung der Ursachen durchzuführen,"@de .


dtd:LA-id907 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn die Schäden nicht nach § 15 gegenüber der Entschädigungsstelle geltend gemacht werden können,"@de .

dtd:LA-id908 a dtd:LA ;

dtd:LA-id949 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit dem Liquidator,"@de .

dtd:LA-id950 a dtd:LA ;
    dtd:has_Size dtd:normal ;

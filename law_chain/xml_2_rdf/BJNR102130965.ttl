dtd:LA-id830 a dtd:LA ;
    dtd:has_DL dtd:DL-id477 ;
    dtd:has_Value "„Fahrzeug“ a)jedes Kraftfahrzeug, das ausschließlich maschinell an Land angetrieben wird, jedoch nicht auf Schienen fährt und dessen durch die Bauart bedingte Höchstgeschwindigkeit 6 Kilometer pro Stunde übersteigt,b)jedes Landfahrzeug, das durch Muskelkraft fortbewegt wird und mit einem elektromotorischen Hilfsantrieb ausgestattet ist, sofern es unter Berücksichtigung des § 1 Absatz 2 und 3 des Straßenverkehrsgesetzes als Kraftfahrzeug anzusehen ist,c)jeden Anhänger, der mit einem in Buchstabe a genannten Fahrzeug zu verwenden ist, unabhängig davon, ob er angekuppelt oder abgekuppelt ist;"@de .

dtd:LA-id831 a dtd:LA ;
    dtd:has_Value "jedes Kraftfahrzeug, das ausschließlich maschinell an Land angetrieben wird, jedoch nicht auf Schienen fährt und dessen durch die Bauart bedingte Höchstgeschwindigkeit 6 Kilometer pro Stunde übersteigt,"@de .

dtd:LA-id832 a dtd:LA ;
    dtd:has_Value "jedes Landfahrzeug, das durch Muskelkraft fortbewegt wird und mit einem elektromotorischen Hilfsantrieb ausgestattet ist, sofern es unter Berücksichtigung des § 1 Absatz 2 und 3 des Straßenverkehrsgesetzes als Kraftfahrzeug anzusehen ist,"@de .

dtd:LA-id833 a dtd:LA ;
    dtd:has_Value "jeden Anhänger, der mit einem in Buchstabe a genannten Fahrzeug zu verwenden ist, unabhängig davon, ob er angekuppelt oder abgekuppelt ist;"@de .

dtd:LA-id834 a dtd:LA ;
    dtd:has_Value "„regelmäßiger Standort“ den regelmäßigen Standort im Sinne des § 46 Absatz 1 Satz 1, Absatz 2 und Absatz 3 Satz 1 der Fahrzeug-Zulassungsverordnung;"@de .

dtd:LA-id835 a dtd:LA ;
    dtd:has_Value "„Staaten des Europäischen Wirtschaftsraums“ die Mitgliedstaaten der Europäischen Union sowie die anderen Vertragsstaaten des Abkommens über den Europäischen Wirtschaftsraum;"@de .

dtd:LA-id836 a dtd:LA ;
    dtd:has_Value "„Herkunftsstaat“ denjenigen Staat des Europäischen Wirtschaftsraums, in dem ein Versicherungsunternehmen seinen Sitz hat;"@de .

dtd:LA-id837 a dtd:LA ;
    dtd:has_Value "„Drittstaaten“ alle Staaten, die nicht Staaten des Europäischen Wirtschaftsraums sind;"@de .

dtd:LA-id838 a dtd:LA ;
    dtd:has_Value "„nationales Versicherungsbüro“ ein nationales Versicherungsbüro im Sinne des Artikels 1 Nummer 3 der Richtlinie 2009/103/EG des Europäischen Parlaments und des Rates vom 16. September 2009 über die Kraftfahrzeug-Haftpflichtversicherung und die Kontrolle der entsprechenden Versicherungspflicht (ABl. L 263 vom 7.10.2009, S. 11), die durch die Richtlinie (EU) 2021/2118 (ABl. L 430 vom 2.12.2021, S. 1) geändert worden ist;"@de .

dtd:LA-id839 a dtd:LA ;
    dtd:has_Value "„Deutsches Büro Grüne Karte“ den rechtsfähigen Verein „Deutsches Büro Grüne Karte eingetragener Verein“ oder im Falle eines Zuständigkeitswechsels den jeweiligen Rechtsträger des deutschen nationalen Versicherungsbüros."@de .

dtd:LA-id840 a dtd:LA ;
    dtd:has_Value "den Staat, dessen amtliches Kennzeichen das Fahrzeug trägt, unabhängig davon, ob es sich um ein endgültiges oder vorläufiges Kennzeichen handelt,"@de .

dtd:LA-id841 a dtd:LA ;
    dtd:has_Value "sofern es für eine Fahrzeugart keine Zulassung gibt, das betreffende Fahrzeug jedoch eine Versicherungsplakette oder ein dem amtlichen Kennzeichen ähnliches Unterscheidungszeichen trägt, den Staat, in dem diese Plakette oder dieses Unterscheidungszeichen verliehen wurde, oder,"@de .

dtd:LA-id842 a dtd:LA ;
    dtd:has_Value "sofern es für bestimmte Fahrzeugarten weder eine Zulassung noch eine Versicherungsplakette noch ein unterscheidendes Kennzeichen gibt, den Staat, in dem der Fahrzeughalter seinen Wohnsitz hat."@de .

dtd:LA-id843 a dtd:LA ;
    dtd:has_Value "den Merkmalen des Fahrzeugs,"@de .

dtd:LA-id844 a dtd:LA ;
    dtd:has_Value "dem Gelände, auf dem das Fahrzeug verwendet wird, und"@de .

dtd:LA-id845 a dtd:LA ;
    dtd:has_Value "der Tatsache, ob es sich in Bewegung befindet oder nicht."@de .

dtd:LA-id846 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bundesrepublik Deutschland,"@de .

dtd:LA-id847 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Länder,"@de .

dtd:LA-id848 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gemeinden mit mehr als einhunderttausend Einwohnern,"@de .

dtd:LA-id849 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gemeindeverbände sowie Zweckverbände, denen ausschließlich Körperschaften des öffentlichen Rechts angehören,"@de .

dtd:LA-id850 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "juristische Personen, die von einem nach § 3 Absatz 1 Nummer 4 des Versicherungsaufsichtsgesetzes von der Versicherungsaufsicht freigestellten Haftpflichtschadenausgleich Deckung erhalten."@de .

dtd:LA-id851 a dtd:LA ;
    dtd:has_Value "Kraftfahrzeuge nach § 3 Absatz 3 Satz 1 Nummer 1 Buchstabe a der Fahrzeug-Zulassungsverordnung, deren bauartbedingte Höchstgeschwindigkeit 20 Kilometer pro Stunde nicht übersteigt, wenn sie den Vorschriften über das Zulassungsverfahren nicht unterliegen,"@de .

dtd:LA-id852 a dtd:LA ;
    dtd:has_Value "Anhänger nach § 3 Absatz 3 Satz 1 Nummer 2 und Satz 2 der Fahrzeug-Zulassungsverordnung, wenn sie den Vorschriften über das Zulassungsverfahren nicht unterliegen,"@de .

dtd:LA-id853 a dtd:LA ;
    dtd:has_Value "Fahrzeuge, die keinem genehmigten Typ entsprechen und für die keine Betriebserlaubnis oder andere Genehmigung erteilt ist, für die aber eine Zulassung nach den Vorschriften der Fahrzeug-Zulassungsverordnung vorgeschrieben ist."@de .

dtd:LA-id854 a dtd:LA ;
    dtd:has_Value "Fahrzeuge, die den Vorschriften über das Zulassungsverfahren unterliegen, jedoch nicht zum Gebrauch auf öffentlichen Straßen zugelassen wurden,"@de .

dtd:LA-id855 a dtd:LA ;
    dtd:has_Value "Kraftfahrzeuge nach § 3 Absatz 3 Satz 1 Nummer 1 Buchstabe b bis d, f und g der Fahrzeug-Zulassungsverordnung."@de .

dtd:LA-id856 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein mit Gründen versehenes Schadensersatzangebot vorzulegen, wenn die Eintrittspflicht unstreitig ist und der Schaden beziffert wurde, oder"@de .

dtd:LA-id857 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine mit Gründen versehene Antwort auf die in dem Antrag enthaltenen Darlegungen zu erteilen, sofern die Eintrittspflicht bestritten wird oder nicht eindeutig feststeht oder der Schaden nicht oder nicht vollständig beziffert worden ist."@de .

dtd:LA-id858 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einer Änderung der wirtschaftlichen Verhältnisse oder der verkehrstechnischen Umstände einen hinreichenden Schutz der Geschädigten sicherzustellen oder"@de .

dtd:LA-id859 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mindesthöhen der Versicherungssummen an die nach Artikel 9 Absatz 2 der Richtlinie 2009/103/EG erhöhten Beträge anzupassen."@de .

dtd:LA-id860 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Halters,"@de .

dtd:LA-id861 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Eigentümers,"@de .

dtd:LA-id862 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Fahrers,"@de .

dtd:LA-id863 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Person der Technischen Aufsicht, wenn es sich um ein Kraftfahrzeug mit autonomer Fahrfunktion im Sinne des § 1d des Straßenverkehrsgesetzes handelt,"@de .

dtd:LA-id864 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Personen, die im Rahmen ihres Arbeitsverhältnisses zum Versicherungsnehmer oder Halter den berechtigten Fahrer zu seiner Ablösung oder zur Vornahme von Lade- und Hilfsarbeiten nicht nur gelegentlich als Beifahrer begleiten,"@de .

dtd:LA-id865 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Omnibusschaffnern, soweit sie im Rahmen ihres Arbeitsverhältnisses zum Versicherungsnehmer oder Halter tätig werden, und"@de .

dtd:LA-id866 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Arbeitgebern oder öffentlichen Dienstherrn des Versicherungsnehmers, wenn das versicherte Fahrzeug mit Zustimmung des Versicherungsnehmers für dienstliche Zwecke gebraucht wird."@de .

dtd:LA-id867 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Versicherungsvertrag wegen Drohung oder arglistiger Täuschung angefochten hat,"@de .

dtd:LA-id868 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vom Versicherungsvertrag wegen Verletzung der vorvertraglichen Anzeigepflicht oder wegen Nichtzahlung der ersten Prämie zurückgetreten ist oder"@de .

dtd:LA-id869 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Versicherungsvertrag wegen Prämienverzugs oder nach Eintritt eines Versicherungsfalls gekündigt hat."@de .

dtd:LA-id870 a dtd:LA ;
    dtd:has_Value "wenn es am ersten Tag eines Monats begonnen hat, spätestens ein Jahr nach diesem Zeitpunkt,"@de .

dtd:LA-id871 a dtd:LA ;
    dtd:has_Value "wenn es zu einem anderen als dem in Nummer 1 genannten Zeitpunkt begonnen hat, spätestens an dem nach Ablauf eines Jahres folgenden Monatsersten."@de .

dtd:LA-id872 a dtd:LA ;
    dtd:has_Value "jederzeit innerhalb von 15 Tagen ab Zugang eines entsprechenden Verlangens des Versicherungsnehmers und"@de .

dtd:LA-id873 a dtd:LA ;
    dtd:has_Value "bei Beendigung des Versicherungsverhältnisses."@de .

dtd:LA-id874 a dtd:LA ;
    dtd:has_Value "für Personenschäden 7 500 000 Euro,"@de .

dtd:LA-id875 a dtd:LA ;
    dtd:has_Value "für Sachschäden 1 300 000 Euro,"@de .

dtd:LA-id876 a dtd:LA ;
    dtd:has_Value "für die weder mittelbar noch unmittelbar mit einem Personen- oder Sachschaden zusammenhängenden Vermögensschäden 50 000 Euro."@de .

dtd:LA-id877 a dtd:LA ;
    dtd:has_Value "bei einer Änderung der wirtschaftlichen Verhältnisse oder der verkehrstechnischen Umstände einen hinreichenden Schutz der Geschädigten sicherzustellen oder"@de .

dtd:LA-id878 a dtd:LA ;
    dtd:has_Value "die Mindesthöhen der Versicherungssummen an diejenigen für eine Haftpflichtversicherung nach § 1 anzugleichen."@de .

dtd:LA-id879 a dtd:LA ;

    dtd:has_DL dtd:DL-id6225 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kapitalverwaltungsgesellschaft muss darlegen können, dass das Auslagerungsunternehmen a)unter Berücksichtigung der ihm übertragenen Aufgaben über die erforderliche Qualifikation verfügt,b)in der Lage ist, die übernommenen Aufgaben ordnungsgemäß wahrzunehmen undc)sorgfältig ausgewählt wurde;"@de .

dtd:LA-id10341 a dtd:LA ;
    dtd:has_Size dtd:normal ;

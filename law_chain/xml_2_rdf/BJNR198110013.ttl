dtd:LA-id11354 a dtd:LA ;
    dtd:has_Value "vorliegen,"@de .

dtd:LA-id11355 a dtd:LA ;
    dtd:has_Value "von dem nicht börsennotierten Unternehmen zur Verfügung gestellt werden können oder"@de .

dtd:LA-id11356 a dtd:LA ;
    dtd:has_Value "über ein Register, zu dem die AIF-Kapitalverwaltungsgesellschaft Zugang hat oder erhalten kann, zur Verfügung gestellt werden können und"@de .


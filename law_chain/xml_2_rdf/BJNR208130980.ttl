dtd:P-id100 a dtd:P ;
    dtd:has_Value "(1) Dieses Gesetz tritt am Tage nach seiner Verkündung in Kraft."@de .

dtd:P-id101 a dtd:P ;
    dtd:has_Value "(2) Der Tag, an dem das Abkommen nach seinem Artikel 12 Abs. 2 in Kraft tritt, ist im Bundesgesetzblatt bekanntzugeben."@de .

dtd:P-id80 a dtd:P ;
    dtd:has_Value ""@de .


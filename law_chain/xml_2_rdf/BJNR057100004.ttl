dtd:LA-id802 a dtd:LA ;
    dtd:has_Value "Auswirkungen von Mängeln in Werkstoffen, Hilfsstoffen und Zubehör auf Verarbeitung und Erzeugnisqualität beurteilen"@de .

dtd:LA-id803 a dtd:LA ;
    dtd:has_Value "Auswirkungen von Veredlungsmaßnahmen unterscheiden"@de .

dtd:LA-id804 a dtd:LA ;
    dtd:has_Value "Gestaltungstechniken anwenden, insbesondere Zierarbeiten, traditionelle Nähtechniken, Drapieren, Plissieren, Färben und Stäbchenverarbeitung"@de .

dtd:LA-id805 a dtd:LA ;
    dtd:has_Value "Entwürfe nach modischen, historischen, funktionalen und technologischen Gesichtspunkten gestalten und ausarbeiten, Entwürfe präsentieren"@de .

dtd:LA-id806 a dtd:LA ;

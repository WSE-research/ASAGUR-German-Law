dtd:LA-id816 a dtd:LA ;
    dtd:has_Value "Nähergebnisse, insbesondere anhand des Kundenauftrags, prüfen"@de .

dtd:LA-id817 a dtd:LA ;
    dtd:has_Value "Bekleidung reparieren und instand halten"@de .

dtd:LA-id818 a dtd:LA ;
    dtd:has_Value "Bekleidung modernisieren"@de .

dtd:LA-id819 a dtd:LA ;
    dtd:has_Value "körperliche Veränderungen und Abweichungen feststellen"@de .

dtd:LA-id820 a dtd:LA ;
    dtd:has_Value "Bekleidung anpassen"@de .

dtd:LA-id821 a dtd:LA ;
    dtd:has_Value "Änderungen hinsichtlich Kosten und Umsetzbarkeit prüfen"@de .

dtd:LA-id822 a dtd:LA ;
    dtd:has_Value "Reklamationen entgegennehmen und bearbeiten"@de .

dtd:LA-id823 a dtd:LA ;
    dtd:has_Value "Prüftechniken anwenden, Prüfergebnisse bewerten und dokumentieren"@de .

dtd:LA-id824 a dtd:LA ;
    dtd:has_Value "Lösungen zur Fehlerbeseitigung erarbeiten, Fehler beheben"@de .

dtd:LA-id825 a dtd:LA ;
    dtd:has_Value "Ursachen von Qualitätsabweichungen feststellen und auswerten, Korrektur- und Vorbeugungsmaßnahmen durchführen"@de .

dtd:LA-id826 a dtd:LA ;
    dtd:has_Value "Endkontrollen anhand von Arbeitsaufträgen durchführen, Qualitätsvorgaben einhalten, insbesondere Toleranzbereiche, Fertigmaße und Verarbeitung, Ergebnisse dokumentieren"@de .

dtd:LA-id827 a dtd:LA ;
    dtd:has_Value "Bekleidungsstücke präsentieren"@de .


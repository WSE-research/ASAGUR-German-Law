    dtd:has_Value "Bekleidungsstücke für die Auslieferung vorbereiten"@de .

dtd:LA-id829 a dtd:LA ;
    dtd:has_Value "zur kontinuierlichen Verbesserung von Arbeitsvorgängen beitragen"@de .

dtd:LA-id830 a dtd:LA ;
    dtd:has_Value "Kleider, insbesondere mit unterschiedlichen Ausschnitt-, Ärmel- und Rockformen, Taillenverarbeitung sowie Ausschmückungen"@de .

dtd:LA-id831 a dtd:LA ;
    dtd:has_Value "Gesellschaftskleidung, insbesondere Cocktail-, Abend- oder Brautkleider sowie Corsagen"@de .


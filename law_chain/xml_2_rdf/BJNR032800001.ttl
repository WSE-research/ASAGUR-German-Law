dtd:LA-id552 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "betriebliche Aufgaben und Problemfälle in den Kraftwerksbereichen Dampferzeuger, Turbosatz, Kraftwerkshilfs- und Nebenanlagen einschließlich Wasseraufbereitung, elektrotechnische Anlagen und Leittechnik zu erfassen, zu analysieren und zu lösen;"@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich auf neue Arbeitsstrukturen, Elektrizitätserzeugungsmethoden und -technologien flexibel einzustellen."@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Kraftwerk auf der Grundlage der einschlägigen Vorschriften nach ökonomischen und ökologischen Gesichtspunkten anlagenschonend fahren;"@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Haupt-, Hilfs- und Nebenanlagen eines Kraftwerks an- und abfahren sowie bedienen und überwachen;"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Betriebszustände dieser Anlagen beurteilen und auf Betriebsstörungen folgerichtig reagieren;"@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fehlersuche, Analyse und Fehlerbehebung bei Störungen während des laufenden Betriebes im Rahmen seines Verantwortungsbereiches vornehmen."@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kraftwerkstechnologie,"@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kraftwerksbetrieb."@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Value "eine mit Erfolg abgelegte Abschlussprüfung in einem anerkannten Ausbildungsberuf, der den Metall- oder Elektroberufen oder den Produktionsberufen der Chemie zugeordnet werden kann oder"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Value "eine mindestens vierjährige Berufspraxis im Fahrbetrieb und in der Instandhaltung eines Kraftwerks."@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den erfolgreichen Abschluss des Prüfungsteils \"Kraftwerkstechnologie\" nachweist, der nicht länger als zwei Jahre zurückliegt,"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu den in Absatz 1 Nummer 1 und 2 genannten Praxiszeiten mindestens ein weiteres Jahr Berufspraxis im Fahrbetrieb nachweist und"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Dokumentation nach Absatz 4 vorlegt."@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Value "Dampferzeuger,"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Value "Turbosatz,"@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Value "Kraftwerkshilfs- und Nebenanlagen einschließlich Wasseraufbereitung,"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Value "elektrotechnische Anlagen und Leittechnik."@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "räumliche Anordnung der Anlagenteile kennen und Anlagenteile den technischen Unterlagen zuordnen;"@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebszustand der Anlagen beurteilen, Messungen durchführen und Fehler erkennen;"@de .

dtd:LA-id571 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anlagenteile vor Ort bedienen, Wartungsarbeiten durchführen und an Instandsetzungsarbeiten mitwirken;"@de .

dtd:LA-id572 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anlagen und Anlagenteile mit Voll- und Halbautomatik sowie manuell bedienen und Reserveaggregate in Betrieb nehmen;"@de .

dtd:LA-id573 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Messungen und Meldungen auswerten, auf Betriebszustände und Störungen sowie deren Ursachen schließen und entsprechende Maßnahmen ergreifen und veranlassen;"@de .

dtd:LA-id574 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Betriebs- und Arbeitssicherheit, zum Umweltschutz sowie zur Einhaltung von Bestimmungen und Auflagen der Aufsichtsbehörde ergreifen;"@de .

dtd:LA-id575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen nach dem Ansprechen von Schutzvorrichtungen und Verriegelungen ergreifen;"@de .

dtd:LA-id576 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Fehler in den elektrotechnischen Anlagen, der Energieversorgung und der Leittechnik innerhalb des Kraftwerks schließen;"@de .

dtd:LA-id577 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arbeitsabläufe beim Freischalten von Anlagenteilen planen und mit Mitarbeitern, Vorgesetzten und anderen Organisationseinheiten abstimmen;"@de .

dtd:LA-id578 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen bei Unfällen und Bränden ergreifen."@de .

dtd:LA-id579 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tätigkeitsnachweis über die strukturierte praktische Fortbildung;"@de .

dtd:LA-id580 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unterlagen über den Aufbau des Kraftwerks, insbesondere Schemata;"@de .

dtd:LA-id581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Protokolle der zu prüfenden Person über das An- und Abfahren von Haupt-, Hilfs- und Nebenanlagen des Kraftwerks, über das Fahren eines Kraftwerks auf der Grundlage der einschlägigen Vorschriften bei unterschiedlichen Betriebsweisen unter ökonomischen und ökologischen Gesichtspunkten;"@de .

dtd:LA-id582 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens sechs Fachberichte, aus denen hervorgeht, dass kraftwerksspezifische Probleme bearbeitet wurden."@de .


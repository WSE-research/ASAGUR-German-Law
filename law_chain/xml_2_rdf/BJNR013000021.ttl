    dtd:has_Group dtd:column ;
    dtd:has_ID "F1_813775_BJNR013000021BJNE000400000"@de ;
    dtd:has_Pos dtd:normal ;
    dtd:has_Value "Innerstaatliche Institution nach Artikel 9 des Übereinkommens ist nach Artikel 1 des Staatsvertrags der Länder über die Bestimmung einer innerstaatlichen Institution nach dem Gesetz zu dem Übereinkommen vom 9. September 1996 über die Sammlung, Abgabe und Annahme von Abfällen in der Rhein- und Binnenschifffahrt (Bilgenentwässerungsverband-Staatsvertrag) von 2008 der Bilgenentwässerungsverband mit Sitz in Duisburg."@de .

dtd:Footnotes-id1017 a dtd:Footnotes ;
    dtd:has_Footnote dtd:Footnote-id1018 .

dtd:LA-id715 a dtd:LA ;
    dtd:has_Value "auf den Binnenwasserstraßen, die für Deutschland in Anlage 1 zum Übereinkommen vom 9. September 1996 über die Sammlung, Abgabe und Annahme von Abfällen in der Rhein- und Binnenschifffahrt (BGBl. 2003 II S. 1799), das zuletzt durch die Beschlüsse vom 22. Juni 2017, 13. Dezember 2018 und 18. Dezember 2019 (BGBl. 2020 II S. 618) geändert worden ist, genannt sind, und"@de .

dtd:LA-id716 a dtd:LA ;
    dtd:has_Value "für die Schleusen, Häfen, Umschlagsanlagen, Liege- und Anlegestellen, die an den Binnenwasserstraßen nach Nummer 1 liegen."@de .

dtd:LA-id717 a dtd:LA ;
    dtd:has_DL dtd:DL-id296 ;
    dtd:has_Value "außerhalb von Häfen sind verpflichtet, Annahmestellen einzurichten und zu betreiben für a)Abfälle aus dem Ladungsbereich, sofern beim Entladen von Gütern nach Anhang III der Anlage 2 zum Übereinkommen entsprechende Abfälle anfallen,b)Dämpfe, sofern beim Entladen von Gütern nach Anhang IIIa der Anlage 2 zum Übereinkommen entsprechende Dämpfe anfallen,c)Hausmüll;"@de .

dtd:LA-id718 a dtd:LA ;
    dtd:has_Value "Abfälle aus dem Ladungsbereich, sofern beim Entladen von Gütern nach Anhang III der Anlage 2 zum Übereinkommen entsprechende Abfälle anfallen,"@de .

dtd:LA-id719 a dtd:LA ;
    dtd:has_Value "Dämpfe, sofern beim Entladen von Gütern nach Anhang IIIa der Anlage 2 zum Übereinkommen entsprechende Dämpfe anfallen,"@de .

dtd:LA-id720 a dtd:LA ;
    dtd:has_Value "Hausmüll;"@de .

dtd:LA-id721 a dtd:LA ;
    dtd:has_DL dtd:DL-id303 ;
    dtd:has_Value "innerhalb von Häfen sind verpflichtet, Annahmestellen einzurichten und zu betreiben für a)Abfälle aus dem Ladungsbereich, sofern beim Entladen von Gütern nach Anhang III der Anlage 2 zum Übereinkommen entsprechende Abfälle anfallen,b)Dämpfe, sofern beim Entladen von Gütern nach Anhang IIIa der Anlage 2 zum Übereinkommen entsprechende Dämpfe anfallen."@de .

dtd:LA-id722 a dtd:LA ;
    dtd:has_Value "Abfälle aus dem Ladungsbereich, sofern beim Entladen von Gütern nach Anhang III der Anlage 2 zum Übereinkommen entsprechende Abfälle anfallen,"@de .

dtd:LA-id723 a dtd:LA ;
    dtd:has_Value "Dämpfe, sofern beim Entladen von Gütern nach Anhang IIIa der Anlage 2 zum Übereinkommen entsprechende Dämpfe anfallen."@de .

dtd:LA-id724 a dtd:LA ;
    dtd:has_Value "Hausmüll und"@de .

dtd:LA-id725 a dtd:LA ;
    dtd:has_Value "Slops und übrigen Sonderabfall."@de .

dtd:LA-id726 a dtd:LA ;
    dtd:has_Value "nach § 2 Absatz 1 Satz 1 Nummer 1 Buchstabe b und Nummer 2 Buchstabe b verpflichtete Betreiber von Umschlagsanlagen sowie im Falle flüssiger Ladung die diese Umschlagsanlagen nutzenden Befrachter hinsichtlich der Annahme von Dämpfen;"@de .

dtd:LA-id727 a dtd:LA ;
    dtd:has_Value "nach § 2 Absatz 2 verpflichtete Betreiber von Häfen hinsichtlich der Annahme von Slops und übrigem Sonderabfall."@de .

dtd:LA-id728 a dtd:LA ;
    dtd:has_Value "das in bestimmten Wasserstraßenbereichen unterschiedliche regionale Verkehrsaufkommen und"@de .

dtd:LA-id729 a dtd:LA ;
    dtd:has_Value "die in den Umschlagsanlagen je nach Art und Menge der anfallenden Abfälle oder Dämpfe geltenden unterschiedlichen Anforderungen an die Annahmestelle."@de .

dtd:LA-id730 a dtd:LA ;
    dtd:has_Value "die Namen der an der Vereinbarung Beteiligten,"@de .

dtd:LA-id731 a dtd:LA ;
    dtd:has_Value "den Namen des Betreibers der gemeinsam zu nutzenden Annahmestelle,"@de .

dtd:LA-id732 a dtd:LA ;
    dtd:has_Value "Standort, Art und Umfang der gemeinsam zu nutzenden Annahmestelle,"@de .

dtd:LA-id733 a dtd:LA ;
    dtd:has_Value "Anmelde- und Abfertigungsmodalitäten für die gemeinsam zu nutzende Annahmestelle und"@de .

dtd:LA-id734 a dtd:LA ;
    dtd:has_Value "sonstige spezifische Anforderungen, die durch die Schifffahrt vorgegeben werden wie die Längen der Anlegestellen, Anzahl gleichzeitig liegender Schiffe, Gefahrgut transportierende Schiffe."@de .

dtd:LA-id735 a dtd:LA ;
    dtd:has_Value "Annahmestellen nach § 2 Absatz 1 Satz 1 Nummer 1 Buchstabe b und Nummer 2 Buchstabe b für Dämpfe zu errichten und zu betreiben, sofern die im Bedarfsplan ausgewiesenen Annahmestellen der Annahme von Dämpfen dienen,"@de .

dtd:LA-id736 a dtd:LA ;
    dtd:has_Value "Annahmestellen nach § 2 Absatz 2 Nummer 2 für Slops und übrigen Sonderabfall zu errichten und zu betreiben, sofern die im Bedarfsplan ausgewiesenen Annahmestellen der Annahme von Slops und übrigem Sonderabfall dienen."@de .

dtd:LA-id737 a dtd:LA ;
    dtd:has_Value "die Überwachung der Einhaltung der Anforderungen des Übereinkommens und"@de .

dtd:LA-id738 a dtd:LA ;

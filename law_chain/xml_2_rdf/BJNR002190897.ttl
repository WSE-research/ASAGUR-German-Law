    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Statuswechsel zu dem anderen Register nach Absatz 3 angemeldet wurde,"@de .

dtd:LA-id8832 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Statuswechselvermerk in das andere Register eingetragen wurde und"@de .

dtd:LA-id8833 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das fr die Fhrung des anderen Registers zustndige Gericht das Verfahren an das fr die Fhrung des Handelsregisters zustndige Gericht abgegeben hat."@de .

dtd:LA-id8834 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch seinen Inhalt Rechtsvorschriften verletzt, auf deren Einhaltung die Gesellschafter nicht verzichten knnen, oder"@de .

dtd:LA-id8835 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach einer Anfechtungsklage durch Urteil rechtskrftig fr nichtig erklrt worden ist."@de .

dtd:LA-id8836 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tod des Gesellschafters;"@de .

dtd:LA-id8837 a dtd:LA ;

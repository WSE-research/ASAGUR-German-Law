    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beschlagnahme, Pfndung, Arrest, Verfgungsbeschrnkung oder Festhalten sowie deren Folgen oder dahingehenden Versuchen,"@de .

dtd:LA-id9838 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zurckgelassenen Minen, Torpedos, Bomben oder sonstigen zurckgelassenen Kriegswaffen,"@de .

dtd:LA-id9839 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anschlgen von Terroristen oder Personen, die die Anschlge bswillig oder aus politischen Beweggrnden begehen, und Manahmen, die zur Verhinderung oder Bekmpfung solcher Anschlge ergriffen werden,"@de .


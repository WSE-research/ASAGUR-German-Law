    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kapitalgesellschaft erfüllt die Voraussetzungen des § 267 Absatz 3 Satz 1,"@de .

dtd:LA-id9142 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kapitalgesellschaft ist kapitalmarktorientiert im Sinne des § 264d und"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kapitalgesellschaft hat im Jahresdurchschnitt mehr als 500 Arbeitnehmer beschäftigt."@de .

dtd:LA-id9144 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kapitalgesellschaft in den Konzernlagebericht eines Mutterunternehmens einbezogen ist und"@de .


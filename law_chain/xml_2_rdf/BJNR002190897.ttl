    dtd:has_DL dtd:DL-id6379 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "folgende Angaben zur Gesellschaft: a)die Firma,b)den Sitz undc)die Geschftsanschrift in einem Mitgliedstaat der Europischen Union;"@de .


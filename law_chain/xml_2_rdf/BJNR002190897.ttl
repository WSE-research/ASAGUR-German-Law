dtd:LA-id8882 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Befreiung des Tochterunternehmens ist im Anhang des Konzernabschlusses des Mutterunternehmens angegeben und"@de .


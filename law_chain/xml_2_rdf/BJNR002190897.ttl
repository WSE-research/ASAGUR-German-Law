    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch die Löschung wegen Vermögenslosigkeit nach § 394 des Gesetzes über das Verfahren in Familiensachen und in den Angelegenheiten der freiwilligen Gerichtsbarkeit."@de .

dtd:LA-id8849 a dtd:LA ;

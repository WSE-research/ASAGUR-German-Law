dtd:LA-id2566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gehaltene Vögel: Geflügel oder in Gefangenschaft gehaltene Vögel anderer Arten;"@de .

dtd:LA-id2567 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geflügel: Hühner, Truthühner, Perlhühner, Rebhühner, Fasane, Laufvögel, Wachteln, Enten und Gänse, die in Gefangenschaft aufgezogen oder gehalten werden;"@de .

dtd:LA-id2568 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Gefangenschaft gehaltene Vögel anderer Arten: andere gehaltene Vögel als das in Nummer 2 genannte Geflügel, ausgenommen Tauben;"@de .

dtd:LA-id2569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Federwild: Vögel freilebender Arten, die für den menschlichen Verzehr gejagt werden;"@de .

dtd:LA-id2570 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bruteier: Eier von Geflügel, die zur Bebrütung bestimmt sind;"@de .

dtd:LA-id2571 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Eintagsküken: weniger als 72 Stunden alte, noch nicht gefütterte Küken und weniger als 72 Stunden alte Barbarie-Enten (Cairina moschata) und ihre Kreuzungen, gefüttert oder nicht gefüttert;"@de .

dtd:LA-id2572 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wildvogel: ein freilebender Vogel der Ordnungen Hühnervögel, Gänsevögel, Greifvögel, Eulen, Regenpfeiferartige, Lappentaucherartige oder Schreitvögel sowie ein zu wissenschaftlichen Zwecken gehaltener Vogel dieser Ordnungen;"@de .

dtd:LA-id2573 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Impfung:"@de .

dtd:LA-id2574 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schutzimpfung oder Notimpfung;"@de .

dtd:LA-id2575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schutzimpfung:"@de .

dtd:LA-id2576 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine vorbeugende Impfung gehaltener Vögel zur Verminderung klinischer Erscheinungen oder der Virusausscheidung für den Fall der Ansteckung mit dem hochpathogenen oder dem niedrigpathogenen aviären Influenzavirus;"@de .

dtd:LA-id2577 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Notimpfung:"@de .

dtd:LA-id2578 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Impfung gehaltener Vögel nach dem Ausbruch der Geflügelpest zur Verhinderung der Verschleppung des hochpathogenen aviären Influenzavirus in einen Bestand oder eine sonstige Vogelhaltung oder innerhalb eines bestimmten Gebiets."@de .

dtd:LA-id2579 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Falle des Zugangs von Geflügel Name und Anschrift des Transportunternehmens und des bisherigen Tierhalters, Datum des Zugangs sowie Art des Geflügels,"@de .

dtd:LA-id2580 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Falle des Abgangs von Geflügel Name und Anschrift des Transportunternehmens und des künftigen Tierhalters, Datum des Abgangs sowie Art des Geflügels,"@de .

dtd:LA-id2581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den Fall, dass mehr als 100 Stück Geflügel gehalten werden, je Werktag die Anzahl der verendeten Tiere,"@de .

dtd:LA-id2582 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den Fall, dass mehr als 1 000 Stück Geflügel gehalten werden, je Werktag zusätzlich die Gesamtzahl der gelegten Eier jedes Bestandes,"@de .

dtd:LA-id2583 a dtd:LA ;
    dtd:has_DL dtd:DL-id1215 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Falle der Abgabe von Geflügel auf einer Geflügelausstellung oder einer Veranstaltung ähnlicher Art zusätzlich a)die Anzahl undb)die Kennzeichnung"@de .

dtd:LA-id2584 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Geflügels."@de .

dtd:LA-id2585 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anzahl und"@de .

dtd:LA-id2586 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kennzeichnung"@de .

dtd:LA-id2587 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Tiere nur an Stellen gefüttert werden, die für Wildvögel nicht zugänglich sind,"@de .

dtd:LA-id2588 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Tiere nicht mit Oberflächenwasser, zu dem Wildvögel Zugang haben, getränkt werden und"@de .

dtd:LA-id2589 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Futter, Einstreu und sonstige Gegenstände, mit denen Geflügel in Berührung kommen kann, für Wildvögel unzugänglich aufbewahrt werden."@de .

dtd:LA-id2590 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens drei Tieren bei einer Größe des Bestandes oder des räumlich abgegrenzten Teils des Bestandes von bis einschließlich 100 Tieren oder"@de .

dtd:LA-id2591 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mehr als 2 vom Hundert der Tiere bei einer Größe des Bestandes oder des räumlich abgegrenzten Teils des Bestandes von mehr als 100 Tieren"@de .

dtd:LA-id2592 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verluste von mehr als der dreifachen üblichen Sterblichkeit der Tiere des Bestandes oder des räumlich abgegrenzten Teils des Bestandes oder"@de .

dtd:LA-id2593 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Abnahme der üblichen Gewichtszunahme oder Legeleistung von mehr als 5 vom Hundert"@de .

dtd:LA-id2594 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ein- und Ausgänge zu den Ställen oder die sonstigen Standorte des Geflügels gegen unbefugten Zutritt oder unbefugtes Befahren gesichert sind,"@de .

dtd:LA-id2595 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ställe oder die sonstigen Standorte des Geflügels von betriebsfremden Personen nur mit betriebseigener Schutzkleidung oder Einwegschutzkleidung betreten werden und dass diese Personen die Schutz- oder Einwegschutzkleidung nach Verlassen des Stalles oder sonstigen Standorts des Geflügels unverzüglich ablegen,"@de .

dtd:LA-id2596 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schutzkleidung nach Gebrauch unverzüglich gereinigt und desinfiziert und Einwegschutzkleidung nach Gebrauch unverzüglich unschädlich beseitigt wird,"@de .

dtd:LA-id2597 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach jeder Einstallung oder Ausstallung von Geflügel die dazu eingesetzten Gerätschaften und der Verladeplatz gereinigt und desinfiziert werden und dass nach jeder Ausstallung die frei gewordenen Ställe einschließlich der dort vorhandenen Einrichtungen und Gegenstände gereinigt und desinfiziert werden,"@de .

dtd:LA-id2598 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "betriebseigene Fahrzeuge abweichend von § 17 Absatz 1 Satz 1 und 2 der Viehverkehrsverordnung unmittelbar nach Abschluss eines Geflügeltransports auf einem befestigten Platz gereinigt und desinfiziert werden,"@de .

dtd:LA-id2599 a dtd:LA ;
    dtd:has_DL dtd:DL-id1256 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fahrzeuge, Maschinen und sonstige Gerätschaften, die in der Geflügelhaltung eingesetzt und a)in mehreren Ställen oderb)von mehreren Betrieben gemeinsambenutzt werden, jeweils vor der Benutzung in einem anderen Stall oder, in den Fällen des Buchstaben b, im abgebenden Betrieb vor der Abgabe gereinigt und desinfiziert werden,"@de .

dtd:LA-id2600 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in mehreren Ställen oder"@de .

dtd:LA-id2601 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von mehreren Betrieben gemeinsam"@de .

dtd:LA-id2602 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine ordnungsgemäße Schadnagerbekämpfung durchgeführt wird und hierüber Aufzeichnungen gemacht werden,"@de .

dtd:LA-id2603 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Raum, der Behälter oder die sonstigen Einrichtungen zur Aufbewahrung verendeten Geflügels nach jeder Abholung, mindestens jedoch einmal im Monat, gereinigt und desinfiziert wird oder werden,"@de .

dtd:LA-id2604 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine betriebsbereite Einrichtung zum Waschen der Hände sowie eine Einrichtung zum Wechseln und Ablegen der Kleidung und zur Desinfektion der Schuhe vorgehalten wird."@de .

dtd:LA-id2605 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die auf der jeweiligen Veranstaltung aufgestellten gehaltenen Vögel vor der jeweiligen Veranstaltung klinisch tierärztlich untersucht werden und"@de .


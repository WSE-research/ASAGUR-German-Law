dtd:LA-id783 a dtd:LA ;
    dtd:has_Value "durch schriftlichen oder elektronischen Verzicht des Prüfsachverständigen gegenüber der zuständigen Behörde,"@de .

dtd:LA-id784 a dtd:LA ;
    dtd:has_Value "mit der Vollendung des 70. Lebensjahres des Prüfsachverständigen oder"@de .

dtd:LA-id785 a dtd:LA ;
    dtd:has_Value "mit dem Ablauf der Geltungsdauer der Anerkennung."@de .

dtd:LA-id786 a dtd:LA ;
    dtd:has_Value "die Anerkennungsvoraussetzungen nicht mehr erfüllt,"@de .

dtd:LA-id787 a dtd:LA ;
    dtd:has_Value "nicht über einen Versicherungsschutz gemäß § 16 verfügt oder"@de .

dtd:LA-id788 a dtd:LA ;
    dtd:has_Value "gegen eine Pflicht nach den §§ 14 bis 23 wiederholt oder gröblich verstoßen hat oder gegen mehrere Pflichten nach den §§ 14 bis 23 verstoßen hat."@de .


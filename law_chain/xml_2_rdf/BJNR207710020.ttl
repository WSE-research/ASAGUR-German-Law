dtd:LA-id766 a dtd:LA ;
    dtd:has_DL dtd:DL-id654 ;
    dtd:has_Value "Nachweise über die bisherige berufliche Tätigkeit unter Darlegung der Sachkunde nach § 4 Absatz 2 Nummer 2 und 3, insbesondere a)Zeugnisse der bisherigen Arbeitgeber undb)Nachweise über die Fachkunde in dem Fachgebiet, für das die Anerkennung beantragt wird,"@de .

dtd:LA-id767 a dtd:LA ;
    dtd:has_Value "Zeugnisse der bisherigen Arbeitgeber und"@de .

dtd:LA-id768 a dtd:LA ;
    dtd:has_Value "Nachweise über die Fachkunde in dem Fachgebiet, für das die Anerkennung beantragt wird,"@de .

dtd:LA-id769 a dtd:LA ;
    dtd:has_Value "bereits vorhandene staatliche Anerkennungen,"@de .

dtd:LA-id770 a dtd:LA ;
    dtd:has_Value "bei Antragstellern, die in einem Arbeits- oder Beamtenverhältnis stehen, eine Erklärung des Arbeitgebers, dass der Arbeitgeber den Antragsteller für die Tätigkeit als Prüfsachverständiger weisungsfrei stellt,"@de .

dtd:LA-id771 a dtd:LA ;
    dtd:has_Value "soweit der Schul- oder Hochschulabschluss nicht in deutscher Sprache abgelegt worden ist, ein Nachweis über die notwendigen deutschen Sprachkenntnisse nach § 4 Absatz 2 Nummer 5,"@de .

dtd:LA-id772 a dtd:LA ;
    dtd:has_Value "ein Führungszeugnis, das nicht älter als drei Monate ist, und"@de .

dtd:LA-id773 a dtd:LA ;
    dtd:has_Value "ein Nachweis, der auf die Feststellung der körperlichen Eignung nach § 4 Absatz 2 Nummer 7 beschränkt ist."@de .

dtd:LA-id774 a dtd:LA ;
    dtd:has_Value "Nachweise über Arbeitsergebnisse, die der Antragsteller nach der Erteilung der vorhandenen Anerkennung in demjenigen Fachgebiet erbracht hat, für das er die Verlängerung der Anerkennung beantragt,"@de .

dtd:LA-id775 a dtd:LA ;
    dtd:has_Value "Nachweise über relevante Lehr- oder Fortbildungsveranstaltungen, die der Antragsteller nach der Erteilung der vorhandenen Anerkennung besucht hat,"@de .

dtd:LA-id776 a dtd:LA ;
    dtd:has_Value "Nachweise über Prüfungen, die der Antragsteller nach der Erteilung der vorhandenen Anerkennung bestanden hat,"@de .

dtd:LA-id777 a dtd:LA ;
    dtd:has_Value "Nachweise über Veränderungen bei der bisherigen beruflichen Tätigkeit nach Absatz 3 Nummer 3 und bei bereits vorhandenen staatlichen Anerkennungen nach Absatz 3 Nummer 4, die nach der Erteilung der vorhandenen Anerkennung eingetreten sind,"@de .

dtd:LA-id778 a dtd:LA ;
    dtd:has_Value "ein Führungszeugnis, das nicht älter als drei Monate ist, und"@de .

dtd:LA-id779 a dtd:LA ;
    dtd:has_Value "ein Nachweis, der auf die Feststellung der körperlichen Eignung nach § 4 Absatz 2 Nummer 7 beschränkt ist."@de .

dtd:LA-id780 a dtd:LA ;
    dtd:has_Value "die Fachgebiete und Tätigkeiten nach § 2, für die der Prüfsachverständige anerkannt ist,"@de .

dtd:LA-id781 a dtd:LA ;
    dtd:has_Value "die Geltungsdauer der Anerkennung und"@de .

dtd:LA-id782 a dtd:LA ;

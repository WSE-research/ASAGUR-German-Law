dtd:LA-id1002 a dtd:LA ;
    dtd:has_Value "Anfertigung von orthopädieschuhtechnischen Hilfsmitteln,"@de .

dtd:LA-id1003 a dtd:LA ;
    dtd:has_Value "Beratung,"@de .

dtd:LA-id1004 a dtd:LA ;
    dtd:has_Value "Orthopädieschuhtechnik sowie"@de .


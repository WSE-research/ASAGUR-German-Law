dtd:LA-id4573 a dtd:LA ;
    dtd:has_Value "die Feststellung der Seediensttauglichkeit von Personen,"@de .

dtd:LA-id4574 a dtd:LA ;
    dtd:has_Value "die Zulassung von Ärzten zur Durchführung von Seediensttauglichkeitsuntersuchungen sowie die Qualitätssicherung dieser Untersuchungen,"@de .

dtd:LA-id4575 a dtd:LA ;
    dtd:has_Value "die medizinische Betreuung an Bord,"@de .

dtd:LA-id4576 a dtd:LA ;
    dtd:has_Value "die Zulassung von medizinischen Wiederholungslehrgängen und"@de .

dtd:LA-id4577 a dtd:LA ;
    dtd:has_Value "die Registrierung von Schiffsärzten."@de .

dtd:LA-id4578 a dtd:LA ;
    dtd:has_Value "das Seearbeitsübereinkommen: das Seearbeitsübereinkommen 2006 der Internationalen Arbeitsorganisation vom 23. Februar 2006 (BGBl. 2013 II S. 763, 765) in der jeweils geltenden Fassung,"@de .

dtd:LA-id4579 a dtd:LA ;
    dtd:has_Value "das STCW-Übereinkommen: das Internationale Übereinkommen vom 7. Juli 1978 über Normen für die Ausbildung, die Erteilung von Befähigungszeugnissen und den Wachdienst von Seeleuten (BGBl. 1982 II S. 297, 298) in der jeweils geltenden Fassung,"@de .

dtd:LA-id4580 a dtd:LA ;
    dtd:has_Value "die Berufsgenossenschaft: die Berufsgenossenschaft für Transport und Verkehrswirtschaft,"@de .

dtd:LA-id4581 a dtd:LA ;
    dtd:has_Value "der seeärztliche Dienst: eine mit Ärzten ausgestattete Arbeitseinheit der Berufsgenossenschaft, die schifffahrtsmedizinische Aufgaben wahrnimmt."@de .

dtd:LA-id4582 a dtd:LA ;
    dtd:has_Value "den Vordruck des Seediensttauglichkeitszeugnisses vollständig auszufüllen und zu unterschreiben,"@de .

dtd:LA-id4583 a dtd:LA ;
    dtd:has_Value "den Vordruck mit einem Stempel nach dem Muster der Anlage 3 zu versehen und"@de .

dtd:LA-id4584 a dtd:LA ;
    dtd:has_Value "das Seediensttauglichkeitszeugnis der untersuchten Person auszuhändigen oder zu übermitteln."@de .

dtd:LA-id4585 a dtd:LA ;
    dtd:has_Value "des Ausübens von Tätigkeiten in Anwesenheit eines oder mehrerer anderer Besatzungsmitglieder oder"@de .

dtd:LA-id4586 a dtd:LA ;
    dtd:has_Value "des Tragens oder Verwendens von Brillen, Kontaktlinsen, Hörgeräten oder anderen Hilfsmitteln und des Mitführens von Ersatzgeräten für die Hilfsmittel."@de .

dtd:LA-id4587 a dtd:LA ;
    dtd:has_Value "Kapitäne und Schiffsoffiziere des Decksdienstes,"@de .

dtd:LA-id4588 a dtd:LA ;
    dtd:has_Value "Schiffsleute des Decksdienstes,"@de .

dtd:LA-id4589 a dtd:LA ;
    dtd:has_Value "Schiffsoffiziere des technischen Dienstes,"@de .

dtd:LA-id4590 a dtd:LA ;
    dtd:has_Value "Schiffsleute des technischen Dienstes,"@de .

dtd:LA-id4591 a dtd:LA ;
    dtd:has_Value "Personal der weiteren Dienstzweige."@de .

dtd:LA-id4592 a dtd:LA ;
    dtd:has_Value "die Anerkennung als Arzt für Allgemeinmedizin, Anästhesiologie, Arbeitsmedizin, Chirurgie oder Innere Medizin besitzt,"@de .

dtd:LA-id4593 a dtd:LA ;
    dtd:has_Value "in der Lage ist, das Farbsehvermögen einer zu untersuchenden Person zu beurteilen,"@de .

dtd:LA-id4594 a dtd:LA ;
    dtd:has_Value "eine mindestens vierwöchige praktische Erfahrung auf einem Seeschiff und umfassende Kenntnisse der gesundheitlichen Anforderungen im Schiffsdienst nachweist,"@de .

dtd:LA-id4595 a dtd:LA ;
    dtd:has_Value "eine stationäre oder ambulante Tätigkeit über mindestens vier Jahre mit Schwerpunkt der Erkennung und Behandlung von Erkrankungen, die einen Bezug zur Tätigkeit an Bord haben, nachweist,"@de .

dtd:LA-id4596 a dtd:LA ;
    dtd:has_Value "an einem Seminar des seeärztlichen Dienstes zur Einführung in die Grundlagen der Seediensttauglichkeitsuntersuchung teilgenommen hat und"@de .

dtd:LA-id4597 a dtd:LA ;
    dtd:has_Value "sicherstellt, dass er für den Zweck der Seediensttauglichkeitsuntersuchungen auf das Seediensttauglichkeitsverzeichnis zurückgreifen kann."@de .

dtd:LA-id4598 a dtd:LA ;
    dtd:has_Value "mindestens an einem Fortbildungsseminar des seeärztlichen Dienstes teilgenommen hat und"@de .

dtd:LA-id4599 a dtd:LA ;
    dtd:has_Value "regelmäßig Seediensttauglichkeitsuntersuchungen durchgeführt hat."@de .

dtd:LA-id4600 a dtd:LA ;
    dtd:has_Value "einer Kennung des zum Abruf berechtigten Nutzers und"@de .

dtd:LA-id4601 a dtd:LA ;
    dtd:has_Value "eines Passwortes"@de .

dtd:LA-id4602 a dtd:LA ;
    dtd:has_Value "Familienname, Vorname, Geburtsdatum und Geburtsort der untersuchten Person nach § 19 Absatz 3 Nummer 1 bis 3 des Seelotsgesetzes,"@de .

dtd:LA-id4603 a dtd:LA ;
    dtd:has_Value "Sperrvermerke der Berufsgenossenschaft nach § 19 Absatz 3 Nummer 17 des Seearbeitsgesetzes."@de .

dtd:LA-id4604 a dtd:LA ;
    dtd:has_Value "die in der Anlage 1 vorgesehenen gesundheitlichen Anforderungen für den Dienstzweig Decksdienst und"@de .

dtd:LA-id4605 a dtd:LA ;
    dtd:has_Value "die Anforderungen an die Sehschärfe nach Maßgabe des Absatzes 2"@de .

dtd:LA-id4606 a dtd:LA ;
    dtd:has_Value "Schiffen in der weltweiten Fahrt,"@de .

dtd:LA-id4607 a dtd:LA ;
    dtd:has_Value "Schiffen in dem in § 46 Absatz 1 des Seearbeitsgesetzes bezeichneten Fahrtgebiet (Europäische Fahrt),"@de .

dtd:LA-id4608 a dtd:LA ;
    dtd:has_Value "Fischereifahrzeugen in der Großen Hochseefischerei und in der Kleinen Hochseefischerei"@de .

dtd:LA-id4609 a dtd:LA ;
    dtd:has_Value "er die für ihn in Anlage 4 vorgesehenen Inhalte umfasst,"@de .

dtd:LA-id4610 a dtd:LA ;
    dtd:has_Value "der Anbieter des Lehrgangs (Anbieter) über ausreichend fachlich qualifizierte Personen für die praktische und theoretische Durchführung des Lehrgangs verfügt,"@de .

dtd:LA-id4611 a dtd:LA ;
    dtd:has_Value "der Anbieter unabhängig und zuverlässig ist und dadurch die Gewähr für eine ordnungsgemäße Erfüllung seiner Aufgaben bietet und"@de .

dtd:LA-id4612 a dtd:LA ;
    dtd:has_Value "der Anbieter über geeignete Schulungsräume und eine medizinische Ausstattung zur Durchführung des Lehrgangs nach Anlage 5 verfügt."@de .

dtd:LA-id4613 a dtd:LA ;
    dtd:has_Value "durch arglistige Täuschung, Drohung oder Bestechung oder"@de .

dtd:LA-id4614 a dtd:LA ;
    dtd:has_Value "vorsätzlich oder grob fahrlässig durch Angaben, die in wesentlicher Beziehung unrichtig oder unvollständig waren,"@de .

dtd:LA-id4615 a dtd:LA ;
    dtd:has_Value "die Geschäftsräume und die Schulungsräume während der üblichen Dienststunden des Anbieters zu betreten und deren Ausstattung, insbesondere die medizinische Ausstattung, zu prüfen,"@de .

dtd:LA-id4616 a dtd:LA ;
    dtd:has_Value "die Qualifikation der Lehrkräfte anhand entsprechender Nachweise zu prüfen,"@de .

dtd:LA-id4617 a dtd:LA ;
    dtd:has_Value "die Unterrichtsmaterialien und die Lehrgangspläne einzusehen und zu prüfen,"@de .

dtd:LA-id4618 a dtd:LA ;
    dtd:has_Value "Auskunft über die durchgeführten Lehrgänge zu verlangen,"@de .

dtd:LA-id4619 a dtd:LA ;
    dtd:has_Value "bei Lehrgängen gegenwärtig zu sein."@de .

dtd:LA-id4620 a dtd:LA ;
    dtd:has_Value "die Vorlage der Approbationsurkunde,"@de .

dtd:LA-id4621 a dtd:LA ;
    dtd:has_Value "einen Nachweis der Anerkennung als Arzt für Allgemeinmedizin, Anästhesiologie, Chirurgie oder Innere Medizin,"@de .

dtd:LA-id4622 a dtd:LA ;
    dtd:has_Value "einen Nachweis der Zusatzbezeichnung „Notfallmedizin“ oder Fachkundenachweis „Rettungsmedizin“,"@de .

dtd:LA-id4623 a dtd:LA ;
    dtd:has_Value "einen Nachweis über mindestens vierwöchige praktische Erfahrungen auf einem Seeschiff und über umfassende Kenntnisse der gesundheitlichen Anforderungen im Schiffsdienst,"@de .

dtd:LA-id4624 a dtd:LA ;
    dtd:has_Value "einen Nachweis, dass er auf einem Kauffahrteischiff unter deutscher Flagge als Schiffsarzt tätig werden wird oder tätig ist, insbesondere einen Heuervertrag nach § 28 des Seearbeitsgesetzes."@de .

dtd:LA-id4625 a dtd:LA ;
    dtd:has_Value "durch arglistige Täuschung, Drohung oder Bestechung oder"@de .

dtd:LA-id4626 a dtd:LA ;
    dtd:has_Value "vorsätzlich oder grob fahrlässig durch Angaben, die in wesentlicher Beziehung unrichtig oder unvollständig waren,"@de .

dtd:LA-id4627 a dtd:LA ;
    dtd:has_Value "wenn nicht bis zum 1. August 2015 die Zulassung beantragt wird, oder"@de .

dtd:LA-id4628 a dtd:LA ;
    dtd:has_Value "im Falle rechtzeitiger Antragstellung mit Eintritt der Unanfechtbarkeit der Entscheidung über den Antrag."@de .

dtd:LA-id4629 a dtd:LA ;
    dtd:has_B dtd:B-id824 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundsatz"@de .

dtd:LA-id4630 a dtd:LA ;
    dtd:has_DL dtd:DL-id4243 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Seediensttauglich im Sinne des § 11 des Seearbeitsgesetzes ist, 1.wer über ein ausreichendes Sehvermögen verfügt,2.wer über ein ausreichendes Hörvermögen verfügt,3.wer über eine ausreichende körperliche Leistungsfähigkeit verfügt,4.wer trotz regelmäßiger medikamentöser Behandlung nicht wesentlich körperlich oder geistig beeinträchtigt ist,5.wer trotz bestehender Gesundheitsstörungen nicht wesentlich körperlich oder geistig beeinträchtigt ist,6.bei dem keine Ausschlussgründe für eine Seediensttauglichkeit vorliegen."@de .

dtd:LA-id4631 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer über ein ausreichendes Sehvermögen verfügt,"@de .

dtd:LA-id4632 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer über ein ausreichendes Hörvermögen verfügt,"@de .

dtd:LA-id4633 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer über eine ausreichende körperliche Leistungsfähigkeit verfügt,"@de .

dtd:LA-id4634 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer trotz regelmäßiger medikamentöser Behandlung nicht wesentlich körperlich oder geistig beeinträchtigt ist,"@de .

dtd:LA-id4635 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer trotz bestehender Gesundheitsstörungen nicht wesentlich körperlich oder geistig beeinträchtigt ist,"@de .

dtd:LA-id4636 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei dem keine Ausschlussgründe für eine Seediensttauglichkeit vorliegen."@de .

dtd:LA-id4637 a dtd:LA ;
    dtd:has_B dtd:B-id827 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anforderungen an das Sehvermögen"@de .


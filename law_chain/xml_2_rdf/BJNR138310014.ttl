    dtd:has_ID "F785641_03_BJNR138310014BJNE002500000"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Personensorgeberechtigte sind nach § 1626 BGB unter anderem die Eltern eines Jugendlichen."@de .

dtd:Footnote-id5012 a dtd:Footnote ;

    dtd:has_Value "der Theorie des Fahrverhaltens,"@de .

dtd:LA-id320 a dtd:LA ;
    dtd:has_Value "der Gefahrenerkennung und Unfallvermeidung,"@de .

dtd:LA-id321 a dtd:LA ;
    dtd:has_Value "der Anforderungen an die Fahrprüfung,"@de .

dtd:LA-id322 a dtd:LA ;
    dtd:has_Value "der einschlägigen Straßenverkehrsvorschriften einschließlich der einschlägigen gemeinschaftlichen und nationalen Rechtsvorschriften und Auslegungsleitlinien,"@de .


dtd:LA-id300 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bau und Betrieb von Kraftfahrzeugen und ihren Anhängern;"@de .

dtd:LA-id301 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Prüfung von Bewerbern um die Fahrerlaubnis gemäß den Anforderungen nach Anlage 1;"@de .

dtd:LA-id302 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Begutachtung von Kraftfahrzeugen und ihren Anhängern sowie von Fahrzeugteilen, insbesondere zur Erlangung von Betriebserlaubnissen und Bauartgenehmigungen;"@de .

dtd:LA-id303 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Untersuchung der Kraftfahrzeuge und ihrer Anhänger im Rahmen der regelmäßigen technischen Überwachung;"@de .

dtd:LA-id304 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Straßenverkehrsrecht sowie die die Sachverständigen- und Prüfertätigkeit berührenden anderen Rechtsgebiete;"@de .

dtd:LA-id305 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Organisation der Verkehrsverwaltung in Bund und Ländern."@de .

dtd:LA-id306 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Eine Person, die ein Studium des Maschinenbaufachs, des Kraftfahrzeugbaufachs oder der Elektrotechnik an einer deutschen Universität oder Technischen Hochschule erfolgreich abgeschlossen hat, amtlich anerkannter Sachverständiger für den Kraftfahrzeugverkehr ist oder die die Voraussetzungen für die Anerkennung erfüllt; sie braucht jedoch einer Technischen Prüfstelle nicht anzugehören und ihre fachliche Eignung nicht durch eine Prüfung nachgewiesen zu haben;"@de .

dtd:LA-id307 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Angehöriger des höheren nichttechnischen Verwaltungsdiensts;"@de .

dtd:LA-id308 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Leiter einer Technischen Prüfstelle für den Kraftfahrzeugverkehr oder dessen Stellvertreter."@de .

dtd:LA-id309 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bau und Betrieb von Kraftfahrzeugen und ihren Anhängern;"@de .

dtd:LA-id310 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Straßenverkehrsrecht sowie die die Sachverständigentätigkeit berührenden anderen Rechtsgebiete;"@de .

dtd:LA-id311 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tätigkeit des Sachverständigen."@de .

dtd:LA-id312 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der praktische Teil der Prüfung bestanden ist,"@de .

dtd:LA-id313 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im schriftlichen und mündlichen Teil die Einzelnote \"ungenügend\" nicht erteilt worden ist und"@de .

dtd:LA-id314 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Leistungen in jedem der drei Fachgebiete mindestens mit der Gesamtnote \"ausreichend\" bewertet worden sind."@de .

dtd:LA-id315 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber den praktischen Teil der Prüfung nicht bestanden hat oder"@de .

dtd:LA-id316 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Leistungen des Bewerbers im schriftlichen oder mündlichen Teil der Prüfung in einem Fachgebiet mit der Einzelnote \"ungenügend\" bewertet worden sind."@de .

dtd:LA-id317 a dtd:LA ;
    dtd:has_Value "Befähigung, die Fahrleistung eines Bewerbers zu bewerten, der eine Fahrerlaubnis der Klasse erhalten möchte, für die die Fahrprüfung stattfindet."@de .


    dtd:has_NB dtd:NB-id656,
        dtd:NB-id657 ;
    dtd:has_Value "Anleger-Abkommensgewinne eines Dach-Spezial-Investmentfonds aus dem Besitz eines Spezial-Investmentanteils an einem Ziel-Spezial-Investmentfonds, die bei der Bewertung des Dach-Spezial-Investmentfonds ermittelt werden."@de .

dtd:LA-id1569 a dtd:LA ;
    dtd:has_Value "Erträge aus einem Investmentanteil, soweit diese nach § 20 von der Besteuerung freizustellen sind,"@de .

dtd:LA-id1570 a dtd:LA ;
    dtd:has_Value "Wertveränderungen von Investmentanteilen, soweit auf diese bei einer Veräußerung § 20 anwendbar wäre,"@de .

dtd:LA-id1571 a dtd:LA ;

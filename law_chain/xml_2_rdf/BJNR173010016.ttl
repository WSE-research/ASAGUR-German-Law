dtd:LA-id1581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einem ausländischen Spezial-Investmentfonds die inländische oder ausländische Verwaltungsgesellschaft oder der inländische Anleger."@de .

dtd:LA-id1582 a dtd:LA ;

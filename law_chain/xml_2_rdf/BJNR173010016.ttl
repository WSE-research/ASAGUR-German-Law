dtd:LA-id1433 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einnahmen nach  43 Absatz 1 Satz 1 Nummer 1 und 1a des Einkommensteuergesetzes und"@de .

dtd:LA-id1434 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Entgelte, Einnahmen und Bezge nach  2 Nummer 2 Buchstabe a bis c des Krperschaftsteuergesetzes."@de .


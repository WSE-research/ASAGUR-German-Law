    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Anleger-Abkommensgewinn von der Besteuerung freizustellen und § 44 anzuwenden und"@de .

dtd:LA-id1575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Anleger-Teilfreistellungsgewinn von der Besteuerung freizustellen und § 44 anzuwenden."@de .

dtd:LA-id1576 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "um die während der Besitzzeit bereits besteuerten ausschüttungsgleichen Erträge zu mindern sowie"@de .


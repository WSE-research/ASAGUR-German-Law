    dtd:has_Value "Derivate,"@de .

dtd:LA-id1480 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bankguthaben,"@de .

dtd:LA-id1481 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundstcke, grundstcksgleiche Rechte und vergleichbare Rechte nach dem Recht anderer Staaten,"@de .


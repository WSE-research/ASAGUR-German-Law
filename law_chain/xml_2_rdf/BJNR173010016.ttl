    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kapitalbeteiligungsgesellschaften, die im öffentlichen Interesse mit Eigenmitteln oder mit staatlicher Hilfe Beteiligungen erwerben, und"@de .


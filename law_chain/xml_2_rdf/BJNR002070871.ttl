    dtd:has_BR dtd:BR-id102 ;
    dtd:has_Value " (+++ Textnachweis Geltung ab: 1.1.1978 +++)(+++ Maßgaben aufgrund EinigVtr vgl. HaftPflG Anhang EV;     die Maßgaben sind nicht mehr anzuwenden +++)"@de ;

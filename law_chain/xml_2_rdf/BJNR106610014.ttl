    dtd:has_Size dtd:normal ;
    dtd:has_Value "„PZusatz“ die zusätzlich bereitgestellte installierte Leistung für die bedarfsorientierte Erzeugung von Strom in Kilowatt und in dem jeweiligen Kalenderjahr,"@de .

dtd:LA-id7841 a dtd:LA ;

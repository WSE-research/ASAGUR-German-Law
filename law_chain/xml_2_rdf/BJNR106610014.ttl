    dtd:has_B dtd:B-id2583 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berechnung"@de .

dtd:LA-id7845 a dtd:LA ;
    dtd:has_I dtd:I-id2601 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Höhe der Flexibilitätsprämie nach § 50b („FP“) in Cent pro Kilowattstunde direkt vermarkteten und tatsächlich eingespeisten Stroms wird nach der folgenden Formel berechnet:"@de .

dtd:LA-id7846 a dtd:LA ;
    dtd:has_IMG dtd:IMG-id7857 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value ""@de .

dtd:LA-id7847 a dtd:LA ;
    dtd:has_I dtd:I-id2602 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„PZusatz“ wird nach der folgenden Formel berechnet:"@de .

dtd:LA-id7848 a dtd:LA ;
    dtd:has_I dtd:I-id2604 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "PZusatz = Pinst – (fKor x PBem)"@de .

dtd:LA-id7849 a dtd:LA ;
    dtd:has_DL dtd:DL-id6675 ;
    dtd:has_I dtd:I-id2609 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dabei beträgt „fKor“–bei Biomethan: 1,6 und–bei Biogas, das kein Biomethan ist: 1,1."@de .

dtd:LA-id7850 a dtd:LA ;
    dtd:has_DL dtd:DL-id6680 ;
    dtd:has_I dtd:I-id2611 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Abweichend von Satz 1 wird der Wert „PZusatz“ festgesetzt –mit dem Wert null, wenn die Bemessungsleistung die 0,2fache installierte Leistung unterschreitet,–mit dem 0,5fachen Wert der installierten Leistung „Pinst“, wenn die Berechnung ergibt, dass er größer als der 0,5fache Wert der installierten Leistung ist."@de .

dtd:LA-id7851 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Biomethan: 1,6 und"@de .

dtd:LA-id7852 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Biogas, das kein Biomethan ist: 1,1."@de .

dtd:LA-id7853 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit dem Wert null, wenn die Bemessungsleistung die 0,2fache installierte Leistung unterschreitet,"@de .

dtd:LA-id7854 a dtd:LA ;
    dtd:has_SUB dtd:SUB-id2640 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit dem 0,5fachen Wert der installierten Leistung „Pinst“, wenn die Berechnung ergibt, dass er größer als der 0,5fache Wert der installierten Leistung ist."@de .

dtd:LA-id7855 a dtd:LA ;
    dtd:has_I dtd:I-id2613 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„KK“ beträgt 130 Euro pro Kilowatt."@de .

dtd:LA-id7856 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ergibt sich bei der Berechnung der Flexibilitätsprämie ein Wert kleiner null, wird abweichend von Nummer 2.1 der Wert „FP“ mit dem Wert null festgesetzt."@de .

dtd:P-id7919 a dtd:P ;

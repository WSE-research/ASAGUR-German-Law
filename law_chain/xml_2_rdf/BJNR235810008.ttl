dtd:LA-id356 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angaben, die in den Zertifikaten nach Artikel 35 Absatz 1 der Verordnung (EU) 2018/848 zu machen und nach dem Muster in Anhang VI zu der Verordnung (EU) 2018/848 abzubilden sind,"@de .

dtd:LA-id357 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die in einer auf Grund des § 6 erlassenen Rechtsverordnung geforderten Angaben und Abbildungen."@de .

dtd:LA-id358 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "spätestens drei Monate vor dem voraussichtlichen Ende ihrer Tätigkeit oder"@de .


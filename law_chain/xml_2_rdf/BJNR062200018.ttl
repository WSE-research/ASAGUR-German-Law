dtd:LA-id1716 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kunden und Kundinnen über Serviceleistungen und Produkte des Betriebes unter Berücksichtigung der Nachhaltigkeit informieren"@de .

dtd:LA-id1717 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zusammenhang von Fußgesundheit und Lebensqualität gegenüber Kunden und Kundinnen herausstellen"@de .

dtd:LA-id1718 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dienstleistungen, Waren und Produkte verkaufen"@de .

dtd:LA-id1719 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angebote erstellen und unterbreiten, Geschäftsvorgänge durchführen und dokumentieren"@de .

dtd:LA-id1720 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Perspektiven, Voraussetzungen, Rahmenbedingungen, Chancen und Risiken von Selbständigkeit aufzeigen"@de .

dtd:LA-id1721 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ziele, Aufgaben und Instrumente der qualitätssichernden Maßnahmen unterscheiden"@de .

dtd:LA-id1722 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zwischen- und Endkontrollen durchführen und dokumentieren"@de .

dtd:LA-id1723 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Qualität prüfen, insbesondere auf Maßhaltigkeit, Funktionen und Verarbeitung"@de .

dtd:LA-id1724 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fachbezogene Regelungen und gesetzliche Vorschriften einhalten"@de .

dtd:LA-id1725 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Qualitätsabweichungen und ihre Ursachen feststellen sowie Maßnahmen zur Behebung ergreifen und dokumentieren"@de .

dtd:LA-id1726 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur kontinuierlichen Verbesserung von Arbeitsabläufen beitragen"@de .

dtd:LA-id1727 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zusammenhänge zwischen qualitätssichernden Maßnahmen, Produktivität, Wirtschaftlichkeit und Kundenzufriedenheit berücksichtigen"@de .

dtd:LA-id1728 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Einkauf und Herstellung Ursprung und Herkunft der Werk- und Hilfsstoffe im Hinblick auf Umwelt-, Arbeits- und Sozialstandards berücksichtigen"@de .

dtd:LA-id1729 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Herstellung von Maßschuhen auf die Langlebigkeit hinweisen und als Beitrag zur ressourcensparenden Produktion verdeutlichen"@de .

dtd:LA-id1730 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch die Reparatur von Maß- und Konfektionsschuhen die Wertigkeit optimieren, um die Verschwendung von Ressourcen zu vermeiden"@de .

dtd:LA-id1731 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alternative und recycelte Materialien, insbesondere Sohlen, Absätze und Ausballungsmaterialien, verarbeiten"@de .


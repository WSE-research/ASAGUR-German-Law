    dtd:has_BR dtd:BR-id103,
        dtd:BR-id104 ;
    dtd:has_Value """(4) Der schriftliche Teil der PrÃ¼fung dauert hÃ¶chstens:  
	
		
		
		
		
			
				1.
				im PrÃ¼fungsbereich Arbeits-, Schutz-und TraggerÃ¼ste
				150 Minuten,
			
			
				2.

dtd:LA-id1068 a dtd:LA ;
    dtd:has_Value "entgegen Artikel 56 Absatz 8 Satz 1 eine Information nicht, nicht richtig, nicht vollständig oder nicht unverzüglich nach Feststellung einer Sicherheitslücke oder Unregelmäßigkeit gibt."@de .

dtd:LA-id863 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in informationstechnischen Systemen, Komponenten oder Prozessen oder"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "die, soweit sie nicht in einem Mitgliedstaat der Europäischen Union niedergelassen sind, einen Vertreter in einem anderen Mitgliedstaat der Europäischen Union benannt haben, in dem die digitalen Dienste ebenfalls angeboten werden."@de .

dtd:LA-id1011 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "schutzwürdige Interessen des betroffenen Betreibers einer Kritischen Infrastruktur, des Unternehmens im besonderen öffentlichen Interesse oder des Anbieters digitaler Dienste dem nicht entgegenstehen und"@de .

dtd:LA-id1012 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch die Auskunft keine Beeinträchtigung von Sicherheitsinteressen eintreten kann."@de .

dtd:LA-id1013 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "schutzwürdige Interessen des betroffenen Betreibers einer Kritischen Infrastruktur, des Unternehmens im besonderen öffentlichen Interesse oder des Anbieters digitaler Dienste dem nicht entgegenstehen und"@de .

dtd:LA-id1014 a dtd:LA ;

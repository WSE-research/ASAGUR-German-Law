dtd:LA-id940 a dtd:LA ;
    dtd:has_Value "die Gesamtzahl der Vorgänge, in denen Daten nach Absatz 1 oder Absatz 2 an das Bundesamt übermittelt wurden und"@de .

dtd:LA-id941 a dtd:LA ;
    dtd:has_Value "die Übermittlungen nach Absatz 5."@de .

dtd:LA-id942 a dtd:LA ;

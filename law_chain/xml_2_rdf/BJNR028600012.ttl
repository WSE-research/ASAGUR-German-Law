dtd:LA-id602 a dtd:LA ;
    dtd:has_Value "in Betrieben unterschiedlicher Größe in verschiedenen Bereichen und Tätigkeitsfeldern des Personen- und Güterkraftverkehrs Sach-, Organisations- und Führungsaufgaben wahrzunehmen und"@de .

dtd:LA-id603 a dtd:LA ;
    dtd:has_Value "sich auf Änderungen von Methoden und Systemen in Transport und Verkehr, auf neue Formen der Arbeitsorganisation sowie auf neue Anforderungen der Organisationsentwicklung, der Personalführung und der Personalentwicklung einzustellen sowie den organisatorisch-technischen Wandel im Betrieb mitzugestalten."@de .

dtd:LA-id604 a dtd:LA ;
    dtd:has_Value "Planen, Steuern und Überwachen des Einsatzes von Fahrzeugen und sonstigen Betriebsmitteln unter Beachtung technischer, rechtlicher und wirtschaftlicher Anforderungen,"@de .

dtd:LA-id605 a dtd:LA ;
    dtd:has_Value "Gewährleisten der Einsatzfähigkeit von technischen Ressourcen,"@de .

dtd:LA-id606 a dtd:LA ;
    dtd:has_Value "Veranlassen und Überwachen der Instandhaltung von Fahrzeugen und sonstigen Betriebsmitteln,"@de .

dtd:LA-id607 a dtd:LA ;
    dtd:has_Value "Mitwirken bei der Spezifikation und Beschaffung von technischen Systemen,"@de .

dtd:LA-id608 a dtd:LA ;
    dtd:has_Value "Steuern und Überwachen der Kostenentwicklung sowie Mitwirken beim bereichsbezogenen Controlling,"@de .

dtd:LA-id609 a dtd:LA ;
    dtd:has_Value "Mitwirken bei der Personalplanung und -auswahl sowie Sicherstellen des bedarfsgerechten Einsatzes von Eigen- und Fremdpersonal,"@de .

dtd:LA-id610 a dtd:LA ;
    dtd:has_Value "Führen von Mitarbeitern und Mitarbeiterinnen sowie Fördern ihrer Eigenverantwortung und ihrer beruflichen Entwicklung,"@de .

dtd:LA-id611 a dtd:LA ;
    dtd:has_Value "Wahrnehmen der Ausbildungsverantwortung,"@de .

dtd:LA-id612 a dtd:LA ;
    dtd:has_Value "Fördern der Kommunikation und Zusammenarbeit,"@de .

dtd:LA-id613 a dtd:LA ;
    dtd:has_Value "Mitwirken bei der Kundenbetreuung; Beraten von Kunden und Fördern der Kundenzufriedenheit,"@de .

dtd:LA-id614 a dtd:LA ;
    dtd:has_Value "Gewährleisten der Einhaltung der Vorschriften der Arbeitssicherheit, des Umwelt- und des Gesundheitsschutzes,"@de .

dtd:LA-id615 a dtd:LA ;
    dtd:has_Value "Umsetzen von Qualitätszielen."@de .


dtd:LA-id3107 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "oberirdische Gewässer,"@de .

dtd:LA-id3108 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Küstengewässer,"@de .

dtd:LA-id3109 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundwasser."@de .

dtd:LA-id3110 a dtd:LA ;
    dtd:has_BR dtd:BR-id1346 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Oberirdische Gewässer das ständig oder zeitweilig in Betten fließende oder stehende oder aus Quellen wild abfließende Wasser;"@de .

dtd:LA-id3111 a dtd:LA ;
    dtd:has_BR dtd:BR-id1347 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Küstengewässer das Meer zwischen der Küstenlinie bei mittlerem Hochwasser oder zwischen der seewärtigen Begrenzung der oberirdischen Gewässer und der seewärtigen Begrenzung des Küstenmeeres; die seewärtige Begrenzung von oberirdischen Gewässern, die nicht Binnenwasserstraßen des Bundes sind, richtet sich nach den landesrechtlichen Vorschriften;"@de .

dtd:LA-id3112 a dtd:LA ;
    dtd:has_BR dtd:BR-id1348 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Meeresgewässer die Küstengewässer sowie die Gewässer im Bereich der deutschen ausschließlichen Wirtschaftszone und des Festlandsockels, jeweils einschließlich des Meeresgrundes und des Meeresuntergrundes;"@de .

dtd:LA-id3113 a dtd:LA ;
    dtd:has_BR dtd:BR-id1349 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundwasser das unterirdische Wasser in der Sättigungszone, das in unmittelbarer Berührung mit dem Boden oder dem Untergrund steht;"@de .

dtd:LA-id3114 a dtd:LA ;
    dtd:has_BR dtd:BR-id1350 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Künstliche Gewässer von Menschen geschaffene oberirdische Gewässer oder Küstengewässer;"@de .

dtd:LA-id3115 a dtd:LA ;
    dtd:has_BR dtd:BR-id1351 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erheblich veränderte Gewässer durch den Menschen in ihrem Wesen physikalisch erheblich veränderte oberirdische Gewässer oder Küstengewässer;"@de .

dtd:LA-id3116 a dtd:LA ;
    dtd:has_BR dtd:BR-id1352 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wasserkörper einheitliche und bedeutende Abschnitte eines oberirdischen Gewässers oder Küstengewässers (Oberflächenwasserkörper) sowie abgegrenzte Grundwasservolumen innerhalb eines oder mehrerer Grundwasserleiter (Grundwasserkörper);"@de .

dtd:LA-id3117 a dtd:LA ;
    dtd:has_BR dtd:BR-id1353 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gewässereigenschaften die auf die Wasserbeschaffenheit, die Wassermenge, die Gewässerökologie und die Hydromorphologie bezogenen Eigenschaften von Gewässern und Gewässerteilen;"@de .

dtd:LA-id3118 a dtd:LA ;
    dtd:has_BR dtd:BR-id1354 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gewässerzustand die auf Wasserkörper bezogenen Gewässereigenschaften als ökologischer, chemischer oder mengenmäßiger Zustand eines Gewässers; bei als künstlich oder erheblich verändert eingestuften Gewässern tritt an die Stelle des ökologischen Zustands das ökologische Potenzial;"@de .

dtd:LA-id3119 a dtd:LA ;
    dtd:has_BR dtd:BR-id1355 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wasserbeschaffenheit die physikalische, chemische oder biologische Beschaffenheit des Wassers eines oberirdischen Gewässers oder Küstengewässers sowie des Grundwassers;"@de .

dtd:LA-id3120 a dtd:LA ;
    dtd:has_BR dtd:BR-id1356 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schädliche Gewässerveränderungen Veränderungen von Gewässereigenschaften, die das Wohl der Allgemeinheit, insbesondere die öffentliche Wasserversorgung, beeinträchtigen oder die nicht den Anforderungen entsprechen, die sich aus diesem Gesetz, aus auf Grund dieses Gesetzes erlassenen oder aus sonstigen wasserrechtlichen Vorschriften ergeben;"@de .

dtd:LA-id3121 a dtd:LA ;
    dtd:has_BR dtd:BR-id1357 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Stand der Technik der Entwicklungsstand fortschrittlicher Verfahren, Einrichtungen oder Betriebsweisen, der die praktische Eignung einer Maßnahme zur Begrenzung von Emissionen in Luft, Wasser und Boden, zur Gewährleistung der Anlagensicherheit, zur Gewährleistung einer umweltverträglichen Abfallentsorgung oder sonst zur Vermeidung oder Verminderung von Auswirkungen auf die Umwelt zur Erreichung eines allgemein hohen Schutzniveaus für die Umwelt insgesamt gesichert erscheinen lässt; bei der Bestimmung des Standes der Technik sind insbesondere die in der Anlage 1 aufgeführten Kriterien zu berücksichtigen;"@de .

dtd:LA-id3122 a dtd:LA ;
    dtd:has_BR dtd:BR-id1358 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "EMAS-Standort diejenige Einheit einer Organisation, die nach § 32 Absatz 1 Satz 1 des Umweltauditgesetzes in der Fassung der Bekanntmachung vom 4. September 2002 (BGBl. I S. 3490), das zuletzt durch Artikel 1 des Gesetzes vom 6. Dezember 2011 (BGBl. I S. 2509) geändert worden ist, in der jeweils geltenden Fassung in das EMAS-Register eingetragen ist;"@de .

dtd:LA-id3123 a dtd:LA ;
    dtd:has_BR dtd:BR-id1359 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einzugsgebiet ein Gebiet, aus dem über oberirdische Gewässer der gesamte Oberflächenabfluss an einer einzigen Flussmündung, einem Ästuar oder einem Delta ins Meer gelangt;"@de .

dtd:LA-id3124 a dtd:LA ;
    dtd:has_BR dtd:BR-id1360 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Teileinzugsgebiet ein Gebiet, aus dem über oberirdische Gewässer der gesamte Oberflächenabfluss an einem bestimmten Punkt in ein oberirdisches Gewässer gelangt;"@de .

dtd:LA-id3125 a dtd:LA ;

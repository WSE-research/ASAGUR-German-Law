dtd:LA-id1676 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einer Wohnung in der Rechtsform des eigentumsähnlichen Dauerwohnrechts für die Wohnung und den Teil des Grundstücks, auf den sich das Dauerwohnrecht erstreckt."@de .

dtd:LA-id1677 a dtd:LA ;
    dtd:has_Size dtd:normal ;

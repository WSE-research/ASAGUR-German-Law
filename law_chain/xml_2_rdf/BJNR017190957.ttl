dtd:LA-id1734 a dtd:LA ;
    dtd:has_SP dtd:SP-id655 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Kosten der besonderen Betriebseinrichtungen"@de .

dtd:LA-id1735 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Das sind z. B. die Kosten für Personen- und Lastenaufzüge, Müllbeseitigungsanlagen, Hausfernsprecher, Uhrenanlagen, gemeinschaftliche Wasch- und Badeeinrichtungen usw.."@de .

dtd:LA-id1736 a dtd:LA ;
    dtd:has_SP dtd:SP-id656 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Kosten des Gerätes und sonstiger Wirtschaftsausstattungen"@de .

dtd:LA-id1737 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Das sind"@de .


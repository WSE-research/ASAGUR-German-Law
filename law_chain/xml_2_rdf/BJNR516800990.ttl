    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bestehen einer betrieblichen Altersversorgung;"@de .

dtd:LA-id166 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arbeitnehmer mit bereits erteilten oder verbindlich vorgesehenen Zusagen auf eine betriebliche Altersversorgung des befragten Unternehmens, nach Geschlecht, leitenden und sonstigen Angestellten, Arbeitern sowie Vollzeit- oder Teilzeitbeschäftigung;"@de .


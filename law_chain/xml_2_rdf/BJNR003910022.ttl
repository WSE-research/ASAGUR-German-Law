dtd:LA-id735 a dtd:LA ;
    dtd:has_Value "Ehegatten,"@de .

dtd:LA-id736 a dtd:LA ;
    dtd:has_Value "Lebenspartner,"@de .

dtd:LA-id737 a dtd:LA ;
    dtd:has_Value "Verwandte und Verschwägerte in gerader Linie,"@de .

dtd:LA-id738 a dtd:LA ;
    dtd:has_Value "Geschwister,"@de .

dtd:LA-id739 a dtd:LA ;
    dtd:has_Value "Kinder der Geschwister,"@de .

dtd:LA-id740 a dtd:LA ;
    dtd:has_Value "Ehegatten sowie Lebenspartner der Geschwister und Geschwister der Ehegatten sowie der Lebenspartner,"@de .

dtd:LA-id741 a dtd:LA ;
    dtd:has_Value "Geschwister der Eltern,"@de .

dtd:LA-id742 a dtd:LA ;
    dtd:has_Value "Personen, die durch ein auf längere Dauer angelegtes Pflegeverhältnis mit häuslicher Gemeinschaft wie Eltern und Kinder miteinander verbunden sind (Pflegeeltern und Pflegekinder)."@de .

dtd:LA-id743 a dtd:LA ;
    dtd:has_Value "in den Fällen des Satzes 1 Nummer 2, 3, 4 und 7 die die Beziehung begründende Ehe oder die Lebenspartnerschaft nicht mehr besteht;"@de .

dtd:LA-id744 a dtd:LA ;
    dtd:has_Value "in den Fällen des Satzes 1 Nummer 4 bis 8 die Verwandtschaft oder Schwägerschaft durch Annahme als Kind erloschen ist;"@de .

dtd:LA-id745 a dtd:LA ;
    dtd:has_Value "im Falle des Satzes 1 Nummer 9 die häusliche Gemeinschaft nicht mehr besteht, sofern die Personen weiterhin wie Eltern und Kinder miteinander verbunden sind."@de .

dtd:LA-id746 a dtd:LA ;
    dtd:has_Value "ein Ausschlussgrund nach Absatz 1 vorliegt oder Zweifel bestehen, ob die dort genannten Voraussetzungen gegeben sind, oder"@de .

dtd:LA-id747 a dtd:LA ;
    dtd:has_Value "ein Grund vorliegt, der geeignet ist, Misstrauen gegen eine unparteiische Mitwirkung zu begründen, oder ein Prüfling das Vorliegen eines solchen Grundes vorbringt."@de .

dtd:LA-id748 a dtd:LA ;
    dtd:has_DL dtd:DL-id611 ;
    dtd:has_Value "die Abnahme und abschließende Bewertung der Prüfungsleistung für a)Meisterprüfungsprojekte oder Meisterprüfungsarbeiten (§ 17),b)Fachgespräche, Ergänzungsprüfungen und sonstige mündliche Prüfungen (§ 18) sowiec)Situationsaufgaben oder Arbeitsproben in Teil I sowie Präsentationen oder praktische Durchführungen einer Ausbildungssituation in Teil IV der Meisterprüfung (§ 19 Absatz 1);"@de .

dtd:LA-id749 a dtd:LA ;
    dtd:has_Value "Meisterprüfungsprojekte oder Meisterprüfungsarbeiten (§ 17),"@de .

dtd:LA-id750 a dtd:LA ;
    dtd:has_Value "Fachgespräche, Ergänzungsprüfungen und sonstige mündliche Prüfungen (§ 18) sowie"@de .

dtd:LA-id751 a dtd:LA ;
    dtd:has_Value "Situationsaufgaben oder Arbeitsproben in Teil I sowie Präsentationen oder praktische Durchführungen einer Ausbildungssituation in Teil IV der Meisterprüfung (§ 19 Absatz 1);"@de .

dtd:LA-id752 a dtd:LA ;
    dtd:has_Value "die abschließende Bewertung der Prüfungsleistung für schriftliche Prüfungen (§ 20)."@de .

dtd:LA-id753 a dtd:LA ;
    dtd:has_Value "in Prüfungskommissionen für Prüfungsleistungen in den Teilen I und II der Meisterprüfung müssen beide Mitglieder in dem Handwerk oder handwerksähnlichen Gewerbe, in dem der Prüfling geprüft wird, die Meisterprüfung abgelegt haben, das Recht zum Ausbilden von Lehrlingen besitzen oder, im Falle eines zulassungspflichtigen Handwerks, in ihm als Betriebsleiter tätig sein;"@de .

dtd:LA-id754 a dtd:LA ;
    dtd:has_Value "Prüfungskommissionen für Prüfungsleistungen in Teil III der Meisterprüfung sollen mindestens ein Mitglied haben, das besonders sachkundig in der wirtschaftlichen Betriebsführung sowie in den kaufmännischen und rechtlichen Kenntnissen ist;"@de .

dtd:LA-id755 a dtd:LA ;
    dtd:has_Value "Prüfungskommissionen für Prüfungsleistungen in Teil IV der Meisterprüfung sollen mindestens ein Mitglied haben, das besonders sachkundig in den berufserzieherischen Kenntnissen ist;"@de .

dtd:LA-id756 a dtd:LA ;
    dtd:has_Value "jede Prüfungskommission soll mindestens ein Mitglied haben, das in einem Handwerk oder handwerksähnlichen Gewerbe die Meisterprüfung erfolgreich abgelegt hat oder das Recht zum Ausbilden von Lehrlingen besitzt und in ihm als Geselle tätig ist."@de .

dtd:LA-id757 a dtd:LA ;
    dtd:has_Value "der Nachweis, der die Zuständigkeit des Meisterprüfungsausschusses nach § 2 begründet, und"@de .

dtd:LA-id758 a dtd:LA ;
    dtd:has_Value "die für die Zulassung nach § 49 Absatz 1 bis 4 oder § 51a Absatz 5 der Handwerksordnung erforderlichen Zeugnisse, Nachweise und Bescheide."@de .

dtd:LA-id759 a dtd:LA ;
    dtd:has_Value "den Nachweis nach Absatz 1 Satz 3 Nummer 1,"@de .

dtd:LA-id760 a dtd:LA ;
    dtd:has_Value "den Bescheid über die Zulassung zur Meisterprüfung und"@de .

dtd:LA-id761 a dtd:LA ;
    dtd:has_Value "die Eigenerklärung, ob und bei welchen anderen Meisterprüfungsausschüssen sich der Prüfling bereits zu dem Teil der Meisterprüfung angemeldet hat."@de .

dtd:LA-id762 a dtd:LA ;
    dtd:has_Value "Bezeichnung des durchführenden Meisterprüfungsausschusses sowie der dem Prüfling zugeordneten Prüfungskommission,"@de .

dtd:LA-id763 a dtd:LA ;
    dtd:has_Value "Ort, Datum und Uhrzeit der Prüfungsleistung,"@de .

dtd:LA-id764 a dtd:LA ;
    dtd:has_Value "notwendige und zulässige Arbeits- und Hilfsmittel,"@de .

dtd:LA-id765 a dtd:LA ;
    dtd:has_Value "Hinweis auf § 7."@de .

dtd:LA-id766 a dtd:LA ;
    dtd:has_Value "über die Anforderungen an die Meisterprüfungsprojekte oder Meisterprüfungsarbeiten,"@de .

dtd:LA-id767 a dtd:LA ;
    dtd:has_Value "über die Prüfungsaufgaben für Situationsaufgaben oder Arbeitsproben in Teil I, für Präsentationen oder praktische Durchführungen einer Ausbildungssituation in Teil IV der Meisterprüfung sowie für schriftliche Prüfungsleistungen und"@de .

dtd:LA-id768 a dtd:LA ;
    dtd:has_Value "über Richtlinien für Fachgespräche, für Ergänzungsprüfungen und für sonstige mündliche Prüfungen."@de .

dtd:LA-id769 a dtd:LA ;
    dtd:has_Value "für die Teile I und II der Meisterprüfung jedenfalls die Beisitzer nach § 48 Absatz 3 und 4 oder § 51b Absatz 4 und 5 der Handwerksordnung mit und"@de .

dtd:LA-id770 a dtd:LA ;
    dtd:has_Value "für die Teile III und IV der Meisterprüfung jedenfalls der Beisitzer nach § 48 Absatz 5 oder § 51b Absatz 6 der Handwerksordnung mit."@de .

dtd:LA-id771 a dtd:LA ;
    dtd:has_Value "die Handwerkskammer hat die erforderlichen digitalen Endgeräte mit der erforderlichen digitalen Ausstattung (digitales Prüfungssystem) zur Verfügung zu stellen;"@de .

dtd:LA-id772 a dtd:LA ;
    dtd:has_Value "den Prüflingen ist vor der Prüfung ausreichend Gelegenheit zu geben, sich mit dem digitalen Prüfungssystem vertraut zu machen;"@de .

dtd:LA-id773 a dtd:LA ;
    dtd:has_Value "während der Prüfungsleistung hat eine für das digitale Prüfungssystem technisch sachkundige Person zur Verfügung zu stehen;"@de .

dtd:LA-id774 a dtd:LA ;
    dtd:has_Value "bei nicht durch den Prüfling zu vertretenden technischen Störungen ist der damit verbundene Zeitverlust durch entsprechende Schreibzeitverlängerung auszugleichen;"@de .

dtd:LA-id775 a dtd:LA ;
    dtd:has_Value "es ist sicher zu stellen, dass nach dem jeweiligen Stand der Technik von den Prüflingen eingegebene Daten diesen, auch während der Bewertung, stets eindeutig und innerhalb der Aufbewahrungsfristen nach § 25 Absatz 2 dauerhaft zugeordnet werden können."@de .

dtd:LA-id776 a dtd:LA ;
    dtd:has_Value "weichen die Einzelbewertungen um bis zu 10 Prozent der erreichbaren Punkte voneinander ab, errechnet sich die abschließende Bewertung aus dem arithmetischen Mittel der Einzelbewertungen,"@de .

dtd:LA-id777 a dtd:LA ;
    dtd:has_Value "weichen die Einzelbewertungen um mehr als 10 Prozent der erreichbaren Punkte voneinander ab, ist die abschließende Bewertung nach dem Verfahren der Sätze 4 und 5 festzulegen."@de .


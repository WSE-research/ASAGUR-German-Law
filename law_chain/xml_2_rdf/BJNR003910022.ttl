dtd:LA-id722 a dtd:LA ;
    dtd:has_Value "seinen ersten Wohnsitz hat,"@de .

dtd:LA-id723 a dtd:LA ;
    dtd:has_Value "in einem Arbeitsverhältnis steht,"@de .

dtd:LA-id724 a dtd:LA ;
    dtd:has_Value "eine Maßnahme zur Vorbereitung auf die Meisterprüfung besucht oder"@de .

dtd:LA-id725 a dtd:LA ;
    dtd:has_Value "ein Handwerk oder ein sonstiges Gewerbe selbständig betreibt."@de .

dtd:LA-id726 a dtd:LA ;
    dtd:has_Value "die Zulassung zur Meisterprüfung, soweit darüber nicht der Vorsitzende des Meisterprüfungsausschusses entscheidet,"@de .

dtd:LA-id727 a dtd:LA ;
    dtd:has_Value "die Folgen von Täuschungshandlungen und Ordnungsverstößen,"@de .

dtd:LA-id728 a dtd:LA ;
    dtd:has_Value "die Bildung der Prüfungskommissionen für die Abnahme und die Bewertung der einzelnen Prüfungsleistungen,"@de .

dtd:LA-id729 a dtd:LA ;
    dtd:has_Value "die Feststellung der Note für jeden der Teile der Meisterprüfung und"@de .

dtd:LA-id730 a dtd:LA ;
    dtd:has_Value "das Bestehen oder Nichtbestehen der Teile der Meisterprüfung und der Meisterprüfung insgesamt."@de .

dtd:LA-id731 a dtd:LA ;
    dtd:has_Value "Arbeitgeber des Prüflings,"@de .

dtd:LA-id732 a dtd:LA ;
    dtd:has_Value "Geschäftsteilhaber, Vorgesetzte oder Mitarbeiter des Prüflings,"@de .


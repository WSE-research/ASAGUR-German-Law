dtd:LA-id714 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in privaten und öffentlichen Unternehmen unterschiedlicher Größe sowie in verschiedenen Bereichen und Tätigkeitsfeldern eines Unternehmens Sach-, Organisations- und Führungsaufgaben wahrzunehmen und"@de .

dtd:LA-id715 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich auf veränderte Methoden und Systeme, auf sich verändernde Strukturen der Arbeitsorganisation und auf neue Methoden der Organisationsentwicklung, der Personalführung und -entwicklung flexibel einzustellen sowie den technisch-organisatorischen Wandel im Unternehmen mitzugestalten."@de .

dtd:LA-id716 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mitwirken bei der Planung von Anlagen; Bauen und Einrichten von Anlagen und Arbeitsstätten; Betreiben und Überwachen der Anlagen im Hinblick auf Qualitäts- und Sicherheitsanforderungen; Planen und Überwachen des Einsatzes von Betriebsmitteln; Erkennen und Beurteilen von Störungen sowie Einleiten von Maßnahmen zu deren Behebung; Veranlassen und Beaufsichtigen der Instandhaltung von Anlagen und Betriebsmitteln;"@de .

dtd:LA-id717 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Steuern des Betriebes und Überwachen von Betriebsabläufen; Aufstellen von Budgets und Kostenplänen; Überwachen der Kostenentwicklung; Kalkulieren und Vorbereiten der Vergabe, Durchführen, Überwachen sowie Abnehmen von Baumaßnahmen; Koordinieren der Zusammenarbeit mit anderen Betriebseinheiten und Dritten; Informieren und Beraten von Kunden; Berücksichtigen und Anwenden fachspezifischer Rechtsvorschriften sowie der Regelungen zum Arbeits- und Gesundheitsschutz;"@de .

dtd:LA-id718 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Führen der Mitarbeiter und Mitarbeiterinnen im Sinne der Unternehmensziele unter Berücksichtigung ihrer Befähigungen; Anleiten der Mitarbeiter und Mitarbeiterinnen zu selbständigem und verantwortlichem Handeln; Planen des Personalbedarfs und Mitwirken bei Stellenbesetzungen; Fördern der Kommunikation zwischen Mitarbeitern und Mitarbeiterinnen, mit den Führungskräften sowie mit den Personalvertretungen; Beurteilen von Mitarbeitern und Mitarbeiterinnen; Fördern der Innovationsbereitschaft, der Entwicklung und der Weiterbildung der Mitarbeiter und Mitarbeiterinnen; Verantworten der Ausbildung; Durchführen von Maßnahmen zur Erreichung der Qualitätsmanagementziele."@de .

dtd:LA-id719 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berufs- und arbeitspädagogische Qualifikationen,"@de .

dtd:LA-id720 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundlegende Qualifikationen,"@de .

dtd:LA-id721 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Handlungsspezifische Qualifikationen."@de .

dtd:LA-id722 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundlegende Qualifikationen und"@de .

dtd:LA-id723 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Handlungsspezifische Qualifikationen."@de .

dtd:LA-id724 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine mit Erfolg abgelegte Abschlussprüfung zur Fachkraft für Wasserversorgungstechnik und danach eine mindestens einjährige Berufspraxis oder"@de .

dtd:LA-id725 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine mit Erfolg abgelegte Abschlussprüfung zum Ver- und Entsorger/zur Ver- und Entsorgerin und die elektrotechnische Qualifikation und danach eine mindestens einjährige Berufspraxis oder"@de .

dtd:LA-id726 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine mit Erfolg abgelegte Abschlussprüfung in einem sonstigen anerkannten Ausbildungsberuf und die elektrotechnische Qualifikation und danach eine mindestens zweijährige Berufspraxis oder"@de .

dtd:LA-id727 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine mindestens dreijährige Berufspraxis und die elektrotechnische Qualifikation und danach eine mindestens zweijährige Berufspraxis."@de .

dtd:LA-id728 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Ablegen des Prüfungsteils \"Grundlegende Qualifikationen\", das nicht länger als fünf Jahre zurückliegt, und"@de .

dtd:LA-id729 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu den in Absatz 1 Nr. 1 bis 4 beinhalteten Praxiszeiten mindestens ein weiteres Jahr Berufspraxis."@de .

dtd:LA-id730 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundgrößen und deren Zusammenhänge beschreiben,"@de .

dtd:LA-id731 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gefahren des elektrischen Stromes an festen und wechselnden Arbeitsplätzen erkennen,"@de .

dtd:LA-id732 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schutzmaßnahmen zur Vermeidung von Gefahren durch Strom ergreifen und veranlassen,"@de .

dtd:LA-id733 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verhaltensweisen bei Unfällen durch elektrischen Strom beschreiben und erste Maßnahmen einleiten,"@de .

dtd:LA-id734 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Messgeräte und Arbeitsmittel auswählen und handhaben,"@de .

dtd:LA-id735 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "betriebsspezifische Schaltpläne lesen,"@de .

dtd:LA-id736 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sicherungen, Sensoren, Messeinrichtungen, Beleuchtungsmittel und Signallampen prüfen und austauschen,"@de .

dtd:LA-id737 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebsstörungen beurteilen, Anlagenteile, insbesondere Pumpen und Motoren austauschen und wieder in Betrieb nehmen,"@de .

dtd:LA-id738 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "unmittelbar freischaltbare elektrische Bauteile außerhalb von Schaltschränken austauschen,"@de .

dtd:LA-id739 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ersatzstromerzeuger einsetzen und bedienen,"@de .

dtd:LA-id740 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Batterieanlagen einsetzen, prüfen und warten."@de .

dtd:LA-id741 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rechtsbewusstes Handeln;"@de .

dtd:LA-id742 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebswirtschaftliches Handeln;"@de .

dtd:LA-id743 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anwenden von Methoden der Information, Kommunikation und Planung;"@de .

dtd:LA-id744 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zusammenarbeit im Betrieb;"@de .

dtd:LA-id745 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen naturwissenschaftlicher und technischer Gesetzmäßigkeiten."@de .

dtd:LA-id746 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen arbeitsrechtlicher Vorschriften und Bestimmungen bei der Gestaltung individueller Arbeitsverhältnisse und bei Fehlverhalten von Mitarbeitern und Mitarbeiterinnen, insbesondere unter Berücksichtigung des Arbeitsvertragsrechts, des Tarifvertragsrechts und betrieblicher Vereinbarungen;"@de .

dtd:LA-id747 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen der Vorschriften des Betriebsverfassungsgesetzes und des Personalvertretungsrechts;"@de .

dtd:LA-id748 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen rechtlicher Bestimmungen hinsichtlich der Sozialversicherungen, der Entgeltfindung sowie der Arbeitsförderung;"@de .

dtd:LA-id749 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen arbeitsschutz- und arbeitssicherheitsrechtlicher Vorschriften und Bestimmungen in Abstimmung mit betrieblichen und außerbetrieblichen Institutionen;"@de .

dtd:LA-id750 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen der Vorschriften des Umweltrechts, insbesondere hinsichtlich des Gewässer- und Bodenschutzes, der Kreislauf- und Abfallwirtschaft, der Luftreinhaltung und Lärmbekämpfung, des Strahlenschutzes und des Schutzes vor gefährlichen Stoffen;"@de .

dtd:LA-id751 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen wirtschaftsrechtlicher Vorschriften und Bestimmungen sowie des Datenschutzes."@de .

dtd:LA-id752 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen von ökonomischen Handlungsprinzipien unter Einbeziehung volkswirtschaftlicher Zusammenhänge und sozialer Wirkungen;"@de .

dtd:LA-id753 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen der Grundsätze betrieblicher Aufbau- und Ablauforganisation;"@de .

dtd:LA-id754 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nutzen der Möglichkeiten der Organisationsentwicklung;"@de .

dtd:LA-id755 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anwenden von Methoden der Entgeltfindung und der kontinuierlichen betrieblichen Verbesserung;"@de .

dtd:LA-id756 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Durchführen von Kostenarten-, Kostenstellen- und Kostenträgerrechnungen sowie von Kalkulationsverfahren."@de .

dtd:LA-id757 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erfassen, Analysieren und Aufbereiten von Betriebs-, Produkt- und Prozessdaten mittels EDV-Systemen und Bewerten visualisierter Daten;"@de .

dtd:LA-id758 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bewerten von Planungstechniken und Analysemethoden sowie deren Anwendungsmöglichkeiten;"@de .

dtd:LA-id759 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anwenden von Präsentationstechniken;"@de .

dtd:LA-id760 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erstellen von technischen Unterlagen, Entwürfen, Statistiken, Tabellen und Diagrammen;"@de .

dtd:LA-id761 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anwenden von Projektmanagementmethoden;"@de .

dtd:LA-id762 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auswählen und Anwenden von Informations- und Kommunikationsformen einschließlich des Einsatzes entsprechender Informations- und Kommunikationsmittel."@de .

dtd:LA-id763 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beurteilen und Fördern der beruflichen Entwicklung Einzelner unter Beachtung des bisherigen Berufsweges und unter Berücksichtigung persönlicher und sozialer Gegebenheiten;"@de .

dtd:LA-id764 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beurteilen und Berücksichtigen des Einflusses von Arbeitsorganisation und Arbeitsplatz auf das Sozialverhalten und das Betriebsklima sowie Ergreifen von Maßnahmen zu deren Verbesserung;"@de .

dtd:LA-id765 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beurteilen von Einflüssen der Gruppenstruktur auf das Gruppenverhalten und die Zusammenarbeit sowie Entwickeln und Umsetzen von Alternativen;"@de .

dtd:LA-id766 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auseinandersetzen mit eigenem und fremdem Führungsverhalten, Umsetzen von Führungsgrundsätzen;"@de .


    dtd:has_Value "bei Hotels, Gasthöfen, Pensionen und Hotels garnis zusätzlich Zahl der Gästezimmer,"@de .

dtd:LA-id140 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei den in Nummer 4 genannten Beherbergungsbetrieben mit 25 und mehr Gästezimmern darüber hinaus die Zahl der belegten und angebotenen Zimmertage; für Letztere hilfsweise die Auslastung als Prozentangabe."@de .

dtd:LA-id141 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:P-id117 a dtd:P ;
    dtd:has_Value "Art. 23 Kursivdruck: § 3 Abs. 4 Satz 1 Halbsatz 2 des Gesetzes zur vorläufigen Regelung des Rechts der Industrie- und Handelskammern eingef. durch Art. 22 Nr. 2 dieses G"@de .

dtd:P-id119 a dtd:P ;

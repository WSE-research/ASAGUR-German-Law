    dtd:has_Class "S0"@de ;
    dtd:has_Value "Vierter Abschnitt"@de .

dtd:Ident-id2898 a dtd:Ident ;
    dtd:has_Class "S1"@de ;
    dtd:has_Value "Dritter Teil"@de .

dtd:Ident-id2899 a dtd:Ident ;
    dtd:has_Class "S0"@de ;
    dtd:has_Value "Anhang I"@de .

dtd:Ident-id2900 a dtd:Ident ;
    dtd:has_Class "S0"@de ;
    dtd:has_Value "Anhang II"@de .

dtd:Ident-id2901 a dtd:Ident ;
    dtd:has_Class "S0"@de ;
    dtd:has_Value "Anhang III"@de .

dtd:Ident-id2902 a dtd:Ident ;
    dtd:has_Class "S0"@de ;
    dtd:has_Value "Anhang IV"@de .

dtd:Ident-id2903 a dtd:Ident ;
    dtd:has_Class "S0"@de ;
    dtd:has_Value "Anhang V"@de .

dtd:Ident-id2904 a dtd:Ident ;
    dtd:has_Class "S0"@de ;
    dtd:has_Value "Anhang VI"@de .

dtd:Ident-id2905 a dtd:Ident ;
    dtd:has_Class "S0"@de ;
    dtd:has_Value "Anhang VII"@de .

dtd:LA-id2558 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebsbereich der unteren Klasse:"@de .

dtd:LA-id2559 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Betriebsbereich, in dem gefährliche Stoffe in Mengen vorhanden sind, die die in Spalte 4 der Stoffliste in Anhang I genannten Mengenschwellen erreichen oder überschreiten, aber die in Spalte 5 der Stoffliste in Anhang I genannten Mengenschwellen unterschreiten;"@de .

dtd:LA-id2560 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebsbereich der oberen Klasse:"@de .

dtd:LA-id2561 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Betriebsbereich, in dem gefährliche Stoffe in Mengen vorhanden sind, die die in Spalte 5 der Stoffliste in Anhang I genannten Mengenschwellen erreichen oder überschreiten;"@de .

dtd:LA-id2562 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "benachbarter Betriebsbereich:"@de .

dtd:LA-id2563 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Betriebsbereich, der sich so nah bei einem anderen Betriebsbereich befindet, dass dadurch das Risiko oder die Folgen eines Störfalls vergrößert werden;"@de .

dtd:LA-id2564 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gefährliche Stoffe:"@de .

dtd:LA-id2565 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Stoffe oder Gemische, die in Anhang I aufgeführt sind oder die dort festgelegten Kriterien erfüllen, einschließlich in Form von Rohstoffen, Endprodukten, Nebenprodukten, Rückständen oder Zwischenprodukten;"@de .

dtd:LA-id2566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorhandensein gefährlicher Stoffe:"@de .

dtd:LA-id2567 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das tatsächliche oder vorgesehene Vorhandensein gefährlicher Stoffe oder ihr Vorhandensein im Betriebsbereich, soweit vernünftigerweise vorhersehbar ist, dass sie bei außer Kontrolle geratenen Prozessen, auch bei Lagerung in einer Anlage innerhalb des Betriebsbereichs, anfallen, und zwar in Mengen, die die in Anhang I genannten Mengenschwellen erreichen oder überschreiten;"@de .

dtd:LA-id2568 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ereignis:"@de .

dtd:LA-id2569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Störung des bestimmungsgemäßen Betriebs in einem Betriebsbereich unter Beteiligung eines oder mehrerer gefährlicher Stoffe;"@de .

dtd:LA-id2570 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Störfall:"@de .

dtd:LA-id2571 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Ereignis, das unmittelbar oder später innerhalb oder außerhalb des Betriebsbereichs zu einer ernsten Gefahr oder zu Sachschäden nach Anhang VI Teil 1 Ziffer I Nummer 4 führt;"@de .

dtd:LA-id2572 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ernste Gefahr:"@de .

dtd:LA-id2573 a dtd:LA ;
    dtd:has_DL dtd:DL-id1935 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Gefahr, bei der a)das Leben von Menschen bedroht wird oder schwerwiegende Gesundheitsbeeinträchtigungen von Menschen zu befürchten sind,b)die Gesundheit einer großen Zahl von Menschen beeinträchtigt werden kann oderc)die Umwelt, insbesondere Tiere und Pflanzen, der Boden, das Wasser, die Atmosphäre sowie Kultur- oder sonstige Sachgüter geschädigt werden können, falls durch eine Veränderung ihres Bestandes oder ihrer Nutzbarkeit das Gemeinwohl beeinträchtigt würde;"@de .

dtd:LA-id2574 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Leben von Menschen bedroht wird oder schwerwiegende Gesundheitsbeeinträchtigungen von Menschen zu befürchten sind,"@de .

dtd:LA-id2575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gesundheit einer großen Zahl von Menschen beeinträchtigt werden kann oder"@de .

dtd:LA-id2576 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Umwelt, insbesondere Tiere und Pflanzen, der Boden, das Wasser, die Atmosphäre sowie Kultur- oder sonstige Sachgüter geschädigt werden können, falls durch eine Veränderung ihres Bestandes oder ihrer Nutzbarkeit das Gemeinwohl beeinträchtigt würde;"@de .

dtd:LA-id2577 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Überwachungssystem:"@de .

dtd:LA-id2578 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "umfasst den Überwachungsplan, das Überwachungsprogramm und die Vor-Ort-Besichtigung sowie alle Maßnahmen, die von der zuständigen Behörde oder in ihrem Namen durchgeführt werden, um die Einhaltung der Bestimmungen dieser Verordnung durch die Betriebsbereiche zu überprüfen und zu fördern;"@de .

dtd:LA-id2579 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Stand der Sicherheitstechnik:"@de .

dtd:LA-id2580 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Entwicklungsstand fortschrittlicher Verfahren, Einrichtungen und Betriebsweisen, der die praktische Eignung einer Maßnahme zur Verhinderung von Störfällen oder zur Begrenzung ihrer Auswirkungen gesichert erscheinen lässt. Bei der Bestimmung des Standes der Sicherheitstechnik sind insbesondere vergleichbare Verfahren, Einrichtungen oder Betriebsweisen heranzuziehen, die mit Erfolg im Betrieb erprobt worden sind."@de .

dtd:LA-id2581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "betriebliche Gefahrenquellen,"@de .

dtd:LA-id2582 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "umgebungsbedingte Gefahrenquellen, wie Erdbeben oder Hochwasser, und"@de .

dtd:LA-id2583 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Eingriffe Unbefugter"@de .

dtd:LA-id2584 a dtd:LA ;
    dtd:has_DL dtd:DL-id1960 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zu treffen, damit Brände und Explosionen a)innerhalb des Betriebsbereichs vermieden werden,b)nicht in einer die Sicherheit beeinträchtigenden Weise von einer Anlage auf andere Anlagen des Betriebsbereichs einwirken können undc)nicht in einer die Sicherheit des Betriebsbereichs beeinträchtigenden Weise von außen auf ihn einwirken können,"@de .

dtd:LA-id2585 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "innerhalb des Betriebsbereichs vermieden werden,"@de .

dtd:LA-id2586 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht in einer die Sicherheit beeinträchtigenden Weise von einer Anlage auf andere Anlagen des Betriebsbereichs einwirken können und"@de .

dtd:LA-id2587 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht in einer die Sicherheit des Betriebsbereichs beeinträchtigenden Weise von außen auf ihn einwirken können,"@de .

dtd:LA-id2588 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zu treffen, damit Freisetzungen gefährlicher Stoffe in Luft, Wasser oder Boden vermieden werden,"@de .

dtd:LA-id2589 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Betriebsbereich mit ausreichenden Warn-, Alarm- und Sicherheitseinrichtungen auszurüsten,"@de .

dtd:LA-id2590 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anlagen des Betriebsbereichs mit zuverlässigen Messeinrichtungen und Steuer- oder Regeleinrichtungen auszustatten, die, soweit dies sicherheitstechnisch geboten ist, jeweils mehrfach vorhanden, verschiedenartig und voneinander unabhängig sind,"@de .

dtd:LA-id2591 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die sicherheitsrelevanten Teile des Betriebsbereichs vor Eingriffen Unbefugter zu schützen."@de .

dtd:LA-id2592 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zu treffen, damit durch die Beschaffenheit der Fundamente und der tragenden Gebäudeteile bei Störfällen keine zusätzlichen Gefahren hervorgerufen werden können,"@de .

dtd:LA-id2593 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anlagen des Betriebsbereichs mit den erforderlichen sicherheitstechnischen Einrichtungen auszurüsten sowie die erforderlichen technischen und organisatorischen Schutzvorkehrungen zu treffen."@de .

dtd:LA-id2594 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Errichtung und den Betrieb der sicherheitsrelevanten Anlagenteile zu prüfen sowie die Anlagen des Betriebsbereichs in sicherheitstechnischer Hinsicht ständig zu überwachen und regelmäßig zu warten,"@de .

dtd:LA-id2595 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Wartungs- und Reparaturarbeiten nach dem Stand der Technik durchzuführen,"@de .

dtd:LA-id2596 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die erforderlichen sicherheitstechnischen Vorkehrungen zur Vermeidung von Fehlbedienungen zu treffen,"@de .

dtd:LA-id2597 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch geeignete Bedienungs- und Sicherheitsanweisungen und durch Schulung des Personals Fehlverhalten vorzubeugen."@de .

dtd:LA-id2598 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "untereinander alle erforderlichen Informationen auszutauschen, damit sie in ihrem Konzept zur Verhinderung von Störfällen, in ihren Sicherheitsmanagementsystemen, in ihren Sicherheitsberichten und ihren internen Alarm- und Gefahrenabwehrplänen der Art und dem Ausmaß der Gesamtgefahr eines Störfalls Rechnung tragen können, und"@de .

dtd:LA-id2599 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Information der Öffentlichkeit und benachbarter Betriebsstätten, die nicht unter den Anwendungsbereich dieser Verordnung fallen, sowie zur Übermittlung von Angaben an die für die Erstellung von externen Alarm- und Gefahrenabwehrplänen zuständige Behörde zusammenzuarbeiten."@de .

dtd:LA-id2600 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Möglichkeit des Eintritts eines Störfalls in voller Sachkenntnis beurteilen kann,"@de .

dtd:LA-id2601 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ermitteln kann, inwieweit sich die Wahrscheinlichkeit des Eintritts eines Störfalls erhöhen kann oder die Auswirkungen eines Störfalls verschlimmern können,"@de .

dtd:LA-id2602 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Entscheidungen über die Ansiedlung oder die störfallrelevante Änderung von Betriebsbereichen sowie über Entwicklungen in der Nachbarschaft von Betriebsbereichen treffen kann,"@de .

dtd:LA-id2603 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "externe Alarm- und Gefahrenabwehrpläne erstellen kann und"@de .

dtd:LA-id2604 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Stoffe berücksichtigen kann, die auf Grund ihrer physikalischen Form, ihrer besonderen Merkmale oder des Ortes, an dem sie vorhanden sind, zusätzliche Vorkehrungen erfordern."@de .

dtd:LA-id2605 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name oder Firma des Betreibers sowie vollständige Anschrift des betreffenden Betriebsbereichs,"@de .

dtd:LA-id2606 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eingetragener Firmensitz und vollständige Anschrift des Betreibers,"@de .

dtd:LA-id2607 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name und Funktion der für den Betriebsbereich verantwortlichen Person, falls von der unter Nummer 1 genannten Person abweichend,"@de .

dtd:LA-id2608 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ausreichende Angaben zur Identifizierung der gefährlichen Stoffe und der Gefahrenkategorie von Stoffen, die gemäß § 2 Nummer 5 vorhanden sind,"@de .

dtd:LA-id2609 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Menge und physikalische Form der gefährlichen Stoffe,"@de .

dtd:LA-id2610 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tätigkeit oder beabsichtigte Tätigkeit in den Anlagen des Betriebsbereichs,"@de .

dtd:LA-id2611 a dtd:LA ;
    dtd:has_DL dtd:DL-id2012 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gegebenheiten in der unmittelbaren Umgebung des Betriebsbereichs, die einen Störfall auslösen oder dessen Folgen verschlimmern können, einschließlich, soweit verfügbar, Einzelheiten zu a)benachbarten Betriebsbereichen,b)anderen Betriebsstätten, die nicht unter den Anwendungsbereich dieser Verordnung fallen, undc)Bereichen und Entwicklungen, von denen ein Störfall ausgehen könnte oder bei denen sich die Wahrscheinlichkeit des Eintritts eines Störfalls erhöhen kann oder die Auswirkungen eines Störfalls und von Domino-Effekten nach § 15 verschlimmern können."@de .

dtd:LA-id2612 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "benachbarten Betriebsbereichen,"@de .

dtd:LA-id2613 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "anderen Betriebsstätten, die nicht unter den Anwendungsbereich dieser Verordnung fallen, und"@de .

dtd:LA-id2614 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bereichen und Entwicklungen, von denen ein Störfall ausgehen könnte oder bei denen sich die Wahrscheinlichkeit des Eintritts eines Störfalls erhöhen kann oder die Auswirkungen eines Störfalls und von Domino-Effekten nach § 15 verschlimmern können."@de .

dtd:LA-id2615 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Änderungen der Angaben nach Absatz 1 Nummer 1 bis 3 und"@de .

dtd:LA-id2616 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einstellung des Betriebs, des Betriebsbereichs oder einer Anlage des Betriebsbereichs."@de .

dtd:LA-id2617 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens alle fünf Jahre nach erstmaliger Erstellung oder Änderung,"@de .

dtd:LA-id2618 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vor einer Änderung nach § 7 Absatz 3 und"@de .

dtd:LA-id2619 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "unverzüglich nach einem Ereignis nach Anhang VI Teil 1."@de .

dtd:LA-id2620 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Konzept zur Verhinderung von Störfällen umgesetzt wurde und ein Sicherheitsmanagementsystem zu seiner Anwendung gemäß Anhang III vorhanden ist und umgesetzt wurde,"@de .

dtd:LA-id2621 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gefahren von Störfällen und mögliche Störfallszenarien ermittelt sowie alle erforderlichen Maßnahmen zur Verhinderung derartiger Störfälle und zur Begrenzung ihrer Auswirkungen auf die menschliche Gesundheit und die Umwelt ergriffen wurden,"@de .

dtd:LA-id2622 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Auslegung, die Errichtung sowie der Betrieb und die Wartung sämtlicher Teile eines Betriebsbereichs, die im Zusammenhang mit der Gefahr von Störfällen im Betriebsbereich stehen, ausreichend sicher und zuverlässig sind,"@de .

dtd:LA-id2623 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "interne Alarm- und Gefahrenabwehrpläne vorliegen und die erforderlichen Informationen zur Erstellung externer Alarm- und Gefahrenabwehrpläne gegeben werden sowie"@de .

dtd:LA-id2624 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ausreichende Informationen bereitgestellt werden, damit die zuständige Behörde Entscheidungen über die Ansiedlung neuer Tätigkeiten oder Entwicklungen in der Nachbarschaft bestehender Betriebsbereiche treffen kann."@de .

dtd:LA-id2625 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens alle fünf Jahre,"@de .

dtd:LA-id2626 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einer störfallrelevanten Änderung nach § 3 Absatz 5b des Bundes-Immissionsschutzgesetzes,"@de .

dtd:LA-id2627 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach einem Ereignis nach Anhang VI Teil 1 und"@de .

dtd:LA-id2628 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu jedem anderen Zeitpunkt, wenn neue Umstände dies erfordern, oder um den neuen sicherheitstechnischen Kenntnisstand sowie aktuelle Erkenntnisse zur Beurteilung der Gefahren zu berücksichtigen."@de .

dtd:LA-id2629 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "interne Alarm- und Gefahrenabwehrpläne zu erstellen, die die in Anhang IV aufgeführten Informationen enthalten müssen, und"@de .

dtd:LA-id2630 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der zuständigen Behörde die für die Erstellung externer Alarm- und Gefahrenabwehrpläne erforderlichen Informationen zu übermitteln."@de .

dtd:LA-id2631 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens alle drei Jahre und"@de .

dtd:LA-id2632 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einer störfallrelevanten Änderung nach § 3 Absatz 5b des Bundes-Immissionsschutzgesetzes."@de .

dtd:LA-id2633 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Verlangen der zuständigen Behörde zu einer von ihr benannten, zur Informationsweitergabe geeigneten Stelle der öffentlichen Verwaltung eine jederzeit verfügbare und gegen Missbrauch geschützte Verbindung einzurichten und zu unterhalten sowie"@de .

dtd:LA-id2634 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Person oder Stelle mit der Begrenzung der Auswirkungen von Störfällen zu beauftragen und diese der zuständigen Behörde zu benennen."@de .

dtd:LA-id2635 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Prüfung der Errichtung und des Betriebs der sicherheitsrelevanten Anlagenteile,"@de .

dtd:LA-id2636 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Überwachung und regelmäßigen Wartung der Anlage in sicherheitstechnischer Hinsicht,"@de .

dtd:LA-id2637 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der sicherheitsrelevanten Wartungs- und Reparaturarbeiten sowie"@de .

dtd:LA-id2638 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Funktionsprüfungen der Warn-, Alarm- und Sicherheitseinrichtungen"@de .

dtd:LA-id2639 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angaben, die der Betreiber in der Anzeige nach § 7 und im Sicherheitsbericht nach § 9 übermittelt hat,"@de .

dtd:LA-id2640 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angaben, die im Anschluss an ein Ersuchen der zuständigen Behörde um zusätzliche Auskünfte vom Betreiber übermittelt wurden, und"@de .

dtd:LA-id2641 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Informationen, die die zuständige Behörde durch Überwachungsmaßnahmen erlangt hat."@de .

dtd:LA-id2642 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass der Betreiber nachweisen kann, dass er im Zusammenhang mit den verschiedenen betriebsspezifischen Tätigkeiten die zur Verhinderung von Störfällen erforderlichen Maßnahmen ergriffen hat,"@de .

dtd:LA-id2643 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass der Betreiber nachweisen kann, dass er angemessene Mittel zur Begrenzung von Störfallauswirkungen innerhalb und außerhalb des Betriebsbereichs vorgesehen hat,"@de .

dtd:LA-id2644 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass die im Sicherheitsbericht oder in anderen vorgelegten Berichten enthaltenen Angaben und Informationen die Gegebenheiten in dem Betriebsbereich zutreffend wiedergeben,"@de .

dtd:LA-id2645 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass die Informationen nach § 8a Absatz 1 und § 11 Absatz 1 der Öffentlichkeit zugänglich gemacht worden sind und dass die Informationen nach § 11 Absatz 3 erfolgt sind."@de .

dtd:LA-id2646 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach jeder Vor-Ort-Besichtigung von der zuständigen Behörde ein Bericht erstellt wird, welcher die relevanten Feststellungen der Behörde und erforderlichen Folgemaßnahmen enthält,"@de .

dtd:LA-id2647 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bericht dem Betreiber innerhalb von vier Monaten nach der Vor-Ort-Besichtigung durch die zuständige Behörde übermittelt wird,"@de .

dtd:LA-id2648 a dtd:LA ;
    dtd:has_DL dtd:DL-id2100 ;
    dtd:has_Size dtd:normal ;

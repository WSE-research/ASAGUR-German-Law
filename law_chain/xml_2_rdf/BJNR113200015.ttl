dtd:LA-id169 a dtd:LA ;
    dtd:has_Value "in gerader Linie verwandte oder verschwägerte Personen,"@de .

dtd:LA-id170 a dtd:LA ;
    dtd:has_Value "Dauereinsatzaufgaben Einsatzaufgaben, die die Streitkräfte aufgrund nationaler oder internationaler Verpflichtungen ständig zu erfüllen haben,"@de .

dtd:LA-id171 a dtd:LA ;
    dtd:has_Value "einsatzgleiche Verpflichtungen für bestimmte Zeiträume gegenüber den Vereinten Nationen, der NATO oder der Europäischen Union erklärte Übernahme von Aufgaben mit Einsatzcharakter,"@de .


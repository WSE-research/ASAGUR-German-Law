    dtd:has_Size dtd:normal ;
    dtd:has_Value "zwei Jahre ununterbrochen als Selbständiger oder in leitender Stellung sowie außerdem in dem betreffenden Beruf drei Jahre als Unselbständiger oder"@de .

dtd:LA-id110 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "drei Jahre ununterbrochen als Unselbständiger, wenn er für den betreffenden Beruf eine vorherige Ausbildung nachweisen kann, die durch ein staatlich anerkanntes Zeugnis bestätigt oder von einer zuständigen Berufsinstitution als vollwertig anerkannt ist."@de .

dtd:LA-id111 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Leiter des Unternehmens oder einer Zweigniederlassung;"@de .

dtd:LA-id112 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Stellvertreter des Unternehmers oder des Leiters des Unternehmens, wenn mit dieser Stellung eine Verantwortung verbunden ist, die der des vertretenden Unternehmers oder Leiters entspricht oder"@de .

dtd:LA-id113 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in leitender Stellung mit kaufmännischen Aufgaben und mit der Verantwortung für mindestens eine Abteilung des Unternehmens."@de .

dtd:LA-id114 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sechs Jahre ununterbrochen als Selbständiger oder als Betriebsleiter,"@de .

dtd:LA-id115 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "drei Jahre ununterbrochen als Selbständiger oder als Betriebsleiter, nachdem er in dem betreffenden Beruf eine dreijährige Ausbildung erhalten hat,"@de .

dtd:LA-id116 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "drei Jahre ununterbrochen als Selbständiger sowie außerdem in dem betreffenden Beruf fünf Jahre als Unselbständiger oder"@de .

dtd:LA-id117 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fünf Jahre ununterbrochen in leitender Stellung, davon mindestens drei Jahre in einer Tätigkeit mit technischen Aufgaben und der Verantwortung für mindestens eine Abteilung des Unternehmens, nachdem er in dem betreffenden Beruf eine dreijährige Ausbildung erhalten hat."@de .

dtd:LA-id118 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Angehörige des Handwerks und handwerksähnlicher Berufe: die örtlich zuständige Handwerkskammer;"@de .

dtd:LA-id119 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Angehörige anderer Berufe, vorbehaltlich der Zuständigkeit anderer öffentlich-rechtlicher Berufskammern für die ihnen zugehörigen Berufsangehörigen: die örtlich zuständige Industrie- und Handelskammer."@de .


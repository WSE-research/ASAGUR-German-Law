dtd:LA-id1260 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Wiedereinfhrung der gesamten oder eines Teils der Nukleinsuresequenz (oder eines synthetischen quivalents) in Zellen derselben Art oder in Zellen phylogenetisch eng verwandter Arten, die genetisches Material durch natrliche physiologische Prozesse austauschen knnen, und"@de .

dtd:LA-id1261 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer eventuell vorausgehenden enzymatischen oder mechanischen Behandlung."@de .

dtd:LA-id1262 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gentechnische Anlage"@de .

dtd:LA-id1263 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einrichtung, in der gentechnische Arbeiten im Sinne der Nummer 2 im geschlossenen System durchgefhrt werden und bei der spezifische Einschlieungsmanahmen angewendet werden, um den Kontakt der verwendeten Organismen mit Menschen und der Umwelt zu begrenzen und ein dem Gefhrdungspotenzial angemessenes Sicherheitsniveau zu gewhrleisten,"@de .

dtd:LA-id1264 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Freisetzung"@de .

dtd:LA-id1265 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das gezielte Ausbringen von gentechnisch vernderten Organismen in die Umwelt, soweit noch keine Genehmigung fr das Inverkehrbringen zum Zweck des spteren Ausbringens in die Umwelt erteilt wurde,"@de .

dtd:LA-id1266 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Inverkehrbringen"@de .

dtd:LA-id1267 a dtd:LA ;
    dtd:has_DL dtd:DL-id803 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Abgabe von Produkten an Dritte, einschlielich der Bereitstellung fr Dritte, und das Verbringen in den Geltungsbereich des Gesetzes, soweit die Produkte nicht zu gentechnischen Arbeiten in gentechnischen Anlagen oder fr genehmigte Freisetzungen bestimmt sind; jedoch gelten a)unter zollamtlicher berwachung durchgefhrter Transitverkehr,b)die Bereitstellung fr Dritte, die Abgabe sowie das Verbringen in den Geltungsbereich des Gesetzes zum Zweck einer genehmigten klinischen Prfungnicht als Inverkehrbringen,"@de .

dtd:LA-id1268 a dtd:LA ;
    dtd:has_Value "unter zollamtlicher berwachung durchgefhrter Transitverkehr,"@de .

dtd:LA-id1269 a dtd:LA ;
    dtd:has_Value "die Bereitstellung fr Dritte, die Abgabe sowie das Verbringen in den Geltungsbereich des Gesetzes zum Zweck einer genehmigten klinischen Prfung"@de .

dtd:LA-id1270 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Umgang mit gentechnisch vernderten Organismen"@de .

dtd:LA-id1271 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anwendung, Vermehrung, Anbau, Lagerung, Befrderung und Beseitigung sowie Verbrauch und sonstige Verwendung und Handhabung von zum Inverkehrbringen zugelassenen Produkten, die gentechnisch vernderte Organismen enthalten oder daraus bestehen,"@de .

dtd:LA-id1272 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Risikomanagement"@de .

dtd:LA-id1273 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der von der Risikobewertung unterschiedene Prozess der Abwgung von Alternativen bei der Vermeidung oder Beherrschung von Risiken,"@de .

dtd:LA-id1274 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betreiber"@de .

dtd:LA-id1275 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine juristische oder natrliche Person oder eine sonstige Personenvereinigung, die unter ihrem Namen eine gentechnische Anlage errichtet oder betreibt, gentechnische Arbeiten oder Freisetzungen durchfhrt oder Produkte, die gentechnisch vernderte Organismen enthalten oder aus solchen bestehen, erstmalig in Verkehr bringt; wenn eine Genehmigung nach  16 Abs. 2 erteilt worden ist, die nach  14 Abs. 1 Satz 2 das Inverkehrbringen auch der Nachkommen oder des Vermehrungsmaterials gestattet, ist insoweit nur der Genehmigungsinhaber Betreiber,"@de .

dtd:LA-id1276 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Projektleiter"@de .

dtd:LA-id1277 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Person, die im Rahmen ihrer beruflichen Obliegenheiten die unmittelbare Planung, Leitung oder Beaufsichtigung einer gentechnischen Arbeit oder einer Freisetzung durchfhrt,"@de .

dtd:LA-id1278 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beauftragter fr die Biologische Sicherheit"@de .

dtd:LA-id1279 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Person oder eine Mehrheit von Personen (Ausschu fr Biologische Sicherheit), die die Erfllung der Aufgaben des Projektleiters berprft und den Betreiber bert,"@de .

dtd:LA-id1280 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sicherheitsstufen"@de .

dtd:LA-id1281 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gruppen gentechnischer Arbeiten nach ihrem Gefhrdungspotential,"@de .

dtd:LA-id1282 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Laborsicherheitsmanahmen oder Produktionssicherheitsmanahmen"@de .

dtd:LA-id1283 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "festgelegte Arbeitstechniken und eine festgelegte Ausstattung von gentechnischen Anlagen,"@de .

dtd:LA-id1284 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "biologische Sicherheitsmanahme"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "Produkte in den Verkehr bringt, die aus freigesetzten gentechnisch vernderten Organismen gewonnen oder hergestellt wurden, fr die keine Genehmigung nach Nummer 2 vorliegt."@de .

dtd:LA-id1334 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit in  3 Nr. 3c genannten Verfahren hergestellt worden sind und"@de .

dtd:LA-id1335 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in eine Anlage abgegeben werden, in der Einschlieungsmanahmen nach Magabe des Satzes 2 angewandt werden."@de .

dtd:LA-id1336 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id1337 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fr die Genehmigung der Freisetzung ein von dem Verfahren des Dritten Teils dieses Gesetzes abweichendes vereinfachtes Verfahren gilt,"@de .

dtd:LA-id1338 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fr Genehmigungen nach Nummer 1 der Absatz 3 entsprechend anzuwenden ist,"@de .

dtd:LA-id1339 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Namen und die Anschrift des Betreibers,"@de .

dtd:LA-id1340 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beschreibung des Freisetzungsvorhabens hinsichtlich seines Zweckes und Standortes, des Zeitpunktes und des Zeitraums,"@de .

dtd:LA-id1341 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die dem Stand der Wissenschaft entsprechende Beschreibung der sicherheitsrelevanten Eigenschaften des freizusetzenden Organismus und der Umstnde, die fr das berleben, die Fortpflanzung und die Verbreitung des Organismus von Bedeutung sind; Unterlagen ber vorangegangene Arbeiten in einer gentechnischen Anlage und ber Freisetzungen sind beizufgen,"@de .

dtd:LA-id1342 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Risikobewertung nach  6 Abs. 1 und eine Darlegung der vorgesehenen Sicherheitsvorkehrungen,"@de .

dtd:LA-id1343 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Plan zur Ermittlung der Auswirkung des freizusetzenden Organismus auf die menschliche Gesundheit und die Umwelt,"@de .


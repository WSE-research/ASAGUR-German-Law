    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu bestätigen, dass eine Annahme über das Auftreten und die Wirkung einer etwaigen schädlichen Auswirkung eines gentechnisch veränderten Organismus oder dessen Verwendung in der Risikobewertung zutrifft (fallspezifische Beobachtung), und"@de .

dtd:LA-id1379 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Auftreten schädlicher Auswirkungen des gentechnisch veränderten Organismus oder dessen Verwendung auf die menschliche Gesundheit oder die Umwelt zu ermitteln, die in der Risikobewertung nicht vorhergesehen wurden (allgemeine Beobachtung)."@de .

dtd:LA-id1380 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Verwendungszweck,"@de .

dtd:LA-id1381 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die besonderen Bedingungen für den Umgang mit dem Produkt und seine Verpackung,"@de .

dtd:LA-id1382 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bedingungen für den Schutz besonderer Ökosysteme, Umweltgegebenheiten oder geographischer Gebiete,"@de .

dtd:LA-id1383 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kennzeichnung,"@de .

dtd:LA-id1384 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anforderungen an die Einzelheiten der Beobachtung auf der Grundlage der Risikobewertung, die Laufzeit des Beobachtungsplans,"@de .

dtd:LA-id1385 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vorlagepflicht für Kontrollproben."@de .

dtd:LA-id1386 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "allgemeine Merkmale oder Beschreibung der gentechnisch veränderten Organismen,"@de .

dtd:LA-id1387 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name und Anschrift des Betreibers,"@de .

dtd:LA-id1388 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ort der gentechnischen Anlage oder Freisetzung und der Freisetzungszweck,"@de .

dtd:LA-id1389 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "beabsichtigte Verwendung,"@de .

dtd:LA-id1390 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sicherheitsstufe und Sicherheitsmaßnahmen,"@de .

dtd:LA-id1391 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Methoden und Pläne zur Überwachung der gentechnisch veränderten Organismen und für Notfallmaßnahmen,"@de .

dtd:LA-id1392 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Risikobewertung."@de .

dtd:LA-id1393 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu den Betriebs- und Geschäftszeiten Grundstücke, Geschäftsräume und Betriebsräume zu betreten und zu besichtigen,"@de .

dtd:LA-id1394 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle zur Erfüllung ihrer Aufgaben erforderlichen Prüfungen einschließlich der Entnahme von Proben durchzuführen,"@de .


    dtd:has_Value "mechanische Eigenschaften: abriebfest, reißfest und aus luftundurchlässigem Material,"@de .

dtd:LA-id2951 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "chemische Eigenschaften: beständig gegenüber dem bei der Desinfektionsdusche verwendeten Desinfektionsmittel und gegenüber den bei den Arbeiten verwendeten Chemikalien."@de .

dtd:LA-id2952 a dtd:LA ;

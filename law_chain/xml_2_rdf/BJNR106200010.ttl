    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Absatz 1 Satz 1, auch in Verbindung mit Satz 4, eine Aufzeichnung nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig erstellt,"@de .


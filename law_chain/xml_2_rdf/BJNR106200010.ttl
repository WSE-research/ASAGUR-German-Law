    dtd:has_Value "soweit die Handlungen innerhalb eines Umkreises von 50 Kilometern um den Betrieb, in dem die Stoffe angefallen sind, a)innerhalb eines Betriebes,b)zwischen zwei Betrieben desselben Verfügungsberechtigtenvorgenommen werden,"@de .

dtd:LA-id151 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "innerhalb eines Betriebes,"@de .

dtd:LA-id152 a dtd:LA ;

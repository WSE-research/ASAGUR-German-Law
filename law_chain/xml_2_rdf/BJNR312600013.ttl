    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur bei groen Reptilien angewendet werden."@de .

dtd:Footnote-id1892 a dtd:Footnote ;
    dtd:has_FnZ "3"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f781900_04_BJNR312600013BJNE005100001"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur unter schrittweiser Befllung des Behltnisses angewendet werden. Das Verfahren darf nicht bei Ften und Neugeborenen angewendet werden."@de .

dtd:Footnote-id1893 a dtd:Footnote ;

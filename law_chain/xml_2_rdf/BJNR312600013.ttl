    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur bei Neugeborenen angewendet werden."@de .

dtd:Footnote-id1900 a dtd:Footnote ;
    dtd:has_FnZ "11"@de ;
    dtd:has_Group dtd:manuell ;

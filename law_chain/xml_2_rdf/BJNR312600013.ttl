    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur bei Vögeln mit einem Gewicht von unter 5 kg angewendet werden."@de .

dtd:Footnote-id1897 a dtd:Footnote ;

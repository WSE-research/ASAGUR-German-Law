dtd:Footnote-id1890 a dtd:Footnote ;
    dtd:has_FnZ "1"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f781900_02_BJNR312600013BJNE005100001"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren muss in Verbindung mit einem vorherigen Sedieren der Tiere eingesetzt werden, es sei denn, dies ist unangemessen."@de .

dtd:Footnote-id1891 a dtd:Footnote ;
    dtd:has_FnZ "2"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f781900_03_BJNR312600013BJNE005100001"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur bei großen Reptilien angewendet werden."@de .

dtd:Footnote-id1892 a dtd:Footnote ;
    dtd:has_FnZ "3"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f781900_04_BJNR312600013BJNE005100001"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur unter schrittweiser Befüllung des Behältnisses angewendet werden. Das Verfahren darf nicht bei Föten und Neugeborenen angewendet werden."@de .

dtd:Footnote-id1893 a dtd:Footnote ;
    dtd:has_FnZ "4"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f781900_05_BJNR312600013BJNE005100001"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur bei Vögeln mit einem Gewicht von unter 1 kg angewendet werden. Vögel mit einem Gewicht von über 250 g müssen zuvor sediert werden."@de .

dtd:Footnote-id1894 a dtd:Footnote ;
    dtd:has_FnZ "5"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f781900_06_BJNR312600013BJNE005100001"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur bei Nagetieren mit einem Gewicht von unter 1 kg angewendet werden. Nagetiere mit einem Gewicht von über 150 g müssen zuvor sediert werden."@de .

dtd:Footnote-id1895 a dtd:Footnote ;
    dtd:has_FnZ "6"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f781900_07_BJNR312600013BJNE005100001"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur bei Kaninchen mit einem Gewicht von unter 1 kg angewendet werden. Kaninchen mit einem Gewicht von über 150 g müssen zuvor sediert werden."@de .

dtd:Footnote-id1896 a dtd:Footnote ;

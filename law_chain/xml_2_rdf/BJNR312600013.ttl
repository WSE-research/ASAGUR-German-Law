    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f781900_05_BJNR312600013BJNE005100001"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur bei Vgeln mit einem Gewicht von unter 1 kg angewendet werden. Vgel mit einem Gewicht von ber 250 g mssen zuvor sediert werden."@de .

dtd:Footnote-id1894 a dtd:Footnote ;

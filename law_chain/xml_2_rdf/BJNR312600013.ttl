    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur bei Vögeln mit einem Gewicht von unter 250 g angewendet werden."@de .

dtd:Footnote-id1901 a dtd:Footnote ;
    dtd:has_FnZ "12"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f781900_13_BJNR312600013BJNE005100001"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur angewendet werden, wenn die Anwendung anderer Verfahren nicht möglich ist."@de .

dtd:Footnote-id1902 a dtd:Footnote ;
    dtd:has_FnZ "13"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f781900_14_BJNR312600013BJNE005100001"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Für die Anwendung des Verfahrens sind dafür geeignete Anlagen und Geräte erforderlich."@de .

dtd:Footnote-id1903 a dtd:Footnote ;
    dtd:has_FnZ "14"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f781900_15_BJNR312600013BJNE005100001"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur bei Schweinen angewendet werden."@de .

dtd:Footnote-id1904 a dtd:Footnote ;
    dtd:has_FnZ "15"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f781900_16_BJNR312600013BJNE005100001"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur in den Fällen des § 2 Absatz 1 Satz 3 und nur von einem erfahrenen Schützen angewendet werden."@de .

dtd:Footnote-id1905 a dtd:Footnote ;
    dtd:has_FnZ "16"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f781900_17_BJNR312600013BJNE005100001"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Verfahren darf nur in den Fällen des § 2 Absatz 1 Satz 3 und nur von einem erfahrenen Schützen angewendet werden. Darüber hinaus darf es nur dann angewendet werden, wenn die Anwendung anderer Verfahren nicht möglich ist."@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sicherungsanlagen,"@de .

dtd:LA-id725 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schalt- und Steuerungsanlagen und"@de .

dtd:LA-id726 a dtd:LA ;

    dtd:has_Value "Anlagen zur Energiezuführung."@de .

dtd:LA-id727 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu Versuchs- und Probezwecken von den Vorschriften dieser Verordnung,"@de .

dtd:LA-id728 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Einzelfall aus besonderen Gründen von den Vorschriften des § 13 Abs. 2 Satz 2, des § 15 Abs. 1 Satz 3 und des § 22 Abs. 3"@de .

dtd:LA-id729 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die bauaufsichtliche Genehmigung zugestellt worden ist und"@de .

dtd:LA-id730 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Unternehmer den Beginn der Bauarbeiten dem Eisenbahn-Bundesamt mindestens eine Woche vorher schriftlich angezeigt hat."@de .

dtd:LA-id731 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beseitigung anordnen,"@de .

dtd:LA-id732 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Nutzung untersagen oder"@de .

dtd:LA-id733 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Räumung anordnen,"@de .

dtd:LA-id734 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in welcher Lage sich die beweglichen Fahrwegelemente befinden und"@de .

dtd:LA-id735 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "daß diese gegen Veränderung ihrer Lage gesichert sind."@de .

dtd:LA-id736 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der konstruktiven Gestaltung und Ausrüstung die für Schienenfahrzeuge geltenden Anforderungen gemäß der höchsten Brandschutzstufe nach DIN 5510 Teil 1, Ausgabe Oktober 1988, beachtet werden,"@de .

dtd:LA-id737 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fahrgasträume so beschaffen sein, daß ein systemeigener Brand nicht entstehen kann,"@de .

dtd:LA-id738 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "beim Brand in einer Fahrzeugsektion die Personen in den anderen Fahrzeugsektionen bis zu ihrer Rettung, mindestens jedoch 30 Minuten, geschützt sein,"@de .

dtd:LA-id739 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Fahrzeuge mit automatischen Brandmeldern und tragbaren Feuerlöschern ausgerüstet sein."@de .

dtd:LA-id740 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Einrichtungen versehen sein, die ein Anfahren verhindern, bevor die Außentüren in geschlossener Stellung verriegelt sind, und den verriegelten Zustand der Außentüren während der Fahrt überwachen,"@de .

dtd:LA-id741 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Stillstand eine Entriegelung der Außentüren zulassen,"@de .

dtd:LA-id742 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Notrufeinrichtungen ausgerüstet sein, mit denen eine Betriebszentrale im Gegensprechverkehr erreicht werden kann,"@de .

dtd:LA-id743 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Mitteln zur Leistung von Erster Hilfe ausgerüstet sein."@de .

dtd:LA-id744 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Fahrzeuge betriebsbereit sind,"@de .

dtd:LA-id745 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Fahrzeuge den baulichen, betrieblichen und sicherungstechnischen Verhältnissen des Fahrwegs entsprechen,"@de .

dtd:LA-id746 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die erforderlichen Sicherungssysteme wirksam sind,"@de .

dtd:LA-id747 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Fahrweg frei von erkennbaren Hindernissen ist und die beweglichen Fahrwegelemente richtig eingestellt sowie gegen Veränderung ihrer Lage gesichert sind,"@de .

dtd:LA-id748 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von ihnen sowie von anderen Fahrten oder nicht technisch gesicherten Fahrzeugbewegungen keine Gefährdung ausgeht,"@de .

dtd:LA-id749 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Fahrzeuge und Betriebsanlagen mit den für die Durchführung des Betriebs erforderlichen Betriebsbediensteten besetzt sind,"@de .

dtd:LA-id750 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einrichtungen, die der Steuerung oder Überwachung des Betriebsablaufs dienen, jeweils mit mindestens zwei Betriebsbediensteten besetzt sind."@de .

dtd:LA-id751 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fahrbetrieb,"@de .

dtd:LA-id752 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Steuerung oder Überwachung des Betriebsablaufs,"@de .

dtd:LA-id753 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Verantwortlicher bei der Instandhaltung der Betriebsanlagen oder Fahrzeuge,"@de .

dtd:LA-id754 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Leitender oder Aufsichtsführender über Betriebspersonal nach den Nummern 1 bis 3"@de .

dtd:LA-id755 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 4 Abs. 1 den Fahrbetrieb aufnimmt,"@de .

dtd:LA-id756 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 6 Abs. 1 Satz 1 eine Betriebsanlage oder ein Fahrzeug in Betrieb nimmt,"@de .

dtd:LA-id757 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ohne Genehmigung nach § 9 Abs. 1 eine bauliche Anlage errichtet, ändert, abbricht oder die Nutzung verändert,"@de .

dtd:LA-id758 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 16 eine Betriebsanlage oder deren Umfeld nicht oder nicht in der vorgeschriebenen Weise überwacht,"@de .

dtd:LA-id759 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 21 Abs. 2 eine überwachungsbedürftige Anlage nicht oder nicht rechtzeitig prüfen läßt,"@de .

dtd:LA-id760 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 25 Abs. 1 Satz 1 eine Meldung nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig macht,"@de .

dtd:LA-id761 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 25 Abs. 2 Satz 1 Daten nicht aufzeichnet,"@de .

dtd:LA-id762 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 25 Abs. 3 eine Aufzeichnung nicht oder nicht mindestens 5 Arbeitstage aufbewahrt oder nicht oder nicht rechtzeitig vorlegt,"@de .

dtd:LA-id763 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 29 Satz 1 eine Betriebsanlage oder ein Fahrzeug betritt oder benutzt,"@de .

dtd:LA-id764 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 30 eine Einrichtung betätigt, ein Fahrthindernis bereitet oder eine andere betriebsstörende oder betriebsgefährdende Handlung vornimmt oder"@de .

dtd:LA-id765 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 31 im sicherheitsrelevanten Betriebsbereich tätig wird."@de .


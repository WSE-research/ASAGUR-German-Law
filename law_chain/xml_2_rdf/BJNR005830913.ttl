dtd:LA-id664 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Geburt (§ 4),"@de .

dtd:LA-id665 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Erklärung (§ 5),"@de .

dtd:LA-id666 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Annahme als Kind (§ 6),"@de .

dtd:LA-id667 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Ausstellung der Bescheinigung nach § 15 Absatz 1 oder 2 des Bundesvertriebenengesetzes (§ 7),"@de .

dtd:LA-id668 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Einbürgerung (§§ 8 bis 16, 40b und 40c)."@de .

dtd:LA-id669 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seit acht Jahren rechtmäßig seinen gewöhnlichen Aufenthalt im Inland hat und"@de .

dtd:LA-id670 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein unbefristetes Aufenthaltsrecht oder als Staatsangehöriger der Schweiz oder dessen Familienangehöriger eine Aufenthaltserlaubnis auf Grund des Abkommens vom 21. Juni 1999 zwischen der Europäischen Gemeinschaft und ihren Mitgliedstaaten einerseits und der Schweizerischen Eidgenossenschaft andererseits über die Freizügigkeit (BGBl. 2001 II S. 810) besitzt."@de .

dtd:LA-id671 a dtd:LA ;
    dtd:has_Value "für Abkömmlinge eines deutschen Staatsangehörigen, der die deutsche Staatsangehörigkeit nach Artikel 116 Absatz 2 des Grundgesetzes oder nach § 15 erworben hat, und"@de .

dtd:LA-id672 a dtd:LA ;
    dtd:has_Value "für Abkömmlinge eines deutschen Staatsangehörigen, wenn dieser ohne den Erwerb der deutschen Staatsangehörigkeit einen Anspruch nach Artikel 116 Absatz 2 des Grundgesetzes oder nach § 15 gehabt hätte."@de .

dtd:LA-id673 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kinder eines deutschen Elternteils, die durch Geburt nicht die deutsche Staatsangehörigkeit erworben haben,"@de .

dtd:LA-id674 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kinder einer Mutter, die vor der Kindesgeburt durch Eheschließung mit einem Ausländer die deutsche Staatsangehörigkeit verloren hat,"@de .

dtd:LA-id675 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kinder, die ihre durch Geburt erworbene deutsche Staatsangehörigkeit durch eine von einem Ausländer bewirkte und nach den deutschen Gesetzen wirksame Legitimation verloren haben, und"@de .

dtd:LA-id676 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Abkömmlinge der Kinder nach Nummer 1 bis 3"@de .

dtd:LA-id677 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach seiner Geburt oder nach deren Verlust auf Grund einer nach den deutschen Gesetzen wirksamen Legitimation durch einen Ausländer besessen, aber wieder aufgegeben oder verloren oder ausgeschlagen hat oder nach deren Aufgabe, Verlust oder Ausschlagung als dessen Abkömmling geboren oder als Kind angenommen worden ist, oder"@de .

dtd:LA-id678 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 4 Absatz 4 Satz 2 in Verbindung mit Absatz 1 erwerben konnte, aber nicht erworben hat oder noch erwerben kann."@de .

dtd:LA-id679 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "handlungsfähig nach § 37 Absatz 1 Satz 1 oder gesetzlich vertreten ist,"@de .

dtd:LA-id680 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "weder wegen einer rechtswidrigen Tat zu einer Strafe verurteilt noch gegen ihn auf Grund seiner Schuldunfähigkeit eine Maßregel der Besserung und Sicherung angeordnet worden ist,"@de .

dtd:LA-id681 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine eigene Wohnung oder ein Unterkommen gefunden hat,"@de .

dtd:LA-id682 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich und seine Angehörigen zu ernähren imstande ist und"@de .

dtd:LA-id683 a dtd:LA ;
    dtd:has_DL dtd:DL-id498 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich zur freiheitlichen demokratischen Grundordnung des Grundgesetzes für die Bundesrepublik Deutschland bekennt und erklärt, dass er keine Bestrebungen verfolgt oder unterstützt oder verfolgt oder unterstützt hat, diea)gegen die freiheitliche demokratische Grundordnung, den Bestand oder die Sicherheit des Bundes oder eines Landes gerichtet sind oderb)eine ungesetzliche Beeinträchtigung der Amtsführung der Verfassungsorgane des Bundes oder eines Landes oder ihrer Mitglieder zum Ziele haben oderc)durch Anwendung von Gewalt oder darauf gerichtete Vorbereitungshandlungen auswärtige Belange der Bundesrepublik Deutschland gefährden,"@de .

dtd:LA-id684 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "oder glaubhaft macht, dass er sich von der früheren Verfolgung oder Unterstützung derartiger Bestrebungen abgewandt hat,"@de .

dtd:LA-id685 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegen die freiheitliche demokratische Grundordnung, den Bestand oder die Sicherheit des Bundes oder eines Landes gerichtet sind oder"@de .

dtd:LA-id686 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine ungesetzliche Beeinträchtigung der Amtsführung der Verfassungsorgane des Bundes oder eines Landes oder ihrer Mitglieder zum Ziele haben oder"@de .

dtd:LA-id687 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Anwendung von Gewalt oder darauf gerichtete Vorbereitungshandlungen auswärtige Belange der Bundesrepublik Deutschland gefährden,"@de .

dtd:LA-id688 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein unbefristetes Aufenthaltsrecht oder als Staatsangehöriger der Schweiz oder dessen Familienangehöriger eine Aufenthaltserlaubnis auf Grund des Abkommens vom 21. Juni 1999 zwischen der Europäischen Gemeinschaft und ihren Mitgliedstaaten einerseits und der Schweizerischen Eidgenossenschaft andererseits über die Freizügigkeit, eine Blaue Karte EU oder eine Aufenthaltserlaubnis für andere als die in den §§ 16a, 16b, 16d, 16e, 16f, 17, 18d, 18f, 19, 19b, 19e, 20, 22, 23 Absatz 1, den §§ 23a, 24, 25 Absatz 3 bis 5 und § 104c des Aufenthaltsgesetzes aufgeführten Aufenthaltszwecke besitzt,"@de .

dtd:LA-id689 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Lebensunterhalt für sich und seine unterhaltsberechtigten Familienangehörigen ohne Inanspruchnahme von Leistungen nach dem Zweiten oder Zwölften Buch Sozialgesetzbuch bestreiten kann oder deren Inanspruchnahme nicht zu vertreten hat,"@de .

dtd:LA-id690 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seine bisherige Staatsangehörigkeit aufgibt oder verliert,"@de .

dtd:LA-id691 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "weder wegen einer rechtswidrigen Tat zu einer Strafe verurteilt noch gegen ihn auf Grund seiner Schuldunfähigkeit eine Maßregel der Besserung und Sicherung angeordnet worden ist,"@de .

dtd:LA-id692 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über ausreichende Kenntnisse der deutschen Sprache verfügt,"@de .

dtd:LA-id693 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über Kenntnisse der Rechts- und Gesellschaftsordnung und der Lebensverhältnisse in Deutschland verfügt und"@de .

dtd:LA-id694 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "tatsächliche Anhaltspunkte die Annahme rechtfertigen, dass der Ausländer Bestrebungen verfolgt oder unterstützt oder verfolgt oder unterstützt hat, die gegen die freiheitliche demokratische Grundordnung, den Bestand oder die Sicherheit des Bundes oder eines Landes gerichtet sind oder eine ungesetzliche Beeinträchtigung der Amtsführung der Verfassungsorgane des Bundes oder eines Landes oder ihrer Mitglieder zum Ziele haben oder die durch die Anwendung von Gewalt oder darauf gerichtete Vorbereitungshandlungen auswärtige Belange der Bundesrepublik Deutschland gefährden, es sei denn, der Ausländer macht glaubhaft, dass er sich von der früheren Verfolgung oder Unterstützung derartiger Bestrebungen abgewandt hat, oder"@de .

dtd:LA-id695 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 54 Absatz 1 Nummer 2 oder 4 des Aufenthaltsgesetzes ein besonders schwerwiegendes Ausweisungsinteresse vorliegt."@de .

dtd:LA-id696 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Recht des ausländischen Staates das Ausscheiden aus dessen Staatsangehörigkeit nicht vorsieht,"@de .

dtd:LA-id697 a dtd:LA ;

    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einbürgerung nach Artikel 116 Absatz 2 Satz 1 des Grundgesetzes sowie die Bescheinigung der Staatsangehörigkeit nach Artikel 116 Absatz 2 Satz 2 des Grundgesetzes,"@de .

dtd:LA-id759 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:LA-id1809 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Richtlinie 98/58/EG des Rates vom 20. Juli 1998 über den Schutz landwirtschaftlicher Nutztiere (ABl. EG Nr. L 221 S. 23), geändert durch die Verordnung (EG) Nr. 806/2003 des Rates vom 14. April 2003 (ABl. EU Nr. L 122 S 1),"@de .

dtd:LA-id1810 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Richtlinie 91/629/EWG des Rates vom 19. November 1991 über Mindestanforderungen für den Schutz von Kälbern (ABl. EG Nr. L 340 S. 28), zuletzt geändert durch die Verordnung (EG) Nr. 806/2003 des Rates vom 14. April 2003 (ABl. EU Nr. L 122 S 1),"@de .

dtd:LA-id1811 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Richtlinie 1999/74/EG des Rates vom 19. Juli 1999 zur Festlegung von Mindestanforderungen zum Schutz von Legehennen (ABl. EG Nr. L 203 S. 53), zuletzt geändert durch die Richtlinie 2013/64/EU (ABl. L 353 vom 28.12.2013, S. 8),"@de .

dtd:LA-id1812 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Richtlinie 91/630/EWG des Rates vom 19. November 1991 über Mindestanforderungen für den Schutz von Schweinen (ABl. EG Nr. L 340 S. 33), geändert durch die Verordnung (EG) Nr. 806/2003 des Rates vom 14. April 2003 (ABl. EU Nr. L 122 S 1)."@de .

dtd:LA-id1813 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Richtlinie 2007/43/EG des Rates vom 28. Juni 2007 mit Mindestvorschriften zum Schutz von Masthühnern (ABl. L 182 vom 12.7.2007, S 19)."@de .

dtd:LA-id1814 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf die vorübergehende Unterbringung von Tieren während Wettbewerben, Ausstellungen, Absatzveranstaltungen sowie kultureller Veranstaltungen;"@de .

dtd:LA-id1815 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "während einer tierärztlichen Behandlung, soweit nach dem Urteil des Tierarztes im Einzelfall andere Anforderungen an das Halten zu stellen sind;"@de .

dtd:LA-id1816 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "während eines Tierversuchs im Sinne des § 7 Absatz 2 des Tierschutzgesetzes, soweit für den verfolgten Zweck andere Anforderungen an das Halten unerlässlich sind."@de .

dtd:LA-id1817 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nutztiere: landwirtschaftliche Nutztiere sowie andere warmblütige Wirbeltiere, die zur Erzeugung von Nahrungsmitteln, Wolle, Häuten oder Fellen oder zu anderen landwirtschaftlichen Zwecken gehalten werden oder deren Nachzucht zu diesen Zwecken gehalten werden soll;"@de .

dtd:LA-id1818 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Haltungseinrichtungen: Gebäude und Räume (Ställe) oder Behältnisse sowie sonstige Einrichtungen zur dauerhaften Unterbringung von Tieren;"@de .

dtd:LA-id1819 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kälber: Hausrinder im Alter von bis zu sechs Monaten;"@de .

dtd:LA-id1820 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Legehennen: legereife Hennen der Art Gallus gallus, die zur Erzeugung von Eiern, die nicht für Vermehrungszwecke bestimmt sind, gehalten werden;"@de .

dtd:LA-id1821 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nest: ein gesonderter Bereich zur Eiablage;"@de .

dtd:LA-id1822 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gruppennest: ein Nest zur Eiablage für Gruppen von Legehennen;"@de .

dtd:LA-id1823 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nutzbare Fläche: Fläche, ausgenommen Nestflächen, deren Seitenlängen an keiner Stelle weniger als 30 Zentimeter beträgt, die über eine lichte Höhe von mindestens 45 Zentimeter verfügt und deren Boden ein Gefälle von höchstens 14 Prozent aufweist, einschließlich der Fläche unter Futter- und Tränkeeinrichtungen, Sitz- und Anflugstangen oder Vorrichtungen zum Krallenabrieb, die von den Legehennen über- oder unterquert werden können;"@de .

dtd:LA-id1824 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kaltscharrraum: witterungsgeschützter, mit einer flüssigkeitsundurchlässigen Bodenplatte versehener, nicht der Klimaführung des Stalles unterliegender Teil der Stallgrundfläche, der vom Stallgebäude räumlich abgetrennt, den Legehennen unmittelbar zugänglich und mit Einstreumaterial ausgestattet ist;"@de .

dtd:LA-id1825 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Masthuhn: ein zum Zweck der Fleischerzeugung gehaltenes Tier der Art Gallus gallus;"@de .

dtd:LA-id1826 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Masthühnerstall: ein Betriebsgebäude, in dem ein Masthühnerbestand gehalten wird;"@de .

dtd:LA-id1827 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Masthühnerbestand: die in einem Masthühnerstall eines Betriebes untergebrachten und sich gleichzeitig dort befindenden Masthühner;"@de .

dtd:LA-id1828 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Masthühnernutzfläche: ein den Masthühnern jederzeit zugänglicher eingestreuter Bereich;"@de .

dtd:LA-id1829 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Masthühnerbesatzdichte: das Gesamtlebendgewicht der sich gleichzeitig in einem Masthühnerstall befindenden Masthühner je Quadratmeter Masthühnernutzfläche;"@de .

dtd:LA-id1830 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schweine: Tiere der Art Sus scrofa f. domestica;"@de .

dtd:LA-id1831 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Saugferkel: Ferkel vom Zeitpunkt der Geburt bis zum Absetzen;"@de .

dtd:LA-id1832 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Absatzferkel: abgesetzte Ferkel bis zum Alter von zehn Wochen;"@de .

dtd:LA-id1833 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zuchtläufer: Schweine, die zur Zucht bestimmt sind, vom Alter von zehn Wochen bis zum Decken oder zur sonstigen Verwendung zur Zucht;"@de .

dtd:LA-id1834 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mastschweine: Schweine, die zur Schlachtung bestimmt sind, vom Alter von zehn Wochen bis zur Schlachtung;"@de .

dtd:LA-id1835 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Jungsauen: weibliche Schweine nach dem Decken bis vor dem ersten Wurf;"@de .

dtd:LA-id1836 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sauen: weibliche Schweine nach dem ersten Wurf;"@de .

dtd:LA-id1837 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Eber: geschlechtsreife männliche Schweine, die zur Zucht bestimmt sind;"@de .

dtd:LA-id1838 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kaninchen: Tiere der Art Oryctolagus cuniculus forma domestica;"@de .

dtd:LA-id1839 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zuchtkaninchen: zum Zweck der Zucht gehaltene geschlechtsreife Kaninchen;"@de .

dtd:LA-id1840 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mastkaninchen: Kaninchen, die der Gewinnung von Lebensmitteln dienen, vom Absetzen bis zur Schlachtung;"@de .

dtd:LA-id1841 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Häsin: geschlechtsreifes weibliches Kaninchen;"@de .

dtd:LA-id1842 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rammler: geschlechtsreifes männliches Kaninchen;"@de .

dtd:LA-id1843 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)."@de .

dtd:LA-id1844 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach ihrer Bauweise, den verwendeten Materialien und ihrem Zustand so beschaffen sein, dass eine Verletzung oder sonstige Gefährdung der Gesundheit der Tiere so sicher ausgeschlossen wird, wie dies nach dem Stand der Technik möglich ist;"@de .

dtd:LA-id1845 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Fütterungs- und Tränkeinrichtungen ausgestattet sein, die so beschaffen und angeordnet sind, dass jedem Tier Zugang zu einer ausreichenden Menge Futter und Wasser gewährt wird und dass Verunreinigungen des Futters und des Wassers sowie Auseinandersetzungen zwischen den Tieren auf ein Mindestmaß begrenzt werden;"@de .

dtd:LA-id1846 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "so ausgestattet sein, dass den Tieren, soweit für den Erhalt der Gesundheit erforderlich, ausreichend Schutz vor widrigen Witterungseinflüssen geboten wird und die Tiere, soweit möglich, vor Beutegreifern geschützt werden, wobei es im Fall eines Auslaufes ausreicht, wenn den Nutztieren Möglichkeiten zum Unterstellen geboten werden."@de .

dtd:LA-id1847 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Vorrichtungen ausgestattet sein, die jederzeit eine zur Inaugenscheinnahme der Tiere ausreichende Beleuchtung und einen Zugriff auf alle Nutztiere durch die mit der Fütterung und Pflege betrauten Personen ermöglichen;"@de .

dtd:LA-id1848 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erforderlichenfalls ausreichend wärmegedämmt und so ausgestattet sein, dass Zirkulation, Staubgehalt, Temperatur, relative Feuchte und Gaskonzentration der Luft in einem Bereich gehalten werden, der für die Tiere unschädlich ist."@de .

dtd:LA-id1849 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Fütterung und Pflege der Tiere ausreichend viele Personen mit den hierfür erforderlichen Kenntnissen und Fähigkeiten vorhanden sind;"@de .

dtd:LA-id1850 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Befinden der Tiere mindestens einmal täglich durch direkte Inaugenscheinnahme von einer für die Fütterung und Pflege verantwortlichen Person überprüft wird und dabei vorgefundene tote Tiere entfernt werden;"@de .

dtd:LA-id1851 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit erforderlich, unverzüglich Maßnahmen für die Behandlung, Absonderung in geeignete Haltungseinrichtungen mit trockener und weicher Einstreu oder Unterlage oder die Tötung kranker oder verletzter Tiere ergriffen werden sowie ein Tierarzt hinzugezogen wird;"@de .

dtd:LA-id1852 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle Tiere täglich entsprechend ihrem Bedarf mit Futter und Wasser in ausreichender Menge und Qualität versorgt sind;"@de .

dtd:LA-id1853 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorhandene Beleuchtungs-, Lüftungs- und Versorgungseinrichtungen mindestens einmal täglich, Notstromaggregate und Alarmanlagen in technisch erforderlichen Abständen auf ihre Funktionsfähigkeit überprüft werden;"@de .

dtd:LA-id1854 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einer Überprüfung nach Nummer 5 oder sonstige an Haltungseinrichtungen festgestellte Mängel unverzüglich abgestellt werden oder wenn dies nicht möglich ist, bis zu ihrer Behebung andere Vorkehrungen zum Schutz der Gesundheit und des Wohlbefindens der Tiere getroffen werden und die Mängel spätestens behoben sind, bevor neue Tiere eingestallt werden;"@de .

dtd:LA-id1855 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorsorge für eine ausreichende Versorgung der Tiere mit Frischluft, Licht, Futter und Wasser für den Fall einer Betriebsstörung getroffen ist;"@de .

dtd:LA-id1856 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der betriebsbedingte Geräuschpegel so gering wie möglich gehalten und dauernder oder plötzlicher Lärm vermieden wird;"@de .

dtd:LA-id1857 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die tägliche Beleuchtungsintensität und Beleuchtungsdauer bei Tieren, die in Ställen untergebracht sind, für die Deckung der ihrer Art entsprechenden Bedürfnisse ausreichen und bei hierfür unzureichendem natürlichen Lichteinfall der Stall entsprechend künstlich beleuchtet wird, wobei bei Geflügel das künstliche Licht flackerfrei entsprechend dem tierartspezifischen Wahrnehmungsvermögen sein muss;"@de .

dtd:LA-id1858 a dtd:LA ;
    dtd:has_Size dtd:normal ;

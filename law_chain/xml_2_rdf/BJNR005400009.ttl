dtd:LA-id536 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ernährung von Nutzpflanzen sicherzustellen,"@de .

dtd:LA-id537 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Fruchtbarkeit des Bodens, insbesondere den standort- und nutzungstypischen Humusgehalt, zu erhalten oder nachhaltig zu verbessern,"@de .

dtd:LA-id538 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gefahren für die Gesundheit von Menschen und Tieren sowie für den Naturhaushalt vorzubeugen oder abzuwenden, die durch das Herstellen, Inverkehrbringen oder die Anwendung von Düngemitteln, Bodenhilfsstoffen, Pflanzenhilfsmitteln sowie Kultursubstraten oder durch andere Maßnahmen des Düngens entstehen können,"@de .

dtd:LA-id539 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen nachhaltigen und ressourceneffizienten Umgang mit Nährstoffen bei der landwirtschaftlichen Erzeugung sicherzustellen, insbesondere Nährstoffverluste in die Umwelt so weit wie möglich zu vermeiden,"@de .

dtd:LA-id540 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rechtsakte der Europäischen Gemeinschaft oder der Europäischen Union, die Sachbereiche dieses Gesetzes, insbesondere über den Verkehr mit oder die Anwendung von Düngemitteln betreffen, umzusetzen oder durchzuführen."@de .

dtd:LA-id541 a dtd:LA ;
    dtd:has_DL dtd:DL-id225 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sind Düngemittel Stoffe, ausgenommen Kohlendioxid und Wasser, die dazu bestimmt sind, a)Nutzpflanzen Nährstoffe zuzuführen, um ihr Wachstum zu fördern, ihren Ertrag zu erhöhen oder ihre Qualität zu verbessern, oderb)die Bodenfruchtbarkeit zu erhalten oder zu verbessern;"@de .

dtd:LA-id542 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nutzpflanzen Nährstoffe zuzuführen, um ihr Wachstum zu fördern, ihren Ertrag zu erhöhen oder ihre Qualität zu verbessern, oder"@de .

dtd:LA-id543 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bodenfruchtbarkeit zu erhalten oder zu verbessern;"@de .

dtd:LA-id544 a dtd:LA ;
    dtd:has_DL dtd:DL-id230 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sind Wirtschaftsdünger: Düngemittel, die a)als tierische Ausscheidungen aa)bei der Haltung von Tieren zur Erzeugung von Lebensmitteln oderbb)bei der sonstigen Haltung von Tieren in der Landwirtschaft oderb)als pflanzliche Stoffe im Rahmen der pflanzlichen Erzeugung oder in der Landwirtschaft,auch in Mischungen untereinander oder nach aerober oder anaerober Behandlung, anfallen oder erzeugt werden;"@de .

dtd:LA-id545 a dtd:LA ;
    dtd:has_DL dtd:DL-id235 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als tierische Ausscheidungen aa)bei der Haltung von Tieren zur Erzeugung von Lebensmitteln oderbb)bei der sonstigen Haltung von Tieren in der Landwirtschaft oder"@de .

dtd:LA-id546 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Haltung von Tieren zur Erzeugung von Lebensmitteln oder"@de .

dtd:LA-id547 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der sonstigen Haltung von Tieren in der Landwirtschaft oder"@de .

dtd:LA-id548 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als pflanzliche Stoffe im Rahmen der pflanzlichen Erzeugung oder in der Landwirtschaft,"@de .

dtd:LA-id549 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ist Festmist: Wirtschaftsdünger aus tierischen Ausscheidungen, auch mit Einstreu, insbesondere Stroh, Sägemehl, Torf oder anderes pflanzliches Material, das im Rahmen der Tierhaltung zugefügt worden ist, oder mit Futterresten vermischt, dessen Trockensubstanzgehalt 15 vom Hundert übersteigt;"@de .

dtd:LA-id550 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ist Gülle: Wirtschaftsdünger aus allen tierischen Ausscheidungen, auch mit geringen Mengen Einstreu oder Futterresten oder Zugabe von Wasser, dessen Trockensubstanzgehalt 15 vom Hundert nicht übersteigt;"@de .

dtd:LA-id551 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ist Jauche: Wirtschaftsdünger aus tierischen Ausscheidungen, bei dem es sich um ein Gemisch aus Harn und ausgeschwemmten feinen Bestandteilen des Kotes oder der Einstreu sowie von Wasser handelt; Jauche kann in geringem Umfang Futterreste sowie Reinigungs- und Niederschlagswasser enthalten;"@de .

dtd:LA-id552 a dtd:LA ;
    dtd:has_DL dtd:DL-id240 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sind Bodenhilfsstoffe: Stoffe ohne wesentlichen Nährstoffgehalt sowie Mikroorganismen, die dazu bestimmt sind, a)die biologischen, chemischen oder physikalischen Eigenschaften des Bodens zu beeinflussen, um die Wachstumsbedingungen für Nutzpflanzen zu verbessern oderb)die symbiotische Bindung von Stickstoff zu fördern;"@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die biologischen, chemischen oder physikalischen Eigenschaften des Bodens zu beeinflussen, um die Wachstumsbedingungen für Nutzpflanzen zu verbessern oder"@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die symbiotische Bindung von Stickstoff zu fördern;"@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sind Pflanzenhilfsmittel: Stoffe ohne wesentlichen Nährstoffgehalt, die dazu bestimmt sind, auf Pflanzen biologisch oder chemisch einzuwirken, um einen pflanzenbaulichen, produktionstechnischen oder anwendungstechnischen Nutzen zu erzielen, soweit sie nicht Pflanzenstärkungsmittel im Sinne des § 2 Nummer 10 des Pflanzenschutzgesetzes sind;"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sind Kultursubstrate: Stoffe, die dazu bestimmt sind, Nutzpflanzen als Wurzelraum zu dienen und die dazu in Böden eingebracht, auf Böden aufgebracht oder in bodenunabhängigen Anwendungen genutzt werden;"@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ist Herstellen: das Gewinnen, Behandeln, Verarbeiten, Mischen oder sonstige Aufbereiten von Stoffen nach den Nummern 1 und 6 bis 8;"@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ist Inverkehrbringen: das Anbieten, Vorrätighalten zur Abgabe, Feilhalten und jedes Abgeben von Stoffen nach Satz 1 Nr. 1 und 6 bis 8 an andere;"@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ist gewerbsmäßig: Tätigkeit im Rahmen eines Gewerbes oder sonst zu Erwerbszwecken."@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem durch einen unmittelbar geltenden Rechtsakt der Europäischen Gemeinschaft oder der Europäischen Union über den Verkehr mit oder die Anwendung von Düngemitteln zugelassenen Typ oder"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Anforderungen für das Inverkehrbringen nach einer Rechtsverordnung auf Grund des § 5 Abs. 2 oder 5"@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in einem anderen Mitgliedstaat der Europäischen Union, der Türkei oder einem Staat, der zugleich Vertragspartei des Abkommens über die Gründung der Europäischen Freihandelsassoziation und des Abkommens über den Europäischen Wirtschaftsraum ist, rechtmäßig hergestellt oder rechtmäßig in den Verkehr gebracht worden sind und"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Anforderungen zum Schutz vor Gefahren für die Gesundheit von Mensch oder Tier oder den Naturhaushalt gleichermaßen wie inländische Stoffe genügen."@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anforderungen der guten fachlichen Praxis im Sinne des Absatzes 2 näher bestimmt werden,"@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorschriften zur Sicherung der Bodenfruchtbarkeit erlassen werden,"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bestimmte Anwendungen verboten oder beschränkt werden."@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zeiträume, in denen das Aufbringen bestimmter Stoffe nach § 2 Nummer 1 und 6 bis 8 auf landwirtschaftlichen Flächen verboten ist,"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "flächen- oder betriebsbezogene Obergrenzen für das Aufbringen von Nährstoffen aus Stoffen nach § 2 Nummer 1 und 6 bis 8,"@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Aufbringen von Stoffen nach § 2 Nummer 1 und 6 bis 8 auf stark geneigten landwirtschaftlichen Flächen,"@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Aufbringen von Stoffen nach § 2 Nummer 1 und 6 bis 8 auf wassergesättigten, überschwemmten, gefrorenen oder schneebedeckten Böden,"@de .

dtd:LA-id571 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bedingungen für das Aufbringen von Stoffen nach § 2 Nummer 1 und 6 bis 8 auf landwirtschaftlichen Flächen in der Nähe von Wasserläufen,"@de .

dtd:LA-id572 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Berücksichtigung von beim Weidegang anfallenden sowie durch andere Maßnahmen als der Düngung zugeführten Nährstoffen,"@de .

dtd:LA-id573 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Aufzeichnungen der Anwendung von Stoffen nach § 2 Nummer 1 und 6 bis 8 sowie die Vorlage-, Melde- und Mitteilungspflichten der Anwender,"@de .

dtd:LA-id574 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Technik und die Verfahren zum Aufbringen von Stoffen nach § 2 Nummer 1 und 6 bis 8,"@de .

dtd:LA-id575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Lagerkapazität für Wirtschaftsdünger und Düngemittel, bei denen es sich um Gärrückstände aus dem Betrieb einer Biogasanlage handelt,"@de .

dtd:LA-id576 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anordnungen der zuständigen Behörden, die zum Schutz der Gewässer vor Verunreinigung, insbesondere zur Einhaltung der nach den Nummern 1 bis 9 erlassenen Vorschriften erforderlich sind."@de .

dtd:LA-id577 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach Absatz 4 Satz 1 in Verbindung mit Satz 2 Nummer 1 und 2 oder"@de .

dtd:LA-id578 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach Absatz 4 Satz 1 in Verbindung mit Satz 2 Nummer 3, soweit Vorschriften zum Schutz der Gewässer im Sinne des Absatzes 5 erlassen werden,"@de .

dtd:LA-id579 a dtd:LA ;
    dtd:has_Value "das Wachstum von Nutzpflanzen wesentlich zu fördern,"@de .

dtd:LA-id580 a dtd:LA ;
    dtd:has_Value "ihren Ertrag wesentlich zu erhöhen,"@de .

dtd:LA-id581 a dtd:LA ;
    dtd:has_Value "ihre Qualität wesentlich zu verbessern oder"@de .

dtd:LA-id582 a dtd:LA ;
    dtd:has_Value "die Fruchtbarkeit des Bodens, insbesondere den standort- und nutzungstypischen Humusgehalt, zu erhalten oder nachhaltig zu verbessern,"@de .

dtd:LA-id583 a dtd:LA ;
    dtd:has_Value "in einem anderen Mitgliedstaat der Europäischen Union oder der Türkei oder einem Staat, der zugleich Vertragspartei des Abkommens über die Gründung der Europäischen Freihandelsassoziation und des Abkommens über den Europäischen Wirtschaftsraum ist, rechtmäßig hergestellt oder rechtmäßig in den Verkehr gebracht worden sind und"@de .

dtd:LA-id584 a dtd:LA ;
    dtd:has_Value "den Anforderungen zum Schutz vor Gefahren für die Gesundheit von Mensch oder Tier oder den Naturhaushalt gleichermaßen wie inländische Stoffe genügen."@de .

dtd:LA-id585 a dtd:LA ;
    dtd:has_Value "die näheren Anforderungen an das Inverkehrbringen zu bestimmen,"@de .

dtd:LA-id586 a dtd:LA ;
    dtd:has_Value "das Inverkehrbringen bestimmter Stoffe nach § 2 Nr. 1 und 6 bis 8 zu verbieten oder zu beschränken oder"@de .

dtd:LA-id587 a dtd:LA ;
    dtd:has_Value "vorzuschreiben, dass sie nur verpackt oder in Packungen oder Behältnissen von bestimmter Art oder mit bestimmtem Verschluss in den Verkehr gebracht werden dürfen."@de .

dtd:LA-id588 a dtd:LA ;
    dtd:has_Value "zugelassene Ausgangsstoffe,"@de .

dtd:LA-id589 a dtd:LA ;
    dtd:has_Value "Art der Herstellung,"@de .

dtd:LA-id590 a dtd:LA ;
    dtd:has_Value "Zusammensetzung nach Haupt- und Nebenbestandteilen, insbesondere über Nährstoffgehalt, Nährstoffform sowie Art und Gehalt von Nebenbestandteilen,"@de .

dtd:LA-id591 a dtd:LA ;
    dtd:has_Value "Nährstoffverfügbarkeit,"@de .

dtd:LA-id592 a dtd:LA ;
    dtd:has_Value "Wirkung von Nebenbestandteilen,"@de .

dtd:LA-id593 a dtd:LA ;
    dtd:has_Value "äußere Merkmale, insbesondere Korngröße, Mahlfeinheit, Siebdurchgang oder Färbung,"@de .

dtd:LA-id594 a dtd:LA ;
    dtd:has_Value "andere für die Aufbereitung, Anwendung oder Wirkung des Stoffes wichtige Anforderungen."@de .

dtd:LA-id595 a dtd:LA ;
    dtd:has_DL dtd:DL-id329 ;
    dtd:has_Value "vorgeschrieben werden, dass der Hersteller eines Stoffes nach § 2 Nr. 1 und 6 bis 8 Aufzeichnungen zu erstellen hat über a)die Zusammensetzung des Stoffes oderb)die zur Herstellung verwendeten Ausgangsstoffe und deren Herkunft, sowie"@de .

dtd:LA-id596 a dtd:LA ;
    dtd:has_Value "die Zusammensetzung des Stoffes oder"@de .

dtd:LA-id597 a dtd:LA ;
    dtd:has_Value "die zur Herstellung verwendeten Ausgangsstoffe und deren Herkunft, sowie"@de .

dtd:LA-id598 a dtd:LA ;
    dtd:has_Value "die Art und Weise der Aufzeichnungen sowie die Dauer ihrer Aufbewahrung geregelt werden."@de .

dtd:LA-id599 a dtd:LA ;
    dtd:has_Value "zu Forschungs- oder Versuchszwecken eine von Absatz 1 abweichende Regelung oder"@de .

dtd:LA-id600 a dtd:LA ;
    dtd:has_Value "bis zu einer Änderung einer auf Grund des Absatzes 2 erlassenen Rechtsverordnung längstens für einen Zeitraum von vier Jahren eine vorläufige Regelung"@de .

dtd:LA-id601 a dtd:LA ;
    dtd:has_Value "Verkehrsbezeichnung,"@de .

dtd:LA-id602 a dtd:LA ;
    dtd:has_Value "zur Herstellung verwendete Ausgangsstoffe,"@de .

dtd:LA-id603 a dtd:LA ;
    dtd:has_Value "Art der Herstellung,"@de .

dtd:LA-id604 a dtd:LA ;
    dtd:has_Value "Zusammensetzung nach Haupt- und Nebenbestandteilen, insbesondere über Nährstoffgehalt, Nährstoffform sowie Art und Gehalt von Nebenbestandteilen sowie deren Einteilung in Aufbereitungshilfsmittel, Anwendungshilfsmittel und Fremdbestandteile,"@de .

dtd:LA-id605 a dtd:LA ;
    dtd:has_Value "Nährstoffverfügbarkeit,"@de .

dtd:LA-id606 a dtd:LA ;
    dtd:has_Value "Wirkung von Nebenbestandteilen,"@de .

dtd:LA-id607 a dtd:LA ;
    dtd:has_Value "äußere Merkmale, insbesondere Korngröße, Mahlfeinheit, Siebdurchgang oder Färbung,"@de .

dtd:LA-id608 a dtd:LA ;
    dtd:has_Value "andere für die Aufbereitung, Anwendung oder Wirkung des Stoffes wichtige Anforderungen,"@de .

dtd:LA-id609 a dtd:LA ;
    dtd:has_Value "das Gewicht oder das Volumen der Verpackungseinheit,"@de .

dtd:LA-id610 a dtd:LA ;
    dtd:has_Value "der Name oder die Firma des für das Inverkehrbringen Verantwortlichen,"@de .

dtd:LA-id611 a dtd:LA ;
    dtd:has_Value "Hinweise zur sachgerechten Anwendung, Lagerung oder Behandlung,"@de .

dtd:LA-id612 a dtd:LA ;
    dtd:has_Value "die Rechtsvorschrift oder rechtliche Grundlage, auf Grund derer das Düngemittel, der Bodenhilfsstoff, das Pflanzenhilfsmittel oder das Kultursubstrat in den Verkehr gebracht worden ist."@de .

dtd:LA-id613 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Rechtsform des Entschädigungsfonds,"@de .

dtd:LA-id614 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bildung und die weitere Ausgestaltung des Entschädigungsfonds einschließlich der erforderlichen finanziellen Ausstattung bis zu einer Höhe von 125 Millionen Euro,"@de .

dtd:LA-id615 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verwaltung des Entschädigungsfonds,"@de .

dtd:LA-id616 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Höhe und die Festlegung der Beiträge und die Art ihrer Aufbringung unter Berücksichtigung der Art und Menge des abgegebenen Klärschlamms sowie eine Nachschusspflicht im Falle der Erschöpfung der in Nummer 2 vorgesehenen finanziellen Ausstattung,"@de .

dtd:LA-id617 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen angemessenen Selbstbehalt für Sachschäden sowie einen angemessenen Entschädigungshöchstbetrag insbesondere unter Berücksichtigung des Umfanges der geschädigten Fläche,"@de .

dtd:LA-id618 a dtd:LA ;
    dtd:has_Size dtd:normal ;

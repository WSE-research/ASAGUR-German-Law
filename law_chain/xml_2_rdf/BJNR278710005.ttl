    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Internationalen Codes für die Gefahrenabwehr auf Schiffen und in Hafenanlagen (BGBl. 2003 II S. 2018, 2043 und VkBl. 2004 S. 32)"@de .

dtd:LA-id294 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "\"SOLAS-Übereinkommen\" das Internationale Übereinkommen von 1974 zum Schutz des menschlichen Lebens auf See und"@de .

dtd:LA-id295 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "\"ISPS-Code\" der Internationale Code für die Gefahrenabwehr auf Schiffen und in Hafenanlagen."@de .

dtd:LA-id296 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Verlangen die notwendigen Auskünfte zu erteilen,"@de .

dtd:LA-id297 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Verlangen die erforderlichen Unterlagen vorzulegen und"@de .

dtd:LA-id298 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zugang zu den von ihnen betriebenen Schiffen zu gewähren, die dem Anwendungsbereich des Kapitels XI-2 der Anlage des SOLAS-Übereinkommens unterliegen. Dies gilt nicht für geschlossene Räume, die zum privaten Aufenthalt bestimmt sind."@de .

dtd:LA-id299 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zugang zu dem von ihm betriebenen Schiff zu gewähren, welches dem Anwendungsbereich des Kapitels XI-2 der Anlage des SOLAS-Übereinkommens unterliegt. Dies gilt nicht für geschlossene Räume, die zum privaten Aufenthalt bestimmt sind,"@de .

dtd:LA-id300 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Verlangen die notwendigen Auskünfte zu erteilen,"@de .

dtd:LA-id301 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Verlangen die notwendigen Dokumente und Unterlagen vorzulegen,"@de .

dtd:LA-id302 a dtd:LA ;
    dtd:has_Size dtd:normal ;

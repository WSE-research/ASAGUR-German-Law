dtd:LA-id1306 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 24 Abs. 1 die Verbandsversammlung oder der Verbandsausschuß"@de .

dtd:LA-id1307 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Pacht- oder Mietverhältnis unter Einhaltung einer Frist von drei Monaten zum Ende des Vertragsjahres kündigen,"@de .

dtd:LA-id1308 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Aufhebung eines anderen Nutzungsrechts ohne Einhaltung einer Frist verlangen."@de .

dtd:LA-id1309 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Eigentum an Grundstücken entzogen oder belastet werden,"@de .

dtd:LA-id1310 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "andere Rechte an Grundstücken entzogen oder belastet werden,"@de .

dtd:LA-id1311 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rechte entzogen werden, die zum Erwerb, zum Besitz oder zur Nutzung von Grundstücken berechtigen oder die den Verpflichteten in der Benutzung von Grundstücken beschränken oder"@de .

dtd:LA-id1312 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rechtsverhältnisse begründet werden, die Rechte der in Nummer 3 bezeichneten Art gewähren."@de .

dtd:LA-id1313 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wahl und Abberufung der Vorstandsmitglieder sowie ihrer Stellvertreter,"@de .

dtd:LA-id1314 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beschlußfassung über Änderungen der Satzung, des Unternehmens, des Plans oder der Aufgaben sowie über die Grundsätze der Geschäftspolitik,"@de .

dtd:LA-id1315 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beschlußfassung über die Umgestaltung und die Auflösung des Verbands,"@de .

dtd:LA-id1316 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wahl der Schaubeauftragten,"@de .

dtd:LA-id1317 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Festsetzung des Haushaltsplans sowie von Nachtragshaushaltsplänen,"@de .

dtd:LA-id1318 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einspruch gegen eine Zwangsfestsetzung des Haushaltsplans,"@de .

dtd:LA-id1319 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Entlastung des Vorstands,"@de .

dtd:LA-id1320 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Festsetzung von Grundsätzen für Dienst- und Anstellungsverhältnisse und von Vergütungen für Vorstandsmitglieder und Mitglieder des Verbandsausschusses,"@de .

dtd:LA-id1321 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beschlußfassung über Rechtsgeschäfte zwischen Vorstandsmitgliedern und den Verband,"@de .

dtd:LA-id1322 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beratung des Vorstands in allen wichtigen Angelegenheiten."@de .

dtd:LA-id1323 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Übertragung der Aufgaben, des Vermögens sowie der Verpflichtungen eines Verbands oder mehrerer Verbände als Ganzes auf einen der sich zusammenschließenden Verbände oder"@de .

dtd:LA-id1324 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Gründung eines neuen Verbands und Übertragung der Aufgaben, des Vermögens sowie der Verpflichtungen anderer Verbände als Ganzes auf den neuen Verband."@de .

dtd:LA-id1325 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur unentgeltlichen Veräußerung von Vermögensgegenständen,"@de .

dtd:LA-id1326 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Aufnahme von Darlehen, die über eine in der Satzung festzulegende Höhe hinausgehen,"@de .

dtd:LA-id1327 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Übernahme von Bürgschaften, zu Verpflichtungen aus Gewährverträgen und zur Bestellung von Sicherheiten,"@de .

dtd:LA-id1328 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu Rechtsgeschäften mit einem Vorstandsmitglied einschließlich der Vereinbarung von Vergütungen, soweit sie über den Ersatz von Aufwendungen hinausgehen."@de .


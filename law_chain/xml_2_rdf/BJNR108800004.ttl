    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Leiterin oder den Leiter des für die Personalentwicklung und -planung des gehobenen Dienstes zuständigen Referats."@de .

dtd:LA-id747 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Ausfertigung der Geburtsurkunde,"@de .

dtd:LA-id748 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Nachweis der Staatsangehörigkeit,"@de .

dtd:LA-id749 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegebenenfalls eine Ausfertigung der Eheurkunde und Ausfertigungen der Geburtsurkunden der Kinder,"@de .

dtd:LA-id750 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine beglaubigte Abschrift des Zeugnisses über die Diplom- oder Hochschulprüfung,"@de .

dtd:LA-id751 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine beglaubigte Abschrift des Zeugnisses der Hochschul- oder Fachhochschulreife,"@de .

dtd:LA-id752 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegebenenfalls eine beglaubigte Ablichtung einer Dienstzeitbescheinigung über die Ableistung von Wehr-, Zivil- oder Entwicklungsdienst."@de .

dtd:LA-id753 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wegen einer Erkrankung,"@de .

dtd:LA-id754 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wegen eines Beschäftigungsverbots für die Zeit vor oder nach einer Entbindung nach mutterschutzrechtlichen Vorschriften oder wegen einer Elternzeit,"@de .

dtd:LA-id755 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Ableistung des Grundwehrdienstes, eines Ersatzdienstes oder"@de .

dtd:LA-id756 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus anderen zwingenden Gründen"@de .

dtd:LA-id757 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "theoretische Lehrveranstaltungen in den Fächern Geschichte/Politik, Wirtschaftswissenschaften und Völker- und Europarecht sowie zu weiteren berufspraktischen Themen (Absatz 2),"@de .

dtd:LA-id758 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sprachausbildung im In- und Ausland in den Prüfungsfächern Englisch und Französisch sowie gegebenenfalls einer weiteren Sprache (Absatz 3),"@de .

dtd:LA-id759 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rechtsausbildung (Absatz 4) und"@de .

dtd:LA-id760 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "praktische Ausbildung (Absatz 5)."@de .

dtd:LA-id761 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bestimmt Prüfungsort und Prüfungszeitpunkt,"@de .

dtd:LA-id762 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bestimmt die Aufgaben der schriftlichen Prüfung,"@de .

dtd:LA-id763 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "trägt Sorge für die Entwicklung und gleichmäßige Anwendung der Bewertungsmaßstäbe,"@de .

dtd:LA-id764 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entscheidet über die Gewährung von Erleichterungen nach § 26,"@de .

dtd:LA-id765 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "trifft die Entscheidung, ob Verhinderung (§ 16 Abs. 1) oder ein wichtiger Grund für einen Rücktritt (§ 16 Abs. 2) vorliegt,"@de .

dtd:LA-id766 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "teilt den Prüfungsteilnehmerinnen und Prüfungsteilnehmern Ort und Zeitpunkt der schriftlichen und mündlichen Prüfungen mit,"@de .

dtd:LA-id767 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "stellt die Zulassung zur mündlichen Prüfung fest,"@de .

dtd:LA-id768 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erteilt die Zeugnisse und vollzieht die sonstigen Entscheidungen der Prüfungskommission,"@de .

dtd:LA-id769 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bewahrt die Prüfungsakten auf und entscheidet über Anträge auf Einsichtnahme."@de .

dtd:LA-id770 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Leiterin oder der Leiter der Aus- und Fortbildungsstätte des Auswärtigen Amts,"@de .

dtd:LA-id771 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Leiterin oder der Leiter des Personalreferats für den höheren Auswärtigen Dienst,"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "geeignete Maßnahmen zum Schutz und zur Sicherheit der sowjetischen Streitkräfte, der ihnen zugewiesenen Liegenschaften und ihres Eigentums,"@de .

dtd:LA-id120 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bewegungen, Manöver, Übungen und Ausbildung sowie sonstige Aktivitäten der sowjetischen Streitkräfte,"@de .

dtd:LA-id121 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Flugbetrieb sowjetischer Streitkräfte,"@de .

dtd:LA-id122 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nutzung der den sowjetischen Streitkräften zugewiesenen Liegenschaften,"@de .

dtd:LA-id123 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Polizeigewalt gegenüber den sowjetischen Streitkräften, deren Mitgliedern und Familienangehörigen,"@de .

dtd:LA-id124 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Versorgung der sowjetischen Streitkräfte,"@de .

dtd:LA-id125 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nutzung von Verkehrseinrichtungen durch die sowjetischen Streitkräfte und deren Mitglieder,"@de .

dtd:LA-id126 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einrichtungen des Post- und Fernmeldewesens sowie deren Benutzung durch die sowjetischen Streitkräfte, deren Mitglieder und Familienangehörige,"@de .

dtd:LA-id127 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gesundheitswesen und Umweltschutz,"@de .

dtd:LA-id128 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ein- und Ausreise der sowjetischen Streitkräfte, deren Mitglieder und Familienangehörigen,"@de .

dtd:LA-id129 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zoll-, Steuer- und Monopolangelegenheiten in bezug auf die sowjetischen Streitkräfte, deren Mitglieder und Familienangehörigen,"@de .

dtd:LA-id130 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gerichtsbarkeit und Rechtshilfe,"@de .

dtd:LA-id131 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Haftung bei Schädigung Dritter und bei Schäden der Vertragsparteien,"@de .

dtd:LA-id132 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beilegung von Streitigkeiten und Einsetzung einer deutsch-sowjetischen Kommission,"@de .

dtd:LA-id133 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vereinbarung, daß ein Vertrag unter Berücksichtigung des erreichten Verhandlungsstandes baldmöglichst unterzeichnet und ratifiziert wird."@de .

dtd:LA-id134 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Höchststärke, innerstädtische Standorte sowie Art und Umfang der Bewaffnung,"@de .

dtd:LA-id135 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Regelungen und Beschränkungen hinsichtlich der Bewegungen der Streitkräfte außerhalb ihrer Standorte,"@de .

dtd:LA-id136 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Regelungen betreffend die Bewegungen der Streitkräfte der Französischen Republik, des Vereinigten Königreichs Großbritannien und Nordirland und der Vereinigten Staaten von Amerika zwischen dem Land Berlin und den Ländern Baden-Württemberg, Bayern, Bremen, Hamburg, Hessen, Niedersachsen, Nordrhein-Westfalen, Rheinland-Pfalz, Saarland und Schleswig-Holstein sowie betreffend die Bewegungen der Mitglieder aller in den vorgenannten Ländern stationierten Streitkräfte und deren Familienangehörigen in den in Artikel 1 Abs. 1 genannten Ländern,"@de .

dtd:LA-id137 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Modalitäten des befristeten Aufenthalts und Abzugs."@de .

dtd:LA-id138 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "künftige Zuständigkeit deutscher Gerichte und Behörden für aus der Besatzungszeit herrührende Gegenstände,"@de .

dtd:LA-id139 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorläufige Bestandskraft von Rechten und Verpflichtungen, die durch alliierte Maßnahmen festgestellt oder begründet wurden,"@de .


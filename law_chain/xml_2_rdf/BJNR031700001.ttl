dtd:LA-id499 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Massenkonzentrationen der Emissionen an Distickstoffoxid und"@de .

dtd:LA-id500 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zur Auswertung und Beurteilung des ordnungsgemäßen Betriebes erforderlichen Bezugsgrößen, insbesondere Abgastemperatur, Abgasvolumenstrom, Druck, Feuchtegehalt an Wasserdampf sowie Masse der zugeführten Einsatzstoffe im Anlieferungszustand"@de .

dtd:LA-id501 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:entry-id885 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Die Augen müssen klar und deutlich erkennbar sein (Reflexionen auf den Brillengläsern, getönte Gläser oder Sonnenbrillen sind nicht zulässig). Der Rand der Gläser oder das Gestell dürfen nicht die Augen verdecken."@de ;
    dtd:has_colname "col1"@de .


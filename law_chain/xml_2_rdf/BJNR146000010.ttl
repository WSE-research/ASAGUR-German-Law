    dtd:has_morerows "1"@de .

dtd:entry-id867 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Das Gesicht muss in allen Bereichen scharf abgebildet, kontrastreich und klar sein."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id868 a dtd:entry ;
    dtd:has_B dtd:B-id619 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Ausleuchtung"@de ;
    dtd:has_colname "col1"@de ;
    dtd:has_rowsep "0"@de .

dtd:entry-id869 a dtd:entry ;
    dtd:has_IMG dtd:IMG-id934 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value ""@de ;
    dtd:has_colname "col2"@de ;
    dtd:has_morerows "1"@de .

dtd:entry-id870 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Das Gesicht muss gleichmäßig ausgeleuchtet werden. Reflexionen oder Schatten im Gesicht sowie rote Augen sind zu vermeiden."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id871 a dtd:entry ;
    dtd:has_B dtd:B-id620 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Hintergrund"@de ;
    dtd:has_colname "col1"@de ;
    dtd:has_rowsep "0"@de .

dtd:entry-id872 a dtd:entry ;
    dtd:has_IMG dtd:IMG-id935 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value ""@de ;
    dtd:has_colname "col2"@de ;
    dtd:has_morerows "1"@de .

dtd:entry-id873 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Der Hintergrund muss einfarbig hell sein (idealerweise neutral grau) und einen Kontrast zum Gesicht und zu den Haaren aufweisen. Bei hellen Haaren eignet sich ein mittelgrauer Hintergrund, bei dunklen Haaren ein hellgrauer. Der Hintergrund darf kein Muster aufweisen. Das Foto darf ausschließlich die zu fotografierende Person zeigen (keine weiteren Personen oder Gegenstände im Bild). Auf dem Hintergrund dürfen keine Schatten entstehen."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id874 a dtd:entry ;
    dtd:has_B dtd:B-id621 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Fotoqualität"@de ;
    dtd:has_colname "col1"@de ;
    dtd:has_rowsep "0"@de .

dtd:entry-id875 a dtd:entry ;
    dtd:has_IMG dtd:IMG-id936 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value ""@de ;
    dtd:has_colname "col2"@de ;
    dtd:has_morerows "1"@de .

dtd:entry-id876 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Das Foto sollte mit einer Auflösung von mindestens 600 dpi vorliegen. Das Foto muss farbneutral sein und die natürlichen Hauttöne wiedergeben. Digitale Fotos sind grundsätzlich in Farbe vorzulegen. Ist die Vorlage eines papierbasierten Fotos in Ausnahmefällen gestattet, darf dieses Foto in Farbe oder Schwarzweiß vorgelegt werden; es darf jedoch keine Knicke oder Verunreinigung aufweisen."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id877 a dtd:entry ;
    dtd:has_B dtd:B-id622 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Kopfposition und Gesichtsausdruck"@de ;
    dtd:has_colname "col1"@de ;
    dtd:has_rowsep "0"@de .

dtd:entry-id878 a dtd:entry ;
    dtd:has_IMG dtd:IMG-id937 ;
    dtd:has_VJ "1"@de ;

    dtd:has_morerows "1"@de .

dtd:entry-id882 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Die Person muss auf dem Foto direkt in die Kamera blicken. Die Augen müssen geöffnet und deutlich sichtbar sein und dürfen nicht durch Haare oder Brillengestelle verdeckt werden."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id883 a dtd:entry ;
    dtd:has_B dtd:B-id624 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Brillenträger"@de ;
    dtd:has_colname "col1"@de ;

dtd:LA-id140 a dtd:LA ;
    dtd:has_Value "Sonntage und staatlich anerkannte allgemeine Feiertage an der Ladestelle,"@de .

dtd:LA-id141 a dtd:LA ;
    dtd:has_Value "an Werktagen die Zeit zwischen 20.00 Uhr und 6.00 Uhr,"@de .

dtd:LA-id142 a dtd:LA ;
    dtd:has_Value "die Zeit, in der aus Gründen, die dem Risikobereich des Frachtführers zuzurechnen sind, das Verladen jeder Art von Gut unmöglich ist."@de .

dtd:LA-id143 a dtd:LA ;
    dtd:has_Value "bis zu 1 100 Tonnen 24 Stunden,"@de .

dtd:LA-id144 a dtd:LA ;
    dtd:has_Value "bis zu 1 500 Tonnen 26 Stunden,"@de .

dtd:LA-id145 a dtd:LA ;
    dtd:has_Value "bis zu 2 000 Tonnen 28 Stunden, wenn es sich bei dem Schiff um ein Tankschiff in Doppelhüllenbauweise handelt, sonst 30 Stunden."@de .

dtd:LA-id146 a dtd:LA ;
    dtd:has_Value "im Falle des Ladens Sonntage und staatlich anerkannte allgemeine Feiertage an der Ladestelle, im Falle des Löschens Sonntage und staatlich anerkannte allgemeine Feiertage an der Löschstelle,"@de .

dtd:LA-id147 a dtd:LA ;
    dtd:has_Value "an Werktagen, die einem Sonntag oder einem gesetzlichen Feiertag an der Lade- oder Löschstelle nachfolgen, die Zeit zwischen 0.00 Uhr und 7.00 Uhr, an einem Samstag und am 24. und 31. Dezember zusätzlich die Zeit zwischen 13.00 Uhr und 24.00 Uhr,"@de .

dtd:LA-id148 a dtd:LA ;
    dtd:has_Value "die Zeit, in der aus Gründen, die dem Risikobereich des Frachtführers zuzurechnen sind, das Verladen oder Entladen jeder Art von Gut unmöglich ist."@de .

dtd:LA-id149 a dtd:LA ;
    dtd:has_Value "bis zu   500 Tonnen 25 Euro,"@de .

dtd:LA-id150 a dtd:LA ;
    dtd:has_Value "bis zu 1 000 Tonnen 54 Euro,"@de .

dtd:LA-id151 a dtd:LA ;
    dtd:has_Value "bis zu 1 500 Tonnen 75 Euro,"@de .

dtd:LA-id152 a dtd:LA ;
    dtd:has_Value "über 1 500 Tonnen 75 Euro zuzüglich 10 Euro je weitere angefangene 500 Tonnen."@de .

dtd:LA-id153 a dtd:LA ;
    dtd:has_Value "bis zu   500 Tonnen  60 Euro,"@de .

dtd:LA-id154 a dtd:LA ;
    dtd:has_Value "bis zu 1 000 Tonnen  80 Euro,"@de .

dtd:LA-id155 a dtd:LA ;
    dtd:has_Value "bis zu 1 500 Tonnen 100 Euro,"@de .

dtd:LA-id156 a dtd:LA ;
    dtd:has_Value "über1 500 Tonnen 100 Euro zuzüglich 20 Euro je weitere angefangene 500 Tonnen."@de .


dtd:LA-id179 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Drittland:"@de .

dtd:LA-id180 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Land, das nicht Mitgliedstaat der Europäischen Union ist; das Fürstentum Monaco gilt nicht als Drittland; die Republik San Marino gilt nicht als Drittland in Bezug auf die Verbrauchsteuern;"@de .

dtd:LA-id181 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Drittlandsgebiet:"@de .

dtd:LA-id182 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Gebiet, in dem die Richtlinie 2006/112/EG des Rates vom 28. November 2006 über das gemeinsame Mehrwertsteuersystem (ABl. EU Nr. L 347 S. 1, 2007 Nr. L 335 S. 6), zuletzt geändert durch die Richtlinie 2008/8/EG des Rates vom 12. Februar 2008 (ABl. EU Nr. L 44 S. 11), oder die Richtlinie 92/12/EWG des Rates vom 25. Februar 1992 über das allgemeine System, den Besitz, die Beförderung und die Kontrolle verbrauchsteuerpflichtiger Waren (ABl. EG Nr. L 76 S. 1, 1995 Nr. L 17 S. 20, 1996 Nr. L 135 S. 36), zuletzt geändert durch die Richtlinie 2004/106/EG des Rates vom 16. November 2004 (ABl. EU Nr. L 359 S. 30), nicht gilt;"@de .

dtd:LA-id183 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Flug- oder Seereisende:"@de .

dtd:LA-id184 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Passagiere, die im Luftverkehr oder im Seeverkehr reisen; ausgenommen sind die Binnenschifffahrt sowie die private nichtgewerbliche Luftfahrt und die private nichtgewerbliche Seefahrt;"@de .

dtd:LA-id185 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "private nichtgewerbliche Luftfahrt oder private nichtgewerbliche Seefahrt:"@de .

dtd:LA-id186 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Nutzung eines Luftfahrzeugs oder eines Wasserfahrzeugs für den Seeverkehr durch Eigentümer oder Mieter; nicht dazu gehören Fahrzeuge, die für gewerbliche Zwecke, insbesondere für die entgeltliche Beförderung von Passagieren oder Waren oder für die entgeltliche Erbringung von Dienstleistungen oder für behördliche Zwecke genutzt werden;"@de .

dtd:LA-id187 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "persönliches Gepäck:"@de .

dtd:LA-id188 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sämtliche Gepäckstücke, die Reisende der Zollstelle bei Ankunft, sowie die Gepäckstücke, die derselben Zollstelle später gestellt werden, wobei nachzuweisen ist, dass sie bei Abreise bei der Gesellschaft, die den Reisenden befördert hat, als Reisegepäck aufgegeben wurden; anderer Kraftstoff als der Kraftstoff im Sinne des § 2 Abs. 1 Nr. 4 gilt nicht als persönliches Reisegepäck;"@de .

dtd:LA-id189 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Reisemitbringsel:"@de .

dtd:LA-id190 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Waren, die Reisende gelegentlich und ausschließlich zum persönlichen Ge- oder Verbrauch, für ihre Familienangehörigen oder als Geschenk in ihrem persönlichen Gepäck einführen;"@de .

dtd:LA-id191 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zigarillos:"@de .

dtd:LA-id192 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zigarren mit einem Stückgewicht von höchstens drei Gramm;"@de .

dtd:LA-id193 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grenzgebiet:"@de .

dtd:LA-id194 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gemeinden an der deutsch-schweizerischen Grenze, die in der deutschen Zollgrenzzone im Sinne des Artikels 1 Abs. 1 und 2 des deutsch-schweizerischen Abkommens vom 5. Februar 1958 über den Grenz- und Durchgangsverkehr (BGBl. 1960 II S. 2161, 2283) gelegen sind;"@de .

dtd:LA-id195 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grenzarbeitnehmer und Grenzarbeitnehmerinnen:"@de .

dtd:LA-id196 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, die zur Ausübung ihrer gewöhnlichen beruflichen Tätigkeit an den Tagen, an denen sie arbeiten, die Grenze überschreiten."@de .


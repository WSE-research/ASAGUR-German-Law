    dtd:has_Value "einem Aufsatz sowie"@de .

dtd:LA-id599 a dtd:LA ;
    dtd:has_Value "einem Test zur Erfassung der kognitiven Leistungsfähigkeit und zur Erfassung von Persönlichkeitsmerkmalen."@de .

dtd:LA-id600 a dtd:LA ;
    dtd:has_Value "einer Gruppenaufgabe und"@de .

dtd:LA-id601 a dtd:LA ;
    dtd:has_Value "einem strukturierten Interview."@de .

dtd:LA-id602 a dtd:LA ;
    dtd:has_Value "die Kompetenzbereiche,"@de .

dtd:LA-id603 a dtd:LA ;
    dtd:has_Value "ihre Zuordnung zu den Abschnitten und"@de .

dtd:LA-id604 a dtd:LA ;
    dtd:has_Value "die Gewichtung der beiden Abschnitte für die Ermittlung der Teilnoten der Kompetenzbereiche (§ 11 Absatz 3)."@de .

dtd:LA-id605 a dtd:LA ;
    dtd:has_Value "ein Abschnitt des schriftlichen Teils mit der Note 5 bewertet worden ist oder"@de .

dtd:LA-id606 a dtd:LA ;
    dtd:has_Value "im mündlichen Teil der Kompetenzbereich „soziales Verhalten“ oder mindestens zwei andere Kompetenzbereiche mit der Teilnote 4,50 oder schlechter bewertet worden sind."@de .

dtd:LA-id607 a dtd:LA ;
    dtd:has_Value "der Aufsatz mit 15 Prozent,"@de .

dtd:LA-id608 a dtd:LA ;
    dtd:has_Value "der Test zur Erfassung der kognitiven Leistungsfähigkeit und zur Erfassung von Persönlichkeitsmerkmalen mit 25 Prozent und"@de .

dtd:LA-id609 a dtd:LA ;
    dtd:has_Value "der mündliche Teil mit 60 Prozent."@de .

dtd:LA-id610 a dtd:LA ;
    dtd:has_Value "die Lehrpläne für die Ausbildungslehrgänge sowie"@de .

dtd:LA-id611 a dtd:LA ;
    dtd:has_Value "für jede Anwärterin und jeden Anwärter einen Ausbildungsplan, in dem die Ausbildungsstellen und Arbeitsbereiche sowie die Zeiträume der Ausbildungsabschnitte enthalten sind, und gibt ihn der Anwärterin oder dem Anwärter bekannt."@de .

dtd:LA-id612 a dtd:LA ;
    dtd:has_Value "Bankbetriebslehre,"@de .

dtd:LA-id613 a dtd:LA ;
    dtd:has_Value "Zentralbankbetriebslehre,"@de .

dtd:LA-id614 a dtd:LA ;
    dtd:has_Value "Finanzmathematik,"@de .

dtd:LA-id615 a dtd:LA ;
    dtd:has_Value "Rechnungswesen,"@de .

dtd:LA-id616 a dtd:LA ;
    dtd:has_Value "Rechts- und Staatsbürgerkunde sowie"@de .

dtd:LA-id617 a dtd:LA ;
    dtd:has_Value "Deutsch."@de .

dtd:LA-id618 a dtd:LA ;
    dtd:has_Value "eine oder bei Bedarf mehrere Prüfungskommissionen einzurichten und deren Mitglieder zu bestellen,"@de .

dtd:LA-id619 a dtd:LA ;
    dtd:has_Value "sicherzustellen, dass alle Prüfungskommissionen denselben Bewertungsmaßstab anlegen,"@de .

dtd:LA-id620 a dtd:LA ;
    dtd:has_Value "die Prüfungsorte und Prüfungszeitpunkte zu bestimmen und dafür zu sorgen, dass diese den Anwärterinnen und Anwärtern rechtzeitig mitgeteilt werden,"@de .

dtd:LA-id621 a dtd:LA ;
    dtd:has_Value "über die Zulassung der Anwärterinnen und Anwärter zur schriftlichen Abschlussprüfung zu entscheiden,"@de .

dtd:LA-id622 a dtd:LA ;
    dtd:has_Value "die Aufgaben der Klausuren der schriftlichen Abschlussprüfung, die Bearbeitungszeiten sowie die zulässigen Hilfsmittel zu bestimmen,"@de .

dtd:LA-id623 a dtd:LA ;
    dtd:has_Value "über die Zulassung der Anwärterinnen und Anwärter zur mündlichen Abschlussprüfung zu entscheiden und"@de .

dtd:LA-id624 a dtd:LA ;
    dtd:has_Value "das Abschlusszeugnis zu erteilen."@de .

dtd:LA-id625 a dtd:LA ;
    dtd:has_Value "einer Beamtin oder einem Beamten des höheren oder gehobenen Dienstes als Vorsitzender oder Vorsitzendem und"@de .


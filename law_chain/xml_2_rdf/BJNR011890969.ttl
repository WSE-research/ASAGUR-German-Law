    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Bilanzsumme einer auf den Abschlußstichtag aufgestellten Jahresbilanz übersteigt 65 Millionen Euro."@de .

dtd:LA-id394 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Umsatzerlöse des Unternehmens in den zwölf Monaten vor dem Abschlußstichtag übersteigen 130 Millionen Euro."@de .

dtd:LA-id395 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Das Unternehmen hat in den zwölf Monaten vor dem Abschlußstichtag durchschnittlich mehr als fünftausend Arbeitnehmer beschäftigt."@de .

dtd:LA-id396 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Personenhandelsgesellschaft, für die kein Abschluss nach § 264a oder § 264b des Handelsgesetzbuchs aufgestellt wird, oder des Einzelkaufmanns,"@de .


dtd:LA-id413 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Bilanzsumme einer auf den Konzernabschlußstichtag aufgestellten Konzernbilanz übersteigt 65 Millionen Euro."@de .

dtd:LA-id414 a dtd:LA ;

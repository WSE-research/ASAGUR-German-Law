dtd:I-id133 a dtd:I ;
    dtd:has_Value "Wärmeerzeugungsanlagen"@de .

dtd:LA-id225 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die im Wege einer registrierenden Leistungsmessung beliefert werden, wenn deren Jahresverbrauch mehr als 1 500 000 Kilowattstunden beträgt,"@de .

dtd:LA-id226 a dtd:LA ;
    dtd:has_I dtd:I-id133 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit sie das Erdgas für den kommerziellen Betrieb von Strom- und Wärmeerzeugungsanlagen beziehen, oder"@de .

dtd:LA-id227 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit sie zugelassene Krankenhäuser sind."@de .

dtd:LA-id228 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die das Erdgas weit überwiegend im Zusammenhang mit der Vermietung von Wohnraum oder als Wohnungseigentümergemeinschaft im Sinne des Wohnungseigentumsgesetzes beziehen,"@de .

dtd:LA-id229 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zugelassene Pflege-, Vorsorge- oder Rehabilitationseinrichtungen sowie Kindertagesstätten und andere Einrichtungen der Kinder- und Jugendhilfe sind, die im Aufgabenbereich des Sozialgesetzbuchs soziale Leistungen erbringen,"@de .

dtd:LA-id230 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die staatliche, staatlich anerkannte oder gemeinnützige Einrichtungen des Bildungs-, Wissenschafts- und Forschungsbereichs oder Bildungseinrichtungen der Selbstverwaltung der Wirtschaft in der Rechtsform von Körperschaften des öffentlichen Rechts oder als eingetragener Verein organisiert sind oder"@de .

dtd:LA-id231 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einrichtungen der medizinischen Rehabilitation, Einrichtungen der beruflichen Rehabilitation, Werkstätten für Menschen mit Behinderungen, anderer Leistungsanbieter oder Leistungserbringer der Eingliederungshilfe nach Teil 2 des Neunten Buches Sozialgesetzbuch sind."@de .

dtd:LA-id232 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem arbeitsbezogenen Preiselement nach den Sätzen 2 bis 5 und"@de .

dtd:LA-id233 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "allen anderen Preiselementen, soweit diese nach dem Erdgasliefervertrag anteilig für den Monat Dezember 2022 anfallen."@de .

dtd:LA-id234 a dtd:LA ;
    dtd:has_Value "als vorläufige Leistung im Januar 2023 auf die Zahlung einer vertraglich vereinbarten Vorauszahlung oder einer Abschlagszahlung für diesen Monat entsprechend der Regelung des Absatzes 2 zu verzichten oder"@de .

dtd:LA-id235 a dtd:LA ;
    dtd:has_Value "den Entlastungsbetrag nach § 2 Absatz 2 Satz 2 und 3 bis zum 31. Januar 2023 an den Letztverbraucher gesondert auszuzahlen."@de .

dtd:LA-id236 a dtd:LA ;
    dtd:has_Value "der Kunde bezieht die Wärme im Zusammenhang mit der Vermietung von Wohnraum oder als Wohnungseigentümergesellschaft im Sinne des Wohnungseigentumsgesetzes,"@de .

dtd:LA-id237 a dtd:LA ;
    dtd:has_Value "es handelt sich um zugelassene Pflege-, Vorsorge- und Rehabilitationseinrichtungen, sowie Kindertagestätten und andere Einrichtungen der Kinder- und Jugendhilfe, die im Aufgabenbereich des Sozialgesetzbuchs soziale Leistungen erbringen,"@de .

dtd:LA-id238 a dtd:LA ;
    dtd:has_Value "es handelt sich um die Entnahmestelle einer staatlichen, staatlich anerkannten oder gemeinnützigen Einrichtung des Bildungs-, Wissenschafts- und Forschungsbereichs oder Bildungseinrichtungen der Selbstverwaltung der Wirtschaft in der Rechtsform von Körperschaften des öffentlichen Rechts oder als eingetragener Verein,"@de .

dtd:LA-id239 a dtd:LA ;
    dtd:has_Value "es handelt sich um Einrichtungen der medizinischen Rehabilitation, Einrichtungen der beruflichen Rehabilitation, Werkstätten für Menschen mit Behinderungen, anderer Leistungsanbieter oder Leistungserbringer der Eingliederungshilfe nach Teil 2 des Neunten Buches Sozialgesetzbuch sind."@de .

dtd:LA-id240 a dtd:LA ;
    dtd:has_Value "der Mieter, dessen Vorauszahlungen für Betriebskosten aufgrund der steigenden Kosten für leitungsgebundenes Erdgas und Wärme in den letzten neun Monaten vor dem 19. November 2022 erhöht wurden, in Höhe dieses Erhöhungsbetrags und"@de .

dtd:LA-id241 a dtd:LA ;
    dtd:has_Value "der Mieter, für den in diesem Zeitraum eine Vorauszahlung von Betriebskosten für leitungsgebundenes Erdgas erstmalig vereinbart wurde, in Höhe eines Betrags von 25 Prozent seiner Betriebskostenvorauszahlung für den Monat Dezember 2022."@de .

dtd:LA-id242 a dtd:LA ;
    dtd:has_Value "die Höhe der beantragten Vorauszahlung,"@de .

dtd:LA-id243 a dtd:LA ;
    dtd:has_Value "die IBAN eines auf den Namen des Erdgaslieferanten lautenden Zahlungskontos bei einem Kreditinstitut mit Sitz oder Niederlassung in Deutschland."@de .

dtd:LA-id244 a dtd:LA ;
    dtd:has_Value "die Angaben nach Absatz 2 Satz 1,"@de .

dtd:LA-id245 a dtd:LA ;
    dtd:has_Value "die jeweils auf Arbeitspreis, Grundpreis, Umsatzsteuer und sonstige Abgaben entfallenden Teilsummen der beantragten Vorauszahlung,"@de .

dtd:LA-id246 a dtd:LA ;
    dtd:has_Value "die der beantragten Vorauszahlung zugrunde liegende Anzahl von Letztverbrauchern, aufgeteilt nach Belieferung über ein Standardlastprofil und registrierender Leistungsmessung,"@de .

dtd:LA-id247 a dtd:LA ;
    dtd:has_Value "die der beantragten Vorauszahlung zugrunde liegende prognostizierte Liefermenge, aufgeteilt nach Belieferung über ein Standardlastprofil und registrierender Leistungsmessung,"@de .

dtd:LA-id248 a dtd:LA ;
    dtd:has_Value "die Liefermenge des Jahres 2021, aufgeteilt nach Belieferung über ein Standardlastprofil und registrierender Leistungsmessung,"@de .

dtd:LA-id249 a dtd:LA ;
    dtd:has_Value "die Betriebsnummer des Erdgaslieferanten bei der Bundesnetzagentur."@de .

dtd:LA-id250 a dtd:LA ;
    dtd:has_Value "die Höhe der beantragten Erstattung,"@de .

dtd:LA-id251 a dtd:LA ;
    dtd:has_Value "die IBAN eines auf den Namen des Wärmeversorgungsunternehmens lautenden Zahlungskontos bei einem Kreditinstitut mit Sitz oder Niederlassung in Deutschland."@de .

dtd:LA-id252 a dtd:LA ;
    dtd:has_Value "die Angaben nach Absatz 2 Satz 1,"@de .

dtd:LA-id253 a dtd:LA ;
    dtd:has_NB dtd:NB-id135 ;
    dtd:has_Value "die Angaben zu den der der beantragten Erstattung zugrunde liegenden Kundenbeziehungen, zum Zweck der Plausibilisierung mit Angabe einer E-Mail-Adresse oder einer Telefonnummer, der Postanschrift des Kunden, sowie der Abschlagszahlung des Kunden für September 2022 gemäß § 4 Absatz 3,"@de .

dtd:LA-id254 a dtd:LA ;
    dtd:has_Value "die Liefermenge des Jahres 2021 oder ersatzweise die Liefermenge des letzten Abrechnungszeitraums."@de .

dtd:LA-id255 a dtd:LA ;
    dtd:has_DL dtd:DL-id213 ;
    dtd:has_Value "Ansprüche der Letztverbraucher a)auf Gutschrift des einmaligen Entlastungsbetrages nach § 2 undb)auf die vorläufige Leistung auf diesen Entlastungsanspruch nach § 3,"@de .

dtd:LA-id256 a dtd:LA ;
    dtd:has_Value "auf Gutschrift des einmaligen Entlastungsbetrages nach § 2 und"@de .

dtd:LA-id257 a dtd:LA ;
    dtd:has_Value "auf die vorläufige Leistung auf diesen Entlastungsanspruch nach § 3,"@de .

dtd:LA-id258 a dtd:LA ;
    dtd:has_Value "Ansprüche der Kunden auf Kompensation nach § 4 sowie"@de .

dtd:LA-id259 a dtd:LA ;
    dtd:has_Value "Ansprüche der Mieter und Wohnungseigentümer auf Weitergabe der Entlastung im Rahmen der Heizkostenabrechnung oder Jahresabrechnung nach § 5."@de .

dtd:LA-id260 a dtd:LA ;
    dtd:has_Value "Liefermenge, aufgeteilt nach Belieferung über ein Standardlastprofil und registrierender Lastgangmessung,"@de .

dtd:LA-id261 a dtd:LA ;
    dtd:has_Value "Anzahl der belieferten Marktlokationen, aufgeteilt nach Belieferung über ein Standardlastprofil und registrierender Lastgangmessung und"@de .

dtd:LA-id262 a dtd:LA ;
    dtd:has_Value "die Betriebsnummer des Gaslieferanten."@de .

dtd:NB-id134 a dtd:NB ;
    dtd:has_Value "1 500 000"@de .


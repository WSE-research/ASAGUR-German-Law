dtd:LA-id189 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Feststellung der Anerkennung oder Nichtanerkennung einer in einem anderen Vertragsstaat getroffenen Maßnahme nach Artikel 23 des Übereinkommens,"@de .

dtd:LA-id190 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vollstreckbarerklärung einer in einem anderen Vertragsstaat getroffenen Maßnahme nach Artikel 25 des Übereinkommens sowie"@de .

dtd:LA-id191 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Konsultationsverfahren nach Artikel 33 des Übereinkommens."@de .

dtd:LA-id192 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Durchführung der beabsichtigten Unterbringung dem Wohl des Betroffenen widerspricht, insbesondere weil er keine besondere Bindung zum Inland hat,"@de .

dtd:LA-id193 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die ausländische Behörde kein Gutachten eines Sachverständigen vorlegt, aus dem sich die Notwendigkeit der beabsichtigten Unterbringung ergibt,"@de .

dtd:LA-id194 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Grund für eine Versagung der Anerkennung nach Artikel 22 Abs. 2 des Übereinkommens erkennbar ist,"@de .

dtd:LA-id195 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Betroffenen im ausländischen Verfahren kein rechtliches Gehör gewährt wurde,"@de .

dtd:LA-id196 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer erforderlichen Genehmigung der Ausländerbehörde Gründe entgegenstehen oder"@de .

dtd:LA-id197 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Übernahme der Kosten für die Unterbringung nicht geregelt ist."@de .

dtd:LA-id198 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im ersuchenden Staat über die ersuchte Maßnahme kein Gericht entscheidet oder"@de .

dtd:LA-id199 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Zugrundelegung des mitgeteilten Sachverhalts nach innerstaatlichem Recht die Anordnung der ersuchten Maßnahme nicht zulässig wäre."@de .


    dtd:has_Value "in der knappschaftlichen Rentenversicherung jährlich 100 200 Euro und monatlich 8 350 Euro."@de .

dtd:P-id101 a dtd:P ;
    dtd:has_Value "(1) Die Jahresarbeitsentgeltgrenze nach § 6 Absatz 6 des Fünften Buches Sozialgesetzbuch für das Jahr 2022 beträgt 64 350 Euro."@de .

dtd:P-id102 a dtd:P ;
    dtd:has_Value "(2) Die Jahresarbeitsentgeltgrenze nach § 6 Absatz 7 des Fünften Buches Sozialgesetzbuch für das Jahr 2022 beträgt 58 050 Euro."@de .

dtd:P-id104 a dtd:P ;
    dtd:has_Value "Diese Verordnung tritt am 1. Januar 2022 in Kraft."@de .

dtd:P-id106 a dtd:P ;
    dtd:has_Value "Der Bundesrat hat zugestimmt."@de .


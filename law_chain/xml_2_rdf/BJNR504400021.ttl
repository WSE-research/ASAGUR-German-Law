dtd:LA-id79 a dtd:LA ;
    dtd:has_Value "des § 69 Absatz 2 in Verbindung mit § 68 Absatz 2 Satz 1 und § 228b, des § 160 Nummer 2 in Verbindung mit § 159, § 68 Absatz 2 Satz 1 und § 228b sowie des § 275b in Verbindung mit § 275a des Sechsten Buches Sozialgesetzbuch – Gesetzliche Rentenversicherung –, von denen § 69 Absatz 2 zuletzt durch Artikel 4 Nummer 3 Buchstabe b des Gesetzes vom 22. Dezember 2011 (BGBl. I S. 3057), § 68 Absatz 2 und § 159 zuletzt durch Artikel 5 Nummer 1 Buchstabe b und Nummer 4 des Gesetzes vom 2. Dezember 2006 (BGBl. I S. 2742) sowie § 228b und § 275a zuletzt durch Artikel 1 Nummer 11, Nummer 19 Buchstabe b und Nummer 31 des Gesetzes vom 17. Juli 2017 (BGBl. I S. 2575) geändert worden sind,"@de .

dtd:LA-id80 a dtd:LA ;
    dtd:has_Value "des § 6 Absatz 6 und 7 des Fünften Buches Sozialgesetzbuch – Gesetzliche Krankenversicherung –, dessen Absatz 7 durch Artikel 1 Nummer 1 Buchstabe c des Gesetzes vom 23. Dezember 2002 (BGBl. I S. 4637) eingefügt und dessen Absatz 6 durch Artikel 4 des Gesetzes vom 2. Dezember 2006 (BGBl. I S. 2742) geändert worden ist,"@de .

dtd:LA-id81 a dtd:LA ;
    dtd:has_Value "des § 17 Absatz 2 Satz 1 in Verbindung mit § 18 des Vierten Buches Sozialgesetzbuch – Gemeinsame Vorschriften für die Sozialversicherung –, dessen § 18 durch Artikel 3 Nummer 4 des Gesetzes vom 17. Juli 2017 (BGBl. I S. 2575) geändert worden ist,"@de .

dtd:LA-id82 a dtd:LA ;
    dtd:has_Value "in der allgemeinen Rentenversicherung jährlich 84 600 Euro und monatlich 7 050 Euro,"@de .

dtd:LA-id83 a dtd:LA ;
    dtd:has_Value "in der knappschaftlichen Rentenversicherung jährlich 103 800 Euro und monatlich 8 650 Euro."@de .

dtd:LA-id84 a dtd:LA ;
    dtd:has_Value "in der allgemeinen Rentenversicherung jährlich 81 000 Euro und monatlich 6 750 Euro,"@de .


dtd:LA-id1883 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beförderungsbelege (§ 17b Absatz 3 Satz 1 Nummer 3 bis 5) oder Versendungsbelege (§ 17b Absatz 3 Satz 1 Nummer 1 und 2);"@de .


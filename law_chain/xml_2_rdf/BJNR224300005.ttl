    dtd:has_Value ""@de .

dtd:P-id951 a dtd:P ;
    dtd:has_Value "Die Messung erfolgt nach den Bestimmungen des Messstellenbetriebsgesetzes vom 29. August 2016 (BGBl. I S. 2034) in der jeweils geltenden Fassung."@de .


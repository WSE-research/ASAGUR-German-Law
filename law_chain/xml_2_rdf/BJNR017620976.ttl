dtd:LA-id552 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Diakonie Deutschland,"@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Deutschen Caritasverbandes,"@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Arbeiterwohlfahrt,"@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Fachverbände, die den in den Nummern 1 bis 3 genannten Verbänden angeschlossen sind, sowie"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstiger Organisationen mit Sitz im Inland."@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zentrale Adoptionsstelle des Landesjugendamtes;"@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine anerkannte Auslandsvermittlungsstelle nach § 4 Absatz 2 im Rahmen der ihr erteilten Zulassung."@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu jedem Vermittlungsfall im Sinne des Absatzes 1 von der ersten Beteiligung einer ausländischen Stelle an die jeweils verfügbaren personenbezogenen Daten (Name, Geschlecht, Geburtsdatum, Geburtsort, Staatsangehörigkeit, Familienstand und Wohnsitz oder gewöhnlicher Aufenthalt) des Kindes, seiner Eltern und der Adoptionsbewerber sowie zum Stand des Vermittlungsverfahrens zu übermitteln,"@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jährlich zusammenfassend über Umfang, Verlauf und Ergebnisse ihrer Arbeit auf dem Gebiet der internationalen Adoptionsvermittlung zu berichten und"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf deren Ersuchen über einzelne Vermittlungsfälle im Sinne des Absatzes 1 Auskunft zu geben, soweit dies zur Erfüllung der Aufgaben nach Absatz 5 und nach § 2 Absatz 2 Satz 1 des Adoptionsübereinkommens-Ausführungsgesetzes vom 5. November 2001 (BGBl. I S. 2950) in der jeweils geltenden Fassung erforderlich ist."@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Adoption dem Kindeswohl dient,"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Kind adoptiert werden kann und dass keine Anhaltspunkte dafür vorliegen, dass eine geeignete Unterbringung des Kindes im Heimatstaat nach Prüfung durch die Fachstelle des Heimatstaats möglich ist,"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Eltern oder andere Personen, Behörden und Institutionen, deren Zustimmung zur Adoption erforderlich ist, über die Wirkungen der Adoption aufgeklärt wurden und freiwillig und in der gesetzlich vorgeschriebenen Form der Adoption des Kindes zugestimmt haben und die Eltern ihre Zustimmung nicht widerrufen haben,"@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "unter Berücksichtigung des Alters und der Reife des Kindes das Kind über die Wirkungen der Adoption aufgeklärt wurde, seine Wünsche berücksichtigt wurden und das Kind freiwillig und in der gesetzlich vorgeschriebenen Form der Adoption zugestimmt hat und"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "keine Anhaltspunkte dafür vorliegen, dass die Zustimmung zur Adoption weder der Eltern noch des Kindes durch eine Geldzahlung oder eine andere Gegenleistung herbeigeführt wurde."@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erklärung nach § 2c Absatz 5 Satz 2 vorliegt und an die Fachstelle des Heimatstaats weitergeleitet worden ist und"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Annehmenden einen Antrag auf Anerkennung nach § 1 Absatz 2 des Adoptionswirkungsgesetzes gestellt haben."@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Voraussetzungen des § 3 erfüllt,"@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "insbesondere nach ihrer Arbeitsweise und nach der Finanzlage ihres Rechtsträgers die ordnungsgemäße Erfüllung ihrer Aufgaben erwarten lässt und"@de .

dtd:LA-id571 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von einer juristischen Person oder Personenvereinigung unterhalten wird, die steuerbegünstigte Zwecke im Sinne der §§ 51 bis 68 der Abgabenordnung verfolgt."@de .

dtd:LA-id572 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kann die zentrale Adoptionsstelle des Landesjugendamtes Auskünfte, Einsicht in Unterlagen sowie die Vorlage von Nachweisen verlangen;"@de .

dtd:LA-id573 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dürfen die mit der Prüfung beauftragten Bediensteten Grundstücke und Geschäftsräume innerhalb der üblichen Geschäftszeiten betreten; das Grundrecht der Unverletzlichkeit der Wohnung (Artikel 13 des Grundgesetzes) wird insoweit eingeschränkt."@de .

dtd:LA-id574 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu bestimmen, dort ihr Kind zur Adoption wegzugeben,"@de .

dtd:LA-id575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihnen zu einer solchen Weggabe Hilfe zu leisten."@de .

dtd:LA-id576 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die persönlichen und familiären Umstände der Adoptionsbewerber,"@de .

dtd:LA-id577 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Gesundheitszustand der Adoptionsbewerber,"@de .

dtd:LA-id578 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das soziale Umfeld der Adoptionsbewerber,"@de .

dtd:LA-id579 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beweggründe der Adoptionsbewerber für die Adoption sowie"@de .

dtd:LA-id580 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Eigenschaften der Kinder, für die zu sorgen die Adoptionsbewerber fähig und bereit sind."@de .

dtd:LA-id581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Wissen und die Auseinandersetzung der Adoptionsbewerber mit der Kultur und der sozialen Situation im Heimatstaat des Kindes,"@de .

dtd:LA-id582 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bereitschaft der Adoptionsbewerber, die Herkunft des Kindes in das zukünftige Familienleben zu integrieren, sowie"@de .

dtd:LA-id583 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bereitschaft der Adoptionsbewerber, sich auf die besonderen Bedürfnisse des Kindes auf Grund seiner Herkunft und auf Grund des Wechsels des Kulturkreises einzulassen."@de .

dtd:LA-id584 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Eignungsprüfung (§ 7 und § 7b Absatz 1 und 2),"@de .

dtd:LA-id585 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die sachdienlichen Ermittlungen (§ 7a Absatz 1 und 2),"@de .

dtd:LA-id586 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die länderspezifische Eignungsprüfung (§ 7c Absatz 1 und 2),"@de .

dtd:LA-id587 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Prüfung nach § 7d Absatz 1."@de .

dtd:LA-id588 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die allgemeine Beratung der Adoptionsbewerber, der Eltern und des Kindes zu Fragen im Zusammenhang mit der Adoption und die bedarfsgerechte Unterstützung,"@de .

dtd:LA-id589 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Information über die Voraussetzungen und den Ablauf des Adoptionsverfahrens sowie über die Rechtsfolgen der Adoption,"@de .

dtd:LA-id590 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Information für die abgebenden Eltern über unterstützende Maßnahmen im Rahmen der Kinder- und Jugendhilfe als Alternative zur Adoption sowie die Unterstützung der abgebenden Eltern bei der Bewältigung sozialer und psychischer Auswirkungen im Zusammenhang mit der bevorstehenden oder bereits erfolgten Einwilligung in die Adoption des Kindes,"@de .

dtd:LA-id591 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Information über die Rechte des Kindes, in der die Bedeutung der Kenntnis der Herkunft des Kindes für seine Entwicklung hervorzuheben ist,"@de .

dtd:LA-id592 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Hinwirken darauf, dass die Adoptionsbewerber das Kind von Beginn an entsprechend seinem Alter und seiner Reife über seine Herkunft aufklären,"@de .

dtd:LA-id593 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Information über die Möglichkeiten und Gestaltung von Informationsaustausch oder Kontakt zwischen den Adoptionsbewerbern und dem Kind auf der einen Seite und den Eltern auf der anderen Seite nach Maßgabe der §§ 8a und 8b,"@de .

dtd:LA-id594 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erörterung der Gestaltung eines Informationsaustauschs oder von Kontakten zwischen den Adoptionsbewerbern und dem Kind auf der einen Seite und den Eltern auf der anderen Seite nach Maßgabe des § 8a sowie"@de .

dtd:LA-id595 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Information über das Recht zur Akteneinsicht nach § 9c Absatz 2 und die Information zu Möglichkeiten der Suche nach der Herkunft des Kindes."@de .

dtd:LA-id596 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die bedarfsgerechte Beratung und Unterstützung des Kindes, der Annehmenden und der abgebenden Eltern,"@de .

dtd:LA-id597 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Förderung und die Begleitung eines Informationsaustauschs oder von Kontakten zwischen den Annehmenden und dem Kind auf der einen Seite und den abgebenden Eltern auf der anderen Seite nach Maßgabe der §§ 8a und 8b,"@de .

dtd:LA-id598 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Unterstützung der abgebenden Eltern bei der Bewältigung sozialer und psychischer Auswirkungen auf Grund der Entscheidung zur Einwilligung in die Adoption des Kindes, insbesondere indem die Adoptionsvermittlungsstelle den abgebenden Eltern Hilfen durch andere Fachdienste aufzeigt,"@de .

dtd:LA-id599 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Unterstützung der Annehmenden bei der altersentsprechenden Aufklärung des Kindes über seine Herkunft sowie"@de .

dtd:LA-id600 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Begleitung des Kindes bei der Suche nach der Herkunft, einschließlich der Begleitung des vertraulich geborenen Kindes bei der Einsichtnahme in den Herkunftsnachweis nach § 31 Absatz 1 des Schwangerschaftskonfliktgesetzes."@de .

dtd:LA-id601 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "während eines in der Vereinbarung festzulegenden Zeitraums nach der Adoption die Entwicklung des Kindes beobachtet und"@de .

dtd:LA-id602 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der zuständigen Stelle im Heimatstaat über die Entwicklung berichtet."@de .

dtd:LA-id603 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Eltern des anzunehmenden Kindes,"@de .

dtd:LA-id604 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Annehmende und"@de .

dtd:LA-id605 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Kind gemäß § 8 des Achten Buches Sozialgesetzbuch."@de .

dtd:LA-id606 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "er zur Abgabe einer Erklärung dauernd außerstande ist,"@de .

dtd:LA-id607 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sein Aufenthalt dauernd unbekannt ist,"@de .

dtd:LA-id608 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seine Einwilligung nach § 1748 des Bürgerlichen Gesetzbuchs ersetzt wird oder"@de .

dtd:LA-id609 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "es sich um den abgebenden Elternteil handelt und dieser seinen gewöhnlichen Aufenthalt im Ausland hat."@de .

dtd:LA-id610 a dtd:LA ;
    dtd:has_Size dtd:normal ;

dtd:LA-id467 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Meldebestätigung des Unionsbürgers, den die Familienangehörigen begleiten oder dem sie nachziehen."@de .

dtd:LA-id468 a dtd:LA ;
    dtd:has_DL dtd:DL-id367 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein durch die zuständige Behörde des Ursprungs- oder Herkunftslands ausgestelltes Dokument, aus dem hervorgeht, a)in Fällen nach § 3a Absatz 1 Nummer 1 Buchstabe a, dass und seit wann die nahestehende Person vom Unionsbürger Unterhalt bezieht,b)in Fällen nach § 3a Absatz 1 Nummer 1 Buchstabe b, dass und wie lange die nahestehende Person mit dem Unionsbürger in häuslicher Gemeinschaft gelebt hat,"@de .

dtd:LA-id469 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Fällen nach § 3a Absatz 1 Nummer 1 Buchstabe a, dass und seit wann die nahestehende Person vom Unionsbürger Unterhalt bezieht,"@de .

dtd:LA-id470 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Fällen nach § 3a Absatz 1 Nummer 1 Buchstabe b, dass und wie lange die nahestehende Person mit dem Unionsbürger in häuslicher Gemeinschaft gelebt hat,"@de .

dtd:LA-id471 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Fällen nach § 3a Absatz 1 Nummer 1 Buchstabe c den Nachweis schwerwiegender gesundheitlicher Gründe, die die persönliche Pflege der nahestehenden Person durch den Unionsbürger zwingend erforderlich machen,"@de .

dtd:LA-id472 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Fällen nach § 3a Absatz 1 Nummer 2 den urkundlichen Nachweis des Bestehens der Vormundschaft oder des Pflegekindverhältnisses sowie einen Nachweis der Abhängigkeit der nahestehenden Person vom Unionsbürger und"@de .

dtd:LA-id473 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Fällen nach § 3a Absatz 1 Nummer 3 den Nachweis über die Umstände für das Bestehen einer dauerhaften Beziehung nach § 1 Absatz 2 Nummer 4 Buchstabe c zwischen dem Unionsbürger und der nahestehenden Person."@de .

dtd:LA-id474 a dtd:LA ;
    dtd:has_DL dtd:DL-id379 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Einreise in das oder der Ausreise aus dem Bundesgebiet einen Pass oder anerkannten Passersatza)mit sich zu führen undb)einem zuständigen Beamten auf Verlangen zur Prüfung vorzulegen,"@de .

dtd:LA-id475 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit sich zu führen und"@de .

dtd:LA-id476 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem zuständigen Beamten auf Verlangen zur Prüfung vorzulegen,"@de .

dtd:LA-id477 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Dauer des Aufenthalts im Bundesgebiet den erforderlichen Pass oder Passersatz zu besitzen,"@de .

dtd:LA-id478 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Pass oder Passersatz sowie die Aufenthaltskarte, die Bescheinigung des Daueraufenthalts und die Daueraufenthaltskarte den mit der Ausführung dieses Gesetzes betrauten Behörden auf Verlangen vorzulegen, auszuhändigen und vorübergehend zu überlassen, soweit dies zur Durchführung oder Sicherung von Maßnahmen nach diesem Gesetz erforderlich ist."@de .

dtd:LA-id479 a dtd:LA ;
    dtd:has_Value "entgegen § 8 Absatz 1 Nummer 1 Buchstabe b oder Nummer 3 ein dort genanntes Dokument nicht oder nicht rechtzeitig vorlegt oder"@de .

dtd:LA-id480 a dtd:LA ;
    dtd:has_Value "entgegen § 8 Absatz 1a ein dort genanntes Dokument nicht oder nicht rechtzeitig vorlegt oder einen Abgleich mit dem Lichtbild nicht oder nicht rechtzeitig ermöglicht."@de .

dtd:LA-id481 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich selbst als Familienangehörige im Bundesgebiet aufgehalten haben und nach § 3 Absatz 2 nach dem Tod eines Unionsbürgers ein Aufenthaltsrecht behalten,"@de .

dtd:LA-id482 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht Unionsbürger sind, sich selbst als Ehegatten oder Lebenspartner im Bundesgebiet aufgehalten haben, und die nach der Scheidung oder Aufhebung der Ehe oder Aufhebung der Lebenspartnerschaft nach § 3 Absatz 4 ein Aufenthaltsrecht behalten, oder"@de .

dtd:LA-id483 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich als nahestehende Personen eines verstorbenen Unionsbürgers auf Grund eines Rechts zur Einreise und zum Aufenthalt im Bundesgebiet nach § 3a Absatz 3 in Verbindung mit § 3 Absatz 2 im Bundesgebiet aufgehalten haben,"@de .

dtd:LA-id484 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Inhaber eines Daueraufenthaltsrechts nach Artikel 15 des Austrittsabkommens den Inhabern einer Erlaubnis zum Daueraufenthalt – EU,"@de .

dtd:LA-id485 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Inhaber eines anderen Aufenthaltsrechts nach dem Austrittsabkommen, die britische Staatsangehörige sind, den Inhabern einer Blauen Karte EU und"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "Änderungen der Allgemeinen Verfahrensordnung oder Zusatzprotokolle zur Allgemeinen Verfahrensordnung oder"@de .

dtd:LA-id83 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Änderungen des Weltpostvertrages selbst."@de .

dtd:LA-id84 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sprachliche oder redaktionelle Änderungen,"@de .


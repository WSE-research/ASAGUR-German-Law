dtd:LA-id1061 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Wahrnehmung des Rechts, einen Teil der Mitglieder des Aufsichts- oder Verwaltungsorgans der Gesellschaft zu wählen oder zu bestellen, oder"@de .

dtd:LA-id1062 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Wahrnehmung des Rechts, die Bestellung eines Teils oder aller Mitglieder des Aufsichts- oder Verwaltungsorgans der Gesellschaft zu empfehlen oder abzulehnen."@de .

dtd:LA-id1063 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Identität und Struktur der beteiligten Gesellschaften, betroffenen Tochtergesellschaften und betroffenen Betriebe und deren Verteilung auf die Mitgliedstaaten;"@de .

dtd:LA-id1064 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die in diesen Gesellschaften und Betrieben bestehenden Arbeitnehmervertretungen;"@de .

dtd:LA-id1065 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der in diesen Gesellschaften und Betrieben jeweils beschäftigten Arbeitnehmer sowie die daraus zu errechnende Gesamtzahl der in einem Mitgliedstaat beschäftigten Arbeitnehmer;"@de .

dtd:LA-id1066 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der Arbeitnehmer, denen Mitbestimmungsrechte in den Organen dieser Gesellschaften zustehen."@de .

dtd:LA-id1067 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ort, Tag und Zeit der Versammlung des Wahlgremiums festzulegen;"@de .

dtd:LA-id1068 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anzahl der Mitglieder aus den jeweiligen Arbeitnehmervertretungen nach § 8 Abs. 6 festzulegen;"@de .

dtd:LA-id1069 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Versammlung des Wahlgremiums einzuladen."@de .

dtd:LA-id1070 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall einer SE, die durch Verschmelzung gegründet werden soll, sofern sich die Mitbestimmung auf mindestens 25 Prozent der Gesamtzahl der Arbeitnehmer der beteiligten Gesellschaften und der betroffenen Tochtergesellschaften erstreckt oder"@de .

dtd:LA-id1071 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall einer SE, die als Holding-Gesellschaft oder als Tochtergesellschaft gegründet werden soll, sofern sich die Mitbestimmung auf mindestens 50 Prozent der Gesamtzahl der Arbeitnehmer der beteiligten Gesellschaften und der betroffenen Tochtergesellschaften erstreckt."@de .

dtd:LA-id1072 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Anteil der Arbeitnehmervertreter im Aufsichts- oder Verwaltungsorgan der SE geringer ist als der höchste in den beteiligten Gesellschaften bestehende Anteil oder"@de .

dtd:LA-id1073 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Recht, Mitglieder des Aufsichts- oder Verwaltungsorgans der Gesellschaft zu wählen, zu bestellen, zu empfehlen oder abzulehnen, beseitigt oder eingeschränkt wird."@de .

dtd:LA-id1074 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Beschluss über den Abschluss einer Vereinbarung nach § 13 Abs. 1,"@de .

dtd:LA-id1075 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Beschluss über die Nichtaufnahme oder den Abbruch der Verhandlungen nach § 16 Abs. 1 und"@de .

dtd:LA-id1076 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die jeweiligen Mehrheiten, mit denen die Beschlüsse gefasst worden sind."@de .

dtd:LA-id1077 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Geltungsbereich der Vereinbarung, einschließlich der außerhalb des Hoheitsgebietes der Mitgliedstaaten liegenden Unternehmen und Betriebe, sofern diese in den Geltungsbereich einbezogen werden;"@de .

dtd:LA-id1078 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zusammensetzung des SE-Betriebsrats, die Anzahl seiner Mitglieder und die Sitzverteilung, einschließlich der Auswirkungen wesentlicher Änderungen der Zahl der in der SE beschäftigten Arbeitnehmer;"@de .

dtd:LA-id1079 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Befugnisse und das Verfahren zur Unterrichtung und Anhörung des SE-Betriebsrats;"@de .

dtd:LA-id1080 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Häufigkeit der Sitzungen des SE-Betriebsrats;"@de .

dtd:LA-id1081 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für den SE-Betriebsrat bereitzustellenden finanziellen und materiellen Mittel;"@de .

dtd:LA-id1082 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Zeitpunkt des Inkrafttretens der Vereinbarung und ihre Laufzeit; ferner die Fälle, in denen die Vereinbarung neu ausgehandelt werden soll und das dabei anzuwendende Verfahren."@de .

dtd:LA-id1083 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zahl der Mitglieder des Aufsichts- oder Verwaltungsorgans der SE, welche die Arbeitnehmer wählen oder bestellen können oder deren Bestellung sie empfehlen oder ablehnen können;"@de .

dtd:LA-id1084 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Verfahren, nach dem die Arbeitnehmer diese Mitglieder wählen oder bestellen oder deren Bestellung empfehlen oder ablehnen können und"@de .

dtd:LA-id1085 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Rechte dieser Mitglieder."@de .

dtd:LA-id1086 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Parteien dies vereinbaren oder"@de .

dtd:LA-id1087 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bis zum Ende des in § 20 angegebenen Zeitraums keine Vereinbarung zustande gekommen ist und das besondere Verhandlungsgremium keinen Beschluss nach § 16 gefasst hat."@de .

dtd:LA-id1088 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Geschäftsberichte,"@de .

dtd:LA-id1089 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Tagesordnung aller Sitzungen des Leitungsorgans und des Aufsichts- oder Verwaltungsorgans,"@de .

dtd:LA-id1090 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kopien aller Unterlagen, die der Hauptversammlung der Aktionäre vorgelegt werden."@de .

dtd:LA-id1091 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Struktur der SE sowie die wirtschaftliche und finanzielle Lage;"@de .

dtd:LA-id1092 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die voraussichtliche Entwicklung der Geschäfts-, Produktions- und Absatzlage;"@de .

dtd:LA-id1093 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beschäftigungslage und ihre voraussichtliche Entwicklung;"@de .

dtd:LA-id1094 a dtd:LA ;

dtd:LA-id4838 a dtd:LA ;
    dtd:has_Value "nicht innerhalb von drei Monaten, nachdem sie nach § 52l Absatz 4 in Verbindung mit § 27 Absatz 2 von der Pflicht, eine Kanzlei zu unterhalten, befreit worden ist oder ein bisheriger Zustellungsbevollmächtigter weggefallen ist, einen Zustellungsbevollmächtigten bestellt oder"@de .

dtd:LA-id4839 a dtd:LA ;
    dtd:has_DL dtd:DL-id4259 ;
    dtd:has_Value "nicht innerhalb von drei Monaten einen Zustellungsbevollmächtigen bestellt, nachdem a)sie nach § 59l Absatz 4 in Verbindung mit § 27 Absatz 2 von der Pflicht, eine Kanzlei zu unterhalten, befreit worden ist oderb)ein bisheriger Zustellungsbevollmächtigter weggefallen ist, oder"@de .

dtd:LA-id4840 a dtd:LA ;
    dtd:has_Value "sie nach § 59l Absatz 4 in Verbindung mit § 27 Absatz 2 von der Pflicht, eine Kanzlei zu unterhalten, befreit worden ist oder"@de .

dtd:LA-id4841 a dtd:LA ;
    dtd:has_Value "ein bisheriger Zustellungsbevollmächtigter weggefallen ist, oder"@de .

dtd:LA-id4842 a dtd:LA ;
    dtd:has_Value "ihre Kanzlei aufgibt, ohne dass sie von der Pflicht des § 52l befreit worden ist."@de .

dtd:LA-id4843 a dtd:LA ;
    dtd:has_Value "bei nichtpatentanwaltlichen Mitgliedern von Geschäftsführungsorganen die Aberkennung der Eignung, eine Berufsausübungsgesellschaft zu vertreten und ihre Geschäfte zu führen, und"@de .

dtd:LA-id4844 a dtd:LA ;
    dtd:has_Value "bei nichtpatentanwaltlichen Mitgliedern eines Aufsichtsorgans die Aberkennung der Eignung, Aufsichtsfunktionen einer Berufsausübungsgesellschaft wahrzunehmen."@de .

dtd:LA-id4845 a dtd:LA ;

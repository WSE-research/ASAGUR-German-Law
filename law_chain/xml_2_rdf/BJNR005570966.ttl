dtd:LA-id4773 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die allgemeinen Zulassungsvoraussetzungen zum Beruf des Patentanwalts gemäß § 5 Absatz 1 erfüllt sind,"@de .


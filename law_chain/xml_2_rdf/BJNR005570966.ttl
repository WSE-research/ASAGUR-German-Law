    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kanzleipflicht und Pflichten bei der Einrichtung und Unterhaltung von weiteren Kanzleien und Zweigstellen;"@de .

dtd:LA-id4805 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die besonderen Berufspflichten im Zusammenhang mit der Werbung;"@de .


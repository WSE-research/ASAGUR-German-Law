dtd:LA-id4812 a dtd:LA ;
    dtd:has_DL dtd:DL-id4201 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die besonderen Berufspflichten gegenüber Gerichten und Behörden:a)Pflichten bei der Verwendung von zur Einsicht überlassenen Akten sowie der hieraus erlangten Kenntnisse,b)Pflichten bei Zustellungen,c)Tragen der Berufstracht;"@de .

dtd:LA-id4813 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflichten bei der Verwendung von zur Einsicht überlassenen Akten sowie der hieraus erlangten Kenntnisse,"@de .

dtd:LA-id4814 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflichten bei Zustellungen,"@de .

dtd:LA-id4815 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tragen der Berufstracht;"@de .

dtd:LA-id4816 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die besonderen Berufspflichten bei der Vereinbarung und Abrechnung der Vergütung und bei deren Beitreibung;"@de .

dtd:LA-id4817 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die besonderen Berufspflichten gegenüber der Patentanwaltskammer in Fragen der Aufsicht, das berufliche Verhalten gegenüber anderen Mitgliedern der Patentanwaltskammer, die Pflichten bei der Zustellung von Anwalt zu Anwalt, die Pflichten bei beruflicher Zusammenarbeit sowie die Pflichten im Zusammenhang mit der Beschäftigung von Patentanwälten und der Ausbildung sowie Beschäftigung anderer Personen;"@de .

dtd:LA-id4818 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die besonderen Berufspflichten im grenzüberschreitenden Rechtsverkehr."@de .


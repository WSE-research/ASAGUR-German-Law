    dtd:has_Value "Bremsberechnung"@de .

dtd:LA-id2878 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der Triebfahrzeugführer muss vor Fahrtantritt feststellen können, dass der Zug die für die Strecke vorgeschriebene Bremsleistung erreicht."@de .

dtd:LA-id2879 a dtd:LA ;

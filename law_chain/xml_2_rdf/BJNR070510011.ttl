    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der Triebfahrzeugführer muss beim Abstellen von Zügen oder Zugteilen die erforderlichen Maßnahmen treffen können, damit diese sich nicht unbeabsichtigt in Bewegung setzen."@de .

dtd:LA-id2877 a dtd:LA ;

dtd:LA-id604 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer dauerhaften, friedlichen und gerechten Ordnung in Europa und zwischen den Völkern der Welt,"@de .

dtd:LA-id605 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Wahrung der unverletzlichen und unveräußerlichen Menschenrechte als Grundlage jeder menschlichen Gemeinschaft,"@de .

dtd:LA-id606 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Erhaltung der natürlichen Lebensgrundlagen der Erde und dem Schutz des kulturellen Erbes der Menschheit,"@de .

dtd:LA-id607 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Achtung und Fortentwicklung des Völkerrechts,"@de .

dtd:LA-id608 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Aufbau eines vereinten Europa und"@de .

dtd:LA-id609 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Einheit und Freiheit des deutschen Volkes."@de .

dtd:LA-id610 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Interessen der Bundesrepublik Deutschland im Ausland zu vertreten,"@de .

dtd:LA-id611 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die auswärtigen Beziehungen, insbesondere auf politischem, wirtschaftlichem, entwicklungspolitischem, kulturellem, wissenschaftlichem, technologischem, umweltpolitischem und sozialem Gebiet zu pflegen und zu fördern,"@de .

dtd:LA-id612 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bundesregierung über Verhältnisse und Entwicklungen im Ausland zu unterrichten,"@de .

dtd:LA-id613 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über die Bundesrepublik Deutschland im Ausland zu informieren,"@de .

dtd:LA-id614 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Deutschen im Ausland Hilfe und Beistand zu leisten,"@de .

dtd:LA-id615 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Gestaltung der Beziehungen im internationalen Rechtswesen und bei der Entwicklung der internationalen Rechtsordnung mitzuarbeiten"@de .

dtd:LA-id616 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "und die außenpolitische Beziehungen betreffenden Tätigkeiten von staatlichen und anderen öffentlichen Einrichtungen der Bundesrepublik Deutschland im Ausland im Rahmen der Politik der Bundesregierung zu koordinieren."@de .

dtd:LA-id617 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorübergehende Verstärkung bei besonderen Belastungen infolge auslandsbezogener politischer Entwicklungen, internationaler Konferenzen oder aus sonstigen Gründen,"@de .

dtd:LA-id618 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "angemessene fachliche und fremdsprachliche Aus- und Fortbildung,"@de .

dtd:LA-id619 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorbereitung auf Versetzungen und persönliche Einführung in die Dienstgeschäfte durch den Amtsvorgänger."@de .

dtd:LA-id620 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Ehegatte des Beamten,"@de .

dtd:LA-id621 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kinder, für die dem Beamten Kindergeld nach dem X. Abschnitt des Einkommensteuergesetzes zusteht oder ohne Berücksichtigung des § 64 oder des § 65 des Einkommensteuergesetzes zustünde,"@de .

dtd:LA-id622 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Lebenspartner des Beamten,"@de .

dtd:LA-id623 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kinder des Lebenspartners des Beamten, die der Beamte in seinen Haushalt aufgenommen hat; § 32 Absatz 3 bis 5 des Einkommensteuergesetzes gilt entsprechend."@de .


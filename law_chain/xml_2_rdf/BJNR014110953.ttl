dtd:LA-id3733 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verteilung der Mitglieder und der Stellvertreter auf die im Bezirk der Handwerkskammer vertretenen Handwerke,"@de .

dtd:LA-id3734 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zuwahl zur Handwerkskammer,"@de .

dtd:LA-id3735 a dtd:LA ;

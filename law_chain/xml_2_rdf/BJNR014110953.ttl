dtd:LA-id3730 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die juristische Person oder die Personengesellschaft in der Handwerksrolle oder in dem Verzeichnis nach § 19 gelöscht worden ist."@de .

dtd:LA-id3731 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Namen, den Sitz und den Bezirk der Handwerkskammer,"@de .

dtd:LA-id3732 a dtd:LA ;

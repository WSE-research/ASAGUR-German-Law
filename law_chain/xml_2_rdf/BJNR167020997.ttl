dtd:LA-id508 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des § 3 Abs. 1 Nr. 1 bis 5 und 8 des Binnenschiffahrtsaufgabengesetzes in der Fassung der Bekanntmachung vom 4. August 1986 (BGBl. I S. 1270) und des § 3e Abs. 1 Satz 1 des Binnenschiffahrtsaufgabengesetzes, der durch Artikel 5 Nr. 3 des Gesetzes vom 13. August 1993 (BGBl. I S. 1489) geändert worden ist, und auf Grund des § 27 Abs. 1 und des § 46 Satz 1 Nr. 1 des Bundeswasserstraßengesetzes in der Fassung der Bekanntmachung vom 23. August 1990 (BGBl. I S. 1818) verordnet das Bundesministerium für Verkehr,"@de .

dtd:LA-id509 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des § 3 Abs. 5 Satz 1 und des § 3e Abs. 1 Satz 2 Nr. 1 des Binnenschiffahrtsaufgabengesetzes in Verbindung mit Artikel 56 des Zuständigkeitsanpassungs-Gesetzes vom 18. März 1975 (BGBl. I S. 705) und dem Organisationserlaß des Bundeskanzlers vom 5. Juni 1986 (BGBl. I S. 864) verordnet das Bundesministerium für Verkehr gemeinsam mit dem Bundesministerium für Umwelt, Naturschutz und Reaktorsicherheit,"@de .

dtd:LA-id510 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des § 3 Abs. 5 Satz 2, der gemäß Artikel 66 Nr. 1 der Verordnung vom 26. Februar 1993 (BGBl. I S. 278) insoweit geändert worden ist, und des § 3e Abs. 1 Satz 2 Nr. 2 des Binnenschiffahrtsaufgabengesetzes verordnet das Bundesministerium für Verkehr im Einvernehmen mit dem Bundesministerium für Arbeit und Sozialordnung,"@de .

dtd:LA-id511 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des § 3 Abs. 5 Satz 4, der gemäß Artikel 66 Nr. 2 der Verordnung vom 26. Februar 1993 (BGBl. I S. 278) insoweit geändert worden ist, und des § 3e Abs. 1 Satz 2 Nr. 2 des Binnenschiffahrtsaufgabengesetzes verordnet das Bundesministerium für Verkehr im Einvernehmen mit dem Bundesministerium für Post und Telekommunikation:"@de .

dtd:LA-id512 a dtd:LA ;
    dtd:has_DL dtd:DL-id101 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Umsetzung einer Anordnung der Moselkommission nach § 1.22a der Anlagea)in dringenden Fällen oderb)zu Versuchszwecken oder zur Zulassung einer technischen Neuerung, durch die jeweils die Sicherheit und Leichtigkeit des Schiffsverkehrs nicht beeinträchtigt werden,eine von der Anlage abweichende Regelung vorübergehend bis zur Dauer von drei Jahren zu treffen oder"@de .

dtd:LA-id513 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in dringenden Fällen oder"@de .

dtd:LA-id514 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu Versuchszwecken oder zur Zulassung einer technischen Neuerung, durch die jeweils die Sicherheit und Leichtigkeit des Schiffsverkehrs nicht beeinträchtigt werden,"@de .

dtd:LA-id515 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für öffentliche Zwecke Ausnahmen von § 10.02 Nummer 3 der Anlage zu bestimmen."@de .

dtd:LA-id516 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Auflage nach § 1.21 Nr. 1 Satz 3, § 1.27 oder § 7.01 Nr. 3 der Moselschiffahrtspolizeiverordnung, auch in Verbindung mit Artikel 2 Abs. 9, oder"@de .

dtd:LA-id517 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer mit einer Erlaubnis nach § 1.23 Satz 1, § 3.28, § 3.29 Nr. 2 Satz 1 Buchstabe b, § 6.19 Nr. 1, § 6.28 Nr. 8 Satz 3, § 8.05 oder § 9.01 Satz 4 der Moselschiffahrtspolizeiverordnung, auch in Verbindung mit Artikel 2 Abs. 9, verbundenen vollziehbaren Auflage"@de .

dtd:LA-id518 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.02 Nr. 1 Satz 1 ein Fahrzeug oder einen Schwimmkörper oder entgegen § 1.02 Nr. 2 Satz 1 einen Verband führt, ohne hierfür geeignet zu sein,"@de .

dtd:LA-id519 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.03 Nummer 1 Satz 1 einer Anweisung nicht Folge leistet,"@de .

dtd:LA-id520 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.03 Nr. 2 eine Anweisung des Schiffsführers nicht befolgt,"@de .

dtd:LA-id521 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.03 Nummer 4 Satz 2, auch in Verbindung mit Satz 3, Dienst verrichtet,"@de .

dtd:LA-id522 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.08 Nummer 6 Satz 1 in den dort genannten Fällen keine Rettungsweste trägt,"@de .

dtd:LA-id523 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.08 Nummer 6 Satz 2 Außenbordarbeiten durchführt, obwohl das Schiff nicht stillliegt oder zu erwarten ist, dass die Arbeiten durch den übrigen Schiffsverkehr gefährdet werden,"@de .

dtd:LA-id524 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.09 Nr. 3 Satz 1 nicht in der Lage ist, alle Weisungen oder Informationen zu geben oder zu empfangen,"@de .

dtd:LA-id525 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.10a Nummer 1 Satz 5 dort genannte Schiffspapiere nicht aufbewahrt,"@de .

dtd:LA-id526 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.13 Nr. 1 Schiffahrtszeichen zum Festmachen oder Verholen benutzt, beschädigt oder unbrauchbar macht,"@de .

dtd:LA-id527 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.15 Nr. 1 feste Gegenstände oder andere Stoffe in die Wasserstraße einbringt oder einleitet,"@de .

dtd:LA-id528 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.16 Nr. 3 Satz 1 nicht die dort genannten Feststellungen ermöglicht,"@de .

dtd:LA-id529 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.17 Nummer 2 Satz 2 nicht an Bord oder nicht in der Nähe der Unfallstelle bleibt,"@de .

dtd:LA-id530 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ohne Erlaubnis nach § 1.23 eine dort genannte Veranstaltung, Arbeit oder Übung durchführt oder durchführen läßt,"@de .

dtd:LA-id531 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3.29 Nr. 2 Satz 1 von der Bezeichnung nach § 3.29 Nr. 1 Gebrauch macht,"@de .

dtd:LA-id532 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 4.01 Nr. 3 Schallzeichen von einem Fahrzeug gibt, auf dem sich der Führer des Schleppverbandes nicht befindet,"@de .

dtd:LA-id533 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 6.17 Nr. 3 Satz 1 an einem Fahrzeug oder Schwimmkörper in Fahrt anlegt, sich daran anhängt oder im Sogwasser mitfährt,"@de .

dtd:LA-id534 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 6.17 Nr. 4 nicht ausreichenden Abstand hält,"@de .

dtd:LA-id535 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 6.22 Nr. 4 eine gesperrte Wasserfläche benutzt,"@de .

dtd:LA-id536 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 11.03 Nummer 1 öl- oder fetthaltigen Schiffsbetriebsabfall, Slops, Hausmüll, Klärschlamm oder übrigen Sonderabfall, Teile der Ladung oder Abfälle aus dem Ladungsbereich in die Wasserstraße einbringt oder einleitet,"@de .

dtd:LA-id537 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 11.04 Nummer 2 Buchstabe a, b oder c Satz 1 an Deck gestaute lose Behälter als Altölsammelbehälter verwendet, Abfälle an Bord verbrennt oder öl- oder fettlösende oder emulgierende Reinigungsmittel in die Maschinenraumbilgen einbringt oder"@de .

dtd:LA-id538 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 11.09 die Außenhaut des Fahrzeugs mit Öl anstreicht oder mit einem dort genannten Mittel reinigt."@de .

dtd:LA-id539 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.06 ein Fahrzeug oder einen Verband führt, dessen Geschwindigkeit nicht den Gegebenheiten der Wasserstraße oder der Anlagen angepaßt ist,"@de .

dtd:LA-id540 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Fahrzeug führt, das entgegen § 1.07 Nr. 1 Satz 1 tiefer als bis zur Unterkante der Einsenkungsmarken abgeladen oder auf dem entgegen § 1.07 Nummer 2 Satz 1, auch in Verbindung mit Satz 3, die vorgeschriebene Sicht eingeschränkt ist,"@de .

dtd:LA-id541 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Kanalpeniche führt, die entgegen § 1.07 Nr. 1 Satz 2 tiefer als dort zugelassen abgeladen ist,"@de .

dtd:LA-id542 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 1.07 Nr. 5 ein Fahrzeug führt, das mehr Fahrgäste als zugelassen an Bord hat,"@de .

dtd:LA-id543 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Fahrzeug führt, auf dem entgegen § 1.09 Nr. 4 ein Ausguck oder Horchposten nicht aufgestellt ist,"@de .

dtd:LA-id544 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3.01 Nr. 2 Lichter nicht zusätzlich setzt,"@de .

dtd:LA-id545 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3.05 Nr. 1 andere Lichter oder Sichtzeichen gebraucht oder sie unter Umständen gebraucht, für die sie nicht vorgeschrieben oder zugelassen sind,"@de .

dtd:LA-id546 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Vorschrift des § 3.07 über den Gebrauch von Lichtern, Scheinwerfern, Flaggen, Tafeln, Wimpeln oder anderen Gegenständen zuwiderhandelt,"@de .

dtd:LA-id547 a dtd:LA ;
    dtd:has_DL dtd:DL-id223 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Fahrzeug, einen Verband, einen Schwimmkörper oder eine schwimmende Anlagea)bei Nacht während der Fahrt entgegen § 3.08 Nr. 1 oder 2, § 3.09 Nr. 1 Buchstabe a oder b, Nr. 2 bis 4, § 3.10 Nr. 1 bis 3, § 3.11 Nr. 1, § 3.12 Nr. 1, § 3.13 Nr. 1, 2, 3 Satz 1, Nr. 4 oder 5, § 3.14 Nr. 1 bis 6 oder 8, § 3.16, § 3.18 Satz 1 oder § 3.19 oderb)bei Tag während der Fahrt entgegen § 3.09 Nr. 1 bis 3, § 3.10 Nr. 4, § 3.13 Nr. 6, § 3.14 Nr. 1 bis 6, § 3.15, § 3.17 oder § 3.18 Satz 1"@de .


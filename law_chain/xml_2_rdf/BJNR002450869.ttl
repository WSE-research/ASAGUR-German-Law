dtd:LA-id3836 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine regelmäßige Datenübermittlung nach Absatz 8 zulässig ist,"@de .

dtd:LA-id3837 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kenntnis der Daten zur Abwehr einer gegenwärtigen Gefahr für die öffentliche Sicherheit oder erheblicher Nachteile für das Gemeinwohl erforderlich ist oder"@de .


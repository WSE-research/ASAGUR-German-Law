    dtd:has_Size dtd:normal ;
    dtd:has_Value "Freiräume zur Gewährleistung eines natürlichen Klimaschutzes, insbesondere für Moorerhalt und Moorschutz;"@de .

dtd:LA-id760 a dtd:LA ;
    dtd:has_DL dtd:DL-id576 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den zu sichernden Standorten und Trassen für Infrastruktur; hierzu können gehören a)Verkehrsinfrastruktur und Umschlaganlagen von Gütern,b)Ver- und Entsorgungsinfrastruktur einschließlich Energieleitungen und -anlagen."@de .

dtd:LA-id761 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verkehrsinfrastruktur und Umschlaganlagen von Gütern,"@de .

dtd:LA-id762 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ver- und Entsorgungsinfrastruktur einschließlich Energieleitungen und -anlagen."@de .

dtd:LA-id763 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Gewährleistung der Sicherheit und Leichtigkeit des Schiffsverkehrs,"@de .

dtd:LA-id764 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu weiteren wirtschaftlichen Nutzungen,"@de .

dtd:LA-id765 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu wissenschaftlichen Nutzungen sowie"@de .

dtd:LA-id766 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Schutz und zur Verbesserung der Meeresumwelt."@de .

dtd:LA-id767 a dtd:LA ;
    dtd:has_Value "Vertragliche Vereinbarungen, insbesondere zur Koordinierung oder Verwirklichung von raumordnerischen Entwicklungskonzepten und zur Vorbereitung oder Verwirklichung von Raumordnungsplänen,"@de .

dtd:LA-id768 a dtd:LA ;
    dtd:has_Value "Maßnahmen wie regionale Entwicklungskonzepte, überregionale, regionale und interkommunale Netzwerke und Kooperationsstrukturen, regionale Foren und Aktionsprogramme zu aktuellen Handlungsanforderungen,"@de .

dtd:LA-id769 a dtd:LA ;

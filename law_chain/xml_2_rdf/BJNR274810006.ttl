    dtd:has_Value "die berechtigte Person nach den Angaben im Antrag auf Elterngeld im Bezugszeitraum voraussichtlich Einkommen aus Erwerbstätigkeit hat."@de .

dtd:LA-id735 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 8 Absatz 1 einen Nachweis nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig erbringt,"@de .

dtd:LA-id736 a dtd:LA ;

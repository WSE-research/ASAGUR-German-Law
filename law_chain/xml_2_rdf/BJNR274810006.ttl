    dtd:has_Value "für den Zeitraum zwischen dem dritten Geburtstag und dem vollendeten achten Lebensjahr des Kindes 13 Wochen"@de .

dtd:LA-id754 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in einer Elternzeit zwischen der Geburt und dem vollendeten dritten Lebensjahr des Kindes nicht spätestens vier Wochen nach Zugang des Antrags oder"@de .

dtd:LA-id755 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in einer Elternzeit zwischen dem dritten Geburtstag und dem vollendeten achten Lebensjahr des Kindes nicht spätestens acht Wochen nach Zugang des Antrags"@de .

dtd:LA-id756 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den Zeitraum bis zum vollendeten dritten Lebensjahr des Kindes spätestens sieben Wochen und"@de .

dtd:LA-id757 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den Zeitraum zwischen dem dritten Geburtstag und dem vollendeten achten Lebensjahr des Kindes spätestens 13 Wochen"@de .

dtd:LA-id758 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "frühestens acht Wochen vor Beginn einer Elternzeit bis zum vollendeten dritten Lebensjahr des Kindes und"@de .

dtd:LA-id759 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "frühestens 14 Wochen vor Beginn einer Elternzeit zwischen dem dritten Geburtstag und dem vollendeten achten Lebensjahr des Kindes."@de .

dtd:LA-id760 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "während der Elternzeit bei demselben Arbeitgeber Teilzeitarbeit leisten oder"@de .

dtd:LA-id761 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ohne Elternzeit in Anspruch zu nehmen, Teilzeitarbeit leisten und Anspruch auf Elterngeld nach § 1 während des Zeitraums nach § 4 Absatz 1 Satz 2, 3 und 5 haben."@de .

dtd:LA-id762 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Art der Berechtigung nach § 1,"@de .

dtd:LA-id763 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundlagen der Berechnung des zustehenden Monatsbetrags nach Art und Höhe (§ 2 Absatz 1, 2, 3 oder 4, § 2a Absatz 1 oder 4, § 2c, die §§ 2d, 2e oder § 2f),"@de .

dtd:LA-id764 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Höhe und Art des zustehenden Monatsbetrags (§ 4a Absatz 1 und 2 Satz 1) ohne die Berücksichtigung der Einnahmen nach § 3,"@de .

dtd:LA-id765 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Art und Höhe der Einnahmen nach § 3,"@de .

dtd:LA-id766 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Inanspruchnahme der als Partnerschaftsbonus gewährten Monatsbeträge nach § 4b Absatz 1 und der weiteren Monatsbeträge Elterngeld Plus nach § 4c Absatz 2,"@de .

dtd:LA-id767 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Höhe des monatlichen Auszahlungsbetrags,"@de .

dtd:LA-id768 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geburtstag des Kindes,"@de .

dtd:LA-id769 a dtd:LA ;
    dtd:has_DL dtd:DL-id635 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Elterngeld beziehende Person: a)Geschlecht, Geburtsjahr und -monat,b)Staatsangehörigkeit,c)Wohnsitz oder gewöhnlicher Aufenthalt,d)Familienstand und unverheiratetes Zusammenleben mit dem anderen Elternteil,e)Vorliegen der Voraussetzungen nach § 4c Absatz 1 Nummer 1 undf)Anzahl der im Haushalt lebenden Kinder."@de .

dtd:LA-id770 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geschlecht, Geburtsjahr und -monat,"@de .

dtd:LA-id771 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Staatsangehörigkeit,"@de .

dtd:LA-id772 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wohnsitz oder gewöhnlicher Aufenthalt,"@de .

dtd:LA-id773 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Familienstand und unverheiratetes Zusammenleben mit dem anderen Elternteil,"@de .

dtd:LA-id774 a dtd:LA ;
    dtd:has_Value "Vorliegen der Voraussetzungen nach § 4c Absatz 1 Nummer 1 und"@de .


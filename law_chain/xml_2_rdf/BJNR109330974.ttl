    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit 50 Prozent, wenn seit der Entstehung der anrechenbaren Steuer nicht mehr als zwei Jahre,"@de .

dtd:LA-id1395 a dtd:LA ;

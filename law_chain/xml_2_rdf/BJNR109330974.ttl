    dtd:has_Size dtd:normal ;
    dtd:has_Value "der übrigen Personen der Steuerklasse III in Höhe von 20 000 Euro."@de .

dtd:LA-id1385 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einem Alter bis zu 5 Jahren in Höhe von 52 000 Euro;"@de .

dtd:LA-id1386 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einem Alter von mehr als 5 bis zu 10 Jahren in Höhe von 41 000 Euro;"@de .


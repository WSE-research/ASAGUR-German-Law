dtd:LA-id1377 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der geschiedene Ehegatte und der Lebenspartner einer aufgehobenen Lebenspartnerschaft;"@de .

dtd:LA-id1378 a dtd:LA ;

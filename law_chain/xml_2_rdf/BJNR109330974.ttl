    dtd:has_Value "der Kinder im Sinne der Steuerklasse I Nr. 2 und der Kinder verstorbener Kinder im Sinne der Steuerklasse I Nr. 2 in Höhe von 400 000 Euro;"@de .

dtd:LA-id1380 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Kinder der Kinder im Sinne der Steuerklasse I Nr. 2 in Höhe von 200 000 Euro;"@de .

dtd:LA-id1381 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der übrigen Personen der Steuerklasse I in Höhe von 100 000 Euro;"@de .

dtd:LA-id1382 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Personen der Steuerklasse II in Höhe von 20 000 Euro;"@de .

dtd:LA-id1383 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .


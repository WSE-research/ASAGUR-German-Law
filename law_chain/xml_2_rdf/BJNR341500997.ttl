dtd:LA-id190 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Firma, Rechtsform, Sitz unter Angabe der Postadresse, gegebenenfalls Verband, dem das Institut angehört, und Geschäftszweck des Instituts;"@de .

dtd:LA-id191 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beschreibung der Geschäfte, die fortgeführt werden sollen;"@de .

dtd:LA-id192 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Geschäftsleiter unter Angabe ihrer Wohnadresse und die Zusammensetzung der sonstigen gesetzlichen Organe des Instituts;"@de .

dtd:LA-id193 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die an dem Institut gehaltenen bedeutenden Beteiligungen (§ 1 Abs. 9 KWG) unter Angabe der Inhaber, der Höhe und der Struktur dieser Beteiligungen;"@de .

dtd:LA-id194 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die von dem Institut gehaltenen unmittelbaren Beteiligungen im Sinne des § 24 Abs. 1 Nr. 3 KWG unter Verwendung des Vordrucks der Anlage 4 zur Anzeigenverordnung;"@de .

dtd:LA-id195 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die engen Verbindungen im Sinne des § 1 Abs. 10 KWG unter Verwendung des Vordrucks der Anlage 6 zur Anzeigenverordnung;"@de .

dtd:LA-id196 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die inländischen Zweigstellen unter Angabe der Postadresse;"@de .

dtd:LA-id197 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zweigniederlassungen im Ausland unter Angabe der Postadresse und unter Beifügung einer Kopie der von der zuständigen Aufsichtsbehörde des Aufnahmestaates erteilten Zulassung;"@de .

dtd:LA-id198 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den grenzüberschreitenden Dienstleistungsverkehr unter Angabe der betroffenen Staaten und Beschreibung der Dienstleistung; soweit die grenzüberschreitenden Dienstleistungen in einem Staat des Europäischen Wirtschaftsraums erbracht werden, ist die Zulässigkeit dieser Tätigkeiten nachzuweisen."@de .

dtd:LA-id199 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Geschäftsplan, aus dem die Art der Geschäfte, der organisatorische Aufbau und die internen Kontrollverfahren hervorgehen, und ein Organigramm, das insbesondere die Zuständigkeiten der Geschäftsleiter erkennen läßt;"@de .

dtd:LA-id200 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "beglaubigte Ablichtungen des Gesellschaftsvertrags oder der Satzung, des Beschlusses über die Bestellung der Geschäftsleiter und der Geschäftsordnung für die Geschäftsleitung;"@de .

dtd:LA-id201 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei bereits bilanzierenden Instituten die Jahresabschlüsse der letzten drei vollen Geschäftsjahre (ist das Institut weniger als drei Jahre tätig, sind die fehlenden Unterlagen durch Planbilanzen und Plangewinn- und Planverlustrechnungen zu ersetzen); der Jahresabschluß für das Jahr 1997 ist mit dem Bestätigungsvermerk durch einen Wirtschaftsprüfer, eine Wirtschaftsprüfungsgesellschaft, einen vereidigten Buchprüfer oder eine Buchprüfungsgesellschaft zu versehen;"@de .

dtd:LA-id202 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei bisher nicht bilanzierenden Instituten die Einnahmenüberschußrechnungen der letzten drei vollen Geschäftsjahre (ist das Institut weniger als drei Jahre tätig, sind die fehlenden Unterlagen durch Planbilanzen und Plangewinn- und Planverlustrechnungen zu ersetzen) sowie eine durch einen Wirtschaftsprüfer, eine Wirtschaftsprüfungsgesellschaft, einen vereidigten Buchprüfer oder eine Buchprüfungsgesellschaft mit dem Bestätigungsvermerk versehene Eröffnungsbilanz für das Jahr 1998;"@de .

dtd:LA-id203 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die verwendeten Allgemeinen Geschäftsbedingungen sowie Muster der verwendeten Kundenverträge und des Werbematerials;"@de .

dtd:LA-id204 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wesentliche Verträge mit Vertriebs- oder sonstigen Kooperationspartnern."@de .

dtd:LA-id205 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Aufzählung der im eigenen Namen und für fremde Rechnung gehandelten Finanzinstrumente im Sinne des § 1 Abs. 11 KWG nach Art und Gattung; verbriefte Finanzinstrumente sind gesondert zu kennzeichnen;"@de .

dtd:LA-id206 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Beschreibung der angesprochenen Kundenkreise;"@de .

dtd:LA-id207 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Beschreibung, auf welche Weise das Eigentum an den Finanzinstrumenten auf den Geschäftspartner/Kunden übergeht und ob Gelder (bar oder unbar) oder Wertpapiere des Kunden entgegengenommen werden."@de .

dtd:LA-id208 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Aufzählung der Finanzinstrumente im Sinne des § 1 Abs. 11 KWG nach Art und Gattung, die im eigenen Namen und für eigene Rechnung plaziert oder für die gleichartige Garantien übernommen werden;"@de .

dtd:LA-id209 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Beschreibung der angewandten Plazierungsverfahren einschließlich der Angabe der Märkte (Börse, Neuer Markt, Freiverkehr, OTC);"@de .

dtd:LA-id210 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Beschreibung, auf welche Weise das Eigentum an den emittierten Finanzinstrumenten auf den Geschäftspartner/Kunden übergeht und ob Gelder (bar oder unbar) oder Wertpapiere des Kunden entgegengenommen werden."@de .

dtd:LA-id211 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Aufzählung der Finanzinstrumente im Sinne des § 1 Abs. 11 KWG nach Art und Gattung, die im fremden Namen für fremde Rechnung angeschafft oder veräußert werden oder deren Anschaffung oder Veräußerung vermittelt wird oder in denen Geschäfte nachgewiesen werden;"@de .

dtd:LA-id212 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Beschreibung der angesprochenen Kundenkreise;"@de .

dtd:LA-id213 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Beschreibung des Vermittlungsablaufes, insbesondere auf welche Weise die Kunden geworben werden und ob im Zuge der Vermittlung Gelder (bar oder unbar) oder Finanzinstrumente des Kunden entgegengenommen werden und wie viele Vermittler in den Ablauf eingeschaltet sind;"@de .

dtd:LA-id214 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name und Sitz des kontoführenden Instituts."@de .

dtd:LA-id215 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Aufzählung der Finanzinstrumente im Sinne des § 1 Abs. 11 KWG nach Art und Gattung, die im Rahmen der Verwaltung angeschafft oder veräußert werden; Angaben darüber, ob Kundengelder bar oder unbar entgegengenommen werden;"@de .

dtd:LA-id216 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Beschreibung der angesprochenen Kundenkreise, insbesondere die Angabe eventueller Mindestdepotgrenzen;"@de .

dtd:LA-id217 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Aufzählung der Verwahrer der verwalteten Finanzinstrumente;"@de .

dtd:LA-id218 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Beschreibung der Form der Verwahrung."@de .

dtd:LA-id219 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Aufzählung der Finanzinstrumente im Sinne des § 1 Abs. 11 KWG nach Art und Gattung, die im Wege des Eigenhandels angeschafft und veräußert werden;"@de .

dtd:LA-id220 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Beschreibung der angesprochenen Kundenkreise;"@de .

dtd:LA-id221 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Beschreibung, auf welche Weise das Eigentum an den verkauften Finanzinstrumenten auf den Geschäftspartner/Kunden übergeht und ob Gelder (bar oder unbar) oder Wertpapiere des Kunden entgegengenommen werden."@de .

dtd:LA-id222 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Aufstellung der Unternehmen in Drittstaaten, an welche die Einlagen vermittelt werden mit Angaben darüber, ob diese Unternehmen in ihrem Sitzland einer Solvenzaufsicht unterliegen, welche Behörde diese Aufsicht wahrnimmt und ob, durch wen und in welcher Höhe die Einlagen gesichert sind;"@de .

dtd:LA-id223 a dtd:LA ;

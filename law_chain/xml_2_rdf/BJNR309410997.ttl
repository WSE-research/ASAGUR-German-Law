dtd:LA-id1125 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich zeigt, dass die Überleitungsrechnung gemäß § 48 Abs. 2 Nr. 3 im Vollzug des Wirtschaftsplans trotz Ausnutzung jeder Einsparungsmöglichkeit nicht ausgeglichen werden kann, oder"@de .

dtd:LA-id1126 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über- oder außerplanmäßige Ausgaben in Höhe von mehr als 1 vom Hundert der Gesamtausgaben der Deutschen Welle geleistet werden müssen."@de .

dtd:LA-id1127 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Vermögensrechnung (Bilanz),"@de .

dtd:LA-id1128 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Erfolgsrechnung (Gewinn- und Verlustrechnung),"@de .

dtd:LA-id1129 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Finanzrechnung (Kapitalflussrechnung),"@de .

dtd:LA-id1130 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Rechnung über die im abgelaufenen Wirtschaftsjahr tatsächlich erhobenen Einnahmen und tatsächlich geleisteten Ausgaben entsprechend der Systematik gemäß § 48 Abs. 2 Nr. 4 und"@de .

dtd:LA-id1131 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Geschäftsbericht zur Erläuterung der Vorgänge von besonderer Bedeutung."@de .

dtd:LA-id1132 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dies der Erfüllung ihrer gesetzlichen Aufgabe dient,"@de .

dtd:LA-id1133 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Deckung der damit verbundenen Ausgaben gewährleistet ist,"@de .

dtd:LA-id1134 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einzahlungsverpflichtung der Deutschen Welle auf einen bestimmten Betrag begrenzt ist und"@de .

dtd:LA-id1135 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für die Rechtsform des Unternehmens geltenden gesetzlichen Bestimmungen oder die Satzung des Unternehmens einen Aufsichtsrat oder ein entsprechendes Organ vorsehen."@de .

dtd:LA-id1136 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich allein oder gemeinsam mit anderen öffentlich-rechtlichen Rundfunkanstalten den notwendigen Einfluss auf die Geschäftsführung des Unternehmens, insbesondere eine angemessene Vertretung im Aufsichtsgremium, zu sichern und"@de .

dtd:LA-id1137 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Unternehmen zu verpflichten, ihr die für die finanziellen oder programmlichen Fragen wesentlichen Geschäftsvorfälle mitzuteilen."@de .

dtd:LA-id1138 a dtd:LA ;
    dtd:has_Value "zu den Daten zu nehmen, die zu der betroffenen Person gespeichert sind, und dort für dieselbe Zeitdauer aufzubewahren wie die Daten selbst sowie"@de .

dtd:LA-id1139 a dtd:LA ;
    dtd:has_Value "bei einer Übermittlung der Daten, die zu der betroffenen Person gespeichert sind, gemeinsam mit diesen zu übermitteln."@de .

dtd:LA-id1140 a dtd:LA ;
    dtd:has_Value "von den Daten auf die Personen, die bei der Vorbereitung, Herstellung oder Verbreitung von Angeboten mitwirken oder mitgewirkt haben, geschlossen werden kann,"@de .

dtd:LA-id1141 a dtd:LA ;
    dtd:has_Value "von den Daten auf die Einsenderin oder den Einsender oder die Gewährsträgerin oder den Gewährsträger von Beiträgen, Unterlagen oder Mitteilungen für den redaktionellen Teil geschlossen werden kann oder"@de .

dtd:LA-id1142 a dtd:LA ;
    dtd:has_Value "durch die Mitteilung von recherchierten oder sonst erlangten Daten die Erfüllung der journalistischen Aufgabe der Deutschen Welle durch Ausforschung des Informationsbestandes beeinträchtigt würde."@de .

dtd:LA-id1143 a dtd:LA ;
    dtd:has_Value "Qualifikation, nachgewiesen durch ein abgeschlossenes Hochschulstudium,"@de .

dtd:LA-id1144 a dtd:LA ;
    dtd:has_Value "Erfahrung und"@de .

dtd:LA-id1145 a dtd:LA ;
    dtd:has_Value "Sachkunde, insbesondere im Bereich des Schutzes personenbezogener Daten."@de .

dtd:LA-id1146 a dtd:LA ;
    dtd:has_Value "eine schwere Verfehlung begangen hat oder"@de .

dtd:LA-id1147 a dtd:LA ;
    dtd:has_Value "die Voraussetzungen für die Wahrnehmung seiner Aufgaben nicht mehr erfüllt."@de .

dtd:LA-id1148 a dtd:LA ;
    dtd:has_Value "es sich um unerhebliche Mängel handelt oder"@de .

dtd:LA-id1149 a dtd:LA ;
    dtd:has_Value "sichergestellt ist, dass die Mängel unverzüglich behoben werden."@de .

dtd:P-id1151 a dtd:P ;
    dtd:has_BR dtd:BR-id702 ;
    dtd:has_Value " (+++ Textnachweis ab: 24.12.1997 +++)Das G wurde als Artikel 1 G 2251-5/1 v. 16.12.1997 I 3094 (RdFunkAuslG) vom Bundestag beschlossen und ist gem. Art. 6 Abs. 1 am 24.12.1997 in Kraft getreten. Die §§ 44 bis 57 sind gem. Art. 6 Abs. 2 am 1.1.1998 in Kraft getreten."@de ;

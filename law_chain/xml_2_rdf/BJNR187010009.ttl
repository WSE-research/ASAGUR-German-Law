dtd:LA-id1530 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 23d Absatz 2 Satz 1 eine Anzeige nicht oder nicht rechtzeitig erstattet oder"@de .


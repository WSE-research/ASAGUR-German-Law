dtd:LA-id1029 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Beschäftigten leicht erreichbar an ungefährdeter Stelle und in ausreichender Größe bereitzustellen,"@de .

dtd:LA-id1030 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entsprechend der Anzahl der gleichzeitigen Benutzer mit leicht zu reinigenden Tischen und Sitzgelegenheiten mit Rückenlehne auszustatten,"@de .

dtd:LA-id1031 a dtd:LA ;

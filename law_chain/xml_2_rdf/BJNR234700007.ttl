dtd:LA-id85 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Generaldirektion Wasserstraßen und Schifffahrt für die Ordnungswidrigkeiten übertragen, die in Räumlichkeiten der ihr nachgeordneten Wasserstraßen- und Schifffahrtsämter, einschließlich der Außenbezirke, Bauhöfe, Revierzentralen und sonstigen Einrichtungen, sowie der Wasserstraßen-Neubauämter begangen werden,"@de .

dtd:LA-id86 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Generaldirektion Wasserstraßen und Schifffahrt für die Ordnungswidrigkeiten übertragen, die in den Räumlichkeiten der Bundesanstalt für Wasserbau und der Bundesanstalt für Gewässerkunde begangen werden,"@de .

dtd:LA-id87 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Luftfahrt-Bundesamt für die Ordnungswidrigkeiten übertragen, die in den Räumlichkeiten der Bundesstelle für Flugunfalluntersuchung begangen werden, und"@de .

dtd:LA-id88 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Bundesamt für Seeschifffahrt und Hydrographie für die Ordnungswidrigkeiten übertragen, die in den Räumlichkeiten der Bundesstelle für Seeunfalluntersuchung begangen werden."@de .

dtd:LA-id89 a dtd:LA ;
    dtd:has_Value "Verkehrsmitteln und Personenbahnhöfen nach § 2 Nr. 2 Buchstabe a und Nr. 3 des Bundesnichtraucherschutzgesetzes begangen werden, wird auf das Eisenbahn-Bundesamt übertragen;"@de .

dtd:LA-id90 a dtd:LA ;
    dtd:has_Value "Verkehrsmitteln nach § 2 Nr. 2 Buchstabe c des Bundesnichtraucherschutzgesetzes begangen werden, wird auf das Luftfahrt-Bundesamt übertragen."@de .


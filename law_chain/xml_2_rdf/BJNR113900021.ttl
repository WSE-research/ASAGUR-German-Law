    dtd:has_Value "ein Fachbeirat oder mehrere Fachbeiräte."@de .

dtd:LA-id219 a dtd:LA ;
    dtd:has_Value "einer Vertreterin oder einem Vertreter der Länder, die oder der von der Konferenz der Gleichstellungs- und Frauenministerinnen und -minister, -senatorinnen und -senatoren der Länder benannt wird,"@de .

dtd:LA-id220 a dtd:LA ;
    dtd:has_Value "einer Vertreterin oder einem Vertreter der Kommunen, die oder der durch die Bundesvereinigung der kommunalen Spitzenverbände benannt wird,"@de .

dtd:LA-id221 a dtd:LA ;
    dtd:has_Value "vier Vertreterinnen oder Vertretern aus dem Bereich der Zivilgesellschaft oder einem Verband, die vom Bundesministerium für Familie, Senioren, Frauen und Jugend vorgeschlagen werden und"@de .

dtd:LA-id222 a dtd:LA ;
    dtd:has_Value "vier Mitgliedern, die dem wissenschaftlichen Bereich entstammen und durch das Direktorium vorgeschlagen werden."@de .

dtd:LA-id223 a dtd:LA ;
    dtd:has_Value "eine Vorsitzende oder einen Vorsitzenden und"@de .

dtd:LA-id224 a dtd:LA ;
    dtd:has_Value "eine stellvertretende Vorsitzende oder einen stellvertretenden Vorsitzenden."@de .

dtd:LA-id225 a dtd:LA ;
    dtd:has_Value "eine Vorsitzende oder einen Vorsitzenden und"@de .

dtd:LA-id226 a dtd:LA ;
    dtd:has_Value "eine stellvertretende Vorsitzende oder einen stellvertretenden Vorsitzenden."@de .

dtd:LA-id227 a dtd:LA ;
    dtd:has_Value "zum Stiftungsrat,"@de .

dtd:LA-id228 a dtd:LA ;
    dtd:has_Value "zum Direktorium,"@de .

dtd:LA-id229 a dtd:LA ;

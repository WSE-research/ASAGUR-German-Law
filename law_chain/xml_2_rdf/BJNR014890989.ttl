dtd:LA-id1267 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Studium der Pharmazie von vier Jahren an einer Universität;"@de .

dtd:LA-id1268 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Famulatur von acht Wochen;"@de .

dtd:LA-id1269 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine praktische Ausbildung von zwölf Monaten;"@de .

dtd:LA-id1270 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Pharmazeutische Prüfung, die in drei Prüfungsabschnitten abzulegen ist."@de .

dtd:LA-id1271 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Erste Abschnitt nach einem Studium der Pharmazie von mindestens zwei Jahren,"@de .

dtd:LA-id1272 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Zweite Abschnitt nach Bestehen des Ersten Abschnitts der Pharmazeutischen Prüfung und einem Studium der Pharmazie von mindestens vier Jahren,"@de .

dtd:LA-id1273 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Dritte Abschnitt nach Bestehen des Zweiten Abschnitts und nach Ableistung der sich anschließenden praktischen Ausbildung nach Absatz 1 Nr. 3."@de .

dtd:LA-id1274 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Krankenhaus- oder Bundeswehrapotheke,"@de .

dtd:LA-id1275 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der pharmazeutischen Industrie oder"@de .

dtd:LA-id1276 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Arzneimitteluntersuchungsstelle oder einer vergleichbaren Einrichtung einschließlich solcher der Bundeswehr"@de .

dtd:LA-id1277 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sechs Monaten in einer öffentlichen Apotheke, die keine Zweigapotheke ist, und"@de .

dtd:LA-id1278 a dtd:LA ;
    dtd:has_DL dtd:DL-id1054 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sechs Monaten, die wahlweise ina)einer Apotheke nach Nummer 1,b)einer Krankenhaus- oder Bundeswehrapotheke,c)der pharmazeutischen Industrie,d)einem Universitätsinstitut oder in anderen geeigneten wissenschaftlichen Institutionen einschließlich solchen der Bundeswehr,e)einer Arzneimitteluntersuchungsstelle oder einer vergleichbaren Einrichtung einschließlich solcher der Bundeswehr"@de .

dtd:LA-id1279 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Apotheke nach Nummer 1,"@de .

dtd:LA-id1280 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Krankenhaus- oder Bundeswehrapotheke,"@de .

dtd:LA-id1281 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der pharmazeutischen Industrie,"@de .

dtd:LA-id1282 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Universitätsinstitut oder in anderen geeigneten wissenschaftlichen Institutionen einschließlich solchen der Bundeswehr,"@de .

dtd:LA-id1283 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Arzneimitteluntersuchungsstelle oder einer vergleichbaren Einrichtung einschließlich solcher der Bundeswehr"@de .

dtd:LA-id1284 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Geburtsurkunde, bei Verheirateten auch die Eheurkunde,"@de .

dtd:LA-id1285 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Nachweis der Hochschulzugangsberechtigung, bei Zeugnissen, die außerhalb des Geltungsbereiches dieser Verordnung erworben worden sind, auch der Anerkennungsbescheid der zuständigen Behörde,"@de .

dtd:LA-id1286 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Nachweis über die Famulatur (Anlage 7), oder in Fällen des § 3 Abs. 3 die entsprechenden Nachweise,"@de .

dtd:LA-id1287 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Nachweis über ein Studium der Pharmazie von zwei Jahren,"@de .

dtd:LA-id1288 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bescheinigungen über die regelmäßige und erfolgreiche Teilnahme an den Veranstaltungen zu den in der Anlage 1 zu Buchstaben A bis D angeführten Stoffgebieten nach dem Muster der Anlage 2."@de .

dtd:LA-id1289 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id1290 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Zeugnis über das Bestehen des Ersten Abschnitts der Pharmazeutischen Prüfung (Anlage 10),"@de .

dtd:LA-id1291 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Nachweis über ein Studium der Pharmazie von mindestens vier Jahren,"@de .

dtd:LA-id1292 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bescheinigungen über die regelmäßige und erfolgreiche Teilnahme an den Veranstaltungen zu den in der Anlage 1 zu Buchstaben E bis I angeführten Stoffgebieten nach dem Muster der Anlage 2,"@de .

dtd:LA-id1293 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bescheinigung über das in Anlage 1 Buchstabe K vorgeschriebene Wahlpflichtfach nach dem Muster der Anlage 3."@de .

dtd:LA-id1294 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id1295 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zeugnisse über das Bestehen des Ersten und Zweiten Abschnitts der Pharmazeutischen Prüfung (Anlage 10),"@de .

dtd:LA-id1296 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Nachweis über die praktische Ausbildung (Anlage 5),"@de .

dtd:LA-id1297 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Nachweis über die Teilnahme an den begleitenden Unterrichtsveranstaltungen (Anlage 6)."@de .

dtd:LA-id1298 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Prüfungsbewerber bis zu dem in § 6 Abs. 2 genannten Zeitpunkt den Antrag nicht oder nicht formgerecht stellt oder die vorgeschriebenen Nachweise nicht vorlegt, es sei denn, daß er einen wichtigen Grund hierfür glaubhaft macht, der Stand des Prüfungsverfahrens eine Teilnahme des Prüfungsbewerbers noch zuläßt und die versäumte Handlung spätestens vier Wochen vor dem Prüfungstermin nachgeholt wird,"@de .

dtd:LA-id1299 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Prüfungsbewerber im Falle des § 6 Abs. 6 die fehlenden Nachweise nicht fristgerecht nachreicht,"@de .

dtd:LA-id1300 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der betreffende Prüfungsabschnitt nicht wiederholt werden darf."@de .

dtd:LA-id1301 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Reformziel beschrieben wird und erkennen lässt, welche qualitativen Verbesserungen für die pharmazeutische Ausbildung von diesem Prüfungsverfahren erwartet werden,"@de .

dtd:LA-id1302 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine von der Universität zu erlassende besondere Studienordnung besteht,"@de .

dtd:LA-id1303 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sichergestellt ist, dass die im Ersten Abschnitt der Pharmazeutischen Prüfung nachzuweisenden Kenntnisse und Fähigkeiten in diesem Prüfungsverfahren in einer der schriftlichen Prüfung gleichwertigen Weise geprüft werden,"@de .

dtd:LA-id1304 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine sachgerechte begleitende und abschließende Evaluierung dieses Prüfungsverfahrens gewährleistet ist,"@de .

dtd:LA-id1305 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mindest- und Höchstdauer der Laufzeit dieses Prüfungsverfahrens festgelegt sind und Verlängerungsanträge anhand von Evaluierungsergebnissen zu begründen sind,"@de .

dtd:LA-id1306 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Zugang zu einem Studiengang mit einem der Verordnung entsprechenden Prüfungsverfahren im Ersten Abschnitt der Pharmazeutischen Prüfung oder zu einem Studiengang mit einem alternativen Prüfungsverfahren freigestellt ist,"@de .

dtd:LA-id1307 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Voraussetzungen, unter denen die Universität dieses Prüfungsverfahren beenden kann, benannt sind,"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Entschädigungsbehörden dieses Landes,"@de .

dtd:LA-id3398 a dtd:LA ;
    dtd:has_DL dtd:DL-id3196 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Gebiet der sowjetisch besetzten Zone Deutschlands,die Entschädigungsbehörden des Landes Niedersachsen, "@de .

dtd:LA-id3399 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Entschädigungsbehörden des Landes Niedersachsen,"@de .

dtd:LA-id3400 a dtd:LA ;
    dtd:has_DL dtd:DL-id3199 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im sowjetisch besetzten Sektor von Berlin,die Entschädigungsbehörden des Landes Berlin, "@de .

dtd:LA-id3401 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Entschädigungsbehörden des Landes Berlin,"@de .

dtd:LA-id3402 a dtd:LA ;
    dtd:has_DL dtd:DL-id3202 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Vertreibungsgebieten innerhalb der Grenzen des Deutschen Reiches nach dem Stande vom 31. Dezember 1937 und im Gebiet der Freien Stadt Danzig für Verfolgte mit Wohnsitz oder dauerndem Aufenthalt in europäischen Ländern,die Entschädigungsbehörden des Landes Nordrhein-Westfalen, "@de .

dtd:LA-id3403 a dtd:LA ;
    dtd:has_DL dtd:DL-id3205 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Verfolgte mit Wohnsitz oder dauerndem Aufenthalt in außereuropäischen Ländern,die Entschädigungsbehörden des Landes Rheinland-Pfalz; "@de .

dtd:LA-id3404 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Entschädigungsbehörden des Landes Nordrhein-Westfalen,"@de .

dtd:LA-id3405 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Entschädigungsbehörden des Landes Rheinland-Pfalz;"@de .

dtd:LA-id3406 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hilfsweise:"@de .

dtd:LA-id3407 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die in § 4 Abs. 1 Nr. 1 Buchstaben d bis g genannten Verfolgten die Entschädigungsbehörden des Landes, in dem der Verfolgte nach dem 31. Dezember 1952 erstmals seinen Wohnsitz oder dauernden Aufenthalt genommen hat oder nimmt;"@de .

dtd:LA-id3408 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hilfsweise:"@de .

dtd:LA-id3409 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die in § 4 Abs. 1 Nr. 2 genannten Verfolgten die Entschädigungsbehörden des Landes, in dem der Verfolgte sich am 1. Januar 1947 aufgehalten hat, wobei der Aufenthalt in einem Durchgangslager für Auswanderer außer Betracht bleibt."@de .

dtd:LA-id3410 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Landes Nordrhein-Westfalen für Antragsteller mit Wohnsitz oder dauerndem Aufenthalt in europäischen Ländern,"@de .

dtd:LA-id3411 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Landes Rheinland-Pfalz für Antragsteller mit Wohnsitz oder dauerndem Aufenthalt in außereuropäischen Ländern."@de .

dtd:LA-id3412 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben zur Person und zu den wirtschaftlichen Verhältnissen,"@de .

dtd:LA-id3413 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Darstellung des den Anspruch begründenden Sachverhalts,"@de .

dtd:LA-id3414 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angabe von Beweismitteln,"@de .

dtd:LA-id3415 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben über Art und Umfang des Anspruchs,"@de .

dtd:LA-id3416 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Erklärung, ob und wo der Antragsteller schon früher einen Antrag gestellt oder einen Anspruch angemeldet hat,"@de .

dtd:LA-id3417 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Erklärung über Leistungen, die im Zuge der Entschädigung für Opfer der nationalsozialistischen Verfolgung aus deutschen öffentlichen Mitteln oder von einem nach bürgerlichem Recht Schadensersatzpflichtigen bewirkt worden sind,"@de .

dtd:LA-id3418 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Erklärung darüber, ob und mit welchem Erfolg ein Rückerstattungsverfahren wegen eines dem Antragsteller oder seinem Rechtsvorgänger vor der Entziehung gehörenden Vermögensgegenstandes anhängig gemacht worden ist."@de .

dtd:LA-id3419 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Staatsanwaltschaft oder unmittelbar die Polizeibehörde um die Erforschung eines Verfolgungstatbestandes ersuchen;"@de .

dtd:LA-id3420 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Amtsgericht, in dessen Bezirk der Antragsteller, ein Zeuge oder ein Sachverständiger sich aufhält, um Vernehmung des Antragstellers, des Zeugen oder des Sachverständigen ersuchen, wobei die Tatsachen und Vorgänge anzugeben sind, die Gegenstand der Vernehmung sein sollen;"@de .

dtd:LA-id3421 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Auslandsvertretung der Bundesrepublik, in deren Bezirk der Antragsteller, ein Zeuge oder ein Sachverständiger sich aufhält, um Vernehmung des Antragstellers, des Zeugen oder des Sachverständigen ersuchen, wobei die Tatsachen und Vorgänge anzugeben sind, die Gegenstand der Vernehmung sein sollen;"@de .

dtd:LA-id3422 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Strafregisterbehörden um unbeschränkte Auskunft, auch über getilgte Strafen, ersuchen."@de .

dtd:LA-id3423 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bezeichnung der Entschädigungsbehörde,"@de .

dtd:LA-id3424 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Entscheidungsformel einschließlich etwaiger Leistungsvorbehalte,"@de .

dtd:LA-id3425 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Hinweis, daß Klage erhoben werden kann, soweit der Anspruch abgelehnt worden ist, und die Belehrung, in welcher Form, innerhalb welcher Frist sowie bei welchem Gericht die Klage zu erheben ist,"@de .

dtd:LA-id3426 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Datum und die Unterschrift oder die Namenswiedergabe des Behördenleiters, seines Vertreters oder seines Beauftragten."@de .

dtd:LA-id3427 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Personalangaben des Antragstellers,"@de .

dtd:LA-id3428 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Feststellung des Sachverhalts,"@de .

dtd:LA-id3429 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Entscheidungsgründe."@de .

dtd:LA-id3430 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Rechtsfrage von grundsätzlicher Bedeutung zu entscheiden ist;"@de .

dtd:LA-id3431 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Urteil von einer Entscheidung des Bundesgerichtshofs abweicht und auf dieser Abweichung beruht;"@de .

dtd:LA-id3432 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Fortbildung des Rechts oder die Sicherung einer einheitlichen Rechtsprechung eine Entscheidung des Bundesgerichtshofs erfordert;"@de .

dtd:LA-id3433 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "streitig ist, ob das Land, gegen das der Anspruch auf Entschädigung gerichtet ist (§ 188), zu Recht als zuständig in Anspruch genommen ist."@de .

dtd:LA-id3434 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn es sich um verschiedene Schadenstatbestände handelt, die Entschädigungsbehörden des Landes, auf dessen Recht sich der jeweilige Anspruch gründet,"@de .

dtd:LA-id3435 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn es sich um denselben Schadenstatbestand handelt, die Entschädigungsbehörden des Landes, dessen Recht nach der Erklärung des Antragstellers angewandt werden soll; bei dieser Zuständigkeit behält es sein Bewenden."@de .

dtd:LA-id3436 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Soweit das Verfahren bei einem Gericht anhängig ist, das auch nach diesem Gesetz zuständig ist, entscheidet dieses Gericht auf Grund der Vorschriften dieses Gesetzes;"@de .

dtd:LA-id3437 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit das Verfahren bei einem Gericht anhängig ist, das nach diesem Gesetz nicht zuständig ist, ist das Verfahren an das nach diesem Gesetz zuständige Gericht erster Instanz abzugeben."@de .


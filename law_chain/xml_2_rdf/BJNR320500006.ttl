dtd:entry-id562 a dtd:entry ;
    dtd:has_Value "Note 5 = mangelhaft:"@de ;
    dtd:has_valign dtd:top .

dtd:entry-id563 a dtd:entry ;
    dtd:has_Value "eine Leistung, die den Anforderungen nicht entspricht, jedoch erkennen lässt, dass notwendige Grundlagen für die berufliche Handlungsfähigkeit vorhanden sind."@de ;
    dtd:has_valign dtd:top .

dtd:entry-id564 a dtd:entry ;

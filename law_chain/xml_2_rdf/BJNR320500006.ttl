    dtd:has_Value "Note 4 = ausreichend:"@de ;
    dtd:has_valign dtd:top .

dtd:entry-id561 a dtd:entry ;
    dtd:has_Value "eine Leistung, die zwar Mängel aufweist, aber im Ganzen den Anforderungen noch entspricht."@de ;

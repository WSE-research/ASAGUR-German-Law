dtd:LA-id2881 a dtd:LA ;
    dtd:has_DL dtd:DL-id1462 ;
    dtd:has_Value "bei einer juristischen Person oder bei einer Personengesellschaft: a)Firma, Name oder Bezeichnung,b)Rechtsform,c)Registernummer, falls vorhanden,d)Anschrift des Sitzes oder der Hauptniederlassung unde)die Namen der Mitglieder des Vertretungsorgans oder die Namen der gesetzlichen Vertreter und, sofern ein Mitglied des Vertretungsorgans oder der gesetzliche Vertreter eine juristische Person ist, von dieser juristischen Person die Daten nach den Buchstaben a bis d."@de .

dtd:LA-id2882 a dtd:LA ;
    dtd:has_Value "Firma, Name oder Bezeichnung,"@de .

dtd:LA-id2883 a dtd:LA ;
    dtd:has_Value "Rechtsform,"@de .

dtd:LA-id2884 a dtd:LA ;
    dtd:has_Value "Registernummer, falls vorhanden,"@de .

dtd:LA-id2885 a dtd:LA ;
    dtd:has_Value "Anschrift des Sitzes oder der Hauptniederlassung und"@de .

dtd:LA-id2886 a dtd:LA ;
    dtd:has_Value "die Namen der Mitglieder des Vertretungsorgans oder die Namen der gesetzlichen Vertreter und, sofern ein Mitglied des Vertretungsorgans oder der gesetzliche Vertreter eine juristische Person ist, von dieser juristischen Person die Daten nach den Buchstaben a bis d."@de .

dtd:LA-id2887 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines gültigen amtlichen Ausweises, der ein Lichtbild des Inhabers enthält und mit dem die Pass- und Ausweispflicht im Inland erfüllt wird, insbesondere anhand eines inländischen oder nach ausländerrechtlichen Bestimmungen anerkannten oder zugelassenen Passes, Personalausweises oder Pass- oder Ausweisersatzes,"@de .

dtd:LA-id2888 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines elektronischen Identitätsnachweises nach § 18 des Personalausweisgesetzes, nach § 12 des eID-Karte-Gesetzes oder nach § 78 Absatz 5 des Aufenthaltsgesetzes,"@de .

dtd:LA-id2889 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer qualifizierten elektronischen Signatur nach Artikel 3 Nummer 12 der Verordnung (EU) Nr. 910/2014 des Europäischen Parlaments und des Rates vom 23. Juli 2014 über elektronische Identifizierung und Vertrauensdienste für elektronische Transaktionen im Binnenmarkt und zur Aufhebung der Richtlinie 1999/93/EG (ABl. L 257 vom 28.8.2014, S. 73),"@de .

dtd:LA-id2890 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines nach Artikel 8 Absatz 2 Buchstabe c in Verbindung mit Artikel 9 der Verordnung (EU) Nr. 910/2014 notifizierten elektronischen Identifizierungssystems oder"@de .

dtd:LA-id2891 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Dokumenten nach § 1 Absatz 1 der Verordnung über die Bestimmung von Dokumenten, die zur Identifizierung einer nach dem Geldwäschegesetz zu identifizierenden Person zum Zwecke des Abschlusses eines Zahlungskontovertrags zugelassen werden."@de .

dtd:LA-id2892 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "anderen Mitgliedstaat der Europäischen Union,"@de .

dtd:LA-id2893 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vertragsstaat des Abkommens über den Europäischen Wirtschaftsraum oder"@de .

dtd:LA-id2894 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Drittstaat, in dem das Kreditinstitut Sorgfalts- und Aufbewahrungspflichten unterliegt, die den in der Richtlinie (EU) 2015/849 festgelegten Sorgfalts- und Aufbewahrungspflichten entsprechen und deren Einhaltung in einer mit Kapitel IV Abschnitt 2 der Richtlinie (EU) 2015/849 im Einklang stehenden Weise beaufsichtigt wird."@de .

dtd:LA-id2895 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines Auszuges aus dem Handels- oder Genossenschaftsregister oder aus einem vergleichbaren amtlichen Register oder Verzeichnis,"@de .

dtd:LA-id2896 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Gründungsdokumenten oder von gleichwertigen beweiskräftigen Dokumenten oder"@de .

dtd:LA-id2897 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer eigenen dokumentierten Einsichtnahme des Verpflichteten in die Register- oder Verzeichnisdaten."@de .

dtd:LA-id2898 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch angemessene Prüfung des vor Ort vorgelegten Dokuments oder"@de .

dtd:LA-id2899 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mittels eines sonstigen Verfahrens, das zur geldwäscherechtlichen Überprüfung der Identität geeignet ist und ein Sicherheitsniveau aufweist, das dem in Nummer 1 genannten Verfahren gleichwertig ist."@de .

dtd:LA-id2900 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Konkretisierungen oder weitere Anforderungen an das in Absatz 1 genannte Verfahren und an die sich dieses Verfahrens bedienenden Verpflichteten sowie die Aufzeichnungs- und Aufbewahrungspflichten bei Nutzung dieses Verfahrens festlegen,"@de .

dtd:LA-id2901 a dtd:LA ;
    dtd:has_Size dtd:normal ;

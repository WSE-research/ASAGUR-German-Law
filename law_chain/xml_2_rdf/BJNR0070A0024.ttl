        dtd:BR-id480 ;
    dtd:has_IMG dtd:IMG-id4323 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value ""@de .

dtd:LA-id4281 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Diese Berechnung findet auch auf andere beschichtete Blechteile Anwendung. Die Fläche der hinzukommenden Teile oder die in der Anlage beschichtete Gesamtfläche ist mit Hilfe von Computer Aided Design oder anderen gleichwertigen Verfahren zu berechnen."@de .


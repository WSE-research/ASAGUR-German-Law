    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei den Gesellen und anderen Arbeitnehmern mit abgeschlossener Berufsausbildung, die die Voraussetzungen für die Wahlberechtigung (§ 98) erfüllen."@de .

dtd:LA-id375 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Nachweis der Berechtigung zur Ausübung des Wahlrechts (Wahlschein),"@de .


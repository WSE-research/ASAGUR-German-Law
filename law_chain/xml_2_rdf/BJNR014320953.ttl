    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen neutralen Umschlag der Bezeichnung \"Handwerkskammer-Wahl\" (Wahlumschlag) und"@de .

dtd:LA-id378 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Umschlag für die Rücksendung der Wahlunterlagen (Rücksendeumschlag)."@de .

dtd:LA-id379 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nicht in einem amtlich abgestempelten Umschlag oder die in einem mit Kennzeichen versehenen Umschlag übergeben worden sind,"@de .

dtd:LA-id380 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die als nichtamtlich hergestellte erkennbar sind,"@de .

dtd:LA-id381 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus deren Beantwortung oder zulässiger Kennzeichnung der Wille des Abstimmenden nicht unzweifelhaft zu erkennen ist,"@de .

dtd:LA-id382 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "denen ein durch den Umschlag deutlich fühlbarer Gegenstand beigefügt ist,"@de .


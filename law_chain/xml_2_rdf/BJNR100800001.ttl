    dtd:has_Value "bei der Schleimkrankheit an Kartoffeln das in Anhang II der Richtlinie 98/57/EG und"@de .

dtd:LA-id462 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Schleimkrankheit an Tomatenpflanzen und anderen Wirtspflanzen das in Anhang II Abschnitt I Nr. 1 und 3, Abschnitt II und Abschnitt III Punkt 2 der Richtlinie 98/57/EG"@de .

dtd:LA-id463 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf die Bakterielle Ringfäule entsprechend den Maßgaben des Anhangs II der Richtlinie 93/85/EWG und"@de .

dtd:LA-id464 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf die Schleimkrankheit entsprechend den Maßgaben des Anhangs III der Richtlinie 98/57/EG"@de .


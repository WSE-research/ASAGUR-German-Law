dtd:LA-id442 a dtd:LA ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id539 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bakteriellen Ringfäule der Kartoffel (Bakterielle Ringfäule) (Schadorganismus: Clavibacter michiganensis (Smith) Smith et al. ssp. sepedonicus (Spieck. et Kotth.) Davis et al.) oder"@de .

dtd:LA-id443 a dtd:LA ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id540 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Schleimkrankheit (Schadorganismus: Ralstonia solanacearum (Smith) Yabuuchi et al.) an Kartoffeln, Tomatenpflanzen oder anderen Wirtspflanzen"@de .

dtd:LA-id444 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Bakteriellen Ringfäule und bei der Schleimkrankheit an Kartoffeln die Verfügungsberechtigten und Besitzer von Feldbeständen an Kartoffeln oder geernteter, eingelagerter oder in den Verkehr gebrachter Kartoffeln oder"@de .

dtd:LA-id445 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Schleimkrankheit an Tomaten oder anderen Wirtspflanzen die Verfügungsberechtigten und Besitzer von Feld- oder Gewächshausbeständen dieser Pflanzen oder gelagerter oder in den Verkehr gebrachter Tomatenpflanzen."@de .

dtd:LA-id446 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Kartoffeln Kenntnis über das Auftreten oder den Verdacht des Auftretens der Bakteriellen Ringfäule oder der Schleimkrankheit oder"@de .

dtd:LA-id447 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Tomatenpflanzen Kenntnis über das Auftreten oder den Verdacht des Auftretens der Schleimkrankheit"@de .

dtd:LA-id448 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die geernteten, die gelagerten und die in den Verkehr gebrachten Kartoffelknollen auf den Befall mit der Bakteriellen Ringfäule und der Schleimkrankheit,"@de .

dtd:LA-id449 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tomatenpflanzen, die zur erwerbsmäßigen Weiterkultur bestimmt sind, auf den sichtbaren Befall mit der Schleimkrankheit."@de .

dtd:LA-id450 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an anderen Wirtspflanzen des Schadorganismus,"@de .

dtd:LA-id451 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Oberflächengewässer, soweit diese zur Beregnung oder Bewässerung bei der Erzeugung von Kartoffeln oder Tomatenpflanzen verwendet werden, und"@de .

dtd:LA-id452 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Abwässer, die aus Anlagen zur Verarbeitung oder Verpackung von Kartoffeln oder Tomatenpflanzen stammen und zur Beregnung oder Bewässerung bei der Erzeugung von Kartoffeln oder Tomatenpflanzen verwendet werden,"@de .

dtd:LA-id453 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Kartoffeln, die klonal oder durch gemeinsame Maschinennutzung mit den befallsverdächtigen Kartoffeln in Zusammenhang stehen, oder"@de .

dtd:LA-id454 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn der Verdacht des Befalls mit der Schleimkrankheit an Tomatenpflanzen festgestellt wird."@de .

dtd:LA-id455 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit der Bakteriellen Ringfäule liegt vor, wenn Kartoffeln sichtbare Anzeichen der Krankheit zeigen oder der Immunfluoreszenztest nach Anhang I der Richtlinie 93/85/EWG oder ein anderer geeigneter Test,"@de .

dtd:LA-id456 a dtd:LA ;
    dtd:has_DL dtd:DL-id253 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit der Schleimkrankheit liegt vor, wenna)Kartoffeln sichtbare Anzeichen der Krankheit zeigen und ein Schnelltest nach Anhang II Abschnitt I Nr. 1 und Abschnitt II Nr. 2 oder ein Schnelltest nach Anhang II Abschnitt I Nr. 2 und Abschnitt III der Richtlinie 98/57/EG oderb)Tomatenpflanzen sichtbare Anzeichen zeigen und ein Schnelltest nach Anhang II Abschnitt I Nr. 1 und Abschnitt II Nr. 2 der Richtlinie 98/57/EG"@de .

dtd:LA-id457 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu einem positiven Ergebnis führen."@de .

dtd:LA-id458 a dtd:LA ;

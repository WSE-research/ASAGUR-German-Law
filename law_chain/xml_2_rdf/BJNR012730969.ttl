dtd:LA-id809 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu erwartenden Einnahmen,"@de .

dtd:LA-id810 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "voraussichtlich zu leistenden Ausgaben und"@de .

dtd:LA-id811 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "voraussichtlich benötigten Verpflichtungsermächtigungen."@de .

dtd:LA-id812 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei den Einnahmen: Steuern, Verwaltungseinnahmen, Einnahmen aus Vermögensveräußerungen, Darlehensrückflüsse, Zuweisungen und Zuschüsse, Einnahmen aus Krediten, wozu nicht Kredite zur Aufrechterhaltung einer ordnungsmäßigen Kassenwirtschaft (Kassenverstärkungskredite) zählen, Entnahmen aus Rücklagen, Münzeinnahmen;"@de .

dtd:LA-id813 a dtd:LA ;
    dtd:has_DL dtd:DL-id630 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei den Ausgaben: Personalausgaben, sächliche Verwaltungsausgaben, Zinsausgaben, Zuweisungen an Gebietskörperschaften, Zuschüsse an Unternehmen, Tilgungsausgaben, Schuldendiensthilfen, Zuführungen an Rücklagen, Ausgaben für Investitionen. Ausgaben für Investitionen sind die Ausgaben füra)Baumaßnahmen, soweit sie nicht militärische Anlagen betreffen,b)den Erwerb von beweglichen Sachen, soweit sie nicht als sächliche Verwaltungsausgaben veranschlagt werden oder soweit es sich nicht um Ausgaben für militärische Beschaffungen handelt,c)den Erwerb von unbeweglichen Sachen,d)den Erwerb von Beteiligungen und sonstigem Kapitalvermögen, von Forderungen und Anteilsrechten an Unternehmen, von Wertpapieren sowie für die Heraufsetzung des Kapitals von Unternehmen,e)Darlehen,f)die Inanspruchnahme aus Gewährleistungen,g)Zuweisungen und Zuschüsse zur Finanzierung von Ausgaben für die in den Buchstaben a bis f genannten Zwecke."@de .

dtd:LA-id814 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Baumaßnahmen, soweit sie nicht militärische Anlagen betreffen,"@de .

dtd:LA-id815 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Erwerb von beweglichen Sachen, soweit sie nicht als sächliche Verwaltungsausgaben veranschlagt werden oder soweit es sich nicht um Ausgaben für militärische Beschaffungen handelt,"@de .

dtd:LA-id816 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Erwerb von unbeweglichen Sachen,"@de .

dtd:LA-id817 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Erwerb von Beteiligungen und sonstigem Kapitalvermögen, von Forderungen und Anteilsrechten an Unternehmen, von Wertpapieren sowie für die Heraufsetzung des Kapitals von Unternehmen,"@de .

dtd:LA-id818 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Darlehen,"@de .

dtd:LA-id819 a dtd:LA ;

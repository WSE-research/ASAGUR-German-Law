dtd:LA-id212 a dtd:LA ;
    dtd:has_Value "des § 9 Absatz 1 Satz 1 Nummer 3 in Verbindung mit Satz 2 und mit Absatz 2 Satz 3 des Seeaufgabengesetzes in der Fassung der Bekanntmachung vom 26. Juli 2002 (BGBl. I S. 2876), von denen § 9 Absatz 1 Satz 1 Nummer 3 zuletzt durch Artikel 2 Absatz 1 Nummer 6 des Gesetzes vom 20. April 2013 (BGBl. I S. 868) neu gefasst und § 9 Absatz 2 Satz 3 durch Artikel 2 Absatz 1 Nummer 6 des Gesetzes vom 20. April 2013 (BGBl. I S. 868) eingefügt worden ist, im Einvernehmen mit dem Bundesministerium für Arbeit und Soziales und dem Bundesministerium für Ernährung, Landwirtschaft und Verbraucherschutz und"@de .

dtd:LA-id213 a dtd:LA ;
    dtd:has_Value "des § 113 Absatz 1 Satz 1 Nummer 2 in Verbindung mit Satz 2 Nummer 1 des Seearbeitsgesetzes vom 23. April 2013 (BGBl. I S. 868) im Einvernehmen mit dem Bundesministerium für Ernährung, Landwirtschaft und Verbraucherschutz:"@de .

dtd:LA-id214 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Ausdruck „Seearbeitsübereinkommen” das Seearbeitsübereinkommen, 2006, der Internationalen Arbeitsorganisation vom 23. Februar 2006 (BGBl. 2013 II S. 763, 765) in der jeweils geltenden Fassung,"@de .

dtd:LA-id215 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Ausdruck „Berufsgenossenschaft“ die Berufsgenossenschaft Verkehrswirtschaft Post-Logistik Telekommunikation,"@de .

dtd:LA-id216 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Ausdruck „Unionsbürger” einen Staatsangehörigen eines Mitgliedstaates der Europäischen Union."@de .

dtd:LA-id217 a dtd:LA ;
    dtd:has_Value "die Schiffssicherheit,"@de .

dtd:LA-id218 a dtd:LA ;
    dtd:has_Value "der sichere Wachdienst,"@de .

dtd:LA-id219 a dtd:LA ;
    dtd:has_Value "die Einhaltung der Vorschriften des Arbeitsschutzes einschließlich des Arbeitszeitschutzes, des Gesundheitsschutzes, der medizinischen Betreuung an Bord und des maritimen Umweltschutzes,"@de .

dtd:LA-id220 a dtd:LA ;
    dtd:has_Value "die Erhaltung der öffentlichen Ordnung und Sicherheit an Bord und"@de .

dtd:LA-id221 a dtd:LA ;
    dtd:has_Value "die sprachliche Verständigung der Besatzungsmitglieder untereinander"@de .

dtd:LA-id222 a dtd:LA ;
    dtd:has_Value "das Schiff entsprechend dem auf Grund des § 8 Absatz 1 ausgestellten Schiffsbesatzungszeugnis besetzt ist,"@de .

dtd:LA-id223 a dtd:LA ;
    dtd:has_Value "die Anordnungen der Berufsgenossenschaft nach § 9 Absatz 2 Satz 1 befolgt werden und"@de .

dtd:LA-id224 a dtd:LA ;
    dtd:has_Value "das Schiffsbesatzungszeugnis an Bord mitgeführt wird."@de .

dtd:LA-id225 a dtd:LA ;
    dtd:has_Value "das von ihm geführte Schiff entsprechend dem auf Grund des § 8 Absatz 1 ausgestellten Schiffsbesatzungszeugnis besetzt ist,"@de .

dtd:LA-id226 a dtd:LA ;
    dtd:has_Value "die Anordnungen der Berufsgenossenschaft nach § 9 Absatz 2 Satz 1 befolgt werden,"@de .

dtd:LA-id227 a dtd:LA ;
    dtd:has_DL dtd:DL-id178 ;
    dtd:has_Value "das Schiffsbesatzungszeugnis a)an Bord mitgeführt,b)der Berufsgenossenschaft, der Bundespolizei, der Zollverwaltung und den Wasserschutzpolizeien der Länder auf Verlangen vorgelegt wird und"@de .

dtd:LA-id228 a dtd:LA ;
    dtd:has_Value "an Bord mitgeführt,"@de .

dtd:LA-id229 a dtd:LA ;
    dtd:has_Value "der Berufsgenossenschaft, der Bundespolizei, der Zollverwaltung und den Wasserschutzpolizeien der Länder auf Verlangen vorgelegt wird und"@de .

dtd:LA-id230 a dtd:LA ;
    dtd:has_Value "ein Abdruck des Schiffsbesatzungszeugnisses an geeigneter Stelle an Bord ausgehängt wird."@de .

dtd:LA-id231 a dtd:LA ;
    dtd:has_Value "im Besitz des Zeugnisses über die Abschlussprüfung im Ausbildungsberuf Koch oder eines anderen einschlägigen Ausbildungsberufes nach innerstaatlichem Recht sind oder"@de .

dtd:LA-id232 a dtd:LA ;
    dtd:has_Value "eine Bescheinigung einer Industrie- und Handelskammer über die Teilnahme an einer Gaststättenunterrichtung nach gaststättenrechtlichen Vorschriften nachweisen oder"@de .

dtd:LA-id233 a dtd:LA ;
    dtd:has_Value "einen Nachweis über die Befähigung zum Schiffskoch einer Vertragspartei des Seearbeitsübereinkommens oder"@de .

dtd:LA-id234 a dtd:LA ;
    dtd:has_Value "einen gleichwertigen Nachweis eines anderen als in Nummer 3 genannten Staates besitzen."@de .

dtd:LA-id235 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Brückenwache gehenden Schiffsoffiziere während ihrer Wache auf der Brücke oder in einem unmittelbar damit verbundenen Raum, wie dem Kartenraum oder dem Brückenfahrstand, körperlich anwesend sind und die sichere Führung des Schiffes wahrnehmen,"@de .

dtd:LA-id236 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Offiziere des technischen Bereichs unter der Weisungsbefugnis des Leiters der Maschinenanlage zum Aufsuchen des Maschinenraums unmittelbar zur Verfügung und in Bereitschaft stehen und bei Bedarf während ihres jeweiligen Verantwortlichkeitszeitraums im Maschinenraum körperlich anwesend sind,"@de .

dtd:LA-id237 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jederzeit, während das Schiff vor Anker liegt oder festgemacht hat, aus Sicherheitsgründen zweckmäßige und wirksame Wachen gegangen werden und, soweit das Schiff gefährliche Ladung befördert, bei der Durchführung der Wachen Art, Menge, Verpackung und Stauung der gefährlichen Ladung sowie etwaige besondere Umstände, die an Bord oder in der Umgebung des Schiffes auf dem Wasser oder an Land herrschen, in vollem Umfang berücksichtigt werden und"@de .

dtd:LA-id238 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus Gründen der Gefahrenabwehr zweckmäßige und wirksame Wachen gegangen werden."@de .

dtd:LA-id239 a dtd:LA ;
    dtd:has_Value "entgegen § 2 Absatz 2 Nummer 1 oder § 3 Nummer 1 nicht dafür sorgt, dass das Schiff besetzt ist,"@de .

dtd:LA-id240 a dtd:LA ;
    dtd:has_Value "entgegen § 2 Absatz 2 Nummer 3 oder § 3 Nummer 3 Buchstabe a nicht dafür sorgt, dass das Schiffsbesatzungszeugnis an Bord mitgeführt wird,"@de .

dtd:LA-id241 a dtd:LA ;
    dtd:has_Value "entgegen § 3 Nummer 3 Buchstabe b nicht dafür sorgt, dass das Schiffsbesatzungszeugnis einer dort genannten Stelle vorgelegt wird,"@de .

dtd:LA-id242 a dtd:LA ;
    dtd:has_Value "entgegen § 3 Nummer 4 nicht dafür sorgt, dass ein Abdruck des Schiffsbesatzungszeugnisses ausgehängt wird, oder"@de .

dtd:LA-id243 a dtd:LA ;
    dtd:has_Value "einer vollziehbaren Anordnung oder einer vollziehbaren Auflage nach § 9 Absatz 2 Satz 1 zuwiderhandelt."@de .


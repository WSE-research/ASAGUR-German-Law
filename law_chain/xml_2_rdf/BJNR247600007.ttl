dtd:LA-id602 a dtd:LA ;
    dtd:has_Value "in Verwaltungen, Verbänden und Unternehmen unterschiedlicher Branchenzugehörigkeit mit wasserbaulichen oder wasserwirtschaftlichen Aufgabenstellungen komplexe Sach-, Organisations- und Führungsaufgaben unter Anwendung von Rechtsvorschriften verantwortungsvoll wahrzunehmen,"@de .

dtd:LA-id603 a dtd:LA ;
    dtd:has_Value "die Schnittstellenfunktion zwischen den technischen, betriebswirtschaftlichen und ökologischen Anforderungen wahrzunehmen,"@de .

dtd:LA-id604 a dtd:LA ;
    dtd:has_Value "ökonomische, ökologische und sozialverträgliche Aspekte der Nachhaltigkeit zu berücksichtigen,"@de .

dtd:LA-id605 a dtd:LA ;
    dtd:has_Value "sich auf wandelnde Arbeitsmethoden, Anforderungen und Techniken in den unterschiedlichen Arbeitsgebieten einzustellen,"@de .

dtd:LA-id606 a dtd:LA ;
    dtd:has_Value "die technische und ökologische Weiterentwicklung im Wasserbau und in der Wasserwirtschaft mitzugestalten,"@de .

dtd:LA-id607 a dtd:LA ;
    dtd:has_Value "Entwicklungen in der Aufbau- und Arbeitsorganisation zu berücksichtigen und umzusetzen,"@de .

dtd:LA-id608 a dtd:LA ;
    dtd:has_Value "Maßnahmen zur Personalentwicklung zu fördern sowie die Mitarbeiter zu führen,"@de .

dtd:LA-id609 a dtd:LA ;
    dtd:has_Value "berufs- und arbeitspädagogische Qualifikationen im Rahmen der Berufsausbildung anzuwenden."@de .

dtd:LA-id610 a dtd:LA ;
    dtd:has_Value "Überwachen, Prüfen und Dokumentieren des Zustandes der Gewässer und wasserbaulichen Anlagen sowie Erkennen von Störungen und Schäden an diesen und Einleiten von Maßnahmen zu deren Beseitigung,"@de .

dtd:LA-id611 a dtd:LA ;
    dtd:has_Value "Durchführen und Überwachen von Verkehrssicherungsmaßnahmen,"@de .

dtd:LA-id612 a dtd:LA ;
    dtd:has_Value "Erstellen und Umsetzen von Programmplanungen und Unterhaltungsplänen,"@de .

dtd:LA-id613 a dtd:LA ;
    dtd:has_Value "Berücksichtigen und Umsetzen von Gewässerentwicklungskonzepten und Maßnahmeplänen,"@de .

dtd:LA-id614 a dtd:LA ;
    dtd:has_Value "Planen, Organisieren, Überwachen und Dokumentieren von Personal-, Fahrzeug- und Geräteeinsätzen sowie Gewährleisten von Einsatz- und Betriebsbereitschaft,"@de .

dtd:LA-id615 a dtd:LA ;
    dtd:has_Value "Planen, Vorbereiten, Ausschreiben und Vergeben von Aufträgen an Dritte,"@de .

dtd:LA-id616 a dtd:LA ;
    dtd:has_Value "Überwachen, Dokumentieren und Abnehmen von Leistungen Dritter,"@de .


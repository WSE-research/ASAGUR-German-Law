    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihm ein Angebot im Sinne des § 35 Abs. 2 Satz 1 nach § 15 Abs. 1 Nr. 1, 2 oder 3 untersagt worden ist."@de .

dtd:LA-id1404 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Voraussetzungen für die Anordnung nach Absatz 1 nicht vorgelegen haben oder nicht mehr vorliegen,"@de .

dtd:LA-id1405 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ernstliche Zweifel an der Rechtmäßigkeit der angefochtenen Verfügung bestehen oder"@de .

dtd:LA-id1406 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vollziehung für den Betroffenen eine unbillige, nicht durch überwiegende öffentliche Interessen gebotene Härte zur Folge hätte."@de .

dtd:LA-id1407 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erklärung, inwieweit die Verfügung angefochten und ihre Abänderung oder Aufhebung beantragt wird, und"@de .

dtd:LA-id1408 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angabe der Tatsachen und Beweismittel, auf die sich die Beschwerde stützt."@de .

dtd:LA-id1409 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vorschriften der §§ 169 bis 197 des Gerichtsverfassungsgesetzes über Öffentlichkeit, Sitzungspolizei, Gerichtssprache, Beratung und Abstimmung und"@de .

dtd:LA-id1410 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vorschriften der Zivilprozessordnung über Ausschließung und Ablehnung eines Richters, über Prozessbevollmächtigte und Beistände, über die Zustellung von Amts wegen, über Ladungen, Termine und Fristen, über die Anordnung des persönlichen Erscheinens der Parteien, über die Verbindung mehrerer Prozesse, über die Erledigung des Zeugen- und Sachverständigenbeweises sowie über die sonstigen Arten des Beweisverfahrens, über die Wiedereinsetzung in den vorigen Stand gegen die Versäumung einer Frist."@de .

dtd:LA-id1411 a dtd:LA ;
    dtd:has_DL dtd:DL-id1268 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegena)§ 10 Abs. 1 Satz 1, § 14 Abs. 2 Satz 1 oder § 35 Abs.1 Satz 1 oder Abs. 2 Satz 1,b)§ 21 Abs. 2 Satz 1, § 23 Abs. 1 Satz 1 oder Abs. 2 Satz 1 oder § 27 Abs. 3 Satz 1 oderc)§ 1 Abs. 5 Satz 2 in Verbindung mit einer Rechtsverordnung nach § 1 Abs. 5 Satz 3"@de .

dtd:LA-id1412 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Veröffentlichung nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig vornimmt,"@de .

dtd:LA-id1413 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 10 Abs. 1 Satz 1, § 14 Abs. 2 Satz 1 oder § 35 Abs.1 Satz 1 oder Abs. 2 Satz 1,"@de .

dtd:LA-id1414 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 21 Abs. 2 Satz 1, § 23 Abs. 1 Satz 1 oder Abs. 2 Satz 1 oder § 27 Abs. 3 Satz 1 oder"@de .

dtd:LA-id1415 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 1 Abs. 5 Satz 2 in Verbindung mit einer Rechtsverordnung nach § 1 Abs. 5 Satz 3"@de .

dtd:LA-id1416 a dtd:LA ;
    dtd:has_DL dtd:DL-id1275 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegena)§ 10 Abs. 2 Satz 1, auch in Verbindung mit § 35 Abs.1 Satz 4, § 14 Abs. 1 Satz 1 oder § 35 Abs. 2 Satz 1,b)§ 10 Abs. 5, auch in Verbindung mit § 35 Abs. 1 Satz 4, oder § 14 Abs. 4, auch in Verbindung mit § 21 Abs. 2 Satz 2 oder § 35 Abs. 2 Satz 2, oderc)§ 27 Abs. 3 Satz 2"@de .

dtd:LA-id1417 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Mitteilung, Unterrichtung oder Übermittlung nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig vornimmt,"@de .

dtd:LA-id1418 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 10 Abs. 2 Satz 1, auch in Verbindung mit § 35 Abs.1 Satz 4, § 14 Abs. 1 Satz 1 oder § 35 Abs. 2 Satz 1,"@de .

dtd:LA-id1419 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 10 Abs. 5, auch in Verbindung mit § 35 Abs. 1 Satz 4, oder § 14 Abs. 4, auch in Verbindung mit § 21 Abs. 2 Satz 2 oder § 35 Abs. 2 Satz 2, oder"@de .

dtd:LA-id1420 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 27 Abs. 3 Satz 2"@de .

dtd:LA-id1421 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 10 Abs. 3 Satz 3, auch in Verbindung mit § 35 Abs. 1 Satz 4, oder § 14 Abs. 2 Satz 2, auch in Verbindung mit § 35 Abs. 2 Satz 2, eine Veröffentlichung vornimmt oder eine Angebotsunterlage bekannt gibt,"@de .

dtd:LA-id1422 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 10 Abs. 4 Satz 1, auch in Verbindung mit § 35 Abs. 1 Satz 4, eine Veröffentlichung nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig übersendet,"@de .

dtd:LA-id1423 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 14 Abs. 3 Satz 2, auch in Verbindung mit § 14 Absatz 3 Satz 3, § 21 Abs. 2 Satz 2, § 23 Absatz 1 Satz 2 oder Absatz 2 Satz 2, § 33c Absatz 3 Satz 5 oder § 35 Abs. 2 Satz 2, oder entgegen § 27 Abs. 3 Satz 3 eine Mitteilung nicht, nicht richtig oder nicht rechtzeitig macht,"@de .


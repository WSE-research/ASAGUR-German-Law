dtd:LA-id5582 a dtd:LA ;
    dtd:has_Value "bestehenden Expositionssituationen."@de .

dtd:LA-id5583 a dtd:LA ;
    dtd:has_Value "die Exposition von Einzelpersonen der Bevlkerung oder Arbeitskrften durch kosmische Strahlung, mit Ausnahme des fliegenden und raumfahrenden Personals,"@de .

dtd:LA-id5584 a dtd:LA ;
    dtd:has_Value "die oberirdische Exposition durch Radionuklide, die natrlicherweise in der nicht durch Eingriffe beeintrchtigten Erdrinde vorhanden sind,"@de .

dtd:LA-id5585 a dtd:LA ;
    dtd:has_Value "die Exposition durch Radionuklide, die natrlicherweise im menschlichen Krper vorhanden sind, und durch kosmische Strahlung in Bodennhe."@de .

dtd:LA-id5586 a dtd:LA ;
    dtd:has_Value "Exposition der Bevlkerung,"@de .

dtd:LA-id5587 a dtd:LA ;
    dtd:has_Value "berufliche Exposition,"@de .

dtd:LA-id5588 a dtd:LA ;
    dtd:has_Value "medizinische Exposition."@de .

dtd:LA-id5589 a dtd:LA ;
    dtd:has_Value "einer Person, die zum Ausbenden einer Ttigkeit nach diesem Gesetz in einem Beschftigungsverhltnis steht oder diese Ttigkeit selbst ausbt,"@de .

dtd:LA-id5590 a dtd:LA ;
    dtd:has_Value "von fliegendem und raumfahrendem Personal,"@de .

dtd:LA-id5591 a dtd:LA ;
    dtd:has_Value "einer Person, die eine Aufgabe nach  19 oder  20 des Atomgesetzes, nach  172 oder  178 wahrnimmt,"@de .

dtd:LA-id5592 a dtd:LA ;
    dtd:has_Value "einer Person, die in einer bestehenden Expositionssituation zum Ausbenden einer beruflichen Bettigung in einem Beschftigungsverhltnis steht oder eine solche Bettigung selbst ausbt (Arbeitskraft) oder"@de .

dtd:LA-id5593 a dtd:LA ;
    dtd:has_Value "einer Einsatzkraft whrend ihres Einsatzes in einer Notfallexpositionssituation oder einer anderen Gefahrenlage."@de .

dtd:LA-id5594 a dtd:LA ;
    dtd:has_Value "eines Patienten oder einer asymptomatischen Person, an dem oder der im Rahmen seiner oder ihrer medizinischen oder zahnmedizinischen Untersuchung oder Behandlung, die seiner oder ihrer Gesundheit zugutekommen soll, radioaktive Stoffe oder ionisierende Strahlung angewendet werden,"@de .

dtd:LA-id5595 a dtd:LA ;
    dtd:has_Value "einer Person, an der mit ihrer Einwilligung oder mit Einwilligung des gesetzlichen Vertreters oder Bevollmchtigten radioaktive Stoffe oder ionisierende Strahlung zum Zweck der medizinischen Forschung angewendet werden oder"@de .

dtd:LA-id5596 a dtd:LA ;
    dtd:has_Value "einer einwilligungsfhigen oder mit Einwilligung des gesetzlichen Vertreters oder Bevollmchtigten handelnden Person, die sich wissentlich und willentlich ionisierender Strahlung aussetzt, indem sie auerhalb ihrer beruflichen Ttigkeit freiwillig Personen untersttzt oder betreut, an denen im Rahmen ihrer medizinischen oder zahnmedizinischen Untersuchung oder Behandlung oder im Rahmen der medizinischen Forschung radioaktive Stoffe oder ionisierende Strahlung angewendet werden (Betreuungs-oder Begleitperson)."@de .

dtd:LA-id5597 a dtd:LA ;
    dtd:has_Value "Plutonium 239 und Plutonium 241,"@de .

dtd:LA-id5598 a dtd:LA ;
    dtd:has_Value "mit den Isotopen 235 oder 233 angereichertem Uran,"@de .

dtd:LA-id5599 a dtd:LA ;
    dtd:has_Value "jedem Stoff, der einen oder mehrere der in den Nummern 1 und 2 genannten Stoffe enthlt,"@de .

dtd:LA-id5600 a dtd:LA ;
    dtd:has_Value "Stoffen, mit deren Hilfe in einer geeigneten Anlage eine sich selbst tragende Kettenreaktion aufrechterhalten werden kann und die in einer durch die Bundesregierung mit Zustimmung des Bundesrates erlassenen Rechtsverordnung bestimmt werden."@de .

dtd:LA-id5601 a dtd:LA ;
    dtd:has_Value "festgelegte Freigrenzen unterschreitet,"@de .

dtd:LA-id5602 a dtd:LA ;
    dtd:has_Value "soweit es sich um einen im Rahmen einer genehmigungspflichtigen Ttigkeit nach diesem Gesetz, dem Atomgesetz oder nach einer auf Grund eines dieser Gesetze erlassenen Rechtsverordnung anfallenden Stoff handelt, festgelegte Freigabewerte unterschreitet und der Stoff freigegeben worden ist,"@de .

dtd:LA-id5603 a dtd:LA ;
    dtd:has_Value "soweit es sich um einen Stoff natrlichen Ursprungs handelt, der nicht auf Grund seiner Radioaktivitt, als Kernbrennstoff oder zur Erzeugung von Kernbrennstoff genutzt wird, nicht der berwachung nach dem Atomgesetz, nach diesem Gesetz oder einer auf Grund dieses Gesetzes mit Zustimmung des Bundesrates erlassenen Rechtsverordnung unterliegt."@de .

dtd:LA-id5604 a dtd:LA ;
    dtd:has_Value "der Umgang nach  5 Absatz 39,"@de .

dtd:LA-id5605 a dtd:LA ;
    dtd:has_Value "der Erwerb von knstlich erzeugten radioaktiven Stoffen und von natrlich vorkommenden radioaktiven Stoffen, die auf Grund ihrer Radioaktivitt, als Kernbrennstoff oder zur Erzeugung von Kernbrennstoff genutzt werden, die Abgabe dieser Stoffe an andere, ihre Befrderung und ihre grenzberschreitende Verbringung,"@de .

dtd:LA-id5606 a dtd:LA ;
    dtd:has_Value "die Verwahrung von Kernbrennstoffen nach  5 des Atomgesetzes und die Aufbewahrung von Kernbrennstoffen nach  6 des Atomgesetzes,"@de .

dtd:LA-id5607 a dtd:LA ;

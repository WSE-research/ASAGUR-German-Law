dtd:LA-id5580 a dtd:LA ;
    dtd:has_Value "geplanten Expositionssituationen,"@de .

dtd:LA-id5581 a dtd:LA ;
    dtd:has_Value "Notfallexpositionssituationen,"@de .

dtd:LA-id5582 a dtd:LA ;
    dtd:has_Value "bestehenden Expositionssituationen."@de .

dtd:LA-id5583 a dtd:LA ;
    dtd:has_Value "die Exposition von Einzelpersonen der Bevölkerung oder Arbeitskräften durch kosmische Strahlung, mit Ausnahme des fliegenden und raumfahrenden Personals,"@de .

dtd:LA-id5584 a dtd:LA ;
    dtd:has_Value "die oberirdische Exposition durch Radionuklide, die natürlicherweise in der nicht durch Eingriffe beeinträchtigten Erdrinde vorhanden sind,"@de .

dtd:LA-id5585 a dtd:LA ;
    dtd:has_Value "die Exposition durch Radionuklide, die natürlicherweise im menschlichen Körper vorhanden sind, und durch kosmische Strahlung in Bodennähe."@de .

dtd:LA-id5586 a dtd:LA ;
    dtd:has_Value "Exposition der Bevölkerung,"@de .

dtd:LA-id5587 a dtd:LA ;
    dtd:has_Value "berufliche Exposition,"@de .

dtd:LA-id5588 a dtd:LA ;
    dtd:has_Value "medizinische Exposition."@de .

dtd:LA-id5589 a dtd:LA ;
    dtd:has_Value "einer Person, die zum Ausübenden einer Tätigkeit nach diesem Gesetz in einem Beschäftigungsverhältnis steht oder diese Tätigkeit selbst ausübt,"@de .

dtd:LA-id5590 a dtd:LA ;
    dtd:has_Value "von fliegendem und raumfahrendem Personal,"@de .

dtd:LA-id5591 a dtd:LA ;
    dtd:has_Value "einer Person, die eine Aufgabe nach § 19 oder § 20 des Atomgesetzes, nach § 172 oder § 178 wahrnimmt,"@de .

dtd:LA-id5592 a dtd:LA ;
    dtd:has_Value "einer Person, die in einer bestehenden Expositionssituation zum Ausübenden einer beruflichen Betätigung in einem Beschäftigungsverhältnis steht oder eine solche Betätigung selbst ausübt (Arbeitskraft) oder"@de .

dtd:LA-id5593 a dtd:LA ;
    dtd:has_Value "einer Einsatzkraft während ihres Einsatzes in einer Notfallexpositionssituation oder einer anderen Gefahrenlage."@de .

dtd:LA-id5594 a dtd:LA ;
    dtd:has_Value "eines Patienten oder einer asymptomatischen Person, an dem oder der im Rahmen seiner oder ihrer medizinischen oder zahnmedizinischen Untersuchung oder Behandlung, die seiner oder ihrer Gesundheit zugutekommen soll, radioaktive Stoffe oder ionisierende Strahlung angewendet werden,"@de .

dtd:LA-id5595 a dtd:LA ;
    dtd:has_Value "einer Person, an der mit ihrer Einwilligung oder mit Einwilligung des gesetzlichen Vertreters oder Bevollmächtigten radioaktive Stoffe oder ionisierende Strahlung zum Zweck der medizinischen Forschung angewendet werden oder"@de .

dtd:LA-id5596 a dtd:LA ;
    dtd:has_Value "einer einwilligungsfähigen oder mit Einwilligung des gesetzlichen Vertreters oder Bevollmächtigten handelnden Person, die sich wissentlich und willentlich ionisierender Strahlung aussetzt, indem sie außerhalb ihrer beruflichen Tätigkeit freiwillig Personen unterstützt oder betreut, an denen im Rahmen ihrer medizinischen oder zahnmedizinischen Untersuchung oder Behandlung oder im Rahmen der medizinischen Forschung radioaktive Stoffe oder ionisierende Strahlung angewendet werden (Betreuungs-oder Begleitperson)."@de .

dtd:LA-id5597 a dtd:LA ;
    dtd:has_Value "Plutonium 239 und Plutonium 241,"@de .

dtd:LA-id5598 a dtd:LA ;
    dtd:has_Value "mit den Isotopen 235 oder 233 angereichertem Uran,"@de .

dtd:LA-id5599 a dtd:LA ;
    dtd:has_Value "jedem Stoff, der einen oder mehrere der in den Nummern 1 und 2 genannten Stoffe enthält,"@de .

dtd:LA-id5600 a dtd:LA ;
    dtd:has_Value "Stoffen, mit deren Hilfe in einer geeigneten Anlage eine sich selbst tragende Kettenreaktion aufrechterhalten werden kann und die in einer durch die Bundesregierung mit Zustimmung des Bundesrates erlassenen Rechtsverordnung bestimmt werden."@de .

dtd:LA-id5601 a dtd:LA ;
    dtd:has_Value "festgelegte Freigrenzen unterschreitet,"@de .

dtd:LA-id5602 a dtd:LA ;
    dtd:has_Value "soweit es sich um einen im Rahmen einer genehmigungspflichtigen Tätigkeit nach diesem Gesetz, dem Atomgesetz oder nach einer auf Grund eines dieser Gesetze erlassenen Rechtsverordnung anfallenden Stoff handelt, festgelegte Freigabewerte unterschreitet und der Stoff freigegeben worden ist,"@de .

dtd:LA-id5603 a dtd:LA ;
    dtd:has_Value "soweit es sich um einen Stoff natürlichen Ursprungs handelt, der nicht auf Grund seiner Radioaktivität, als Kernbrennstoff oder zur Erzeugung von Kernbrennstoff genutzt wird, nicht der Überwachung nach dem Atomgesetz, nach diesem Gesetz oder einer auf Grund dieses Gesetzes mit Zustimmung des Bundesrates erlassenen Rechtsverordnung unterliegt."@de .

dtd:LA-id5604 a dtd:LA ;
    dtd:has_Value "der Umgang nach § 5 Absatz 39,"@de .

dtd:LA-id5605 a dtd:LA ;
    dtd:has_Value "der Erwerb von künstlich erzeugten radioaktiven Stoffen und von natürlich vorkommenden radioaktiven Stoffen, die auf Grund ihrer Radioaktivität, als Kernbrennstoff oder zur Erzeugung von Kernbrennstoff genutzt werden, die Abgabe dieser Stoffe an andere, ihre Beförderung und ihre grenzüberschreitende Verbringung,"@de .

dtd:LA-id5606 a dtd:LA ;

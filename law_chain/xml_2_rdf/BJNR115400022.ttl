dtd:LA-id460 a dtd:LA ;
    dtd:has_Value "die nach § 2 Absatz 4 Satz 3 des Betreuungsorganisationsgesetzes örtlich zuständige Stammbehörde die für die Anerkennung der Berufsqualifikation zuständige öffentliche Stelle ist und"@de .

dtd:LA-id461 a dtd:LA ;
    dtd:has_Value "der Dienstleistungserbringer den Nachweis nach § 13a Absatz 5 Satz 1 Nummer 3 der Gewerbeordnung zu übermitteln hat."@de .

dtd:LA-id462 a dtd:LA ;
    dtd:has_Value "Vorhandensein, Anzahl und Beschäftigungsumfang von Mitarbeitern,"@de .

dtd:LA-id463 a dtd:LA ;

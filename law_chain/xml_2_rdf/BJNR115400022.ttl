dtd:LA-id446 a dtd:LA ;
    dtd:has_Value "der Anbieter die Gewähr für eine ordnungsgemäße Durchführung des Lehrbetriebs und des Prüfungsverfahrens bietet,"@de .

dtd:LA-id447 a dtd:LA ;
    dtd:has_Value "der Anbieter eine Prüfungsordnung zur Gewährleistung eines transparenten und nachprüfbaren Verfahrens für die Durchführung der Modulprüfungen nachweist,"@de .

dtd:LA-id448 a dtd:LA ;
    dtd:has_Value "der Anbieter eine Finanzierungsplanung für den Sachkundelehrgang vorlegt, die den Bestand des Lehrgangs für die Dauer der Anerkennung finanziell gesichert erscheinen lässt, und"@de .

dtd:LA-id449 a dtd:LA ;
    dtd:has_Value "der Anbieter die teilnehmerbezogenen Lehrgangskosten nachvollziehbar darlegt."@de .

dtd:LA-id450 a dtd:LA ;

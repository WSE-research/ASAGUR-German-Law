dtd:LA-id2292 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die durch Tatsachen begrndete Besorgnis besteht, dass infolge der personellen Manahme im Betrieb beschftigte Arbeitnehmer gekndigt werden oder sonstige Nachteile erleiden, ohne dass dies aus betrieblichen oder persnlichen Grnden gerechtfertigt ist; als Nachteil gilt bei unbefristeter Einstellung auch die Nichtbercksichtigung eines gleich geeigneten befristet Beschftigten,"@de .

dtd:LA-id2293 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der betroffene Arbeitnehmer durch die personelle Manahme benachteiligt wird, ohne dass dies aus betrieblichen oder in der Person des Arbeitnehmers liegenden Grnden gerechtfertigt ist,"@de .

dtd:LA-id2294 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine nach  93 erforderliche Ausschreibung im Betrieb unterblieben ist oder"@de .

dtd:LA-id2295 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die durch Tatsachen begrndete Besorgnis besteht, dass der fr die personelle Manahme in Aussicht genommene Bewerber oder Arbeitnehmer den Betriebsfrieden durch gesetzwidriges Verhalten oder durch grobe Verletzung der in  75 Abs. 1 enthaltenen Grundstze, insbesondere durch rassistische oder fremdenfeindliche Bettigung, stren werde."@de .

dtd:LA-id2296 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Arbeitgeber bei der Auswahl des zu kndigenden Arbeitnehmers soziale Gesichtspunkte nicht oder nicht ausreichend bercksichtigt hat,"@de .

dtd:LA-id2297 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kndigung gegen eine Richtlinie nach  95 verstt,"@de .

dtd:LA-id2298 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der zu kndigende Arbeitnehmer an einem anderen Arbeitsplatz im selben Betrieb oder in einem anderen Betrieb des Unternehmens weiterbeschftigt werden kann,"@de .

dtd:LA-id2299 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Weiterbeschftigung des Arbeitnehmers nach zumutbaren Umschulungs- oder Fortbildungsmanahmen mglich ist oder"@de .

dtd:LA-id2300 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Weiterbeschftigung des Arbeitnehmers unter genderten Vertragsbedingungen mglich ist und der Arbeitnehmer sein Einverstndnis hiermit erklrt hat."@de .

dtd:LA-id2301 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Klage des Arbeitnehmers keine hinreichende Aussicht auf Erfolg bietet oder mutwillig erscheint oder"@de .

dtd:LA-id2302 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Weiterbeschftigung des Arbeitnehmers zu einer unzumutbaren wirtschaftlichen Belastung des Arbeitgebers fhren wrde oder"@de .

dtd:LA-id2303 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Widerspruch des Betriebsrats offensichtlich unbegrndet war."@de .

dtd:LA-id2304 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die wirtschaftliche und finanzielle Lage des Unternehmens;"@de .

dtd:LA-id2305 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Produktions- und Absatzlage;"@de .

dtd:LA-id2306 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Produktions- und Investitionsprogramm;"@de .

dtd:LA-id2307 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rationalisierungsvorhaben;"@de .

dtd:LA-id2308 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fabrikations- und Arbeitsmethoden, insbesondere die Einfhrung neuer Arbeitsmethoden;"@de .

dtd:LA-id2309 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fragen des betrieblichen Umweltschutzes;"@de .

dtd:LA-id2310 a dtd:LA ;

dtd:LA-id2199 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Verfolgung arbeitstechnischer Zwecke die Betriebsmittel sowie die Arbeitnehmer von den Unternehmen gemeinsam eingesetzt werden oder"@de .

dtd:LA-id2200 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Spaltung eines Unternehmens zur Folge hat, dass von einem Betrieb ein oder mehrere Betriebsteile einem an der Spaltung beteiligten anderen Unternehmen zugeordnet werden, ohne dass sich dabei die Organisation des betroffenen Betriebs wesentlich ändert."@de .

dtd:LA-id2201 a dtd:LA ;
    dtd:has_DL dtd:DL-id1767 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Unternehmen mit mehreren Betriebena)die Bildung eines unternehmenseinheitlichen Betriebsrats oder b)die Zusammenfassung von Betrieben, "@de .

dtd:LA-id2202 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn dies die Bildung von Betriebsräten erleichtert oder einer sachgerechten Wahrnehmung der Interessen der Arbeitnehmer dient;"@de .

dtd:LA-id2203 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bildung eines unternehmenseinheitlichen Betriebsrats oder"@de .

dtd:LA-id2204 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zusammenfassung von Betrieben,"@de .

dtd:LA-id2205 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Unternehmen und Konzerne, soweit sie nach produkt- oder projektbezogenen Geschäftsbereichen (Sparten) organisiert sind und die Leitung der Sparte auch Entscheidungen in beteiligungspflichtigen Angelegenheiten trifft, die Bildung von Betriebsräten in den Sparten (Spartenbetriebsräte), wenn dies der sachgerechten Wahrnehmung der Aufgaben des Betriebsrats dient;"@de .

dtd:LA-id2206 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "andere Arbeitnehmervertretungsstrukturen, soweit dies insbesondere aufgrund der Betriebs-, Unternehmens- oder Konzernorganisation oder aufgrund anderer Formen der Zusammenarbeit von Unternehmen einer wirksamen und zweckmäßigen Interessenvertretung der Arbeitnehmer dient;"@de .

dtd:LA-id2207 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zusätzliche betriebsverfassungsrechtliche Gremien (Arbeitsgemeinschaften), die der unternehmensübergreifenden Zusammenarbeit von Arbeitnehmervertretungen dienen;"@de .

dtd:LA-id2208 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zusätzliche betriebsverfassungsrechtliche Vertretungen der Arbeitnehmer, die die Zusammenarbeit zwischen Betriebsrat und Arbeitnehmern erleichtern."@de .

dtd:LA-id2209 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "räumlich weit vom Hauptbetrieb entfernt oder"@de .

dtd:LA-id2210 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Aufgabenbereich und Organisation eigenständig sind."@de .

dtd:LA-id2211 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Betrieben einer juristischen Person die Mitglieder des Organs, das zur gesetzlichen Vertretung der juristischen Person berufen ist;"@de .

dtd:LA-id2212 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gesellschafter einer offenen Handelsgesellschaft oder die Mitglieder einer anderen Personengesamtheit, soweit sie durch Gesetz, Satzung oder Gesellschaftsvertrag zur Vertretung der Personengesamtheit oder zur Geschäftsführung berufen sind, in deren Betrieben;"@de .

dtd:LA-id2213 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, deren Beschäftigung nicht in erster Linie ihrem Erwerb dient, sondern vorwiegend durch Beweggründe karitativer oder religiöser Art bestimmt ist;"@de .

dtd:LA-id2214 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, deren Beschäftigung nicht in erster Linie ihrem Erwerb dient und die vorwiegend zu ihrer Heilung, Wiedereingewöhnung, sittlichen Besserung oder Erziehung beschäftigt werden;"@de .

dtd:LA-id2215 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Ehegatte, der Lebenspartner, Verwandte und Verschwägerte ersten Grades, die in häuslicher Gemeinschaft mit dem Arbeitgeber leben."@de .

dtd:LA-id2216 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur selbständigen Einstellung und Entlassung von im Betrieb oder in der Betriebsabteilung beschäftigten Arbeitnehmern berechtigt ist oder"@de .

dtd:LA-id2217 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Generalvollmacht oder Prokura hat und die Prokura auch im Verhältnis zum Arbeitgeber nicht unbedeutend ist oder"@de .

dtd:LA-id2218 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "regelmäßig sonstige Aufgaben wahrnimmt, die für den Bestand und die Entwicklung des Unternehmens oder eines Betriebs von Bedeutung sind und deren Erfüllung besondere Erfahrungen und Kenntnisse voraussetzt, wenn er dabei entweder die Entscheidungen im Wesentlichen frei von Weisungen trifft oder sie maßgeblich beeinflusst; dies kann auch bei Vorgaben insbesondere aufgrund von Rechtsvorschriften, Plänen oder Richtlinien sowie bei Zusammenarbeit mit anderen leitenden Angestellten gegeben sein."@de .

dtd:LA-id2219 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus Anlass der letzten Wahl des Betriebsrats, des Sprecherausschusses oder von Aufsichtsratsmitgliedern der Arbeitnehmer oder durch rechtskräftige gerichtliche Entscheidung den leitenden Angestellten zugeordnet worden ist oder"@de .

dtd:LA-id2220 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Leitungsebene angehört, auf der in dem Unternehmen überwiegend leitende Angestellte vertreten sind, oder"@de .

dtd:LA-id2221 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein regelmäßiges Jahresarbeitsentgelt erhält, das für leitende Angestellte in dem Unternehmen üblich ist, oder,"@de .

dtd:LA-id2222 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "falls auch bei der Anwendung der Nummer 3 noch Zweifel bleiben, ein regelmäßiges Jahresarbeitsentgelt erhält, das das Dreifache der Bezugsgröße nach § 18 des Vierten Buches Sozialgesetzbuch überschreitet."@de .

dtd:LA-id2223 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value " Gemäß Artikel 14 Satz 2 des Gesetzes zur Reform des Betriebsverfassungsgesetzes (BetrVerf-Reformgesetz) vom 23. Juli 2001 (BGBl. I S. 1852) gilt § 9 (Artikel 1 Nr. 8 des BetrVerf-Reformgesetzes) für im Zeitpunkt des Inkrafttretens bestehende Betriebsräte erst bei deren Neuwahl. "@de ;
    dtd:has_noindex dtd:noindex-id1468 .

dtd:LA-id2224 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Ablauf von 24 Monaten, vom Tage der Wahl an gerechnet, die Zahl der regelmäßig beschäftigten Arbeitnehmer um die Hälfte, mindestens aber um fünfzig, gestiegen oder gesunken ist,"@de .

dtd:LA-id2225 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gesamtzahl der Betriebsratsmitglieder nach Eintreten sämtlicher Ersatzmitglieder unter die vorgeschriebene Zahl der Betriebsratsmitglieder gesunken ist,"@de .

dtd:LA-id2226 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Betriebsrat mit der Mehrheit seiner Mitglieder seinen Rücktritt beschlossen hat,"@de .

dtd:LA-id2227 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Betriebsratswahl mit Erfolg angefochten worden ist,"@de .

dtd:LA-id2228 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Betriebsrat durch eine gerichtliche Entscheidung aufgelöst ist oder"@de .

dtd:LA-id2229 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Betrieb ein Betriebsrat nicht besteht."@de .

dtd:LA-id2230 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value " Gemäß Artikel 14 Satz 2 des Gesetzes zur Reform des Betriebsverfassungsgesetzes (BetrVerf-Reformgesetz) vom 23. Juli 2001 (BGBl. I S. 1852) gilt § 15 (Artikel 1 Nr. 13 des BetrVerf-Reformgesetzes) für im Zeitraum des Inkrafftretens bestehende Betriebsräte erst bei deren Neuwahl. "@de ;
    dtd:has_noindex dtd:noindex-id1469 .

dtd:LA-id2231 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Frist des § 16 Abs. 1 Satz 1 wird auf vier Wochen und die des § 16 Abs. 2 Satz 1, Abs. 3 Satz 1 auf drei Wochen verkürzt."@de .

dtd:LA-id2232 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 16 Abs. 1 Satz 2 und 3 findet keine Anwendung."@de .

dtd:LA-id2233 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "In den Fällen des § 17 Abs. 2 wird der Wahlvorstand in einer Wahlversammlung von der Mehrheit der anwesenden Arbeitnehmer gewählt. Für die Einladung zu der Wahlversammlung gilt § 17 Abs. 3 entsprechend."@de .

dtd:LA-id2234 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 17 Abs. 4 gilt entsprechend, wenn trotz Einladung keine Wahlversammlung stattfindet oder auf der Wahlversammlung kein Wahlvorstand gewählt wird."@de .

dtd:LA-id2235 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value " Diese Vorschrift dient der Umsetzung des Artikels 6 der Richtlinie 2001/23/EG des Rates vom 12. März 2001 zur Angleichung der Rechtsvorschriften der Mitgliedstaaten über die Wahrung von Ansprüchen der Arbeitnehmer beim Übergang von Unternehmen, Betrieben oder Betriebsteilen (ABl. EG Nr. L 82 S. 16). "@de ;
    dtd:has_noindex dtd:noindex-id1470 .

dtd:LA-id2236 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ablauf der Amtszeit,"@de .

dtd:LA-id2237 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Niederlegung des Betriebsratsamtes,"@de .

dtd:LA-id2238 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beendigung des Arbeitsverhältnisses,"@de .

dtd:LA-id2239 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verlust der Wählbarkeit,"@de .

dtd:LA-id2240 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ausschluss aus dem Betriebsrat oder Auflösung des Betriebsrats aufgrund einer gerichtlichen Entscheidung,"@de .

dtd:LA-id2241 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gerichtliche Entscheidung über die Feststellung der Nichtwählbarkeit nach Ablauf der in § 19 Abs. 2 bezeichneten Frist, es sei denn, der Mangel liegt nicht mehr vor."@de .

dtd:LA-id2242 a dtd:LA ;
    dtd:has_Value "die Voraussetzungen für eine solche Teilnahme in der Geschäftsordnung unter Sicherung des Vorrangs der Präsenzsitzung festgelegt sind,"@de .

dtd:LA-id2243 a dtd:LA ;
    dtd:has_Value "nicht mindestens ein Viertel der Mitglieder des Betriebsrats binnen einer von dem Vorsitzenden zu bestimmenden Frist diesem gegenüber widerspricht und"@de .

dtd:LA-id2244 a dtd:LA ;
    dtd:has_Value "sichergestellt ist, dass Dritte vom Inhalt der Sitzung keine Kenntnis nehmen können."@de .

dtd:LA-id2245 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value " Gemäß Artikel 14 Satz 2 des Gesetzes zur Reform des Betriebsverfassungsgesetzes (BetrVerf-Reformgesetz) vom 23. Juli 2001 (BGBl. I S. 1852) gilt § 47 Abs. 2 (Artikel 1 Nr. 35 Buchstabe a des BetrVerf-Reformgesetzes) für im Zeitpunkt des Inkrafttretens bestehende Betriebsräte erst bei deren Neuwahl. "@de ;
    dtd:has_noindex dtd:noindex-id1471 .

dtd:LA-id2246 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Gesamtbetriebsrat einen Tätigkeitsbericht,"@de .

dtd:LA-id2247 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Unternehmer einen Bericht über das Personal- und Sozialwesen einschließlich des Stands der Gleichstellung von Frauen und Männern im Unternehmen, der Integration der im Unternehmen beschäftigten ausländischen Arbeitnehmer, über die wirtschaftliche Lage und Entwicklung des Unternehmens sowie über Fragen des Umweltschutzes in Unternehmen, soweit dadurch nicht Betriebs- und Geschäftsgeheimnisse gefährdet werden,"@de .

dtd:LA-id2248 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen, die den in § 60 Abs. 1 genannten Arbeitnehmern dienen, insbesondere in Fragen der Berufsbildung und der Übernahme der zu ihrer Berufsausbildung Beschäftigten in ein Arbeitsverhältnis, beim Betriebsrat zu beantragen;"@de .

dtd:LA-id2249 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Durchsetzung der tatsächlichen Gleichstellung der in § 60 Abs. 1 genannten Arbeitnehmer entsprechend § 80 Abs. 1 Nr. 2a und 2b beim Betriebsrat zu beantragen;"@de .

dtd:LA-id2250 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "darüber zu wachen, dass die zugunsten der in § 60 Abs. 1 genannten Arbeitnehmer geltenden Gesetze, Verordnungen, Unfallverhütungsvorschriften, Tarifverträge und Betriebsvereinbarungen durchgeführt werden;"@de .

dtd:LA-id2251 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anregungen von in § 60 Abs. 1 genannten Arbeitnehmern, insbesondere in Fragen der Berufsbildung, entgegenzunehmen und, falls sie berechtigt erscheinen, beim Betriebsrat auf eine Erledigung hinzuwirken. Die Jugend- und Auszubildendenvertretung hat die betroffenen in § 60 Abs. 1 genannten Arbeitnehmer über den Stand und das Ergebnis der Verhandlungen zu informieren;"@de .

dtd:LA-id2252 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Integration ausländischer, in § 60 Abs. 1 genannter Arbeitnehmer im Betrieb zu fördern und entsprechende Maßnahmen beim Betriebsrat zu beantragen."@de .

dtd:LA-id2253 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "festzustellen, dass ein Arbeitsverhältnis nach Absatz 2 oder 3 nicht begründet wird, oder"@de .

dtd:LA-id2254 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das bereits nach Absatz 2 oder 3 begründete Arbeitsverhältnis aufzulösen,"@de .

dtd:LA-id2255 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "darüber zu wachen, dass die zugunsten der Arbeitnehmer geltenden Gesetze, Verordnungen, Unfallverhütungsvorschriften, Tarifverträge und Betriebsvereinbarungen durchgeführt werden;"@de .

dtd:LA-id2256 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen, die dem Betrieb und der Belegschaft dienen, beim Arbeitgeber zu beantragen;"@de .

dtd:LA-id2257 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Durchsetzung der tatsächlichen Gleichstellung von Frauen und Männern, insbesondere bei der Einstellung, Beschäftigung, Aus-, Fort- und Weiterbildung und dem beruflichen Aufstieg, zu fördern;"@de .

dtd:LA-id2258 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vereinbarkeit von Familie und Erwerbstätigkeit zu fördern;"@de .

dtd:LA-id2259 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anregungen von Arbeitnehmern und der Jugend- und Auszubildendenvertretung entgegenzunehmen und, falls sie berechtigt erscheinen, durch Verhandlungen mit dem Arbeitgeber auf eine Erledigung hinzuwirken; er hat die betreffenden Arbeitnehmer über den Stand und das Ergebnis der Verhandlungen zu unterrichten;"@de .

dtd:LA-id2260 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Eingliederung schwerbehinderter Menschen einschließlich der Förderung des Abschlusses von Inklusionsvereinbarungen nach § 166 des Neunten Buches Sozialgesetzbuch und sonstiger besonders schutzbedürftiger Personen zu fördern;"@de .

dtd:LA-id2261 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Wahl einer Jugend- und Auszubildendenvertretung vorzubereiten und durchzuführen und mit dieser zur Förderung der Belange der in § 60 Abs. 1 genannten Arbeitnehmer eng zusammenzuarbeiten; er kann von der Jugend- und Auszubildendenvertretung Vorschläge und Stellungnahmen anfordern;"@de .

dtd:LA-id2262 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beschäftigung älterer Arbeitnehmer im Betrieb zu fördern;"@de .

dtd:LA-id2263 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Integration ausländischer Arbeitnehmer im Betrieb und das Verständnis zwischen ihnen und den deutschen Arbeitnehmern zu fördern, sowie Maßnahmen zur Bekämpfung von Rassismus und Fremdenfeindlichkeit im Betrieb zu beantragen;"@de .

dtd:LA-id2264 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beschäftigung im Betrieb zu fördern und zu sichern;"@de .

dtd:LA-id2265 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen des Arbeitsschutzes und des betrieblichen Umweltschutzes zu fördern."@de .

dtd:LA-id2266 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fragen der Ordnung des Betriebs und des Verhaltens der Arbeitnehmer im Betrieb;"@de .

dtd:LA-id2267 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beginn und Ende der täglichen Arbeitszeit einschließlich der Pausen sowie Verteilung der Arbeitszeit auf die einzelnen Wochentage;"@de .

dtd:LA-id2268 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vorübergehende Verkürzung oder Verlängerung der betriebsüblichen Arbeitszeit;"@de .

dtd:LA-id2269 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zeit, Ort und Art der Auszahlung der Arbeitsentgelte;"@de .

dtd:LA-id2270 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufstellung allgemeiner Urlaubsgrundsätze und des Urlaubsplans sowie die Festsetzung der zeitlichen Lage des Urlaubs für einzelne Arbeitnehmer, wenn zwischen dem Arbeitgeber und den beteiligten Arbeitnehmern kein Einverständnis erzielt wird;"@de .

dtd:LA-id2271 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einführung und Anwendung von technischen Einrichtungen, die dazu bestimmt sind, das Verhalten oder die Leistung der Arbeitnehmer zu überwachen;"@de .

dtd:LA-id2272 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Regelungen über die Verhütung von Arbeitsunfällen und Berufskrankheiten sowie über den Gesundheitsschutz im Rahmen der gesetzlichen Vorschriften oder der Unfallverhütungsvorschriften;"@de .

dtd:LA-id2273 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Form, Ausgestaltung und Verwaltung von Sozialeinrichtungen, deren Wirkungsbereich auf den Betrieb, das Unternehmen oder den Konzern beschränkt ist;"@de .

dtd:LA-id2274 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zuweisung und Kündigung von Wohnräumen, die den Arbeitnehmern mit Rücksicht auf das Bestehen eines Arbeitsverhältnisses vermietet werden, sowie die allgemeine Festlegung der Nutzungsbedingungen;"@de .

dtd:LA-id2275 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fragen der betrieblichen Lohngestaltung, insbesondere die Aufstellung von Entlohnungsgrundsätzen und die Einführung und Anwendung von neuen Entlohnungsmethoden sowie deren Änderung;"@de .

dtd:LA-id2276 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Festsetzung der Akkord- und Prämiensätze und vergleichbarer leistungsbezogener Entgelte, einschließlich der Geldfaktoren;"@de .

dtd:LA-id2277 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundsätze über das betriebliche Vorschlagswesen;"@de .

dtd:LA-id2278 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundsätze über die Durchführung von Gruppenarbeit; Gruppenarbeit im Sinne dieser Vorschrift liegt vor, wenn im Rahmen des betrieblichen Arbeitsablaufs eine Gruppe von Arbeitnehmern eine ihr übertragene Gesamtaufgabe im Wesentlichen eigenverantwortlich erledigt;"@de .

dtd:LA-id2279 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ausgestaltung von mobiler Arbeit, die mittels Informations- und Kommunikationstechnik erbracht wird."@de .

dtd:LA-id2280 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zusätzliche Maßnahmen zur Verhütung von Arbeitsunfällen und Gesundheitsschädigungen;"@de .

dtd:LA-id2281 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen des betrieblichen Umweltschutzes;"@de .

dtd:LA-id2282 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Errichtung von Sozialeinrichtungen, deren Wirkungsbereich auf den Betrieb, das Unternehmen oder den Konzern beschränkt ist;"@de .

dtd:LA-id2283 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Förderung der Vermögensbildung;"@de .

dtd:LA-id2284 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Integration ausländischer Arbeitnehmer sowie zur Bekämpfung von Rassismus und Fremdenfeindlichkeit im Betrieb;"@de .

dtd:LA-id2285 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Eingliederung schwerbehinderter Menschen."@de .

dtd:LA-id2286 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Neu-, Um- und Erweiterungsbauten von Fabrikations-, Verwaltungs- und sonstigen betrieblichen Räumen,"@de .

dtd:LA-id2287 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von technischen Anlagen,"@de .

dtd:LA-id2288 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Arbeitsverfahren und Arbeitsabläufen einschließlich des Einsatzes von Künstlicher Intelligenz oder"@de .

dtd:LA-id2289 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Arbeitsplätze"@de .

dtd:LA-id2290 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die personelle Maßnahme gegen ein Gesetz, eine Verordnung, eine Unfallverhütungsvorschrift oder gegen eine Bestimmung in einem Tarifvertrag oder in einer Betriebsvereinbarung oder gegen eine gerichtliche Entscheidung oder eine behördliche Anordnung verstoßen würde,"@de .

dtd:LA-id2291 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die personelle Maßnahme gegen eine Richtlinie nach § 95 verstoßen würde,"@de .

dtd:LA-id2292 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die durch Tatsachen begründete Besorgnis besteht, dass infolge der personellen Maßnahme im Betrieb beschäftigte Arbeitnehmer gekündigt werden oder sonstige Nachteile erleiden, ohne dass dies aus betrieblichen oder persönlichen Gründen gerechtfertigt ist; als Nachteil gilt bei unbefristeter Einstellung auch die Nichtberücksichtigung eines gleich geeigneten befristet Beschäftigten,"@de .

dtd:LA-id2293 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der betroffene Arbeitnehmer durch die personelle Maßnahme benachteiligt wird, ohne dass dies aus betrieblichen oder in der Person des Arbeitnehmers liegenden Gründen gerechtfertigt ist,"@de .

dtd:LA-id2294 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine nach § 93 erforderliche Ausschreibung im Betrieb unterblieben ist oder"@de .

dtd:LA-id2295 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die durch Tatsachen begründete Besorgnis besteht, dass der für die personelle Maßnahme in Aussicht genommene Bewerber oder Arbeitnehmer den Betriebsfrieden durch gesetzwidriges Verhalten oder durch grobe Verletzung der in § 75 Abs. 1 enthaltenen Grundsätze, insbesondere durch rassistische oder fremdenfeindliche Betätigung, stören werde."@de .

dtd:LA-id2296 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Arbeitgeber bei der Auswahl des zu kündigenden Arbeitnehmers soziale Gesichtspunkte nicht oder nicht ausreichend berücksichtigt hat,"@de .

dtd:LA-id2297 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kündigung gegen eine Richtlinie nach § 95 verstößt,"@de .

dtd:LA-id2298 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der zu kündigende Arbeitnehmer an einem anderen Arbeitsplatz im selben Betrieb oder in einem anderen Betrieb des Unternehmens weiterbeschäftigt werden kann,"@de .

dtd:LA-id2299 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Weiterbeschäftigung des Arbeitnehmers nach zumutbaren Umschulungs- oder Fortbildungsmaßnahmen möglich ist oder"@de .

dtd:LA-id2300 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Weiterbeschäftigung des Arbeitnehmers unter geänderten Vertragsbedingungen möglich ist und der Arbeitnehmer sein Einverständnis hiermit erklärt hat."@de .

dtd:LA-id2301 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Klage des Arbeitnehmers keine hinreichende Aussicht auf Erfolg bietet oder mutwillig erscheint oder"@de .

dtd:LA-id2302 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Weiterbeschäftigung des Arbeitnehmers zu einer unzumutbaren wirtschaftlichen Belastung des Arbeitgebers führen würde oder"@de .

dtd:LA-id2303 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Widerspruch des Betriebsrats offensichtlich unbegründet war."@de .

dtd:LA-id2304 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die wirtschaftliche und finanzielle Lage des Unternehmens;"@de .

dtd:LA-id2305 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Produktions- und Absatzlage;"@de .

dtd:LA-id2306 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Produktions- und Investitionsprogramm;"@de .

dtd:LA-id2307 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rationalisierungsvorhaben;"@de .

dtd:LA-id2308 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fabrikations- und Arbeitsmethoden, insbesondere die Einführung neuer Arbeitsmethoden;"@de .

dtd:LA-id2309 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fragen des betrieblichen Umweltschutzes;"@de .

dtd:LA-id2310 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fragen der unternehmerischen Sorgfaltspflichten in Lieferketten gemäß dem Lieferkettensorgfaltspflichtengesetz;"@de .

dtd:LA-id2311 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einschränkung oder Stillegung von Betrieben oder von Betriebsteilen;"@de .

dtd:LA-id2312 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verlegung von Betrieben oder Betriebsteilen;"@de .

dtd:LA-id2313 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Zusammenschluss oder die Spaltung von Unternehmen oder Betrieben;"@de .

dtd:LA-id2314 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Änderung der Betriebsorganisation oder des Betriebszwecks;"@de .

dtd:LA-id2315 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Übernahme des Unternehmens, wenn hiermit der Erwerb der Kontrolle verbunden ist, sowie"@de .

dtd:LA-id2316 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstige Vorgänge und Vorhaben, welche die Interessen der Arbeitnehmer des Unternehmens wesentlich berühren können."@de .

dtd:LA-id2317 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einschränkung und Stilllegung des ganzen Betriebs oder von wesentlichen Betriebsteilen,"@de .

dtd:LA-id2318 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verlegung des ganzen Betriebs oder von wesentlichen Betriebsteilen,"@de .

dtd:LA-id2319 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zusammenschluss mit anderen Betrieben oder die Spaltung von Betrieben,"@de .

dtd:LA-id2320 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "grundlegende Änderungen der Betriebsorganisation, des Betriebszwecks oder der Betriebsanlagen,"@de .

dtd:LA-id2321 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einführung grundlegend neuer Arbeitsmethoden und Fertigungsverfahren."@de .

dtd:LA-id2322 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sie soll beim Ausgleich oder bei der Milderung wirtschaftlicher Nachteile, insbesondere durch Einkommensminderung, Wegfall von Sonderleistungen oder Verlust von Anwartschaften auf betriebliche Altersversorgung, Umzugskosten oder erhöhte Fahrtkosten, Leistungen vorsehen, die in der Regel den Gegebenheiten des Einzelfalles Rechnung tragen."@de .

dtd:LA-id2323 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sie hat die Aussichten der betroffenen Arbeitnehmer auf dem Arbeitsmarkt zu berücksichtigen. Sie soll Arbeitnehmer von Leistungen ausschließen, die in einem zumutbaren Arbeitsverhältnis im selben Betrieb oder in einem anderen Betrieb des Unternehmens oder eines zum Konzern gehörenden Unternehmens weiterbeschäftigt werden können und die Weiterbeschäftigung ablehnen; die mögliche Weiterbeschäftigung an einem anderen Ort begründet für sich allein nicht die Unzumutbarkeit."@de .

dtd:LA-id2324 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sie soll insbesondere die im Dritten Buch des Sozialgesetzbuches vorgesehenen Förderungsmöglichkeiten zur Vermeidung von Arbeitslosigkeit berücksichtigen."@de .

dtd:LA-id2325 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sie hat bei der Bemessung des Gesamtbetrages der Sozialplanleistungen darauf zu achten, dass der Fortbestand des Unternehmens oder die nach Durchführung der Betriebsänderung verbleibenden Arbeitsplätze nicht gefährdet werden."@de .

dtd:LA-id2326 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Betrieben mit in der Regel weniger als 60 Arbeitnehmern 20 vom Hundert der regelmäßig beschäftigten Arbeitnehmer, aber mindestens 6 Arbeitnehmer,"@de .

dtd:LA-id2327 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Betrieben mit in der Regel mindestens 60 und weniger als 250 Arbeitnehmern 20 vom Hundert der regelmäßig beschäftigten Arbeitnehmer oder mindestens 37 Arbeitnehmer,"@de .

dtd:LA-id2328 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Betrieben mit in der Regel mindestens 250 und weniger als 500 Arbeitnehmern 15 vom Hundert der regelmäßig beschäftigten Arbeitnehmer oder mindestens 60 Arbeitnehmer,"@de .

dtd:LA-id2329 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Betrieben mit in der Regel mindestens 500 Arbeitnehmern 10 vom Hundert der regelmäßig beschäftigten Arbeitnehmer, aber mindestens 60 Arbeitnehmer"@de .

dtd:LA-id2330 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wahlberechtigt sind alle Besatzungsmitglieder des Schiffes."@de .

dtd:LA-id2331 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wählbar sind die Besatzungsmitglieder des Schiffes, die am Wahltag das 18. Lebensjahr vollendet haben und ein Jahr Besatzungsmitglied eines Schiffes waren, das nach dem Flaggenrechtsgesetz die Bundesflagge führt. § 8 Abs. 1 Satz 3 bleibt unberührt."@de .

dtd:LA-id2332 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Bordvertretung besteht auf Schiffen mit in der Regel"@de .

dtd:LA-id2333 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id2334 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 13 Abs. 1 und 3 findet keine Anwendung. Die Bordvertretung ist vor Ablauf ihrer Amtszeit unter den in § 13 Abs. 2 Nr. 2 bis 5 genannten Voraussetzungen neu zu wählen."@de .

dtd:LA-id2335 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die wahlberechtigten Besatzungsmitglieder können mit der Mehrheit aller Stimmen beschließen, die Wahl der Bordvertretung binnen 24 Stunden durchzuführen."@de .

dtd:LA-id2336 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die in § 16 Abs. 1 Satz 1 genannte Frist wird auf zwei Wochen, die in § 16 Abs. 2 Satz 1 genannte Frist wird auf eine Woche verkürzt."@de .

dtd:LA-id2337 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bestellt die im Amt befindliche Bordvertretung nicht rechtzeitig einen Wahlvorstand oder besteht keine Bordvertretung, wird der Wahlvorstand in einer Bordversammlung von der Mehrheit der anwesenden Besatzungsmitglieder gewählt; § 17 Abs. 3 gilt entsprechend. Kann aus Gründen der Aufrechterhaltung des ordnungsgemäßen Schiffsbetriebs eine Bordversammlung nicht stattfinden, so kann der Kapitän auf Antrag von drei Wahlberechtigten den Wahlvorstand bestellen. Bestellt der Kapitän den Wahlvorstand nicht, so ist der Seebetriebsrat berechtigt, den Wahlvorstand zu bestellen. Die Vorschriften über die Bestellung des Wahlvorstands durch das Arbeitsgericht bleiben unberührt."@de .

dtd:LA-id2338 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Frist für die Wahlanfechtung beginnt für Besatzungsmitglieder an Bord, wenn das Schiff nach Bekanntgabe des Wahlergebnisses erstmalig einen Hafen im Geltungsbereich dieses Gesetzes oder einen Hafen, in dem ein Seemannsamt seinen Sitz hat, anläuft. Die Wahlanfechtung kann auch zu Protokoll des Seemannsamtes erklärt werden. Wird die Wahl zur Bordvertretung angefochten, zieht das Seemannsamt die an Bord befindlichen Wahlunterlagen ein. Die Anfechtungserklärung und die eingezogenen Wahlunterlagen sind vom Seemannsamt unverzüglich an das für die Anfechtung zuständige Arbeitsgericht weiterzuleiten."@de .

dtd:LA-id2339 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Amtszeit ein Jahr beträgt,"@de .

dtd:LA-id2340 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mitgliedschaft in der Bordvertretung auch endet, wenn das Besatzungsmitglied den Dienst an Bord beendet, es sei denn, dass es den Dienst an Bord vor Ablauf der Amtszeit nach Nummer 1 wieder antritt."@de .

dtd:LA-id2341 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Bordvertretung ist zuständig für die Behandlung derjenigen nach diesem Gesetz der Mitwirkung und Mitbestimmung des Betriebsrats unterliegenden Angelegenheiten, die den Bordbetrieb oder die Besatzungsmitglieder des Schiffes betreffen und deren Regelung dem Kapitän auf Grund gesetzlicher Vorschriften oder der ihm von der Reederei übertragenen Befugnisse obliegt."@de .

dtd:LA-id2342 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kommt es zwischen Kapitän und Bordvertretung in einer der Mitwirkung oder Mitbestimmung der Bordvertretung unterliegenden Angelegenheit nicht zu einer Einigung, so kann die Angelegenheit von der Bordvertretung an den Seebetriebsrat abgegeben werden. Der Seebetriebsrat hat die Bordvertretung über die weitere Behandlung der Angelegenheit zu unterrichten. Bordvertretung und Kapitän dürfen die Einigungsstelle oder das Arbeitsgericht nur anrufen, wenn ein Seebetriebsrat nicht gewählt ist."@de .

dtd:LA-id2343 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bordvertretung und Kapitän können im Rahmen ihrer Zuständigkeiten Bordvereinbarungen abschließen. Die Vorschriften über Betriebsvereinbarungen gelten für Bordvereinbarungen entsprechend. Bordvereinbarungen sind unzulässig, soweit eine Angelegenheit durch eine Betriebsvereinbarung zwischen Seebetriebsrat und Arbeitgeber geregelt ist."@de .

dtd:LA-id2344 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "In Angelegenheiten, die der Mitbestimmung der Bordvertretung unterliegen, kann der Kapitän, auch wenn eine Einigung mit der Bordvertretung noch nicht erzielt ist, vorläufige Regelungen treffen, wenn dies zur Aufrechterhaltung des ordnungsgemäßen Schiffsbetriebs dringend erforderlich ist. Den von der Anordnung betroffenen Besatzungsmitgliedern ist die Vorläufigkeit der Regelung bekannt zu geben. Soweit die vorläufige Regelung der endgültigen Regelung nicht entspricht, hat das Schifffahrtsunternehmen Nachteile auszugleichen, die den Besatzungsmitgliedern durch die vorläufige Regelung entstanden sind."@de .

dtd:LA-id2345 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Bordvertretung hat das Recht auf regelmäßige und umfassende Unterrichtung über den Schiffsbetrieb. Die erforderlichen Unterlagen sind der Bordvertretung vorzulegen. Zum Schiffsbetrieb gehören insbesondere die Schiffssicherheit, die Reiserouten, die voraussichtlichen Ankunfts- und Abfahrtszeiten sowie die zu befördernde Ladung."@de .

dtd:LA-id2346 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auf Verlangen der Bordvertretung hat der Kapitän ihr Einsicht in die an Bord befindlichen Schiffstagebücher zu gewähren. In den Fällen, in denen der Kapitän eine Eintragung über Angelegenheiten macht, die der Mitwirkung oder Mitbestimmung der Bordvertretung unterliegen, kann diese eine Abschrift der Eintragung verlangen und Erklärungen zum Schiffstagebuch abgeben. In den Fällen, in denen über eine der Mitwirkung oder Mitbestimmung der Bordvertretung unterliegenden Angelegenheit eine Einigung zwischen Kapitän und Bordvertretung nicht erzielt wird, kann die Bordvertretung dies zum Schiffstagebuch erklären und eine Abschrift dieser Eintragung verlangen."@de .

dtd:LA-id2347 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Zuständigkeit der Bordvertretung im Rahmen des Arbeitsschutzes bezieht sich auch auf die Schiffssicherheit und die Zusammenarbeit mit den insoweit zuständigen Behörden und sonstigen in Betracht kommenden Stellen."@de .

dtd:LA-id2348 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wahlberechtigt zum Seebetriebsrat sind alle zum Seeschifffahrtsunternehmen gehörenden Besatzungsmitglieder."@de .

dtd:LA-id2349 a dtd:LA ;
    dtd:has_DL dtd:DL-id2099 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Für die Wählbarkeit zum Seebetriebsrat gilt § 8 mit der Maßgabe, dassa)in Seeschifffahrtsunternehmen, zu denen mehr als acht Schiffe gehören oder in denen in der Regel mehr als 250 Besatzungsmitglieder beschäftigt sind, nur nach § 115 Abs. 2 Nr. 2 wählbare Besatzungsmitglieder wählbar sind; b)in den Fällen, in denen die Voraussetzungen des Buchstabens a nicht vorliegen, nur Arbeitnehmer wählbar sind, die nach § 8 die Wählbarkeit im Landbetrieb des Seeschifffahrtsunternehmens besitzen, es sei denn, dass der Arbeitgeber mit der Wahl von Besatzungsmitgliedern einverstanden ist. "@de .

dtd:LA-id2350 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Seeschifffahrtsunternehmen, zu denen mehr als acht Schiffe gehören oder in denen in der Regel mehr als 250 Besatzungsmitglieder beschäftigt sind, nur nach § 115 Abs. 2 Nr. 2 wählbare Besatzungsmitglieder wählbar sind;"@de .

dtd:LA-id2351 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Fällen, in denen die Voraussetzungen des Buchstabens a nicht vorliegen, nur Arbeitnehmer wählbar sind, die nach § 8 die Wählbarkeit im Landbetrieb des Seeschifffahrtsunternehmens besitzen, es sei denn, dass der Arbeitgeber mit der Wahl von Besatzungsmitgliedern einverstanden ist."@de .

dtd:LA-id2352 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der Seebetriebsrat besteht in Seebetrieben mit in der Regel"@de .

dtd:LA-id2353 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "5 bis 400 wahlberechtigten Besatzungsmitgliedern aus einer Person,"@de .

dtd:LA-id2354 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "401 bis 800 wahlberechtigten Besatzungsmitgliedern aus drei Mitgliedern,"@de .

dtd:LA-id2355 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über 800 wahlberechtigten Besatzungsmitgliedern aus fünf Mitgliedern."@de .

dtd:LA-id2356 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ein Wahlvorschlag ist gültig, wenn er im Fall des § 14 Abs. 4 Satz 1 erster Halbsatz und Satz 2 mindestens von drei wahlberechtigten Besatzungsmitgliedern unterschrieben ist."@de .

dtd:LA-id2357 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 14a findet keine Anwendung."@de .

dtd:LA-id2358 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die in § 16 Abs. 1 Satz 1 genannte Frist wird auf drei Monate, die in § 16 Abs. 2 Satz 1 genannte Frist auf zwei Monate verlängert."@de .

dtd:LA-id2359 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zu Mitgliedern des Wahlvorstands können auch im Landbetrieb des Seeschifffahrtsunternehmens beschäftigte Arbeitnehmer bestellt werden. § 17 Abs. 2 bis 4 findet keine Anwendung. Besteht kein Seebetriebsrat, so bestellt der Gesamtbetriebsrat oder, falls ein solcher nicht besteht, der Konzernbetriebsrat den Wahlvorstand. Besteht weder ein Gesamtbetriebsrat noch ein Konzernbetriebsrat wird der Wahlvorstand gemeinsam vom Arbeitgeber und den im Seebetrieb vertretenen Gewerkschaften bestellt; Gleiches gilt, wenn der Gesamtbetriebsrat oder der Konzernbetriebsrat die Bestellung des Wahlvorstands nach Satz 3 unterlässt. Einigen sich Arbeitgeber und Gewerkschaften nicht, so bestellt ihn das Arbeitsgericht auf Antrag des Arbeitgebers, einer im Seebetrieb vertretenen Gewerkschaft oder von mindestens drei wahlberechtigten Besatzungsmitgliedern. § 16 Abs. 2 Satz 2 und 3 gilt entsprechend."@de .

dtd:LA-id2360 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Frist für die Wahlanfechtung nach § 19 Abs. 2 beginnt für Besatzungsmitglieder an Bord, wenn das Schiff nach Bekanntgabe des Wahlergebnisses erstmalig einen Hafen im Geltungsbereich dieses Gesetzes oder einen Hafen, in dem ein Seemannsamt seinen Sitz hat, anläuft. Nach Ablauf von drei Monaten seit Bekanntgabe des Wahlergebnisses ist eine Wahlanfechtung unzulässig. Die Wahlanfechtung kann auch zu Protokoll des Seemannsamtes erklärt werden. Die Anfechtungserklärung ist vom Seemannsamt unverzüglich an das für die Anfechtung zuständige Arbeitsgericht weiterzuleiten."@de .

dtd:LA-id2361 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Mitgliedschaft im Seebetriebsrat endet, wenn der Seebetriebsrat aus Besatzungsmitgliedern besteht, auch, wenn das Mitglied des Seebetriebsrats nicht mehr Besatzungsmitglied ist. Die Eigenschaft als Besatzungsmitglied wird durch die Tätigkeit im Seebetriebsrat oder durch eine Beschäftigung gemäß Absatz 3 Nr. 2 nicht berührt."@de .

dtd:LA-id2362 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "In Angelegenheiten, in denen der Seebetriebsrat nach diesem Gesetz innerhalb einer bestimmten Frist Stellung zu nehmen hat, kann er, abweichend von § 33 Abs. 2, ohne Rücksicht auf die Zahl der zur Sitzung erschienenen Mitglieder einen Beschluss fassen, wenn die Mitglieder ordnungsgemäß geladen worden sind."@de .

dtd:LA-id2363 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Soweit die Mitglieder des Seebetriebsrats nicht freizustellen sind, sind sie so zu beschäftigen, dass sie durch ihre Tätigkeit nicht gehindert sind, die Aufgaben des Seebetriebsrats wahrzunehmen. Der Arbeitsplatz soll den Fähigkeiten und Kenntnissen des Mitglieds des Seebetriebsrats und seiner bisherigen beruflichen Stellung entsprechen. Der Arbeitsplatz ist im Einvernehmen mit dem Seebetriebsrat zu bestimmen. Kommt eine Einigung über die Bestimmung des Arbeitsplatzes nicht zustande, so entscheidet die Einigungsstelle. Der Spruch der Einigungsstelle ersetzt die Einigung zwischen Arbeitgeber und Seebetriebsrat."@de .

dtd:LA-id2364 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Den Mitgliedern des Seebetriebsrats, die Besatzungsmitglieder sind, ist die Heuer auch dann fortzuzahlen, wenn sie im Landbetrieb beschäftigt werden. Sachbezüge sind angemessen abzugelten. Ist der neue Arbeitsplatz höherwertig, so ist das diesem Arbeitsplatz entsprechende Arbeitsentgelt zu zahlen."@de .

dtd:LA-id2365 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unter Berücksichtigung der örtlichen Verhältnisse ist über die Unterkunft der in den Seebetriebsrat gewählten Besatzungsmitglieder eine Regelung zwischen dem Seebetriebsrat und dem Arbeitgeber zu treffen, wenn der Arbeitsplatz sich nicht am Wohnort befindet. Kommt eine Einigung nicht zustande, so entscheidet die Einigungsstelle. Der Spruch der Einigungsstelle ersetzt die Einigung zwischen Arbeitgeber und Seebetriebsrat."@de .

dtd:LA-id2366 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der Seebetriebsrat hat das Recht, jedes zum Seebetrieb gehörende Schiff zu betreten, dort im Rahmen seiner Aufgaben tätig zu werden sowie an den Sitzungen der Bordvertretung teilzunehmen. § 115 Abs. 7 Nr. 5 Satz 1 gilt entsprechend."@de .

dtd:LA-id2367 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Liegt ein Schiff in einem Hafen innerhalb des Geltungsbereichs dieses Gesetzes, so kann der Seebetriebsrat nach Unterrichtung des Kapitäns Sprechstunden an Bord abhalten und Bordversammlungen der Besatzungsmitglieder durchführen."@de .

dtd:LA-id2368 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Läuft ein Schiff innerhalb eines Kalenderjahres keinen Hafen im Geltungsbereich dieses Gesetzes an, so gelten die Nummern 5 und 6 für europäische Häfen. Die Schleusen des Nordostseekanals gelten nicht als Häfen."@de .

dtd:LA-id2369 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Im Einvernehmen mit dem Arbeitgeber können Sprechstunden und Bordversammlungen, abweichend von den Nummern 6 und 7, auch in anderen Liegehäfen des Schiffes durchgeführt werden, wenn ein dringendes Bedürfnis hierfür besteht. Kommt eine Einigung nicht zustande, so entscheidet die Einigungsstelle. Der Spruch der Einigungsstelle ersetzt die Einigung zwischen Arbeitgeber und Seebetriebsrat."@de .

dtd:LA-id2370 a dtd:LA ;
    dtd:has_DL dtd:DL-id2126 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der Seebetriebsrat ist zuständig für die Behandlung derjenigen nach diesem Gesetz der Mitwirkung oder Mitbestimmung des Betriebsrats unterliegenden Angelegenheiten,a)die alle oder mehrere Schiffe des Seebetriebs oder die Besatzungsmitglieder aller oder mehrerer Schiffe des Seebetriebs betreffen, b)die nach § 115 Abs. 7 Nr. 2 von der Bordvertretung abgegeben worden sind oder c)für die nicht die Zuständigkeit der Bordvertretung nach § 115 Abs. 7 Nr. 1 gegeben ist. "@de .

dtd:LA-id2371 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die alle oder mehrere Schiffe des Seebetriebs oder die Besatzungsmitglieder aller oder mehrerer Schiffe des Seebetriebs betreffen,"@de .

dtd:LA-id2372 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nach § 115 Abs. 7 Nr. 2 von der Bordvertretung abgegeben worden sind oder"@de .

dtd:LA-id2373 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die nicht die Zuständigkeit der Bordvertretung nach § 115 Abs. 7 Nr. 1 gegeben ist."@de .

dtd:LA-id2374 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der Seebetriebsrat ist regelmäßig und umfassend über den Schiffsbetrieb des Seeschifffahrtsunternehmens zu unterrichten. Die erforderlichen Unterlagen sind ihm vorzulegen."@de .

dtd:LA-id2375 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "politischen, koalitionspolitischen, konfessionellen, karitativen, erzieherischen, wissenschaftlichen oder künstlerischen Bestimmungen oder"@de .

dtd:LA-id2376 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zwecken der Berichterstattung oder Meinungsäußerung, auf die Artikel 5 Abs. 1 Satz 2 des Grundgesetzes Anwendung findet,"@de .

dtd:LA-id2377 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Wahl des Betriebsrats, der Jugend- und Auszubildendenvertretung, der Bordvertretung, des Seebetriebsrats oder der in § 3 Abs. 1 Nr. 1 bis 3 oder 5 bezeichneten Vertretungen der Arbeitnehmer behindert oder durch Zufügung oder Androhung von Nachteilen oder durch Gewährung oder Versprechen von Vorteilen beeinflusst,"@de .

dtd:LA-id2378 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Tätigkeit des Betriebsrats, des Gesamtbetriebsrats, des Konzernbetriebsrats, der Jugend- und Auszubildendenvertretung, der Gesamt-Jugend- und Auszubildendenvertretung, der Konzern-Jugend- und Auszubildendenvertretung, der Bordvertretung, des Seebetriebsrats, der in § 3 Abs. 1 bezeichneten Vertretungen der Arbeitnehmer, der Einigungsstelle, der in § 76 Abs. 8 bezeichneten tariflichen Schlichtungsstelle, der in § 86 bezeichneten betrieblichen Beschwerdestelle oder des Wirtschaftsausschusses behindert oder stört, oder"@de .

dtd:LA-id2379 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Mitglied oder ein Ersatzmitglied des Betriebsrats, des Gesamtbetriebsrats, des Konzernbetriebsrats, der Jugend- und Auszubildendenvertretung, der Gesamt-Jugend- und Auszubildendenvertretung, der Konzern-Jugend- und Auszubildendenvertretung, der Bordvertretung, des Seebetriebsrats, der in § 3 Abs. 1 bezeichneten Vertretungen der Arbeitnehmer, der Einigungsstelle, der in § 76 Abs. 8 bezeichneten Schlichtungsstelle, der in § 86 bezeichneten betrieblichen Beschwerdestelle oder des Wirtschaftsausschusses um seiner Tätigkeit willen oder eine Auskunftsperson nach § 80 Absatz 2 Satz 4 um ihrer Tätigkeit willen benachteiligt oder begünstigt."@de .

dtd:LA-id2380 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mitglied oder Ersatzmitglied des Betriebsrats oder einer der in § 79 Abs. 2 bezeichneten Stellen,"@de .

dtd:LA-id2381 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vertreter einer Gewerkschaft oder Arbeitgebervereinigung,"@de .

dtd:LA-id2382 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sachverständiger, der vom Betriebsrat nach § 80 Abs. 3 hinzugezogen oder von der Einigungsstelle nach § 109 Satz 3 angehört worden ist,"@de .

dtd:LA-id2383 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berater, der vom Betriebsrat nach § 111 Satz 2 hinzugezogen worden ist,"@de .

dtd:LA-id2384 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auskunftsperson, die dem Betriebsrat nach § 80 Absatz 2 Satz 4 zur Verfügung gestellt worden ist, oder"@de .

dtd:LA-id2385 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arbeitnehmer, der vom Betriebsrat nach § 107 Abs. 3 Satz 3 oder vom Wirtschaftsausschuss nach § 108 Abs. 2 Satz 2 hinzugezogen worden ist,"@de .

dtd:LA-id2386 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Frist für die Einladung zur Wahlversammlung zur Wahl des Wahlvorstands nach § 14a Abs. 1 des Gesetzes beträgt mindestens sieben Tage. Die Einladung muss Ort, Tag und Zeit der Wahlversammlung sowie den Hinweis enthalten, dass bis zum Ende dieser Wahlversammlung Wahlvorschläge zur Wahl des Betriebsrats gemacht werden können (§ 14a Abs. 2 des Gesetzes)."@de .

dtd:LA-id2387 a dtd:LA ;
    dtd:has_DL dtd:DL-id2177 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 3 findet wie folgt Anwendung:a)Im Fall des § 14a Abs. 1 des Gesetzes erlässt der Wahlvorstand auf der Wahlversammlung das Wahlausschreiben. Die Einspruchsfrist nach § 3 Abs. 2 Nr. 3 verkürzt sich auf drei Tage. Die Angabe nach § 3 Abs. 2 Nr. 4 muss die Zahl der Mindestsitze des Geschlechts in der Minderheit (§ 15 Abs. 2 des Gesetzes) enthalten. Die Wahlvorschläge sind abweichend von § 3 Abs. 2 Nr. 7 bis zum Abschluss der Wahlversammlung zur Wahl des Wahlvorstands bei diesem einzureichen. Ergänzend zu § 3 Abs. 2 Nr. 10 gibt der Wahlvorstand den Ort, Tag und Zeit der nachträglichen Stimmabgabe an (§ 14a Abs. 4 des Gesetzes). b)Im Fall des § 14a Abs. 3 des Gesetzes erlässt der Wahlvorstand unverzüglich das Wahlausschreiben mit den unter Buchstabe a genannten Maßgaben zu § 3 Abs. 2 Nr. 3, 4 und 10. Abweichend von § 3 Abs. 2 Nr. 7 sind die Wahlvorschläge spätestens eine Woche vor der Wahlversammlung zur Wahl des Betriebsrats (§ 14a Abs. 3 Satz 2 des Gesetzes) beim Wahlvorstand einzureichen. "@de .

dtd:LA-id2388 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Im Fall des § 14a Abs. 1 des Gesetzes erlässt der Wahlvorstand auf der Wahlversammlung das Wahlausschreiben. Die Einspruchsfrist nach § 3 Abs. 2 Nr. 3 verkürzt sich auf drei Tage. Die Angabe nach § 3 Abs. 2 Nr. 4 muss die Zahl der Mindestsitze des Geschlechts in der Minderheit (§ 15 Abs. 2 des Gesetzes) enthalten. Die Wahlvorschläge sind abweichend von § 3 Abs. 2 Nr. 7 bis zum Abschluss der Wahlversammlung zur Wahl des Wahlvorstands bei diesem einzureichen. Ergänzend zu § 3 Abs. 2 Nr. 10 gibt der Wahlvorstand den Ort, Tag und Zeit der nachträglichen Stimmabgabe an (§ 14a Abs. 4 des Gesetzes)."@de .

dtd:LA-id2389 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Im Fall des § 14a Abs. 3 des Gesetzes erlässt der Wahlvorstand unverzüglich das Wahlausschreiben mit den unter Buchstabe a genannten Maßgaben zu § 3 Abs. 2 Nr. 3, 4 und 10. Abweichend von § 3 Abs. 2 Nr. 7 sind die Wahlvorschläge spätestens eine Woche vor der Wahlversammlung zur Wahl des Betriebsrats (§ 14a Abs. 3 Satz 2 des Gesetzes) beim Wahlvorstand einzureichen."@de .

dtd:LA-id2390 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Einspruchsfrist des § 4 Abs. 1 verkürzt sich auf drei Tage."@de .

dtd:LA-id2391 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die §§ 6 bis 8 und § 10 Abs. 2 finden entsprechende Anwendung mit der Maßgabe, dass die Wahl aufgrund von Wahlvorschlägen erfolgt. Im Fall des § 14a Abs. 1 des Gesetzes sind die Wahlvorschläge bis zum Abschluss der Wahlversammlung zur Wahl des Wahlvorstands bei diesem einzureichen; im Fall des § 14a Abs. 3 des Gesetzes sind die Wahlvorschläge spätestens eine Woche vor der Wahlversammlung zur Wahl des Betriebsrats (§ 14a Abs. 3 Satz 2 des Gesetzes) beim Wahlvorstand einzureichen."@de .

dtd:LA-id2392 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 9 findet keine Anwendung."@de .

dtd:LA-id2393 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auf das Wahlverfahren finden die §§ 21ff. entsprechende Anwendung. Auf den Stimmzetteln sind die Bewerber in alphabetischer Reihenfolge unter Angabe von Familienname, Vorname und Art der Beschäftigung im Betrieb aufzuführen."@de .

dtd:LA-id2394 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 25 Abs. 5 bis 8 findet keine Anwendung."@de .

dtd:LA-id2395 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 26 Abs. 1 findet mit der Maßgabe Anwendung, dass der Wahlberechtigte sein Verlangen auf schriftliche Stimmabgabe spätestens drei Tage vor dem Tag der Wahlversammlung zur Wahl des Betriebsrats dem Wahlvorstand mitgeteilt haben muss."@de .

dtd:LA-id2396 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 31 findet entsprechende Anwendung mit der Maßgabe, dass die Wahl der Jugend- und Auszubildendenvertretung aufgrund von Wahlvorschlägen erfolgt."@de .

dtd:LA-id2397 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vorbereitung der Wahl, insbesondere die Aufstellung der Wählerlisten und die Errechnung der Vertreterzahl;"@de .

dtd:LA-id2398 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Frist für die Einsichtnahme in die Wählerlisten und die Erhebung von Einsprüchen gegen sie;"@de .

dtd:LA-id2399 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vorschlagslisten und die Frist für ihre Einreichung;"@de .

dtd:LA-id2400 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Wahlausschreiben und die Fristen für seine Bekanntmachung;"@de .

dtd:LA-id2401 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Stimmabgabe;"@de .

dtd:LA-id2402 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verteilung der Sitze im Betriebsrat, in der Bordvertretung, im Seebetriebsrat sowie in der Jugend- und Auszubildendenvertretung auf die Geschlechter, auch soweit die Sitze nicht gemäß § 15 Abs. 2 und § 62 Abs. 3 besetzt werden können;"@de .

dtd:LA-id2403 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Feststellung des Wahlergebnisses und die Fristen für seine Bekanntmachung;"@de .

dtd:LA-id2404 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Aufbewahrung der Wahlakten."@de .

dtd:P-id2406 a dtd:P ;
    dtd:has_Value "§ 21a idF d. Art. 1 Nr. 51 G v. 23.7.2001 I 1852 dient der Umsetzung des Artikels 6 der Richtlinie 2001/23/EG des Rates vom 12. März 2001 zur Angleichung der Rechtsvorschriften der Mitgliedstaaten über die Wahrung von Ansprüchen der Arbeitnehmer beim Übergang von Unternehmen, Betrieben oder Betriebsteilen (ABl. EG Nr. L 82 S. 16). § 75 idF d. Art. 1 Nr. 51 G v. 23.7.2001 I 1852 dient teilweise der Umsetzung der Richtlinie 2000/78/EG des Rates vom 27. November 2000 zur Festlegung eines allgemeinen Rahmens für die Verwirklichung der Gleichbehandlung in Beschäftigung und Beruf (ABl. EG Nr. L 303 S. 16)."@de ;
    dtd:has_noindex dtd:noindex-id1467 .


    dtd:has_Value "Fragen der unternehmerischen Sorgfaltspflichten in Lieferketten gem dem Lieferkettensorgfaltspflichtengesetz;"@de .

dtd:LA-id2311 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einschrnkung oder Stillegung von Betrieben oder von Betriebsteilen;"@de .

dtd:LA-id2312 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verlegung von Betrieben oder Betriebsteilen;"@de .

dtd:LA-id2313 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Zusammenschluss oder die Spaltung von Unternehmen oder Betrieben;"@de .

dtd:LA-id2314 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nderung der Betriebsorganisation oder des Betriebszwecks;"@de .


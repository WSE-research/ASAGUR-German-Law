dtd:LA-id307 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Rahmen der Amtshilfe des Bundes bei einer Naturkatastrophe oder einem besonders schweren Unglücksfall, einschließlich eines terroristischen Anschlags,"@de .

dtd:LA-id308 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einer wirtschaftlichen Krisenlage, durch die die Versorgung mit Gütern des lebenswichtigen Bedarfs gestört ist,"@de .

dtd:LA-id309 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Unterstützung der Streitkräfte bei Einsätzen auf Grund internationaler Vereinbarungen oder im Rahmen der Zusammenarbeit mit den Vereinten Nationen oder"@de .

dtd:LA-id310 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Rahmen der Notfallbewältigung auf Grund internationaler Vereinbarungen"@de .

dtd:LA-id311 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall des § 1 Absatz 1 Nummer 1 das Bundesministerium für Verkehr und digitale Infrastruktur entschieden hat,"@de .

dtd:LA-id312 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Fällen des § 1 Absatz 1 Nummer 2 bis 4 die Bundesregierung durch Beschluss festgestellt hat,"@de .

dtd:LA-id313 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Bundesministerium für Verkehr und digitale Infrastruktur seine Entscheidung nach Absatz 1 Nummer 1 oder"@de .

dtd:LA-id314 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bundesregierung ihre Feststellung nach Absatz 1 Nummer 2"@de .

dtd:LA-id315 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einmalige oder wiederkehrende Beförderungen von Gütern und Personen (Verkehrsleistungen),"@de .

dtd:LA-id316 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Überlassung von Verkehrsmitteln und -anlagen zum Gebrauch, zum Mitgebrauch oder zu anderer Nutzung, die mit diesen Verkehrsmitteln und -anlagen möglich sind,"@de .

dtd:LA-id317 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Benutzung der Verkehrsinfrastruktur einschließlich der Ausrüstung, der Informations- und Kommunikationssysteme."@de .

dtd:LA-id318 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verkehrsleistungen auch die mit ihnen verbundenen Nebenleistungen, insbesondere der Betrieb von Umschlaganlagen, Speditionsleistungen im Sinne des § 453 des Handelsgesetzbuchs und Leistungen der Lagerei im Sinne des § 467 des Handelsgesetzbuchs, soweit sie dem Verkehr dienen,"@de .

dtd:LA-id319 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verkehrsmittel auch die Ausrüstung einschließlich der Informations- und Kommunikationssysteme,"@de .

dtd:LA-id320 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verkehrsanlagen auch Umschlag- und Speditionsanlagen sowie Anlagen von Unternehmen der Lagerei, soweit sie dem Verkehr dienen,"@de .

dtd:LA-id321 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verkehrsinfrastruktur auch die für den Betrieb der Verkehrswege notwendigen Einrichtungen."@de .

dtd:LA-id322 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verkehrs- und Verkehrsinfrastrukturunternehmen, mit Ausnahme der Bergbahnunternehmen,"@de .

dtd:LA-id323 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Reeder oder Ausrüster von Seeschiffen, die die Bundesflagge führen,"@de .

dtd:LA-id324 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstige Eigentümer und Besitzer von Verkehrsmitteln oder von Verkehrsinfrastruktur, wenn diese Verkehrsmittel und diese Verkehrsinfrastruktur zum Betrieb eines Unternehmens gehören."@de .

dtd:LA-id325 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bundesamt für Bevölkerungsschutz und Katastrophenhilfe, auch für Hilfsorganisationen und bei Katastrophenhilfeersuchen der Länder im Rahmen der Amtshilfe,"@de .

dtd:LA-id326 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leitung der Bundesanstalt Technisches Hilfswerk,"@de .

dtd:LA-id327 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bundespolizeipräsidium,"@de .

dtd:LA-id328 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bundesamt für das Personalmanagement der Bundeswehr, für die Streitkräfte einschließlich der verbündeten Streitkräfte,"@de .

dtd:LA-id329 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bundesanstalt für Landwirtschaft und Ernährung,"@de .

dtd:LA-id330 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Robert Koch-Institut,"@de .

dtd:LA-id331 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Paul-Ehrlich-Institut,"@de .

dtd:LA-id332 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Deutsche Bundesbank,"@de .

dtd:LA-id333 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bundesamt für Strahlenschutz,"@de .

dtd:LA-id334 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bundesamt für Wirtschaft und Ausfuhrkontrolle,"@de .

dtd:LA-id335 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bundesamt für die Sicherheit der nuklearen Entsorgung."@de .

dtd:LA-id336 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Bundesamt für Logistik und Mobilität auf dem Gebiet des Straßenverkehrs,"@de .

dtd:LA-id337 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Generaldirektion Wasserstraßen und Schifffahrt auf dem Gebiet der Seeschifffahrt und der Binnenschifffahrt,"@de .

dtd:LA-id338 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Luftfahrt-Bundesamt auf dem Gebiet der Luftfahrt,"@de .

dtd:LA-id339 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Eisenbahn-Bundesamt auf dem Gebiet des Eisenbahnverkehrs."@de .

dtd:LA-id340 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in schriftlicher, fernschriftlicher oder elektronischer Form ohne die Einhaltung der hierfür geltenden Bestimmungen des Verwaltungszustellungsgesetzes oder"@de .

dtd:LA-id341 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch mündliche oder fernmündliche Mitteilung, durch Presse, Rundfunk (Hörfunk und Fernsehen), Funkspruch oder in einer sonstigen ortsüblichen und geeigneten Weise"@de .

dtd:LA-id342 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Anordnung nach § 5 Abs. 1 Satz 1 zuwiderhandelt oder"@de .

dtd:LA-id343 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 8 Abs. 1 eine Auskunft nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig erteilt."@de .

dtd:LA-id344 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine in § 13 Abs. 1 Nr. 1 bezeichnete Handlung beharrlich wiederholt oder"@de .

dtd:LA-id345 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine in § 13 Abs. 1 bezeichnete vorsätzliche Handlung begeht und dabei eine außergewöhnliche Mangellage bei der Versorgung mit Verkehrsleistungen ausnutzt, um bedeutende Vermögensvorteile zu erlangen."@de .


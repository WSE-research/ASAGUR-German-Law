dtd:LA-id3630 a dtd:LA ;
    dtd:has_Value "junge Menschen in ihrer individuellen und sozialen Entwicklung fördern und dazu beitragen, Benachteiligungen zu vermeiden oder abzubauen,"@de .

dtd:LA-id3631 a dtd:LA ;
    dtd:has_Value "jungen Menschen ermöglichen oder erleichtern, entsprechend ihrem Alter und ihrer individuellen Fähigkeiten in allen sie betreffenden Lebensbereichen selbstbestimmt zu interagieren und damit gleichberechtigt am Leben in der Gesellschaft teilhaben zu können,"@de .

dtd:LA-id3632 a dtd:LA ;
    dtd:has_Value "Eltern und andere Erziehungsberechtigte bei der Erziehung beraten und unterstützen,"@de .

dtd:LA-id3633 a dtd:LA ;
    dtd:has_Value "Kinder und Jugendliche vor Gefahren für ihr Wohl schützen,"@de .

dtd:LA-id3634 a dtd:LA ;
    dtd:has_Value "dazu beitragen, positive Lebensbedingungen für junge Menschen und ihre Familien sowie eine kinder- und familienfreundliche Umwelt zu erhalten oder zu schaffen."@de .

dtd:LA-id3635 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angebote der Jugendarbeit, der Jugendsozialarbeit, der Schulsozialarbeit und des erzieherischen Kinder- und Jugendschutzes (§§ 11 bis 14),"@de .

dtd:LA-id3636 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angebote zur Förderung der Erziehung in der Familie (§§ 16 bis 21),"@de .

dtd:LA-id3637 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angebote zur Förderung von Kindern in Tageseinrichtungen und in Kindertagespflege (§§ 22 bis 25),"@de .

dtd:LA-id3638 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Hilfe zur Erziehung und ergänzende Leistungen (§§ 27 bis 35, 36, 37, 39, 40),"@de .

dtd:LA-id3639 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Hilfe für seelisch behinderte Kinder und Jugendliche und ergänzende Leistungen (§§ 35a bis 37, 39, 40),"@de .

dtd:LA-id3640 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Hilfe für junge Volljährige und Nachbetreuung (den §§ 41 und 41a)."@de .

dtd:LA-id3641 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Inobhutnahme von Kindern und Jugendlichen (§ 42),"@de .

dtd:LA-id3642 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die vorläufige Inobhutnahme von ausländischen Kindern und Jugendlichen nach unbegleiteter Einreise (§ 42a),"@de .

dtd:LA-id3643 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erteilung, der Widerruf und die Zurücknahme der Pflegeerlaubnis (§§ 43, 44),"@de .

dtd:LA-id3644 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erteilung, der Widerruf und die Zurücknahme der Erlaubnis für den Betrieb einer Einrichtung sowie die Erteilung nachträglicher Auflagen und die damit verbundenen Aufgaben (§§ 45 bis 47, 48a),"@de .

dtd:LA-id3645 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Tätigkeitsuntersagung (§§ 48, 48a),"@de .

dtd:LA-id3646 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mitwirkung in Verfahren vor den Familiengerichten (§ 50),"@de .

dtd:LA-id3647 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beratung und Belehrung in Verfahren zur Annahme als Kind (§ 51),"@de .

dtd:LA-id3648 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Mitwirkung in Verfahren nach dem Jugendgerichtsgesetz (§ 52),"@de .

dtd:LA-id3649 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beratung und Unterstützung von Müttern bei Vaterschaftsfeststellung und Geltendmachung von Unterhaltsansprüchen sowie von Pflegern und Vormündern (§§ 52a, 53a),"@de .

dtd:LA-id3650 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erteilung, der Widerruf und die Zurücknahme der Anerkennung als Vormundschaftsverein (§ 54),"@de .

dtd:LA-id3651 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beistandschaft, Pflegschaft und Vormundschaft des Jugendamts (§§ 55 bis 57),"@de .

dtd:LA-id3652 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beurkundung (§ 59),"@de .

dtd:LA-id3653 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Aufnahme von vollstreckbaren Urkunden (§ 60)."@de .

dtd:LA-id3654 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kind, wer noch nicht 14 Jahre alt ist, soweit nicht die Absätze 2 bis 4 etwas anderes bestimmen,"@de .

dtd:LA-id3655 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Jugendlicher, wer 14, aber noch nicht 18 Jahre alt ist,"@de .

dtd:LA-id3656 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "junger Volljähriger, wer 18, aber noch nicht 27 Jahre alt ist,"@de .

dtd:LA-id3657 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "junger Mensch, wer noch nicht 27 Jahre alt ist,"@de .

dtd:LA-id3658 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personensorgeberechtigter, wem allein oder gemeinsam mit einer anderen Person nach den Vorschriften des Bürgerlichen Gesetzbuchs die Personensorge zusteht,"@de .

dtd:LA-id3659 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erziehungsberechtigter, der Personensorgeberechtigte und jede sonstige Person über 18 Jahre, soweit sie auf Grund einer Vereinbarung mit dem Personensorgeberechtigten nicht nur vorübergehend und nicht nur für einzelne Verrichtungen Aufgaben der Personensorge wahrnimmt."@de .

dtd:LA-id3660 a dtd:LA ;
    dtd:has_Value "sich dabei einen unmittelbaren Eindruck von dem Kind und von seiner persönlichen Umgebung zu verschaffen sowie"@de .

dtd:LA-id3661 a dtd:LA ;
    dtd:has_Value "Personen, die gemäß § 4 Absatz 3 des Gesetzes zur Kooperation und Information im Kinderschutz dem Jugendamt Daten übermittelt haben, in geeigneter Weise an der Gefährdungseinschätzung zu beteiligen."@de .

dtd:LA-id3662 a dtd:LA ;
    dtd:has_Value "deren Fachkräfte bei Bekanntwerden gewichtiger Anhaltspunkte für die Gefährdung eines von ihnen betreuten Kindes oder Jugendlichen eine Gefährdungseinschätzung vornehmen,"@de .

dtd:LA-id3663 a dtd:LA ;
    dtd:has_Value "bei der Gefährdungseinschätzung eine insoweit erfahrene Fachkraft beratend hinzugezogen wird sowie"@de .

dtd:LA-id3664 a dtd:LA ;
    dtd:has_Value "die Erziehungsberechtigten sowie das Kind oder der Jugendliche in die Gefährdungseinschätzung einbezogen werden, soweit hierdurch der wirksame Schutz des Kindes oder Jugendlichen nicht in Frage gestellt wird."@de .

dtd:LA-id3665 a dtd:LA ;
    dtd:has_Value "zur Sicherung des Kindeswohls und zum Schutz vor Gewalt sowie"@de .

dtd:LA-id3666 a dtd:LA ;
    dtd:has_Value "zu Verfahren der Beteiligung von Kindern und Jugendlichen an strukturellen Entscheidungen in der Einrichtung sowie zu Beschwerdeverfahren in persönlichen Angelegenheiten."@de .

dtd:LA-id3667 a dtd:LA ;
    dtd:has_Value "die von den Personensorgeberechtigten bestimmte Grundrichtung der Erziehung sowie die Rechte der Personensorgeberechtigten und des Kindes oder des Jugendlichen bei der Bestimmung der religiösen Erziehung zu beachten,"@de .

dtd:LA-id3668 a dtd:LA ;
    dtd:has_Value "die wachsende Fähigkeit und das wachsende Bedürfnis des Kindes oder des Jugendlichen zu selbständigem, verantwortungsbewusstem Handeln sowie die jeweiligen besonderen sozialen und kulturellen Bedürfnisse und Eigenarten junger Menschen und ihrer Familien zu berücksichtigen,"@de .

dtd:LA-id3669 a dtd:LA ;
    dtd:has_Value "die unterschiedlichen Lebenslagen von Mädchen, Jungen sowie transidenten, nichtbinären und intergeschlechtlichen jungen Menschen zu berücksichtigen, Benachteiligungen abzubauen und die Gleichberechtigung der Geschlechter zu fördern,"@de .

dtd:LA-id3670 a dtd:LA ;
    dtd:has_Value "die gleichberechtigte Teilhabe von jungen Menschen mit und ohne Behinderungen umzusetzen und vorhandene Barrieren abzubauen."@de .

dtd:LA-id3671 a dtd:LA ;
    dtd:has_Value "die Familiensituation oder die persönliche Situation des jungen Menschen, Bedarfe, vorhandene Ressourcen sowie mögliche Hilfen,"@de .

dtd:LA-id3672 a dtd:LA ;
    dtd:has_Value "die Leistungen der Kinder- und Jugendhilfe einschließlich des Zugangs zum Leistungssystem,"@de .

dtd:LA-id3673 a dtd:LA ;
    dtd:has_Value "die Leistungen anderer Leistungsträger,"@de .

dtd:LA-id3674 a dtd:LA ;
    dtd:has_Value "mögliche Auswirkungen und Folgen einer Hilfe,"@de .

dtd:LA-id3675 a dtd:LA ;
    dtd:has_Value "die Verwaltungsabläufe,"@de .

dtd:LA-id3676 a dtd:LA ;
    dtd:has_Value "Hinweise auf Leistungsanbieter und andere Hilfemöglichkeiten im Sozialraum und auf Möglichkeiten zur Leistungserbringung,"@de .

dtd:LA-id3677 a dtd:LA ;
    dtd:has_Value "Hinweise auf andere Beratungsangebote im Sozialraum."@de .

dtd:LA-id3678 a dtd:LA ;
    dtd:has_Value "außerschulische Jugendbildung mit allgemeiner, politischer, sozialer, gesundheitlicher, kultureller, naturkundlicher und technischer Bildung,"@de .

dtd:LA-id3679 a dtd:LA ;
    dtd:has_Value "Jugendarbeit in Sport, Spiel und Geselligkeit,"@de .

dtd:LA-id3680 a dtd:LA ;

dtd:LA-id645 a dtd:LA ;
    dtd:has_Value "Registrierungen nach § 28 Absatz 2,"@de .

dtd:LA-id646 a dtd:LA ;
    dtd:has_Value "Registrierungen nach § 32 Absatz 1 Satz 1 sowie"@de .

dtd:LA-id647 a dtd:LA ;

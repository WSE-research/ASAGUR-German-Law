dtd:LA-id2221 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Hierbei sind die Dezimalzahlen als Festkommazahlen mit zehn Nachkommastellen zu berechnen."@de .

dtd:LA-id2222 a dtd:LA ;

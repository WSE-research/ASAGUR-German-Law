dtd:I-id221 a dtd:I ;
    dtd:has_Value "Die §§ 627 und 1300 der Reichsversicherungsordnung, § 79 des Angestelltenversicherungsgesetzes und § 93 Abs. 1 des Reichsknappschaftsgesetzes bleiben unberührt."@de .

dtd:LA-id421 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vor dem 1. Januar 1978 eingetreten sind, für Bezugszeiten vom 1. Januar 1979 an,"@de .

dtd:LA-id422 a dtd:LA ;

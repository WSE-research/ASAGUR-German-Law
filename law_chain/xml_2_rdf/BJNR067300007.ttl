dtd:LA-id748 a dtd:LA ;
    dtd:has_Value "Verwaltungsvorgänge bearbeiten"@de .

dtd:LA-id749 a dtd:LA ;
    dtd:has_Value "bei der Kostenermittlung mitwirken"@de .

dtd:LA-id750 a dtd:LA ;
    dtd:has_Value "fremdsprachliche Fachausdrücke anwenden"@de .

dtd:LA-id751 a dtd:LA ;
    dtd:has_Value "Werkzeuge, Geräte, Maschinen und technische Einrichtungen auswählen"@de .

dtd:LA-id752 a dtd:LA ;
    dtd:has_Value "Handwerkzeuge handhaben und instand halten"@de .

dtd:LA-id753 a dtd:LA ;
    dtd:has_Value "Geräte und Maschinen einrichten und unter Verwendung der Schutzeinrichtungen bedienen, technische Einrichtungen anwenden"@de .

dtd:LA-id754 a dtd:LA ;
    dtd:has_Value "Störungen an Geräten, Maschinen und technischen Einrichtungen erkennen, Störungsbeseitigung veranlassen"@de .

dtd:LA-id755 a dtd:LA ;
    dtd:has_Value "Geräte, Maschinen und technische Einrichtungen warten"@de .

dtd:LA-id756 a dtd:LA ;
    dtd:has_Value "Aufgaben und Ziele von qualitätssichernden Maßnahmen anhand betrieblicher Beispiele unterscheiden"@de .

dtd:LA-id757 a dtd:LA ;
    dtd:has_Value "qualitätssichernde Maßnahmen im eigenen Arbeitsbereich anwenden, dabei zur kontinuierlichen Verbesserung von Arbeitsvorgängen beitragen"@de .

dtd:LA-id758 a dtd:LA ;
    dtd:has_Value "Arbeiten kundenorientiert durchführen"@de .

dtd:LA-id759 a dtd:LA ;
    dtd:has_Value "Arbeitsergebnisse kontrollieren und dokumentieren"@de .

dtd:LA-id760 a dtd:LA ;
    dtd:has_Value "Fachnormen zur Qualitätssicherung umsetzen"@de .

dtd:LA-id761 a dtd:LA ;
    dtd:has_Value "Arbeiten von Dritten, insbesondere von beauftragten Firmen, anhand von Vorgaben überwachen und dokumentieren"@de .

dtd:LA-id762 a dtd:LA ;
    dtd:has_Value "Mängel feststellen und Maßnahmen zur Mängelbeseitigung veranlassen"@de .


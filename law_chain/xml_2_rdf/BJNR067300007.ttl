dtd:LA-id743 a dtd:LA ;
    dtd:has_Value "Berichte erstellen"@de .

dtd:LA-id744 a dtd:LA ;
    dtd:has_Value "Zeitaufwand und personellen Bedarf einschließlich Dienstleistungen Dritter abschätzen"@de .

dtd:LA-id745 a dtd:LA ;
    dtd:has_Value "Aufgaben und Arbeitsabläufe im Team planen und umsetzen, Ergebnisse auswerten"@de .

dtd:LA-id746 a dtd:LA ;
    dtd:has_Value "Gespräche situationsgerecht führen, Sachverhalte darstellen"@de .

dtd:LA-id747 a dtd:LA ;

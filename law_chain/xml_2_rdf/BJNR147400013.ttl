    dtd:has_Value "die Voraussetzungen und das Verfahren für die Ausstellung, Gültigkeit und Einziehung der Wrackbeseitigungshaftungsbescheinigung nach § 5,"@de .

dtd:LA-id311 a dtd:LA ;

dtd:LA-id303 a dtd:LA ;
    dtd:has_Value "die Bundesflagge führt oder"@de .

dtd:LA-id304 a dtd:LA ;
    dtd:has_Value "einen Hafen im Inland anläuft oder verlässt oder eine vor der Küste gelegene Einrichtung innerhalb des Küstenmeeres der Bundesrepublik Deutschland anläuft oder verlässt,"@de .

dtd:LA-id305 a dtd:LA ;
    dtd:has_Value "eine entsprechende Versicherung oder sonstige finanzielle Sicherheit besteht und"@de .

dtd:LA-id306 a dtd:LA ;

    dtd:has_Value "nach durchgeführter Ergänzungsprüfung zwei Prüfungsfächer jeweils mit weniger als 50 Punkten bewertet worden sind."@de .

dtd:P-id276 a dtd:P ;
    dtd:has_Value ""@de .


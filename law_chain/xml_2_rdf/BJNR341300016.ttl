dtd:LA-id3104 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Förderung des deutschen Films und zur Verbesserung der Struktur der deutschen Filmwirtschaft einschließlich der Kinos durchzuführen;"@de .

dtd:LA-id3105 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die gesamtwirtschaftlichen Belange der Filmwirtschaft in Deutschland unter Berücksichtigung ökologischer Belange zu unterstützen, insbesondere durch Maßnahmen zur Marktforschung, zur Bekämpfung der Verletzung von Urheber- und Leistungsschutzrechten und zur Filmbildung junger Menschen;"@de .

dtd:LA-id3106 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Digitalisierung zum Zweck des Erhalts und der Zugänglichmachung des deutschen Filmerbes zu unterstützen;"@de .

dtd:LA-id3107 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die internationale Orientierung des deutschen Filmschaffens und die Grundlagen für die Verbreitung und marktgerechte Auswertung des deutschen Films im Inland und seine wirtschaftliche und kulturelle Ausstrahlung im Ausland zu verbessern;"@de .

dtd:LA-id3108 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "deutsch-ausländische Gemeinschaftsproduktionen zu unterstützen;"@de .


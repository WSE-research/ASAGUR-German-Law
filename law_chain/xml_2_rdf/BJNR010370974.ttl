dtd:LA-id70 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Stiftung ausschließlich der Förderung der gewerblichen Wirtschaft dient,"@de .

dtd:LA-id71 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Stiftung ein Organ erhält, das die Geschäftsführung des Vorstandes zu überwachen hat,"@de .


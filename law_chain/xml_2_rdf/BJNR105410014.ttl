dtd:LA-id178 a dtd:LA ;
    dtd:has_DL dtd:DL-id119 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen Artikel 10 Absatz 1 Unterabsatz 1 nicht sicherstellt, dass a)ein kosmetisches Mittel eine dort genannte Sicherheitsbewertung durchlaufen hat, oderb)ein Sicherheitsbericht für das kosmetische Mittel gemäß Anhang I erstellt worden ist."@de .

dtd:LA-id179 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein kosmetisches Mittel eine dort genannte Sicherheitsbewertung durchlaufen hat, oder"@de .


    dtd:has_Value "Durchmesser"@de .

dtd:LA-id983 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der größte Durchmesser, gemessen in der Mitte des Internodiums unter dem obersten Trieb beträgt mindestens 5 mm."@de .


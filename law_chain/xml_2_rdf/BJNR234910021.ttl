dtd:LA-id247 a dtd:LA ;
    dtd:has_Value "das Verfahren zur Sicherstellung einer unverzüglichen Schadensregulierung einschließlich Repatriierung."@de .

dtd:LA-id248 a dtd:LA ;
    dtd:has_Value "Bildung und Verwaltung des Fondsvermögens (§ 2 Absatz 1 Nummer 1 des Reisesicherungsfondsgesetzes),"@de .

dtd:LA-id249 a dtd:LA ;
    dtd:has_Value "Abschluss von Absicherungsverträgen (§ 2 Absatz 1 Nummer 2 des Reisesicherungsfondsgesetzes),"@de .

dtd:LA-id250 a dtd:LA ;

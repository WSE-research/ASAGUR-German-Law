    dtd:has_Value "von Papier auf zellstoffintegrierten Papiermaschinen (Verbundmaschinen), wenn das auf der Verbundmaschine hergestellte Papier zu mehr als 75 vom Hundert des Zellstoffeintrags aus eigenerzeugtem Zellstoff besteht."@de .

dtd:LA-id140 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Antransport der Rohmaterialien vom Betriebslager,"@de .

dtd:LA-id141 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle anderen zur Stoffaufbereitung und Papierherstellung unmittelbar erforderlichen Arbeiten,"@de .

dtd:LA-id142 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Abtransport der Rollen oder Formate zur Lagerung im Zwischenlager."@de .

dtd:LA-id143 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "während des ganzen Sonn- oder Feiertags an den Papiermaschinen, die an diesem Sonn- oder Feiertag laufen,"@de .

dtd:LA-id144 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "und"@de .


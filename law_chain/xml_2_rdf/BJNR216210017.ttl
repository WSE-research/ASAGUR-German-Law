dtd:LA-id1766 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Geburtstag und -ort,"@de .

dtd:LA-id1767 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angabe, welche Auflagen bestehen,"@de .

dtd:LA-id1768 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Ausbildungsverhltnis mit dem Inhaber einer Fahrschule sowie"@de .

dtd:LA-id1769 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gltigkeitsdauer"@de .

dtd:LA-id1770 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seit mindestens drei Jahren im Besitz der Fahrlehrerlaubnisklasse BE ist und"@de .

dtd:LA-id1771 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "innerhalb der letzten zwei Jahre erfolgreich an einem fnftgigen Einweisungsseminar in einer amtlich anerkannten Fahrlehrerausbildungssttte oder von einem Berufsverband der Fahrlehrer, sofern dieser hierfr von der nach Landesrecht zustndigen Behrde anerkannt ist, teilgenommen hat."@de .

dtd:LA-id1772 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber das 25. Lebensjahr vollendet hat und keine Tatsachen vorliegen, die ihn fr die Fhrung einer Fahrschule als unzuverlssig erscheinen lassen,"@de .

dtd:LA-id1773 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "keine Tatsachen vorliegen, welche die Annahme rechtfertigen, dass der Bewerber die Pflichten nach  29 nicht erfllen kann,"@de .

dtd:LA-id1774 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber die Fahrlehrerlaubnis fr die Klasse besitzt, fr die er die Fahrschulerlaubnis beantragt,"@de .


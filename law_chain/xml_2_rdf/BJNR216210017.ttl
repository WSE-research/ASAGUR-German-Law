dtd:LA-id1707 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Fahrlehrerlaubnisklasse BE berechtigt zur Ausbildung in den Fahrerlaubnisklassen B, BE und L."@de .

dtd:LA-id1708 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Fahrlehrerlaubnisklasse A berechtigt zur Ausbildung in den Fahrerlaubnisklassen AM, A1, A2 und A."@de .

dtd:LA-id1709 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Fahrlehrerlaubnisklasse CE berechtigt zur Ausbildung in den Fahrerlaubnisklassen C1, C1E, C, CE und T."@de .

dtd:LA-id1710 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Fahrlehrerlaubnisklasse DE berechtigt zur Ausbildung in den Fahrerlaubnisklassen D1, D1E, D und DE."@de .

dtd:LA-id1711 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber das 21. Lebensjahr vollendet hat,"@de .

dtd:LA-id1712 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber geistig und körperlich geeignet ist,"@de .

dtd:LA-id1713 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber fachlich und pädagogisch geeignet ist,"@de .

dtd:LA-id1714 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegen den Bewerber keine Tatsachen vorliegen, die ihn für den Fahrlehrerberuf als unzuverlässig erscheinen lassen,"@de .

dtd:LA-id1715 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber mindestens eine abgeschlossene Berufsausbildung in einem anerkannten Lehrberuf oder eine gleichwertige Vorbildung besitzt,"@de .

dtd:LA-id1716 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber im Besitz der Fahrerlaubnis der Klasse ist, für die die Fahrlehrerlaubnis erteilt werden soll,"@de .

dtd:LA-id1717 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber seit mindestens drei Jahren die Fahrerlaubnis der Klasse B und, sofern die Fahrlehrerlaubnis zusätzlich für die Klasse A, CE oder DE erteilt werden soll, jeweils auch zwei Jahre die Fahrerlaubnis der Klasse A2, CE oder D besitzt,"@de .

dtd:LA-id1718 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber innerhalb der letzten drei Jahre vor Erteilung der Fahrlehrerlaubnis nach § 7 zum Fahrlehrer ausgebildet worden ist,"@de .

dtd:LA-id1719 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber eine Prüfung nach § 8 bestanden hat und"@de .

dtd:LA-id1720 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber über die für die Ausübung der Berufstätigkeit erforderlichen Kenntnisse der deutschen Sprache verfügt."@de .

dtd:LA-id1721 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber ohne Einschränkung qualifiziert ist, im Herkunftsmitgliedstaat die berufliche Tätigkeit auszuüben, für die eine beschränkte Fahrlehrerlaubnis begehrt wird und"@de .

dtd:LA-id1722 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Unterschiede zwischen der rechtmäßig ausgeübten Berufstätigkeit im Herkunftsmitgliedstaat und der Fahrlehrertätigkeit im Inland so groß sind, dass ein der Ausbildung nach § 7 entsprechender Anpassungslehrgang zu durchlaufen wäre."@de .

dtd:LA-id1723 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein amtlicher Nachweis über Ort und Tag der Geburt,"@de .

dtd:LA-id1724 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Lebenslauf,"@de .

dtd:LA-id1725 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Zeugnis oder ein Gutachten über die Erfüllung der von Bewerbern um eine Fahrerlaubnis der Klasse C1 geforderten Anforderungen an die körperliche und geistige Eignung und eine Bescheinigung oder ein Zeugnis über die Erfüllung der von Bewerbern um eine Fahrerlaubnis der Klasse C1 geforderten Anforderungen an das Sehvermögen, die bei Antragstellung nicht älter als ein Jahr sind,"@de .

dtd:LA-id1726 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Ablichtung des nach dem 1. Januar 1999 ausgestellten Kartenführerscheins; sie muss amtlich beglaubigt sein, wenn der Führerschein nicht zur Einsichtnahme vorgelegt wird,"@de .

dtd:LA-id1727 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Nachweis über die nach § 2 Absatz 1 Satz 1 Nummer 5 geforderte Vorbildung,"@de .

dtd:LA-id1728 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Bescheinigung der amtlich anerkannten Fahrlehrerausbildungsstätte über die Dauer der durchgeführten Ausbildung nach § 7,"@de .

dtd:LA-id1729 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Antrag auf Erteilung der Fahrlehrerlaubnis der Klasse BE zusätzlich eine Bescheinigung der Ausbildungsfahrschule über die Dauer der durchgeführten Ausbildung nach § 7."@de .

dtd:LA-id1730 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem für die Fragestellung zuständigen Facharzt mit verkehrsmedizinischer Qualifikation,"@de .

dtd:LA-id1731 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Arzt des Gesundheitsamtes oder einem anderen Arzt der öffentlichen Verwaltung,"@de .

dtd:LA-id1732 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Arzt mit der Gebietsbezeichnung „Arbeitsmedizin“ oder der Zusatzbezeichnung „Betriebsmedizin“,"@de .

dtd:LA-id1733 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Arzt mit der Gebietsbezeichnung „Facharzt für Rechtsmedizin“ oder"@de .

dtd:LA-id1734 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Arzt in einer Begutachtungsstelle für Fahreignung, der die Anforderungen nach Anlage 14 der Fahrerlaubnis-Verordnung erfüllt,"@de .

dtd:LA-id1735 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur weiteren Klärung von Eignungszweifeln nach Würdigung der Gutachten gemäß Absatz 1 Satz 2 Nummer 3 oder Absatz 3 Satz 2 Nummer 1 bis 4 oder"@de .

dtd:LA-id1736 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Klärung, ob die für die Ausübung des Fahrlehrerberufs notwendige Zuverlässigkeit besteht,"@de .

dtd:LA-id1737 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Identitätsnachweis,"@de .

dtd:LA-id1738 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine amtlich beglaubigte Kopie des Befähigungsnachweises oder des Ausbildungsnachweises, der zur Aufnahme des entsprechenden Berufs im ausstellenden Staat berechtigt,"@de .

dtd:LA-id1739 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine dem Führungszeugnis nach § 30a Absatz 1 Nummer 1 des Bundeszentralregistergesetzes vergleichbare Bescheinigung des Staates, in welchem der Befähigungs- oder Ausbildungsnachweis oder die Berufserfahrung erworben wurde,"@de .

dtd:LA-id1740 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein amtlicher Nachweis des Staates, in welchem der Ausbildungs- oder Befähigungsnachweis oder die Berufserfahrung erworben wurde, dass kein Fall vorliegt, in dem die Ausübung des Berufs wegen fehlender geistiger oder körperlicher Eignung im Sinne des § 3 Absatz 6 in Verbindung mit § 2 Absatz 1 Satz 1 Nummer 2 zu untersagen wäre,"@de .

dtd:LA-id1741 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Bescheinigung darüber, dass die Tätigkeit des Fahrlehrers innerhalb der letzten zehn Jahre vor Ausstellung der Bescheinigung mindestens zwei Jahre lang ausgeübt wurde, wenn in dem ausstellenden Staat die Fahrlehrertätigkeit nicht reglementiert ist, und"@de .

dtd:LA-id1742 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn die Kenntnisse nach § 3 Absatz 2 durch sonstige nachgewiesene einschlägige Qualifikationen erworben wurden, ein amtlicher Nachweis des Staates, in welchem diese Kenntnisse erworben wurden, darüber, dass diese als gültig anerkannt wurden."@de .

dtd:LA-id1743 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu Ausbildung und Prüfung, soweit dies erforderlich ist um festzustellen, ob die Ausbildung oder Prüfung im Sinne des § 3 Absatz 2 Satz 1 wesentlich von den Anforderungen der Fahrlehrer-Ausbildungsordnung und der Fahrlehrer-Prüfungsordnung für die Aufnahme der Fahrlehrertätigkeit im Inland abweicht,"@de .

dtd:LA-id1744 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Berufserfahrung, soweit dies erforderlich ist um festzustellen, ob eine festgestellte wesentliche Abweichung der Ausbildung oder Prüfung von den Anforderungen der Fahrlehrer-Ausbildungsordnung und der Fahrlehrer-Prüfungsordnung für die Aufnahme der Fahrlehrertätigkeit im Inland durch die im Rahmen der Berufserfahrung erworbenen Kenntnisse im Sinne des § 3 Absatz 2 Satz 1 ausgeglichen werden kann."@de .

dtd:LA-id1745 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Identitätsnachweis,"@de .

dtd:LA-id1746 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine amtliche Bescheinigung darüber, dass der Bewerber zum Zeitpunkt der Ausstellung der Bescheinigung in einem anderen Mitgliedstaat der Europäischen Union, einem anderen Vertragsstaat des Abkommens über den Europäischen Wirtschaftsraum oder der Schweiz rechtmäßig als Fahrlehrer niedergelassen ist und dass ihm die Ausübung dieses Berufs zum Zeitpunkt der Ausstellung der Bescheinigung nicht, auch nicht vorübergehend, untersagt ist,"@de .

dtd:LA-id1747 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein amtlich beglaubigter Nachweis über die Berufsqualifikation im Sinne von Artikel 3 Absatz 1 Buchstabe b der Richtlinie 2005/36/EG,"@de .

dtd:LA-id1748 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Fällen, in denen die Fahrlehrertätigkeit oder die Ausbildung zu diesem Beruf in dem Staat seiner Niederlassung nicht im Sinne von Artikel 3 Absatz 1 Buchstabe a oder e der Richtlinie 2005/36/EG reglementiert ist, eine Bescheinigung darüber, dass die Tätigkeit des Fahrlehrers innerhalb der letzten zehn Jahre vor Ausstellung der Bescheinigung mindestens zwei Jahre lang im Staat seiner Niederlassung ausgeübt wurde."@de .

dtd:LA-id1749 a dtd:LA ;
    dtd:has_Value "für die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse BE mindestens zwölf Monate"@de .

dtd:LA-id1750 a dtd:LA ;
    dtd:has_Value "für die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse A zusätzlich zu der Ausbildung nach Nummer 1 mindestens einen Monat,"@de .

dtd:LA-id1751 a dtd:LA ;
    dtd:has_Value "für die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse CE oder DE zusätzlich zu der Ausbildung nach Nummer 1 mindestens zwei Monate."@de .

dtd:LA-id1752 a dtd:LA ;
    dtd:has_Value "für die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse DE die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse CE,"@de .

dtd:LA-id1753 a dtd:LA ;
    dtd:has_Value "für die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse CE die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse DE,"@de .

dtd:LA-id1754 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Erteilung der Fahrlehrerlaubnis,"@de .

dtd:LA-id1755 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach dreimaliger erfolgloser Lehrprobe im theoretischen oder im fahrpraktischen Unterricht (§ 8 Absatz 2) oder"@de .

dtd:LA-id1756 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Ablauf der Frist."@de .

dtd:LA-id1757 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Namen,"@de .

dtd:LA-id1758 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vornamen,"@de .

dtd:LA-id1759 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Geburtstag und -ort,"@de .

dtd:LA-id1760 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angabe, für welche Fahrlehrerlaubnisklassen die Fahrlehrerlaubnis gilt,"@de .

dtd:LA-id1761 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angabe, welche Auflagen bestehen,"@de .

dtd:LA-id1762 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beschäftigungsverhältnisse mit dem Inhaber einer Fahrschule oder die selbstständige Tätigkeit als Inhaber einer Fahrschule sowie"@de .

dtd:LA-id1763 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Fällen des § 3 Absatz 1 Satz 2 den Zusatz, dass die Fahrlehrerlaubnis nur zur vorübergehenden und gelegentlichen Ausbildung von Fahrschülern berechtigt,"@de .

dtd:LA-id1764 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Namen,"@de .

dtd:LA-id1765 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vornamen,"@de .

dtd:LA-id1766 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Geburtstag und -ort,"@de .

dtd:LA-id1767 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angabe, welche Auflagen bestehen,"@de .

dtd:LA-id1768 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Ausbildungsverhältnis mit dem Inhaber einer Fahrschule sowie"@de .

dtd:LA-id1769 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gültigkeitsdauer"@de .

dtd:LA-id1770 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seit mindestens drei Jahren im Besitz der Fahrlehrerlaubnisklasse BE ist und"@de .

dtd:LA-id1771 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "innerhalb der letzten zwei Jahre erfolgreich an einem fünftägigen Einweisungsseminar in einer amtlich anerkannten Fahrlehrerausbildungsstätte oder von einem Berufsverband der Fahrlehrer, sofern dieser hierfür von der nach Landesrecht zuständigen Behörde anerkannt ist, teilgenommen hat."@de .

dtd:LA-id1772 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber das 25. Lebensjahr vollendet hat und keine Tatsachen vorliegen, die ihn für die Führung einer Fahrschule als unzuverlässig erscheinen lassen,"@de .

dtd:LA-id1773 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "keine Tatsachen vorliegen, welche die Annahme rechtfertigen, dass der Bewerber die Pflichten nach § 29 nicht erfüllen kann,"@de .

dtd:LA-id1774 a dtd:LA ;
    dtd:has_Size dtd:normal ;

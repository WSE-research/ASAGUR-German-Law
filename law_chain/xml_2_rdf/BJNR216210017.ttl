    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angabe, fr welche Fahrlehrerlaubnisklassen die Fahrlehrerlaubnis gilt,"@de .

dtd:LA-id1761 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angabe, welche Auflagen bestehen,"@de .

dtd:LA-id1762 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beschftigungsverhltnisse mit dem Inhaber einer Fahrschule oder die selbststndige Ttigkeit als Inhaber einer Fahrschule sowie"@de .

dtd:LA-id1763 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Fllen des  3 Absatz 1 Satz 2 den Zusatz, dass die Fahrlehrerlaubnis nur zur vorbergehenden und gelegentlichen Ausbildung von Fahrschlern berechtigt,"@de .

dtd:LA-id1764 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Namen,"@de .


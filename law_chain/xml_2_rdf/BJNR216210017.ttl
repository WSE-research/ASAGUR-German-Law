dtd:LA-id1707 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Fahrlehrerlaubnisklasse BE berechtigt zur Ausbildung in den Fahrerlaubnisklassen B, BE und L."@de .

dtd:LA-id1708 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Fahrlehrerlaubnisklasse A berechtigt zur Ausbildung in den Fahrerlaubnisklassen AM, A1, A2 und A."@de .

dtd:LA-id1709 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Fahrlehrerlaubnisklasse CE berechtigt zur Ausbildung in den Fahrerlaubnisklassen C1, C1E, C, CE und T."@de .

dtd:LA-id1710 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die Fahrlehrerlaubnisklasse DE berechtigt zur Ausbildung in den Fahrerlaubnisklassen D1, D1E, D und DE."@de .

dtd:LA-id1711 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber das 21. Lebensjahr vollendet hat,"@de .

dtd:LA-id1712 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber geistig und krperlich geeignet ist,"@de .

dtd:LA-id1713 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber fachlich und pdagogisch geeignet ist,"@de .

dtd:LA-id1714 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegen den Bewerber keine Tatsachen vorliegen, die ihn fr den Fahrlehrerberuf als unzuverlssig erscheinen lassen,"@de .

dtd:LA-id1715 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber mindestens eine abgeschlossene Berufsausbildung in einem anerkannten Lehrberuf oder eine gleichwertige Vorbildung besitzt,"@de .

dtd:LA-id1716 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber im Besitz der Fahrerlaubnis der Klasse ist, fr die die Fahrlehrerlaubnis erteilt werden soll,"@de .

dtd:LA-id1717 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber seit mindestens drei Jahren die Fahrerlaubnis der Klasse B und, sofern die Fahrlehrerlaubnis zustzlich fr die Klasse A, CE oder DE erteilt werden soll, jeweils auch zwei Jahre die Fahrerlaubnis der Klasse A2, CE oder D besitzt,"@de .

dtd:LA-id1718 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber innerhalb der letzten drei Jahre vor Erteilung der Fahrlehrerlaubnis nach  7 zum Fahrlehrer ausgebildet worden ist,"@de .

dtd:LA-id1719 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber eine Prfung nach  8 bestanden hat und"@de .

dtd:LA-id1720 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber ber die fr die Ausbung der Berufsttigkeit erforderlichen Kenntnisse der deutschen Sprache verfgt."@de .

dtd:LA-id1721 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bewerber ohne Einschrnkung qualifiziert ist, im Herkunftsmitgliedstaat die berufliche Ttigkeit auszuben, fr die eine beschrnkte Fahrlehrerlaubnis begehrt wird und"@de .

dtd:LA-id1722 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Unterschiede zwischen der rechtmig ausgebten Berufsttigkeit im Herkunftsmitgliedstaat und der Fahrlehrerttigkeit im Inland so gro sind, dass ein der Ausbildung nach  7 entsprechender Anpassungslehrgang zu durchlaufen wre."@de .

dtd:LA-id1723 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein amtlicher Nachweis ber Ort und Tag der Geburt,"@de .

dtd:LA-id1724 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Lebenslauf,"@de .

dtd:LA-id1725 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Zeugnis oder ein Gutachten ber die Erfllung der von Bewerbern um eine Fahrerlaubnis der Klasse C1 geforderten Anforderungen an die krperliche und geistige Eignung und eine Bescheinigung oder ein Zeugnis ber die Erfllung der von Bewerbern um eine Fahrerlaubnis der Klasse C1 geforderten Anforderungen an das Sehvermgen, die bei Antragstellung nicht lter als ein Jahr sind,"@de .

dtd:LA-id1726 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Ablichtung des nach dem 1. Januar 1999 ausgestellten Kartenfhrerscheins; sie muss amtlich beglaubigt sein, wenn der Fhrerschein nicht zur Einsichtnahme vorgelegt wird,"@de .

dtd:LA-id1727 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Nachweis ber die nach  2 Absatz 1 Satz 1 Nummer 5 geforderte Vorbildung,"@de .

dtd:LA-id1728 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Bescheinigung der amtlich anerkannten Fahrlehrerausbildungssttte ber die Dauer der durchgefhrten Ausbildung nach  7,"@de .

dtd:LA-id1729 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Antrag auf Erteilung der Fahrlehrerlaubnis der Klasse BE zustzlich eine Bescheinigung der Ausbildungsfahrschule ber die Dauer der durchgefhrten Ausbildung nach  7."@de .

dtd:LA-id1730 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem fr die Fragestellung zustndigen Facharzt mit verkehrsmedizinischer Qualifikation,"@de .

dtd:LA-id1731 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Arzt des Gesundheitsamtes oder einem anderen Arzt der ffentlichen Verwaltung,"@de .

dtd:LA-id1732 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Arzt mit der Gebietsbezeichnung Arbeitsmedizin oder der Zusatzbezeichnung Betriebsmedizin,"@de .

dtd:LA-id1733 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Arzt mit der Gebietsbezeichnung Facharzt fr Rechtsmedizin oder"@de .

dtd:LA-id1734 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Arzt in einer Begutachtungsstelle fr Fahreignung, der die Anforderungen nach Anlage 14 der Fahrerlaubnis-Verordnung erfllt,"@de .

dtd:LA-id1735 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur weiteren Klrung von Eignungszweifeln nach Wrdigung der Gutachten gem Absatz 1 Satz 2 Nummer 3 oder Absatz 3 Satz 2 Nummer 1 bis 4 oder"@de .

dtd:LA-id1736 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Klrung, ob die fr die Ausbung des Fahrlehrerberufs notwendige Zuverlssigkeit besteht,"@de .

dtd:LA-id1737 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Identittsnachweis,"@de .

dtd:LA-id1738 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine amtlich beglaubigte Kopie des Befhigungsnachweises oder des Ausbildungsnachweises, der zur Aufnahme des entsprechenden Berufs im ausstellenden Staat berechtigt,"@de .

dtd:LA-id1739 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine dem Fhrungszeugnis nach  30a Absatz 1 Nummer 1 des Bundeszentralregistergesetzes vergleichbare Bescheinigung des Staates, in welchem der Befhigungs- oder Ausbildungsnachweis oder die Berufserfahrung erworben wurde,"@de .

dtd:LA-id1740 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein amtlicher Nachweis des Staates, in welchem der Ausbildungs- oder Befhigungsnachweis oder die Berufserfahrung erworben wurde, dass kein Fall vorliegt, in dem die Ausbung des Berufs wegen fehlender geistiger oder krperlicher Eignung im Sinne des  3 Absatz 6 in Verbindung mit  2 Absatz 1 Satz 1 Nummer 2 zu untersagen wre,"@de .

dtd:LA-id1741 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Bescheinigung darber, dass die Ttigkeit des Fahrlehrers innerhalb der letzten zehn Jahre vor Ausstellung der Bescheinigung mindestens zwei Jahre lang ausgebt wurde, wenn in dem ausstellenden Staat die Fahrlehrerttigkeit nicht reglementiert ist, und"@de .

dtd:LA-id1742 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn die Kenntnisse nach  3 Absatz 2 durch sonstige nachgewiesene einschlgige Qualifikationen erworben wurden, ein amtlicher Nachweis des Staates, in welchem diese Kenntnisse erworben wurden, darber, dass diese als gltig anerkannt wurden."@de .

dtd:LA-id1743 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu Ausbildung und Prfung, soweit dies erforderlich ist um festzustellen, ob die Ausbildung oder Prfung im Sinne des  3 Absatz 2 Satz 1 wesentlich von den Anforderungen der Fahrlehrer-Ausbildungsordnung und der Fahrlehrer-Prfungsordnung fr die Aufnahme der Fahrlehrerttigkeit im Inland abweicht,"@de .

dtd:LA-id1744 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Berufserfahrung, soweit dies erforderlich ist um festzustellen, ob eine festgestellte wesentliche Abweichung der Ausbildung oder Prfung von den Anforderungen der Fahrlehrer-Ausbildungsordnung und der Fahrlehrer-Prfungsordnung fr die Aufnahme der Fahrlehrerttigkeit im Inland durch die im Rahmen der Berufserfahrung erworbenen Kenntnisse im Sinne des  3 Absatz 2 Satz 1 ausgeglichen werden kann."@de .

dtd:LA-id1745 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Identittsnachweis,"@de .

dtd:LA-id1746 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine amtliche Bescheinigung darber, dass der Bewerber zum Zeitpunkt der Ausstellung der Bescheinigung in einem anderen Mitgliedstaat der Europischen Union, einem anderen Vertragsstaat des Abkommens ber den Europischen Wirtschaftsraum oder der Schweiz rechtmig als Fahrlehrer niedergelassen ist und dass ihm die Ausbung dieses Berufs zum Zeitpunkt der Ausstellung der Bescheinigung nicht, auch nicht vorbergehend, untersagt ist,"@de .

dtd:LA-id1747 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein amtlich beglaubigter Nachweis ber die Berufsqualifikation im Sinne von Artikel 3 Absatz 1 Buchstabe b der Richtlinie 2005/36/EG,"@de .

dtd:LA-id1748 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Fllen, in denen die Fahrlehrerttigkeit oder die Ausbildung zu diesem Beruf in dem Staat seiner Niederlassung nicht im Sinne von Artikel 3 Absatz 1 Buchstabe a oder e der Richtlinie 2005/36/EG reglementiert ist, eine Bescheinigung darber, dass die Ttigkeit des Fahrlehrers innerhalb der letzten zehn Jahre vor Ausstellung der Bescheinigung mindestens zwei Jahre lang im Staat seiner Niederlassung ausgebt wurde."@de .

dtd:LA-id1749 a dtd:LA ;
    dtd:has_Value "fr die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse BE mindestens zwlf Monate"@de .

dtd:LA-id1750 a dtd:LA ;
    dtd:has_Value "fr die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse A zustzlich zu der Ausbildung nach Nummer 1 mindestens einen Monat,"@de .

dtd:LA-id1751 a dtd:LA ;
    dtd:has_Value "fr die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse CE oder DE zustzlich zu der Ausbildung nach Nummer 1 mindestens zwei Monate."@de .

dtd:LA-id1752 a dtd:LA ;
    dtd:has_Value "fr die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse DE die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse CE,"@de .

dtd:LA-id1753 a dtd:LA ;
    dtd:has_Value "fr die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse CE die Fahrlehrerlaubnis der Fahrlehrerlaubnisklasse DE,"@de .

dtd:LA-id1754 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Erteilung der Fahrlehrerlaubnis,"@de .

dtd:LA-id1755 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach dreimaliger erfolgloser Lehrprobe im theoretischen oder im fahrpraktischen Unterricht ( 8 Absatz 2) oder"@de .

dtd:LA-id1756 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Ablauf der Frist."@de .

dtd:LA-id1757 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Namen,"@de .

dtd:LA-id1758 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vornamen,"@de .

dtd:LA-id1759 a dtd:LA ;

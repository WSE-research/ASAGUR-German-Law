    dtd:has_Value "Prüfung von Kontrollabläufen in der Fracht- und Postkontrolle (höchstens 30 Minuten)."@de .

dtd:LA-id3149 a dtd:LA ;
    dtd:has_B dtd:B-id602 ;
    dtd:has_DL dtd:DL-id2619 ;
    dtd:has_Size dtd:normal ;

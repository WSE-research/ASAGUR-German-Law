dtd:LA-id2921 a dtd:LA ;
    dtd:has_B dtd:B-id512 ;
    dtd:has_Size dtd:normal ;
    dtd:has_U dtd:U-id700 ;
    dtd:has_Value "an der Schulung gemäß Nummer                (alle Schulungen nennen) der Durchführungsverordnung (EU) 2015/1998 sowie den darauf beruhenden ergänzenden inhaltlichen und zeitlichen nationalen Vorgaben mit Erfolg teilgenommen."@de .

dtd:LA-id2922 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_U dtd:U-id701 ;
    dtd:has_Value "Es besteht bereits eine Vorausbildung gemäß Nummer                (sofern vorhanden, alle Schulungen aufzählen) der Durchführungsverordnung (EU) 2015/1998."@de .

dtd:LA-id2923 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ggf. Hinweis auf behördliche Genehmigung einer Teilschulung (§ 6 Absatz 17) Ort, Datum, Unterschrift Ausbilder, Zertifikatsnummer"@de ;

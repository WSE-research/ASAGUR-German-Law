    dtd:has_Value "Luftsicherheitskontrollpersonal nach Nummer 11.2.3.3 des Anhangs der Durchführungsverordnung (EU) 2015/1998"@de .

dtd:LA-id3135 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der theoretische Teil der Prüfung besteht aus 25 Fragen, davon 10 Basisfragen, und dauert 100 Minuten."@de .

dtd:LA-id3136 a dtd:LA ;
    dtd:has_B dtd:B-id592 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Luftsicherheitskontrollpersonal nach Nummer 11.2.3.4 des Anhangs der Durchführungsverordnung (EU) 2015/1998"@de .


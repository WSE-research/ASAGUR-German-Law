dtd:LA-id2175 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf den Binnenschifffahrtsstraßen für Sportboote von weniger als 20 Metern Länge, gemessen ohne Ruder und Bugspriet,"@de .

dtd:LA-id2176 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf den Seeschifffahrtsstraßen: für Sportboote ohne Längenbegrenzung."@de .

dtd:LA-id2177 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Binnenschifffahrtsstraßen:"@de .

dtd:LA-id2178 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bundeswasserstraßen im Sinne des § 1 Absatz 1 Nummer 2 des Binnenschifffahrtsaufgabengesetzes mit Ausnahme der Seeschifffahrtsstraßen und der Elbe im Hamburger Hafen."@de .

dtd:LA-id2179 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Seeschifffahrtsstraßen:"@de .

dtd:LA-id2180 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Seeschifffahrtsstraßen im Sinne des § 1 Absatz 1 der Seeschifffahrtsstraßenordnung und des § 1 der Verordnung zur Einführung der Schifffahrtsordnung Emsmündung."@de .

dtd:LA-id2181 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sportboote:"@de .

dtd:LA-id2182 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht gewerbsmäßig, für Sport- oder Freizeitzwecke verwendete Fahrzeuge, einschließlich Wassermotorräder, ausgenommen Fahrzeuge, die durch Muskelkraft oder nur mit einem Segel von höchstens 6 Quadratmeter Fläche fortbewegt werden."@de .

dtd:LA-id2183 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Schifferpatent für den Bodensee der Kategorien B oder C oder den Hochrhein nach der Bodensee-Schifffahrts-Ordnung,"@de .

dtd:LA-id2184 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein amtlicher Berechtigungsschein zum Führen eines mit Antriebsmaschine ausgerüsteten Fahrzeugs auf den Binnenschifffahrtsstraßen oder anderen Binnengewässern außerhalb der Seeschifffahrtsstraßen, der im Geltungsbereich dieser Verordnung nach anderen Vorschriften erteilt worden ist,"@de .

dtd:LA-id2185 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein amtlicher Berechtigungsschein zum Führen eines mit Antriebsmaschine ausgerüsteten Dienstfahrzeugs auf den Seeschifffahrtsstraßen, der im Geltungsbereich dieser Verordnung vor dem 1. April 1978 erteilt worden ist,"@de .

dtd:LA-id2186 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Befähigungszeugnisse der Gruppen A oder B der Schiffsbesetzungs- und Ausbildungsordnung vom 19. August 1970 (BGBl. I S. 1253), die vor dem 1. April 1978 erteilt worden sind,"@de .

dtd:LA-id2187 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fahrerlaubnisse oder Befähigungszeugnisse, die nach den Bestimmungen der Binnenschifferpatentverordnung vom 15. Dezember 1997 (BGBl. I S. 3066), die zuletzt durch Artikel 2 § 1 der Verordnung vom 21. September 2018 (BGBl. I S. 1398) geändert worden ist, zum Führen von Fahrzeugen berechtigen,"@de .

dtd:LA-id2188 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fahrerlaubnisse oder Befähigungszeugnisse, die nach den Bestimmungen der Rheinschiffs - personalverordnung (Anlage 1 zu § 1 Absatz 2 Nummer 1 der Rheinschiffspersonaleinführungsverordnung vom 5. April 2023 (BGBl. 2023 II Nr. 105, Anlageband)) in der jeweils geltenden und anzuwendenden Fassung zum Führen von Fahrzeugen berechtigen,"@de .

dtd:LA-id2189 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fahrerlaubnisse oder Befähigungszeugnisse, die nach den Bestimmungen der Binnenschiffspersonalverordnung vom 26. November 2021 (BGBl. I S. 4982) in der jeweils geltenden und anzuwendenden Fassung zum Führen von Fahrzeugen berechtigen."@de .

dtd:LA-id2190 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines im Geltungsbereich dieser Verordnung nach anderen Vorschriften erteilten amtlichen Befähigungsnachweises zum Führen eines Fahrzeugs mit Antriebsmaschine oder unter Segel auf Binnengewässern außerhalb der Seeschifffahrtsstraßen, sofern das Bundesministerium für Digitales und Verkehr diesen als Befähigungsnachweis anerkannt hat,"@de .

dtd:LA-id2191 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines Schifferpatents für den Bodensee der Kategorien A oder D nach der Bodensee-Schifffahrts-Ordnung,"@de .

dtd:LA-id2192 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines von einer als gemeinnützig anerkannten Körperschaft oder staatlichen Organisation erteilten Berechtigungsscheins zum Führen eines Wasserrettungsfahrzeugs, sofern das Bundesministerium für Digitales und Verkehr diesen als Befähigungsnachweis anerkannt hat."@de .

dtd:LA-id2193 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "amtlich vorgeschriebenen Befähigungsnachweis nach der Sportbootführerscheinverordnung-Binnen vom 21. März 1978 (BGBl. I S. 420), die zuletzt durch Artikel 48 Absatz 4 des Gesetzes vom 18. Februar 1986 (BGBl. I S. 265) geändert worden ist,"@de .

dtd:LA-id2194 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sportbootführerschein nach der Sportbootführerscheinverordnung-See in der Fassung der Bekanntmachung vom 19. März 2003 (BGBl. I S. 367), die zuletzt durch Artikel 4 Absatz 133 des Gesetzes vom 18. Juli 2016 (BGBl. I S. 1666) geändert worden ist, wenn er vor dem 1. April 1978, im Land Berlin vor dem 1. April 1989, erteilt worden ist,"@de .

dtd:LA-id2195 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Motorbootführerschein nach der Motorbootführerscheinverordnung vom 17. Januar 1967 (BGBl. 1967 II S. 731), die durch Artikel 1 der Verordnung vom 21. Oktober 1968 (BGBl. 1968 II S. 1107) geändert worden ist,"@de .

dtd:LA-id2196 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sportbootführerschein-Binnen nach der Sportbootführerscheinverordnung-Binnen vom 22. März 1989 (BGBl. I S. 536, 1102), die zuletzt durch Artikel 4 Absatz 122 des Gesetzes vom 18. Juli 2016 (BGBl. I S. 1666) geändert worden ist,"@de .

dtd:LA-id2197 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Befähigungsnachweis für das Führen von Sport- und Hausbooten in dem Fahrtbereich Binnengewässer, der nach den Vorschriften der Deutschen Demokratischen Republik erteilt worden ist."@de .

dtd:LA-id2198 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sportbootführerschein mit dem Geltungsbereich Binnenschifffahrtsstraßen nach dieser Verordnung, auch wenn dieser eine Beschränkung der Fahrzeuglänge auf < 15 m auf dem Rhein enthält."@de .

dtd:LA-id2199 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Befähigungszeugnis zum Kapitän, ein Befähigungszeugnis zum nautischen Schiffsoffizier oder ein Befähigungsnachweis zum Schiffsmechaniker oder"@de .

dtd:LA-id2200 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein im Geltungsbereich dieser Verordnung nach anderen Vorschriften erteilter amtlicher Befähigungsnachweis zum Führen eines Fahrzeugs auf den Seeschifffahrtsstraßen, soweit das Bundesministerium für Digitales und Verkehr diesen als Befähigungsnachweis anerkannt hat."@de .

dtd:LA-id2201 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Motorbootführerschein nach der Motorbootführerscheinverordnung vom 17. Januar 1967 (BGBl. 1967 II S. 731), die durch Artikel 1 der Verordnung vom 21. Oktober 1968 (BGBl. II S. 1107) geändert worden ist,"@de .

dtd:LA-id2202 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sportbootführerschein-See nach der Sportbootführerscheinverordnung-See in der Fassung der Bekanntmachung vom 19. März 2003 (BGBl. I S. 367), die zuletzt durch Artikel 4 Absatz 133 des Gesetzes vom 18. Juli 2016 (BGBl. I S. 1666) geändert worden ist,"@de .

dtd:LA-id2203 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Befähigungsnachweis für das Führen von Sportbooten mit dem Fahrtbereich Seewasserstraßen, Küstenfahrt und Seefahrt, der nach den Vorschriften der Deutschen Demokratischen Republik erteilt worden ist."@de .

dtd:LA-id2204 a dtd:LA ;
    dtd:has_DL dtd:DL-id1797 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf allen Binnenschifffahrtsstraßen und auf den Seeschifffahrtsstraßen Personen beim Führen eines Sportbootes, sofern das zu führende Sportboot mit einer Antriebsmaschine ausgerüstet ist, deren größte nicht überschreitbare Nutzleistung bei Verwendung eines a)Verbrennungsmotors höchstens 11,03 Kilowatt,b)Elektromotors höchstens 7,5 Kilowatt in der Betriebsart S1 (Dauerbetrieb) nach DIN EN 60034-1: Ausgabe Februar 2011 beträgt,"@de .

dtd:LA-id2205 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verbrennungsmotors höchstens 11,03 Kilowatt,"@de .

dtd:LA-id2206 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Elektromotors höchstens 7,5 Kilowatt in der Betriebsart S1 (Dauerbetrieb) nach DIN EN 60034-1: Ausgabe Februar 2011 beträgt,"@de .

dtd:LA-id2207 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen beim Führen eines Segelsurfbretts,"@de .

dtd:LA-id2208 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen mit Wohnsitz außerhalb des Geltungsbereichs dieser Verordnung, die sich nicht länger als ein Jahr im Geltungsbereich dieser Verordnung aufhalten, sofern im Wohnsitzstaat keine Fahrerlaubnis für das zu führende Sportboot erforderlich ist,"@de .

dtd:LA-id2209 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Inhaber eines von einer als gemeinnützig anerkannten Körperschaft erteilten Befähigungsnachweises beim Führen von Wasserrettungsfahrzeugen in dem jeweiligen Geltungsbereich."@de .

dtd:LA-id2210 a dtd:LA ;
    dtd:has_DL dtd:DL-id1811 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für das Führen a)eines Sportbootes mit Antriebsmaschine mindestens 16 Jahre alt sein,b)eines Sportbootes unter Segel mindestens 14 Jahre alt sein,"@de .

dtd:LA-id2211 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines Sportbootes mit Antriebsmaschine mindestens 16 Jahre alt sein,"@de .

dtd:LA-id2212 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines Sportbootes unter Segel mindestens 14 Jahre alt sein,"@de .

dtd:LA-id2213 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Führen eines Sportbootes körperlich und psychisch (medizinisch) tauglich sein,"@de .

dtd:LA-id2214 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zuverlässig sein,"@de .

dtd:LA-id2215 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die erforderliche Befähigung in einer Prüfung nach § 8 nachgewiesen haben."@de .

dtd:LA-id2216 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Bescheinigung über das ausreichende Sehvermögen einer nach § 67 der Fahrerlaubnis-Verordnung anerkannten Sehteststelle unter Einhaltung der DIN 58220 Ausgabe September 2013 und"@de .

dtd:LA-id2217 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Bescheinigung über das Hörvermögen eines in der Handwerksrolle eingetragenen Hörakustikerbetriebs"@de .

dtd:LA-id2218 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "rechtskräftige Verurteilung wegen Gefährdung des Schiffsverkehrs,"@de .

dtd:LA-id2219 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wiederholte, mit Geldbuße geahndete Zuwiderhandlungen gegen schifffahrtspolizeiliche Vorschriften,"@de .

dtd:LA-id2220 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "rechtskräftige Verurteilung wegen Verstoßes gegen andere Verkehrsstraftatbestände,"@de .

dtd:LA-id2221 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Einzelfall rechtskräftige Verurteilung wegen Verstoßes gegen andere Straftatbestände oder wiederholte, mit Geldbuße geahndete erhebliche Zuwiderhandlungen gegen andere verkehrsrechtliche Vorschriften, soweit daraus ein Rückschluss auf das künftige Verhalten des Bewerbers im Schiffsverkehr zu ziehen ist, oder"@de .

dtd:LA-id2222 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kenntnis von der Teilnahme am Verkehr unter dem Einfluss von Alkohol oder anderer berauschender Mittel auch ohne abgeschlossene Straf- oder Bußgeldverfahren."@de .

dtd:LA-id2223 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vor- und Nachname, Geschlecht, Geburtsdatum, Geburtsort, Nationalität und Anschrift des Bewerbers,"@de .

dtd:LA-id2224 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Art der Fahrerlaubnis, die erworben werden soll,"@de .

dtd:LA-id2225 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein aktuelles Passbild in der Größe 35 x 45 Millimeter, das den Bewerber ohne Kopfbedeckung zeigt,"@de .

dtd:LA-id2226 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen medizinischen Tauglichkeitsnachweis nach dem Muster nach Anhang 1 der Anlage 2, der vom untersuchenden Arzt unmittelbar dem Vorsitzenden des zuständigen Prüfungsausschusses in einem verschlossenen Umschlag und in Abschrift dem Antragsteller zuzuleiten ist, oder eine Kopie des amtlichen Sportbootführerscheins für den jeweils anderen Geltungsbereich oder die andere Antriebsart, wenn dieser durch Prüfung erworben worden und zum Zeitpunkt der Antragstellung nicht älter als ein Jahr ist,"@de .

dtd:LA-id2227 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kopie eines gültigen amtlichen Kraftfahrzeugführerscheins oder auf Verlangen des Prüfungsausschusses ein Führungszeugnis nach den Vorschriften des Bundeszentralregistergesetzes, wenn ein gültiger amtlicher Kraftfahrzeugführerschein nicht vorgelegt wird,"@de .

dtd:LA-id2228 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Erklärung, ob dem Bewerber die Fahrerlaubnis für Sportboote bereits ein- oder mehrmals entzogen worden ist,"@de .

dtd:LA-id2229 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Bewerbern, die noch nicht 18 Jahre alt sind, die Zustimmung des gesetzlichen Vertreters (§ 6 Absatz 1 Satz 2),"@de .

dtd:LA-id2230 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit erforderlich, eine ärztliche Bescheinigung einer Legasthenie oder Unterlagen wie Atteste, ärztliche Bescheinigungen, Schulzeugnisse oder Gutachten, die zur Glaubhaftmachung nicht ausreichender Deutschkenntnisse geeignet sind,"@de .

dtd:LA-id2231 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit erteilt, eine Kopie des amtlichen Sportbootführerscheins, der zur Befreiung von Prüfungsteilen am Prüfungstag vor Beginn der Prüfung im Original vorzulegen ist,"@de .

dtd:LA-id2232 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ort und Datum der gewünschten Prüfung,"@de .

dtd:LA-id2233 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall eines elektronischen Verfahrens eine E-Mail-Adresse,"@de .

dtd:LA-id2234 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "freiwillig eine Telefonnummer."@de .

dtd:LA-id2235 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn die Voraussetzungen nach den Absätzen 1 und 2 erfüllt sind und"@de .

dtd:LA-id2236 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der angeforderte Vorschuss für die voraussichtlich entstehenden Gebühren bezahlt worden ist."@de .

dtd:LA-id2237 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn der Prüfer die Bewerber zuvor persönlich geschult hat oder"@de .

dtd:LA-id2238 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bewerber in einer Ausbildungsstätte ausgebildet worden sind, der der Prüfer angehört."@de .

dtd:LA-id2239 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Prüfungsausschuss [Name] des Deutschen Motoryachtverbandes e. V. für den amtlichen Sportbootführerschein,"@de .

dtd:LA-id2240 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Prüfungsausschuss [Name] des Deutschen Segler-Verbandes e. V. für den amtlichen Sportbootführerschein oder"@de .

dtd:LA-id2241 a dtd:LA ;
    dtd:has_NB dtd:NB-id803 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Prüfungsausschuss [Name] des Deutschen Motoryachtverbandes e. V. und des Deutschen Segler-Verbandes e. V. für den amtlichen Sportbootführerschein."@de .

dtd:LA-id2242 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Prüfertätigkeit geeignet und zuverlässig sein,"@de .

dtd:LA-id2243 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Führen eines Sportbootes medizinisch tauglich sein,"@de .

dtd:LA-id2244 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ausreichende Kenntnisse und Fähigkeiten zu den in den Prüfungsteilen abgefragten Themen besitzen und"@de .

dtd:LA-id2245 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gewähr bieten, dass die Hoheitsaufgaben nach Maßgabe dieser Verordnung und nach Maßgabe der zu ihrer Durchführung erlassenen Richtlinien ordnungsgemäß ausgeführt werden."@de .

dtd:LA-id2246 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens einen Sportbootführerschein mit dem Geltungsbereich Binnenschifffahrtsstraßen oder einen Sportbootführerschein mit dem Geltungsbereich Seeschifffahrtsstraßen besitzen und"@de .

dtd:LA-id2247 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für eine Bestellung als Prüfer erforderlichen Voraussetzungen nach der Anlage 6 erfüllen."@de .

dtd:LA-id2248 a dtd:LA ;
    dtd:has_DL dtd:DL-id1893 ;
    dtd:has_Value "mehrfach ein Sportboot geführt hat a)mit 0,25 Milligramm oder mehr Alkohol je Liter Atemluft oder mit 0,5 Promille oder mehr Alkohol im Blut oder mit einer Alkoholmenge im Körper, die zu einer solchen Atem- oder Blutalkoholkonzentration führt, oderb)unter erheblicher Einwirkung berauschender Mittel oder"@de .

dtd:LA-id2249 a dtd:LA ;
    dtd:has_Value "mit 0,25 Milligramm oder mehr Alkohol je Liter Atemluft oder mit 0,5 Promille oder mehr Alkohol im Blut oder mit einer Alkoholmenge im Körper, die zu einer solchen Atem- oder Blutalkoholkonzentration führt, oder"@de .

dtd:LA-id2250 a dtd:LA ;
    dtd:has_Value "unter erheblicher Einwirkung berauschender Mittel oder"@de .

dtd:LA-id2251 a dtd:LA ;
    dtd:has_Value "mehrfach eine vorgeschriebene Höchstgeschwindigkeit überschritten hat."@de .

dtd:LA-id2252 a dtd:LA ;
    dtd:has_Value "der Grund der Sicherstellung weggefallen ist oder"@de .

dtd:LA-id2253 a dtd:LA ;
    dtd:has_Value "die nach § 16 Absatz 3 Satz 1 zuständige Behörde die Erlaubnis nicht entzieht oder deren Ruhen nicht anordnet."@de .


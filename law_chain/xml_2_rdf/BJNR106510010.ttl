dtd:P-id3484 a dtd:P ;
    dtd:has_BR dtd:BR-id461 ;
    dtd:has_Value " (+++ Textnachweis ab: 6.8.2010 +++)(+++ Amtlicher Hinweis des Normgebers auf EG-Recht:     Umsetzung der       EGRL 50/2008            (CELEX Nr: 32008L0050)       EGRL 107/2004           (CELEX Nr: 32004L0107)       EGRL 81/2001            (CELEX Nr: 32001L0081) +++)"@de ;

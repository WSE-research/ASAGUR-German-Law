dtd:LA-id3389 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Es gelten die folgenden oberen und unteren Beurteilungsschwellen: ArsenKadmiumNickelB(a)PObere Beurteilungsschwelle in Prozent des Zielwerts60 % (3,6 ng/m3)60 % (3 ng/m3)70 % (14 ng/m3)60 % (0,6 ng/m3)Untere Beurteilungsschwelle in Prozent des Zielwerts40 % (2,4 ng/m3)40 % (2 ng/m3)50 % (10 ng/m3)40 % (0,4 ng/m3)"@de ;

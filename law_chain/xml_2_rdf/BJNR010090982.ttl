dtd:LA-id115 a dtd:LA ;
    dtd:has_DL dtd:DL-id87 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "a)Familienheimen im Sinne des § 7 des Zweiten Wohnungsbaugesetzes, b)Eigentumswohnungen, bei denen die Voraussetzungen des § 7 des Zweiten Wohnungsbaugesetzes vorliegen, "@de .


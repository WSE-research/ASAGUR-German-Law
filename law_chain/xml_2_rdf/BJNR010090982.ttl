    dtd:has_Size dtd:normal ;
    dtd:has_Value "am 1. Oktober 1982 und"@de .

dtd:LA-id117 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Familienheimen im Sinne des § 7 des Zweiten Wohnungsbaugesetzes,"@de .

dtd:LA-id118 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Eigentumswohnungen, bei denen die Voraussetzungen des § 7 des Zweiten Wohnungsbaugesetzes vorliegen,"@de .


dtd:LA-id485 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Anschlussnutzer spätestens im Zeitpunkt der erstmaligen Entnahme einen Vertrag über den Bezug von Elektrizität abgeschlossen hat oder die Voraussetzungen einer Ersatzversorgung nach § 38 des Energiewirtschaftsgesetzes vorliegen und"@de .

dtd:LA-id486 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Anschlussnutzer oder dessen Lieferanten ein Recht auf Netzzugang nach § 20 des Energiewirtschaftsgesetzes zusteht."@de .

dtd:LA-id487 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben zum Anschlussnehmer oder -nutzer (Firma, Registergericht, Registernummer, Familienname, Vorname, Geburtstag, Adresse, Kundennummer),"@de .

dtd:LA-id488 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anlagenadresse und Bezeichnung des Zählers oder des Aufstellungsorts des Zählers,"@de .

dtd:LA-id489 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben zum Netzbetreiber (Firma, Registergericht, Registernummer und Adresse) und"@de .

dtd:LA-id490 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegenüber dem Anschlussnehmer auch die am Ende des Netzanschlusses vorzuhaltende Leistung."@de .

dtd:LA-id491 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Amtlicher Hinweis: Zu beziehen beim Beuth Verlag GmbH, Berlin."@de .

dtd:LA-id492 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Herstellung des Netzanschlusses,"@de .

dtd:LA-id493 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Änderungen des Netzanschlusses, die durch eine Änderung oder Erweiterung der Kundenanlage erforderlich oder aus anderen Gründen vom Anschlussnehmer veranlasst werden,"@de .

dtd:LA-id494 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die an das Elektrizitätsversorgungsnetz angeschlossen sind,"@de .

dtd:LA-id495 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die vom Eigentümer in wirtschaftlichem Zusammenhang mit einem an das Netz angeschlossenen Grundstück genutzt werden oder"@de .

dtd:LA-id496 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die die Möglichkeit des Netzanschlusses sonst wirtschaftlich vorteilhaft ist."@de .

dtd:LA-id497 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach den Umständen nicht rechtzeitig möglich ist und der Netzbetreiber dies nicht zu vertreten hat oder"@de .

dtd:LA-id498 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beseitigung von bereits eingetretenen Unterbrechungen verzögern würde."@de .

dtd:LA-id499 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hinsichtlich eines Vermögensschadens widerleglich vermutet, dass Vorsatz oder grobe Fahrlässigkeit vorliegt,"@de .

dtd:LA-id500 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hinsichtlich der Beschädigung einer Sache widerleglich vermutet, dass Vorsatz oder Fahrlässigkeit vorliegt."@de .

dtd:LA-id501 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "2,5 Millionen Euro bei bis zu 25.000 an das eigene Netz angeschlossenen Anschlussnutzern;"@de .

dtd:LA-id502 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "10 Millionen Euro bei 25.001 bis 100.000 an das eigene Netz angeschlossenen Anschlussnutzern;"@de .

dtd:LA-id503 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "20 Millionen Euro bei 100.001 bis 200.000 an das eigene Netz angeschlossenen Anschlussnutzern;"@de .

dtd:LA-id504 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "30 Millionen Euro bei 200.001 bis einer Million an das eigene Netz angeschlossenen Anschlussnutzern;"@de .

dtd:LA-id505 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "40 Millionen Euro bei mehr als einer Million an das eigene Netz angeschlossenen Anschlussnutzern."@de .

dtd:LA-id506 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine unmittelbare Gefahr für die Sicherheit von Personen oder Sachen von erheblichem Wert abzuwenden,"@de .

dtd:LA-id507 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anschlussnutzung ohne Messeinrichtung, unter Umgehung, Beeinflussung oder vor Anbringung der Messeinrichtungen zu verhindern oder"@de .

dtd:LA-id508 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu gewährleisten, dass Störungen anderer Anschlussnehmer oder -nutzer oder störende Rückwirkungen auf Einrichtungen des Netzbetreibers oder Dritter ausgeschlossen sind."@de .


dtd:entry-id503 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Der Betrag der Bankgarantie oder des gleichwertigen Finanzinstruments wird auf der Grundlage der im vorausgegangenen Jahr von dem EETS-Anbieter für das EETS-Gebiet BFStrMG insgesamt gezahlten Summe für Mauttransaktionen und Zahlungen gemäß § 19 Absatz 1 MautSysG in Abstimmung mit dem Mauterheber bestimmt."@de ;
    dtd:has_colname "col3"@de .


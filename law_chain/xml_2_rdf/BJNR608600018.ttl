dtd:entry-id494 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Die folgenden Kriterien müssen im Fall einer Bankgarantie erfüllt werden:"@de ;
    dtd:has_colname "col3"@de .

dtd:entry-id495 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value ""@de ;
    dtd:has_colname "col1"@de .


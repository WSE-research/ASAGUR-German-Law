dtd:LA-id1809 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "markscheiderische und sonstige vermessungstechnische Arbeiten im Zusammenhang mit Tätigkeiten und Einrichtungen nach § 2 des Bundesberggesetzes,"@de .

dtd:LA-id1810 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Messungen zur Erfassung von bergbaubedingten Bodenbewegungen."@de .

dtd:LA-id1811 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "diejenigen Auszüge aus dem Risswerk oder andere auf der Grundlage des Risswerks angefertigte rissliche Darstellungen, die den Anträgen auf Zulassung von Betriebsplänen oder sonstigen sicherheitlich bedeutsamen Anträgen beizufügen sind, zum Zeitpunkt der Antragstellung vollständig nachgetragen sind und im Übrigen mit den Eintragungen im Risswerk übereinstimmen und"@de .

dtd:LA-id1812 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Risswerk bis zum Ende der Bergaufsicht vollständig nachgetragen und abgeschlossen wird; soweit die Bergaufsicht über Teile des Betriebes endet, kann für diese auf Antrag des Unternehmers und Zustimmung der zuständigen Behörde entsprechend verfahren werden."@de .

dtd:LA-id1813 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Schutz Beschäftigter oder Dritter vor Gefahren im Betrieb,"@de .

dtd:LA-id1814 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Schutz der Oberfläche im Interesse der persönlichen Sicherheit oder des öffentlichen Verkehrs oder"@de .

dtd:LA-id1815 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Durchführung der Bergaufsicht."@de .

dtd:LA-id1816 a dtd:LA ;
    dtd:has_DL dtd:DL-id1030 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Personen nach § 2 Absatz 4 Satz 1 a)rechtzeitig die Mitteilungen und Unterlagen erhalten, die zur Erfüllung ihrer Aufgaben erforderlich sind, undb)auch vor Aufnahme der bergbaulichen Tätigkeit insbesondere einbezogen werden bei der Erstellung der Unterlagen für aa)die Zulassung von Betriebsplänen,bb)die Risswerkführung odercc)Betriebsanlagen und Betriebseinrichtungen, die für die Sicherheit bedeutsam sind,"@de .

dtd:LA-id1817 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "rechtzeitig die Mitteilungen und Unterlagen erhalten, die zur Erfüllung ihrer Aufgaben erforderlich sind, und"@de .

dtd:LA-id1818 a dtd:LA ;
    dtd:has_DL dtd:DL-id1035 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auch vor Aufnahme der bergbaulichen Tätigkeit insbesondere einbezogen werden bei der Erstellung der Unterlagen für aa)die Zulassung von Betriebsplänen,bb)die Risswerkführung odercc)Betriebsanlagen und Betriebseinrichtungen, die für die Sicherheit bedeutsam sind,"@de .

dtd:LA-id1819 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zulassung von Betriebsplänen,"@de .

dtd:LA-id1820 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Risswerkführung oder"@de .

dtd:LA-id1821 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebsanlagen und Betriebseinrichtungen, die für die Sicherheit bedeutsam sind,"@de .

dtd:LA-id1822 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Lage von Grubenbauen oder anderen Gegenständen, die vor der Vermessung unzugänglich geworden sind, schriftlich oder zeichnerisch so beschrieben wird, dass nach diesen Angaben eine möglichst genaue Darstellung im Risswerk erfolgen kann,"@de .

dtd:LA-id1823 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grubenbaue oder andere Gegenstände nach Nummer 2 unverzüglich vermessen und dargestellt werden, sobald dies wieder möglich wird."@de .

dtd:LA-id1824 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen übertägigen Gewinnungsbetrieb,"@de .

dtd:LA-id1825 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Aufsuchungs- oder Gewinnungsbetrieb mit Bohrungen von über Tage, durch den keine untertägigen Hohlräume außerhalb des Bohrlochs hergestellt werden,"@de .

dtd:LA-id1826 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Porenspeicher oder"@de .

dtd:LA-id1827 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Betrieb zur Gewinnung in alten Halden"@de .

dtd:LA-id1828 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gefährliche Bodenbewegungen einschließlich Böschungsbewegungen und damit zusammenhängende Bergschäden nach allgemeiner Erfahrung nicht zu erwarten sind,"@de .

dtd:LA-id1829 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine weiträumige Grundwasserabsenkung nicht verursacht wird,"@de .

dtd:LA-id1830 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Beeinträchtigung weder durch noch für benachbarte Betriebe, auch stillgelegte, eintreten kann,"@de .

dtd:LA-id1831 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für den Betrieb in Anspruch genommenen Flächen, die Anordnung und der räumliche Zusammenhang der Betriebsanlagen und Betriebseinrichtungen so beschaffen sind, dass nachteilige Auswirkungen auf die ordnungsgemäße Leitung des Betriebes und eine Erschwerung der Bergaufsicht nicht zu besorgen sind,"@de .

dtd:LA-id1832 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Wiedernutzbarmachung der Oberfläche der Wiedernutzbarmachungsriss nach Anlage 3 Teil 2 Nummer 15 ausreicht,"@de .

dtd:LA-id1833 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bodenschätze, deren Schutz im öffentlichen Interesse liegt, nicht beeinträchtigt werden können,"@de .

dtd:LA-id1834 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die technische Ausführung und Komplexität der Betriebsanlagen und Betriebseinrichtungen in Verbindung mit der Sicherheit der Oberfläche es zulassen,"@de .

dtd:LA-id1835 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einträge von Stoffen aus Halden, Schlamm- und Klärteichen in den Boden oder das Grundwasser, die zu schädlichen Boden- oder Gewässerveränderungen führen können, nicht stattgefunden haben und nicht zu besorgen sind."@de .

dtd:LA-id1836 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einem übertägigen Gewinnungsbetrieb anstelle des Tagerisses eine besondere rissliche Darstellung anfertigen und nachtragen zu lassen, in der die Angaben nach Anlage 3 Teil 2 Nummer 2 Buchstabe c Doppelbuchstabe aa bis ff, Nummer 2 Buchstabe e, Nummer 2 Buchstabe f, Nummer 3 Buchstabe a Doppelbuchstabe bb, cc, ee, ff und hh, Nummer 4 Buchstabe a Doppelbuchstabe ii, Nummer 7 Buchstabe a Doppelbuchstabe bb bis dd und Nummer 7 Buchstabe b Satz 2 einzutragen sind,"@de .

dtd:LA-id1837 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einem Aufsuchungs- oder Gewinnungsbetrieb mit Bohrungen von über Tage oder bei einem Porenspeicher eine besondere rissliche Darstellung anfertigen und nachtragen zu lassen, in die die Angaben nach Anlage 3 Teil 2 Nummer 2 Buchstabe c Doppelbuchstabe aa bis ff, Nummer 2 Buchstabe e, Nummer 2 Buchstabe f, Nummer 10 Buchstabe b bis f und Nummer 14 Buchstabe a einzutragen sind,"@de .

dtd:LA-id1838 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einem Betrieb zur Gewinnung in alten Halden eine besondere rissliche Darstellung anfertigen und nachtragen zu lassen, in die die Angaben nach Anlage 3 Teil 2 Nummer 2 Buchstabe c Doppelbuchstabe aa bis ff, Nummer 2 Buchstabe e, Nummer 2 Buchstabe f und Nummer 13 Buchstabe a einzutragen sind."@de .

dtd:LA-id1839 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "körperlich geeignet ist und keine Tatsachen vorliegen, die ihn für die Tätigkeit als unzuverlässig erscheinen lassen,"@de .

dtd:LA-id1840 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine in einem Mitgliedstaat der Europäischen Union, einem Vertragsstaat des Europäischen Wirtschaftsraums oder der Schweiz anerkannte Abschlussprüfung in einer markscheiderischen oder vermessungstechnischen Fachrichtung an einer Universität, Technischen Hochschule, Technischen Fachhochschule oder Technikerschule erfolgreich abgelegt oder eine als gleichwertig anerkannte Berufsqualifikation im Ausland erworben oder in anderer Weise, insbesondere durch eine einschlägige, als gleichwertig anerkannte Berufsausbildung, eine vergleichbare überdurchschnittliche Fachkunde erworben hat,"@de .

dtd:LA-id1841 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die erforderlichen Kenntnisse und Fertigkeiten für die Tätigkeit nachweist."@de .

dtd:LA-id1842 a dtd:LA ;
    dtd:has_DL dtd:DL-id1091 ;
    dtd:has_Value "der zuständigen Behörde a)die Übernahme und die Niederlegung von Arbeiten nach § 1 Nummer 1,b)die jeweilige Anschrift ihrer Arbeitsräumeunverzüglich anzuzeigen,"@de .

dtd:LA-id1843 a dtd:LA ;
    dtd:has_Value "die Übernahme und die Niederlegung von Arbeiten nach § 1 Nummer 1,"@de .

dtd:LA-id1844 a dtd:LA ;
    dtd:has_Value "die jeweilige Anschrift ihrer Arbeitsräume"@de .

dtd:LA-id1845 a dtd:LA ;
    dtd:has_DL dtd:DL-id1096 ;
    dtd:has_Value "ein Verzeichnis der a)Risswerke, die sie zu bearbeiten oder aufzubewahren haben, einschließlich der für die Anfertigung und Nachtragung verwendeten Unterlagen,b)Instrumente und Geräte einschließlich eines Nachweises über das Ergebnis der Überprüfungenzu führen,"@de .

dtd:LA-id1846 a dtd:LA ;
    dtd:has_Value "Risswerke, die sie zu bearbeiten oder aufzubewahren haben, einschließlich der für die Anfertigung und Nachtragung verwendeten Unterlagen,"@de .

dtd:LA-id1847 a dtd:LA ;
    dtd:has_Value "Instrumente und Geräte einschließlich eines Nachweises über das Ergebnis der Überprüfungen"@de .

dtd:LA-id1848 a dtd:LA ;
    dtd:has_Value "Aufzeichnungen über Vorgänge im Zusammenhang mit Arbeiten nach § 1 Nummer 1, denen die Mitteilungen und Unterlagen nach § 11 Nummer 1 beizufügen sind, sowie über die Erledigung der Arbeiten anzufertigen und mindestens 5 Jahre nach der letzten Eintragung aufzubewahren,"@de .

dtd:LA-id1849 a dtd:LA ;
    dtd:has_DL dtd:DL-id1101 ;
    dtd:has_Value "bis zum 1. Februar eines jeden Jahres für das vergangene Kalenderjahr der zuständigen Behörde einen Bericht einzureichen über a)Messungen von besonderer Bedeutung und ihre Ergebnisse,b)Bestand des Risswerks sowie Stand und Besonderheiten bei seiner Anfertigung und Nachtragung,c)Neuerungen und Besonderheiten hinsichtlich der Instrumente und Geräte,d)Anzahl der Mitarbeiter mit Angabe der fachlichen Ausbildung und der von ihnen wahrgenommenen Aufgaben."@de .

dtd:LA-id1850 a dtd:LA ;
    dtd:has_Value "Messungen von besonderer Bedeutung und ihre Ergebnisse,"@de .

dtd:LA-id1851 a dtd:LA ;
    dtd:has_Value "Bestand des Risswerks sowie Stand und Besonderheiten bei seiner Anfertigung und Nachtragung,"@de .

dtd:LA-id1852 a dtd:LA ;
    dtd:has_Value "Neuerungen und Besonderheiten hinsichtlich der Instrumente und Geräte,"@de .

dtd:LA-id1853 a dtd:LA ;
    dtd:has_Value "Anzahl der Mitarbeiter mit Angabe der fachlichen Ausbildung und der von ihnen wahrgenommenen Aufgaben."@de .

dtd:LA-id1854 a dtd:LA ;
    dtd:has_Value "eine zuverlässige Vorhersage über Ausdehnung, Größe und zeitlichen Ablauf zu erwartender Einwirkungen auf die Oberfläche durch Bergbaubetriebe und ihre Auswirkungen auf bauliche Anlagen ermöglicht wird und"@de .

dtd:LA-id1855 a dtd:LA ;
    dtd:has_Value "eingetretene Einwirkungen dieser Art in gleicher Hinsicht zuverlässig beobachtet werden können."@de .

dtd:LA-id1856 a dtd:LA ;
    dtd:has_Value "nach Art, Umfang und Ablauf der Gewinnung und nach Art, Beschaffenheit und Ausdehnung der Lagerstätte sowie der diese umgebenden Gebirgsschichten und"@de .

dtd:LA-id1857 a dtd:LA ;
    dtd:has_Value "nach den geologischen Gegebenheiten, insbesondere den tektonischen oder hydrologischen, oder den gebirgsmechanischen oder bodenmechanischen Vorgängen"@de .

dtd:LA-id1858 a dtd:LA ;
    dtd:has_B dtd:B-id259 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vermessungen über Tage"@de .

dtd:LA-id1859 a dtd:LA ;
    dtd:has_SP dtd:SP-id287 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anschlussmessungen"@de .

dtd:LA-id1860 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anschlussmessungen an das amtliche Netz sind so durchzuführen, dass bei allen Punkten eine Lagegenauigkeit von ± 50 mm und eine Höhengenauigkeit von ± 30 mm eingehalten wird."@de .

dtd:LA-id1861 a dtd:LA ;
    dtd:has_SP dtd:SP-id288 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Messungen im Festpunktnetz"@de .

dtd:LA-id1862 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bei Lage- und Höhenmessungen ist eine Genauigkeit von mindestens ± 300 mm einzuhalten."@de .

dtd:LA-id1863 a dtd:LA ;
    dtd:has_SP dtd:SP-id289 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Höhenfestpunktriss"@de .

dtd:LA-id1864 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Messungen für den Höhenfestpunktriss sind mit der Genauigkeit auszuführen, die für Messungen von bergbaubedingten Bodenbewegungen in der Klasse II anzuwenden ist (siehe Nummern 3.4 und 3.5)."@de .

dtd:LA-id1865 a dtd:LA ;
    dtd:has_SP dtd:SP-id290 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bestimmung des Einwirkungswinkels, Grenzwinkels oder Einwirkungsbereichs nach der Einwirkungsbereichs-Bergverordnung"@de .


dtd:LA-id1147 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Allokation“ ist die Zuordnung von Gasmengen zu einem Bilanzkreis;"@de .

dtd:LA-id1148 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Ausgleichsenergie“ ist die Energiemenge, die zum Ausgleich des Saldos aller Ein- und Ausspeisungen in einem Bilanzkreis am Ende der Bilanzierungsperiode rechnerisch benötigt wird;"@de .

dtd:LA-id1149 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Ausspeiseleistung“ ist die vom Netzbetreiber an einem Ausspeisepunkt für den Transportkunden vorgehaltene maximale Leistung in Kilowattstunde pro Stunde;"@de .

dtd:LA-id1150 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Bilanzkreis“ ist die Zusammenfassung von Einspeise- und Ausspeisepunkten, die dem Zweck dient, Einspeisemengen und Ausspeisemengen zu saldieren und die Abwicklung von Handelstransaktionen zu ermöglichen;"@de .

dtd:LA-id1151 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Bilanzkreisverantwortlicher“ ist eine natürliche oder juristische Person, die gegenüber dem Marktgebietsverantwortlichen für die Abwicklung des Bilanzkreises verantwortlich ist;"@de .

dtd:LA-id1152 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Buchung“ ist das Erwerben von Kapazitätsrechten;"@de .

dtd:LA-id1153 a dtd:LA ;
    dtd:has_FnR dtd:FnR-id619 ;
    dtd:has_SUB dtd:SUB-id618 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Brennwert „Hs,n“ “ ist die nach DIN EN ISO 6976 (Ausgabe: September 2005) bei vollständiger Verbrennung freiwerdende Wärme in Kilowattstunde pro Normkubikmeter oder in Megajoule pro Normkubikmeter;"@de .

dtd:LA-id1154 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Datenformat“ ist eine für die elektronische Weiterverarbeitung oder Veröffentlichung geeignete und standardisierte Formatvorgabe für die Datenkommunikation, welche die relevanten Parameter enthält;"@de .

dtd:LA-id1155 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Einspeiser von Biogas“ ist jede juristische oder natürliche Person, die am Einspeisepunkt im Sinne von § 3 Nummer 13b des Energiewirtschaftsgesetzes Biogas in ein Netz oder Teilnetz eines Netzbetreibers einspeist;"@de .

dtd:LA-id1156 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Einspeiseleistung“ ist die vom Netzbetreiber an einem Einspeisepunkt für den Transportkunden vorgehaltene maximale Leistung in Kilowattstunde pro Stunde;"@de .

dtd:LA-id1157 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Marktgebiet“ ist die Zusammenfassung gleichgelagerter und nachgelagerter Netze, in denen Transportkunden gebuchte Kapazitäten frei zuordnen, Gas an Letztverbraucher ausspeisen und in andere Bilanzkreise übertragen können;"@de .

dtd:LA-id1158 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id1159 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Realisierungsfahrplan“ ist ein gemeinsamer Plan von Netzbetreiber und Anschlussnehmer oder Anschlusswilligem über Inhalt, zeitliche Abfolge und Verantwortlichkeit für die einzelnen Schritte zur Herstellung des Netzanschlusses oder zum Kapazitätsausbau, um die einzelnen Schritte der Beteiligten miteinander zu synchronisieren;"@de .

dtd:LA-id1160 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Regelenergie“ sind die Gasmengen, die vom Netzbetreiber zur Gewährleistung der Netzstabilität eingesetzt werden;"@de .

dtd:LA-id1161 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Technische Kapazität“ ist das Maximum an fester Kapazität, das der Netzbetreiber unter Berücksichtigung der Systemintegrität und der Erfordernisse des Netzbetriebs Transportkunden anbieten kann;"@de .

dtd:LA-id1162 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Untertägige Kapazität“ ist die Kapazität, die nach dem Ende der Auktionen für Kapazitäten auf Tagesbasis für den jeweiligen Tag angeboten und zugewiesen wird;"@de .

dtd:LA-id1163 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Verfügbare Kapazität“ ist die Differenz zwischen technischer Kapazität und der Summe der gebuchten Kapazitäten für den jeweiligen Ein- oder Ausspeisepunkt;"@de .

dtd:LA-id1164 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Virtueller Handelspunkt“ ist ein Punkt im Marktgebiet, an dem Gas zwischen Bilanzkreisen übertragen werden kann, der jedoch keinem physischen Ein- oder Ausspeisepunkt im Marktgebiet entspricht;"@de .

dtd:LA-id1165 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Werktage“ sind die Tage Montag bis Freitag, mit Ausnahme der bundeseinheitlichen gesetzlichen Feiertage sowie des 24. und des 31. Dezembers."@de .

dtd:LA-id1166 a dtd:LA ;
    dtd:has_Value "die Nutzung der Ein- oder Ausspeisepunkte;"@de .

dtd:LA-id1167 a dtd:LA ;
    dtd:has_Value "die Abwicklung des Netzzugangs, der Buchung von Kapazitäten und der Nominierung, insbesondere über den Zeitpunkt, bis zu dem eine Nominierung vorgenommen werden muss und inwieweit nachträgliche Änderungen der Nominierungen möglich sind, sowie über ein Nominierungsersatzverfahren;"@de .

dtd:LA-id1168 a dtd:LA ;
    dtd:has_Value "die Gasbeschaffenheit und Drücke des Gases im Netz;"@de .

dtd:LA-id1169 a dtd:LA ;
    dtd:has_Value "die Leistungsmessung oder über ein Standardlastprofilverfahren;"@de .

dtd:LA-id1170 a dtd:LA ;
    dtd:has_Value "den Daten- und Informationsaustausch zwischen Transportkunden und Netzbetreibern sowie Marktgebietsverantwortlichen, die bei elektronischem Datenaustausch auch die dafür zu verwendenden Formate und Verfahren festlegen;"@de .

dtd:LA-id1171 a dtd:LA ;
    dtd:has_Value "die Messung und Ablesung des Gasverbrauchs;"@de .

dtd:LA-id1172 a dtd:LA ;
    dtd:has_Value "mögliche Störungen der Netznutzung und Haftung für Störungen;"@de .

dtd:LA-id1173 a dtd:LA ;
    dtd:has_Value "die Voraussetzungen für die Registrierung als Transportkunde;"@de .

dtd:LA-id1174 a dtd:LA ;
    dtd:has_Value "die Kündigung des Vertrags durch den Netzbetreiber oder den Transportkunden;"@de .

dtd:LA-id1175 a dtd:LA ;
    dtd:has_Value "den Umgang mit Daten, die vom Transportkunden im Rahmen des Vertrags übermittelt wurden;"@de .

dtd:LA-id1176 a dtd:LA ;
    dtd:has_Value "die Abrechnung;"@de .

dtd:LA-id1177 a dtd:LA ;
    dtd:has_Value "die Ansprechpartner beim Netzbetreiber für Fragen zu Ein- und Ausspeiseverträgen und ihre Erreichbarkeit;"@de .

dtd:LA-id1178 a dtd:LA ;
    dtd:has_Value "die Voraussetzungen für die Erhebung einer Sicherheitsleistung in begründeten Fällen;"@de .

dtd:LA-id1179 a dtd:LA ;
    dtd:has_Value "Regelungen betreffend die Freigabe von Kapazitäten nach § 16."@de .

dtd:LA-id1180 a dtd:LA ;
    dtd:has_Value "die bei der Bilanzierung anzuwendenden Prozesse;"@de .

dtd:LA-id1181 a dtd:LA ;
    dtd:has_Value "die Abrechnung der Bilanzkreise, insbesondere über die Ermittlung der Zu- und Abschläge nach § 23 Absatz 3, sowie zur Abrechnung von Mehr- und Mindermengen;"@de .

dtd:LA-id1182 a dtd:LA ;
    dtd:has_Value "den Daten- und Informationsaustausch zwischen Netzbetreibern, Marktgebietsverantwortlichen und Bilanzkreisverantwortlichen, die bei elektronischem Datenaustausch auch die dafür vorgesehenen Formate und Verfahren festlegen;"@de .

dtd:LA-id1183 a dtd:LA ;
    dtd:has_Value "die Haftung des Marktgebietsverantwortlichen und des Bilanzkreisverantwortlichen;"@de .

dtd:LA-id1184 a dtd:LA ;
    dtd:has_Value "die Voraussetzungen für die Registrierung als Bilanzkreisverantwortlicher;"@de .

dtd:LA-id1185 a dtd:LA ;
    dtd:has_Value "die Kündigung des Vertrags durch den Marktgebietsverantwortlichen oder den Bilanzkreisverantwortlichen;"@de .

dtd:LA-id1186 a dtd:LA ;
    dtd:has_Value "den Umgang mit Daten, die vom Bilanzkreisverantwortlichen im Rahmen des Vertrags übermittelt wurden;"@de .

dtd:LA-id1187 a dtd:LA ;
    dtd:has_Value "Ansprechpartner beim Marktgebietsverantwortlichen für Fragen zum Bilanzierungsvertrag und ihre Erreichbarkeit;"@de .

dtd:LA-id1188 a dtd:LA ;
    dtd:has_Value "Voraussetzungen für die Erhebung einer Sicherheitsleistung in begründeten Fällen."@de .

dtd:LA-id1189 a dtd:LA ;
    dtd:has_Value "die notwendigen Informationspflichten der Netzbetreiber untereinander zur Abwicklung von Transporten;"@de .

dtd:LA-id1190 a dtd:LA ;
    dtd:has_Value "die technischen Kriterien des Netzkopplungspunkts, insbesondere Druck, Gasbeschaffenheit und technische Leistung des Netzkopplungspunkts;"@de .

dtd:LA-id1191 a dtd:LA ;
    dtd:has_Value "den Datenaustausch zwischen den Netzbetreibern;"@de .

dtd:LA-id1192 a dtd:LA ;
    dtd:has_Value "die Messung und die Bereitstellung der Messergebnisse;"@de .

dtd:LA-id1193 a dtd:LA ;
    dtd:has_Value "die Nominierung oder alternative Verfahren;"@de .

dtd:LA-id1194 a dtd:LA ;
    dtd:has_Value "die Bedingungen für die Einstellung oder Reduzierung der Gasbereitstellung oder Gasübernahme."@de .

dtd:LA-id1195 a dtd:LA ;
    dtd:has_Value "vertragliche Vereinbarungen mit Dritten, die bestimmte Lastflüsse zusichern sowie geeignet und erforderlich sind, die Ausweisbarkeit frei zuordenbarer Ein- und Ausspeisekapazitäten zu erhöhen (Lastflusszusagen); der Umfang von Lastflusszusagen ist so gering wie möglich zu halten;"@de .

dtd:LA-id1196 a dtd:LA ;
    dtd:has_Value "das Angebot von Ein- und Ausspeisekapazitäten, die abweichend von § 8 Absatz 2 mit bestimmten Zuordnungsauflagen verknüpft sind; diese Vorgaben sind so gering wie möglich zu halten;"@de .

dtd:LA-id1197 a dtd:LA ;
    dtd:has_Value "den Ausschluss einzelner Ein- und Ausspeisepunkte von der freien Zuordenbarkeit; diese Vorgaben sind so gering wie möglich zu halten."@de .

dtd:LA-id1198 a dtd:LA ;
    dtd:has_Value "bei der Ausspeisung aus einer Speicheranlage, soweit der betreffende Ausspeisepunkt nicht nach § 13 Absatz 3 Satz 3 vom Betreiber der Speicheranlage gebucht wurde,"@de .

dtd:LA-id1199 a dtd:LA ;
    dtd:has_Value "bei der Überspeisung in ein anderes Marktgebiet oder einen angrenzenden Staat, sowie"@de .

dtd:LA-id1200 a dtd:LA ;
    dtd:has_Value "bei der Buchung von Transportkapazität an demselben Ausspeisepunkt durch mehrere Transportkunden, sofern dieser Ausspeisepunkt unterschiedlichen Bilanzkreisen zugeordnet ist."@de .

dtd:LA-id1201 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nachweist, dass er die Kapazitäten in Übereinstimmung mit § 16 Absatz 1 auf dem Sekundärmarkt angeboten oder dem Fernleitungsnetzbetreiber für den Zeitraum und im Umfang der Nichtnutzung zur Verfügung gestellt hat,"@de .

dtd:LA-id1202 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "unverzüglich schriftlich oder elektronisch schlüssig darlegt, dass er die Kapazitäten in vollem Umfang weiterhin benötigt, um bestehende vertragliche Verpflichtungen, insbesondere aus Gasbezugs- oder Gaslieferverträgen, zu erfüllen, oder"@de .

dtd:LA-id1203 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "unverzüglich schriftlich oder elektronisch schlüssig darlegt, dass er über verschiedene vertragliche Gasbeschaffungsalternativen verfügt, für die Kapazitäten an unterschiedlichen Einspeisepunkten gebucht sind, die von ihm alternativ genutzt werden, und dass er die nicht benötigten Kapazitäten für den Zeitraum der Nichtnutzung im Umfang der Nichtnutzung auf dem Sekundärmarkt oder dem Fernleitungsnetzbetreiber für den Zeitraum und im Umfang der Nichtnutzung zur Verfügung gestellt hat."@de .

dtd:LA-id1204 a dtd:LA ;
    dtd:has_Value "ihre Erwartungen über die Entwicklung des Verhältnisses von Angebot und Nachfrage,"@de .

dtd:LA-id1205 a dtd:LA ;
    dtd:has_Value "vorliegende Erkenntnisse aus durchgeführten Marktabfragen zum langfristig verbindlich benötigten Kapazitätsbedarf,"@de .

dtd:LA-id1206 a dtd:LA ;
    dtd:has_Value "vorliegende Erkenntnisse aus Lastflusssimulationen nach § 9 Absatz 2 Satz 1,"@de .

dtd:LA-id1207 a dtd:LA ;
    dtd:has_Value "Erkenntnisse über bestehende oder prognostizierte physische Engpässe im Netz,"@de .

dtd:LA-id1208 a dtd:LA ;
    dtd:has_Value "Ergebnisse des Kapazitätsvergabeverfahrens nach § 13 Absatz 1,"@de .

dtd:LA-id1209 a dtd:LA ;
    dtd:has_Value "Erkenntnisse aus Verweigerungen des Netzzugangs nach § 25 Satz 1 und 2 des Energiewirtschaftsgesetzes,"@de .

dtd:LA-id1210 a dtd:LA ;
    dtd:has_Value "Möglichkeiten zur Kapazitätserhöhung durch Zusammenarbeit mit angrenzenden Fernleitungs- oder Verteilernetzbetreibern,"@de .

dtd:LA-id1211 a dtd:LA ;
    dtd:has_Value "vorliegende Erkenntnisse über Kapazitätsbedarf, der sich aus Zusammenlegungen von Marktgebieten nach § 21 ergibt,"@de .

dtd:LA-id1212 a dtd:LA ;
    dtd:has_Value "vorliegende Erkenntnisse aus den gemeinschaftsweiten Netzentwicklungsplänen nach Artikel 8 Absatz 3 Buchstabe b der Verordnung (EG) Nr. 715/2009 des Europäischen Parlaments und des Rates vom 13. Juli 2009 über die Bedingungen für den Zugang zu den Erdgasfernleitungsnetzen und zur Aufhebung der Verordnung (EG) Nr. 1775/2005 (ABl. L 211 vom 14.8.2009, S. 36, L 229 vom 1.9.2009, S. 29), sowie"@de .

dtd:LA-id1213 a dtd:LA ;
    dtd:has_Value "vorliegende sowie abgelehnte Kapazitätsreservierungen nach § 38 sowie Anschlussbegehren nach § 39."@de .

dtd:LA-id1214 a dtd:LA ;
    dtd:has_Value "den Betrieb des Virtuellen Handelspunkts eines Marktgebiets;"@de .

dtd:LA-id1215 a dtd:LA ;
    dtd:has_Value "die Bilanzkreisabwicklung, insbesondere Vertragsabwicklung, Datenübermittlung und –veröffentlichung sowie Abrechnung der Bilanzkreise, sowie"@de .

dtd:LA-id1216 a dtd:LA ;

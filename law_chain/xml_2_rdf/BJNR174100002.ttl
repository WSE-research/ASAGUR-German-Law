    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Unternehmen herrschendes Konzernunternehmen ist oder nach § 5 Abs. 3 des Gesetzes als herrschendes Konzernunternehmen gilt."@de .

dtd:LA-id3191 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der voraussichtliche Beginn der Amtszeit der zu wählenden Aufsichtsratsmitglieder der Arbeitnehmer;"@de .


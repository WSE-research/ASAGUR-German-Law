    dtd:has_Value "den Gesamtbetriebsräten und den Gesamtsprecherausschüssen (Unternehmenssprecherausschüssen),"@de .

dtd:LA-id3198 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den in den Unternehmen bestehenden Betriebsräten und Sprecherausschüssen,"@de .

dtd:LA-id3199 a dtd:LA ;

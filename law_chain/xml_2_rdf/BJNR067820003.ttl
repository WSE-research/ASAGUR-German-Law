    dtd:has_Value "das Ausstellen, die Vorlage und Übermittlung von Bescheinigungen sowie die Verwendung von Vordrucken,"@de .

dtd:LA-id58 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Verfahren beim Erbringen von Geldleistungen,"@de .

dtd:LA-id59 a dtd:LA ;

dtd:LA-id114 a dtd:LA ;
    dtd:has_Value "die Überwachung der Einhaltung der Obergrenze des strukturellen gesamtstaatlichen Finanzierungsdefizits nach § 51 Absatz 2 des Haushaltsgrundsätzegesetzes."@de .

dtd:LA-id115 a dtd:LA ;

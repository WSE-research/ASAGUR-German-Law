dtd:P-id59 a dtd:P ;
    dtd:has_Value "Auf Grund des § 3 des Gesetzes über die Zulassung öffentlicher Spielbanken vom 14. Juli 1933 (Reichsgesetzbl. I S. 480) wird im Einvernehmen mit dem Reichsminister der Finanzen folgendes verordnet:"@de .

dtd:P-id61 a dtd:P ;
    dtd:has_Value ""@de .

dtd:P-id63 a dtd:P ;
    dtd:has_I dtd:I-id50 ;
    dtd:has_Value "(1) Der Spielbankunternehmer ist für den Betrieb der Spielbank von den laufenden Steuern des Reichs, die vom Einkommen, vom Vermögen und vom Umsatz erhoben werden, sowie von der Lotteriesteuer und von der Gesellschaftssteuer befreit."@de .

dtd:P-id64 a dtd:P ;
    dtd:has_I dtd:I-id51,
        dtd:I-id52 ;
    dtd:has_Value "(2) Inwieweit der Spielbankunternehmer für den Betrieb der Spielbank auch von Landes- und Gemeindesteuern zu befreien ist, bestimmt der Reichsminister der Finanzen im Einvernehmen mit dem Reichsminister des Innern."@de .

dtd:P-id66 a dtd:P ;
    dtd:has_Value ""@de .

dtd:P-id68 a dtd:P ;
    dtd:has_Value "(1)"@de .

dtd:P-id69 a dtd:P ;
    dtd:has_Value "(2) Im übrigen tritt die Verordnung an dem auf die Verkündung folgenden Tag in Kraft."@de .

dtd:P-id70 a dtd:P ;
    dtd:has_Value "(3)"@de .


dtd:LA-id1000 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Bewerberinnen und Bewerber sowie für Mitarbeiterinnen und Mitarbeiter des eigenen Nachrichtendienstes und"@de .

dtd:LA-id1001 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für andere betroffene Personen, wenn diese mit einer sicherheitsempfindlichen Tätigkeit nach § 1 Absatz 2 beim jeweiligen Nachrichtendienst betraut werden sollen,"@de .

dtd:LA-id1002 a dtd:LA ;
    dtd:has_Value "STRENG GEHEIM, wenn die Kenntnisnahme durch Unbefugte den Bestand oder lebenswichtige Interessen der Bundesrepublik Deutschland oder eines ihrer Länder gefährden kann,"@de .

dtd:LA-id1003 a dtd:LA ;
    dtd:has_Value "GEHEIM, wenn die Kenntnisnahme durch Unbefugte die Sicherheit der Bundesrepublik Deutschland oder eines ihrer Länder gefährden oder ihren Interessen schweren Schaden zufügen kann,"@de .

dtd:LA-id1004 a dtd:LA ;
    dtd:has_Value "VS-VERTRAULICH, wenn die Kenntnisnahme durch Unbefugte für die Interessen der Bundesrepublik Deutschland oder eines ihrer Länder schädlich sein kann,"@de .

dtd:LA-id1005 a dtd:LA ;
    dtd:has_Value "VS-NUR FÜR DEN DIENSTGEBRAUCH, wenn die Kenntnisnahme durch Unbefugte für die Interessen der Bundesrepublik Deutschland oder eines ihrer Länder nachteilig sein kann."@de .

dtd:LA-id1006 a dtd:LA ;
    dtd:has_Value "ist zur Verschwiegenheit über die ihm dadurch zur Kenntnis gelangten Informationen verpflichtet und"@de .

dtd:LA-id1007 a dtd:LA ;
    dtd:has_Value "hat durch Einhaltung der Schutzmaßnahmen, die auf Grund dieses Gesetzes erlassen worden sind, dafür Sorge zu tragen, dass keine unbefugte Person Kenntnis von der Verschlusssache erlangt."@de .

dtd:LA-id1008 a dtd:LA ;
    dtd:has_Value "Zweifel an der Zuverlässigkeit der betroffenen Person bei der Wahrnehmung einer sicherheitsempfindlichen Tätigkeit,"@de .

dtd:LA-id1009 a dtd:LA ;
    dtd:has_DL dtd:DL-id655 ;
    dtd:has_Value "eine besondere Gefährdung der betroffenen Person, insbesondere die Besorgnis der Erpressbarkeit, bei möglichen Anbahnungs- oder Werbungsversuchen a)ausländischer Nachrichtendienste,b)von Vereinigungen im Sinne der §§ 129 bis 129b des Strafgesetzbuches oderc)extremistischer Organisationen, die Bestrebungen im Sinne des § 3 Absatz 1 des Bundesverfassungsschutzgesetzes verfolgen,oder"@de .

dtd:LA-id1010 a dtd:LA ;
    dtd:has_Value "ausländischer Nachrichtendienste,"@de .

dtd:LA-id1011 a dtd:LA ;
    dtd:has_Value "von Vereinigungen im Sinne der §§ 129 bis 129b des Strafgesetzbuches oder"@de .

dtd:LA-id1012 a dtd:LA ;
    dtd:has_Value "extremistischer Organisationen, die Bestrebungen im Sinne des § 3 Absatz 1 des Bundesverfassungsschutzgesetzes verfolgen,"@de .

dtd:LA-id1013 a dtd:LA ;
    dtd:has_Value "Zweifel am Bekenntnis der betroffenen Person zur freiheitlichen demokratischen Grundordnung im Sinne des Grundgesetzes oder am jederzeitigen Eintreten für deren Erhaltung."@de .

dtd:LA-id1014 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einfache Sicherheitsüberprüfung oder"@de .

dtd:LA-id1015 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erweiterte Sicherheitsüberprüfung oder"@de .

dtd:LA-id1016 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erweiterte Sicherheitsüberprüfung mit Sicherheitsermittlungen"@de .

dtd:LA-id1017 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zugang zu VS-VERTRAULICH eingestuften Verschlußsachen erhalten sollen oder ihn sich verschaffen können,"@de .

dtd:LA-id1018 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tätigkeiten in Bereichen nach § 1 Abs. 2 Nr. 3 wahrnehmen sollen."@de .

dtd:LA-id1019 a dtd:LA ;
    dtd:has_DL dtd:DL-id679 ;
    dtd:has_Value "in den Fällen des Absatzes 1 Nummer 1 a)die Zuverlässigkeit der betroffenen Person durch eine Überprüfung nach dem Luftsicherheitsgesetz festgestellt wurde,b)die Betrauung mit der sicherheitsempfindlichen Tätigkeit unaufschiebbar ist,c)die Einstufung der Verschlusssache voraussichtlich vor Abschluss der Sicherheitsüberprüfung wieder aufgehoben wird undd)das Bundesministerium des Innern, für Bau und Heimat dem zugestimmt hat,"@de .

dtd:LA-id1020 a dtd:LA ;

dtd:LA-id3173 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "außergewöhnliche Schwimmkörper"@de .

dtd:LA-id3174 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einzelne oder zu mehreren zusammengefaßte schwer erkennbare, teilweise getauchte oder nicht über die Wasseroberfläche hinausragende Fahrzeuge und Gegenstände, die im Wasser fortbewegt werden sollen, insbesondere Hölzer, Rohre, Faltbehälter, Sinkstücke oder ähnliche Schwimmkörper. Im Falle ihrer Fortbewegung gelten sie als geschleppte Fahrzeuge oder Gegenstände im Sinne von Regel 24 Buchstabe g der Kollisionsverhütungsregeln;"@de .

dtd:LA-id3175 a dtd:LA ;

    dtd:has_Value "für die Zertifikate nach § 21,"@de .

dtd:LA-id1531 a dtd:LA ;
    dtd:has_Value "für die Mitteilungen und Berichte nach den §§ 37 und 38 und"@de .

dtd:LA-id1532 a dtd:LA ;

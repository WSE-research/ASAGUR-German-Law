    dtd:has_Value ""@de .

dtd:P-id732 a dtd:P ;
    dtd:has_DL dtd:DL-id522 ;
    dtd:has_Value "Ordnungswidrig im Sinne des Â§ 111 Absatz 1 Satz 1 Nummer 8 des Vierten Buches Sozialgesetzbuch handelt, wer vorsÃ¤tzlich oder leichtfertig 1.(weggefallen)2.(weggefallen)3.entgegen Â§ 25 Abs. 1 Satz 1 eine Bescheinigung nicht, nicht richtig, nicht vollstÃ¤ndig oder nicht rechtzeitig Ã¼bergibt oder4.entgegen Â§ 25 Abs. 2 Satz 2 den Inhalt der Bescheinigung nicht oder nicht fÃ¼r die vorgeschriebene Dauer aufbewahrt."@de .


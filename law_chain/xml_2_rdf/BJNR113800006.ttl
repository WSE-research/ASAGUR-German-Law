    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anschrift,"@de .

dtd:LA-id457 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Beginn und das Ende der Beschäftigung,"@de .

dtd:LA-id458 a dtd:LA ;

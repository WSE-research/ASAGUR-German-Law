    dtd:has_Value "den vom Beschäftigten zu tragenden Anteil am Gesamtsozialversicherungsbeitrag, nach Beitragsgruppen getrennt,"@de .

dtd:LA-id469 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für die Erstattung von Meldungen erforderlichen Daten, soweit sie in den Nummern 1 bis 14 nicht enthalten sind,"@de .

dtd:LA-id470 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Entsendung Eigenart und zeitliche Begrenzung der Beschäftigung,"@de .

dtd:LA-id471 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gezahltes Kurzarbeitergeld und die hierauf entfallenden beitragspflichtigen Einnahmen,"@de .

dtd:LA-id472 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wertguthaben aus flexibler Arbeitszeit bis zum 31. Dezember 2009, für die noch Beiträge zur gesetzlichen Unfallversicherung zu entrichten sind."@de .


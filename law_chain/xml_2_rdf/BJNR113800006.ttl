    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Betrag nach § 3 Abs. 1 Nr. 1 Buchstabe b des Altersteilzeitgesetzes,"@de .

dtd:LA-id466 a dtd:LA ;

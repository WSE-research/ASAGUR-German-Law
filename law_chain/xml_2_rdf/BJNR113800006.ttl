dtd:LA-id445 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Barzahlung der Tag des Geldeingangs,"@de .

dtd:LA-id446 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Zahlung durch Scheck, bei Überweisung oder Einzahlung auf ein Konto der Einzugsstelle der Tag der Wertstellung zugunsten der Einzugsstelle, bei rückwirkender Wertstellung das Datum des elektronischen Kontoauszuges des Geldinstituts der Einzugsstelle,"@de .

dtd:LA-id447 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Vorliegen einer Einzugsermächtigung der Tag der Fälligkeit."@de .

dtd:LA-id448 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kranken- und Pflegekassen am Tag der Fälligkeit nach der Satzung,"@de .

dtd:LA-id449 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Trägern der Rentenversicherung und der Bundesagentur für Arbeit am Tag der Fälligkeit in Einnahme zu buchen."@de .

dtd:LA-id450 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die vertraglichen Vereinbarungen mit ihrem Geldinstitut so zu gestalten, dass die Beiträge dem Konto der Einzugsstelle an dem Tag gutgeschrieben werden, an dem sie dem Geldinstitut gutgeschrieben werden,"@de .

dtd:LA-id451 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beiträge am Tag der Gutschrift auf ihrem Konto an die Träger der Rentenversicherung, Pflegeversicherung, den Gesundheitsfonds und die Bundesagentur für Arbeit durch Überweisung weiterzuleiten,"@de .

dtd:LA-id452 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Buchungen auf ihrem Konto bei dem Geldinstitut elektronisch so abzufragen, dass die dort gutgeschriebenen Beiträge taggleich vor Bankannahmeschluss weitergeleitet werden können."@de .

dtd:LA-id453 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Familien- und Vornamen und gegebenenfalls das betriebliche Ordnungsmerkmal,"@de .


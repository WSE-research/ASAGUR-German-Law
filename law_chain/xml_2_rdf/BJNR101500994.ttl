dtd:I-id2385 a dtd:I ;
    dtd:has_Value ", Postleitzahl des Wohnorts vor dem Einzug in eine vollstationäre Pflegeeinrichtung,"@de .

dtd:LA-id6040 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "  (zu § 15) Einzelpunkte der Module 1 bis 6; Bildung der Summe der Einzelpunkte in jedem Modul"@de .

dtd:LA-id6041 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "  (zu § 15) Bewertungssystematik (Summe der Punkte und gewichtete Punkte)"@de .

dtd:LA-id6042 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Gutachtens des Medizinischen Dienstes oder eines anderen von der Pflegekasse beauftragten Gutachters sowie"@de .

dtd:LA-id6043 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der gesonderten Präventions- und Rehabilitationsempfehlung gemäß § 18c Absatz 4."@de .

dtd:LA-id6044 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Hilfebedarf unter Berücksichtigung der Ergebnisse der Begutachtung durch den Medizinischen Dienst sowie, wenn die nach Satz 1 anspruchsberechtigte Person zustimmt, die Ergebnisse der Beratung in der eigenen Häuslichkeit nach § 37 Absatz 3 systematisch zu erfassen und zu analysieren,"@de .

dtd:LA-id6045 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen individuellen Versorgungsplan mit den im Einzelfall erforderlichen Sozialleistungen und gesundheitsfördernden, präventiven, kurativen, rehabilitativen oder sonstigen medizinischen sowie pflegerischen und sozialen Hilfen zu erstellen,"@de .

dtd:LA-id6046 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf die für die Durchführung des Versorgungsplans erforderlichen Maßnahmen einschließlich deren Genehmigung durch den jeweiligen Leistungsträger hinzuwirken, insbesondere hinsichtlich einer Empfehlung zur medizinischen Rehabilitation gemäß § 18b Absatz 1 Satz 1 Nummer 2 und Absatz 2,"@de .

dtd:LA-id6047 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Durchführung des Versorgungsplans zu überwachen und erforderlichenfalls einer veränderten Bedarfslage anzupassen,"@de .

dtd:LA-id6048 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei besonders komplexen Fallgestaltungen den Hilfeprozess auszuwerten und zu dokumentieren sowie"@de .

dtd:LA-id6049 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über Leistungen zur Entlastung der Pflegepersonen zu informieren."@de .

dtd:LA-id6050 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach Landesrecht für die wohnortnahe Betreuung im Rahmen der örtlichen Altenhilfe und für die Gewährung der Hilfe zur Pflege nach dem Zwölften Buch zu bestimmende Stellen,"@de .

dtd:LA-id6051 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unternehmen der privaten Kranken- und Pflegeversicherung,"@de .

dtd:LA-id6052 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflegeeinrichtungen und Einzelpersonen nach § 77,"@de .

dtd:LA-id6053 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mitglieder von Selbsthilfegruppen, ehrenamtliche und sonstige zum bürgerschaftlichen Engagement bereite Personen und Organisationen sowie"@de .

dtd:LA-id6054 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Agenturen für Arbeit und Träger der Grundsicherung für Arbeitsuchende,"@de .

dtd:LA-id6055 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erfahrungen und Weiterentwicklung der Pflegeberatung und Pflegeberatungsstrukturen nach den Absätzen 1 bis 4, 7 und 8, § 7b Absatz 1 und 2 und § 7c und"@de .

dtd:LA-id6056 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Durchführung, Ergebnisse und Wirkungen der Beratung in der eigenen Häuslichkeit sowie die Fortentwicklung der Beratungsstrukturen nach § 37 Absatz 3 bis 8."@de .

dtd:LA-id6057 a dtd:LA ;
    dtd:has_Value "unter Angabe einer Kontaktperson einen konkreten Beratungstermin anzubieten, der spätestens innerhalb von zwei Wochen nach Antragseingang durchzuführen ist, oder"@de .

dtd:LA-id6058 a dtd:LA ;
    dtd:has_Value "einen Beratungsgutschein auszustellen, in dem Beratungsstellen benannt sind, bei denen er zu Lasten der Pflegekasse innerhalb von zwei Wochen nach Antragseingang eingelöst werden kann; § 7a Absatz 4 Satz 5 ist entsprechend anzuwenden."@de .

dtd:LA-id6059 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anforderungen an die Beratungsleistung und die Beratungspersonen,"@de .

dtd:LA-id6060 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Haftung für Schäden, die der Pflegekasse durch fehlerhafte Beratung entstehen, und"@de .

dtd:LA-id6061 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vergütung."@de .

dtd:LA-id6062 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die wohnortnahe Betreuung im Rahmen der örtlichen Altenhilfe oder"@de .

dtd:LA-id6063 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Gewährung der Hilfe zur Pflege nach dem Zwölften Buch"@de .

dtd:LA-id6064 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "umfassende sowie unabhängige Auskunft und Beratung zu den Rechten und Pflichten nach dem Sozialgesetzbuch und zur Auswahl und Inanspruchnahme der bundes- oder landesrechtlich vorgesehenen Sozialleistungen und sonstigen Hilfsangebote einschließlich der Pflegeberatung nach § 7a in Verbindung mit den Richtlinien nach § 17 Absatz 1a,"@de .

dtd:LA-id6065 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Koordinierung aller für die wohnortnahe Versorgung und Betreuung in Betracht kommenden gesundheitsfördernden, präventiven, kurativen, rehabilitativen und sonstigen medizinischen sowie pflegerischen und sozialen Hilfs- und Unterstützungsangebote einschließlich der Hilfestellung bei der Inanspruchnahme der Leistungen,"@de .

dtd:LA-id6066 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vernetzung aufeinander abgestimmter pflegerischer und sozialer Versorgungs- und Betreuungsangebote."@de .

dtd:LA-id6067 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach Landesrecht zu bestimmenden Stellen für die wohnortnahe Betreuung im Rahmen der örtlichen Altenhilfe und für die Gewährung der Hilfe zur Pflege nach dem Zwölften Buch,"@de .

dtd:LA-id6068 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Land zugelassenen und tätigen Pflegeeinrichtungen,"@de .

dtd:LA-id6069 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Land tätigen Unternehmen der privaten Kranken- und Pflegeversicherung"@de .

dtd:LA-id6070 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sollen Pflegefachkräfte in die Tätigkeit der Pflegestützpunkte einbinden,"@de .

dtd:LA-id6071 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "haben nach Möglichkeit Mitglieder von Selbsthilfegruppen sowie ehrenamtliche und sonstige zum bürgerschaftlichen Engagement bereite Personen und Organisationen in die Tätigkeit der Pflegestützpunkte einzubinden,"@de .

dtd:LA-id6072 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sollen interessierten kirchlichen sowie sonstigen religiösen und gesellschaftlichen Trägern und Organisationen sowie nicht gewerblichen, gemeinwohlorientierten Einrichtungen mit öffentlich zugänglichen Angeboten und insbesondere Selbsthilfe stärkender und generationenübergreifender Ausrichtung in kommunalen Gebietskörperschaften die Beteiligung an den Pflegestützpunkten ermöglichen,"@de .

dtd:LA-id6073 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "können sich zur Erfüllung ihrer Aufgaben dritter Stellen bedienen,"@de .

dtd:LA-id6074 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sollen im Hinblick auf die Vermittlung und Qualifizierung von für die Pflege und Betreuung geeigneten Kräften eng mit dem Träger der Arbeitsförderung nach dem Dritten Buch und den Trägern der Grundsicherung für Arbeitsuchende nach dem Zweiten Buch zusammenarbeiten."@de .

dtd:LA-id6075 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach Landesrecht für die wohnortnahe Betreuung im Rahmen der örtlichen Altenhilfe und für die Gewährung der Hilfe zur Pflege nach dem Zwölften Buch zu bestimmende Stellen,"@de .

dtd:LA-id6076 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unternehmen der privaten Kranken- und Pflegeversicherung,"@de .

dtd:LA-id6077 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflegeeinrichtungen und Einzelpersonen nach § 77,"@de .

dtd:LA-id6078 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mitglieder von Selbsthilfegruppen, ehrenamtliche und sonstige zum bürgerschaftlichen Engagement bereite Personen und Organisationen sowie"@de .

dtd:LA-id6079 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Agenturen für Arbeit und Träger der Grundsicherung für Arbeitsuchende,"@de .

dtd:LA-id6080 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine halbe Stelle bei Pflegeeinrichtungen mit bis zu 40 Plätzen,"@de .

dtd:LA-id6081 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Stelle bei Pflegeeinrichtungen mit 41 bis zu 80 Plätzen,"@de .

dtd:LA-id6082 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "anderthalb Stellen bei Pflegeeinrichtungen mit 81 bis zu 120 Plätzen und"@de .

dtd:LA-id6083 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zwei Stellen bei Pflegeeinrichtungen mit mehr als 120 Plätzen."@de .

dtd:LA-id6084 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "individuelle und gemeinschaftliche Betreuungsangebote, die auf die besonderen Arbeitszeiten von Pflegekräften ausgerichtet sind, sowie weitere Maßnahmen zur Entlastung insbesondere der in der Pflege und Betreuung tätigen Mitarbeiterinnen und Mitarbeiter,"@de .

dtd:LA-id6085 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Rückgewinnung von Pflege- und Betreuungspersonal,"@de .

dtd:LA-id6086 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Verbesserung der Arbeitszeit- und Dienstplangestaltung einschließlich Maßnahmen im Zusammenhang mit lebensphasengerechten Arbeitszeitmodellen, Personalpools sowie weiteren betrieblichen Ausfallkonzepten,"@de .

dtd:LA-id6087 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Verbesserung der Kommunikation mit und zwischen den Beschäftigten,"@de .

dtd:LA-id6088 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur kompetenzorientierten Personalentwicklung, Personalqualifizierung und Führung,"@de .

dtd:LA-id6089 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Verbesserung der Kommunikation mit Kunden,"@de .

dtd:LA-id6090 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen zur Schaffung einer familienfreundlichen Unternehmenskultur."@de .

dtd:LA-id6091 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Pflegeeinrichtungen mit bis zu 25 in der Pflege tätigen Mitarbeiterinnen und Mitarbeitern in Höhe von bis zu 10 000 Euro; dabei werden bis zu 70 Prozent der durch die Pflegeeinrichtung für eine Maßnahme verausgabten Mittel gefördert;"@de .

dtd:LA-id6092 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Pflegeeinrichtungen ab 26 in der Pflege tätigen Mitarbeiterinnen und Mitarbeitern in Höhe von bis zu 7 500 Euro; dabei werden bis zu 50 Prozent der durch die Pflegeeinrichtung für eine Maßnahme verausgabten Mittel gefördert."@de .

dtd:LA-id6093 a dtd:LA ;
    dtd:has_Value "der Pflegebedürftigen bei der Tragung der ihnen von den Pflegeeinrichtungen berechneten betriebsnotwendigen Investitionsaufwendungen oder"@de .

dtd:LA-id6094 a dtd:LA ;
    dtd:has_Value "der Pflegeeinrichtungen bei der Tragung ihrer betriebsnotwendigen Investitionsaufwendungen"@de .

dtd:LA-id6095 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach dem Vierzehnten Buch und nach den Gesetzen, die eine entsprechende Anwendung des Vierzehnten Buches vorsehen,"@de .

dtd:LA-id6096 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus der gesetzlichen Unfallversicherung und"@de .

dtd:LA-id6097 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus öffentlichen Kassen auf Grund gesetzlich geregelter Unfallversorgung oder Unfallfürsorge"@de .

dtd:LA-id6098 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach dem Zwölften Buch,"@de .

dtd:LA-id6099 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach dem Lastenausgleichsgesetz, dem Reparationsschädengesetz und dem Flüchtlingshilfegesetz"@de .

dtd:LA-id6100 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id6101 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass im Verhältnis zum Pflegebedürftigen der für die Eingliederungshilfe zuständige Träger die Leistungen der Pflegeversicherung auf der Grundlage des von der Pflegekasse erlassenen Leistungsbescheids zu übernehmen hat,"@de .

dtd:LA-id6102 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass die zuständige Pflegekasse dem für die Eingliederungshilfe zuständigen Träger die Kosten der von ihr zu tragenden Leistungen zu erstatten hat sowie"@de .

dtd:LA-id6103 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Modalitäten der Übernahme und der Durchführung der Leistungen sowie der Erstattung."@de .

dtd:LA-id6104 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Fällen des § 1361 Abs. 3, der §§ 1579, 1603 Abs. 2 und des § 1611 Abs. 1 des Bürgerlichen Gesetzbuchs,"@de .

dtd:LA-id6105 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Unterhaltsansprüche der Pflegeperson, wenn von dieser erwartet werden kann, ihren Unterhaltsbedarf ganz oder teilweise durch eigene Einkünfte zu decken und der Pflegebedürftige mit dem Unterhaltspflichtigen nicht in gerader Linie verwandt ist."@de .

dtd:LA-id6106 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mobilität: Positionswechsel im Bett, Halten einer stabilen Sitzposition, Umsetzen, Fortbewegen innerhalb des Wohnbereichs, Treppensteigen;"@de .

dtd:LA-id6107 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kognitive und kommunikative Fähigkeiten: Erkennen von Personen aus dem näheren Umfeld, örtliche Orientierung, zeitliche Orientierung, Erinnern an wesentliche Ereignisse oder Beobachtungen, Steuern von mehrschrittigen Alltagshandlungen, Treffen von Entscheidungen im Alltagsleben, Verstehen von Sachverhalten und Informationen, Erkennen von Risiken und Gefahren, Mitteilen von elementaren Bedürfnissen, Verstehen von Aufforderungen, Beteiligen an einem Gespräch;"@de .

dtd:LA-id6108 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verhaltensweisen und psychische Problemlagen: motorisch geprägte Verhaltensauffälligkeiten, nächtliche Unruhe, selbstschädigendes und autoaggressives Verhalten, Beschädigen von Gegenständen, physisch aggressives Verhalten gegenüber anderen Personen, verbale Aggression, andere pflegerelevante vokale Auffälligkeiten, Abwehr pflegerischer und anderer unterstützender Maßnahmen, Wahnvorstellungen, Ängste, Antriebslosigkeit bei depressiver Stimmungslage, sozial inadäquate Verhaltensweisen, sonstige pflegerelevante inadäquate Handlungen;"@de .

dtd:LA-id6109 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Selbstversorgung: Waschen des vorderen Oberkörpers, Körperpflege im Bereich des Kopfes, Waschen des Intimbereichs, Duschen und Baden einschließlich Waschen der Haare, An- und Auskleiden des Oberkörpers, An- und Auskleiden des Unterkörpers, mundgerechtes Zubereiten der Nahrung und Eingießen von Getränken, Essen, Trinken, Benutzen einer Toilette oder eines Toilettenstuhls, Bewältigen der Folgen einer Harninkontinenz und Umgang mit Dauerkatheter und Urostoma, Bewältigen der Folgen einer Stuhlinkontinenz und Umgang mit Stoma, Ernährung parenteral oder über Sonde, Bestehen gravierender Probleme bei der Nahrungsaufnahme bei Kindern bis zu 18 Monaten, die einen außergewöhnlich pflegeintensiven Hilfebedarf auslösen;"@de .

dtd:LA-id6110 a dtd:LA ;
    dtd:has_DL dtd:DL-id4556 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bewältigung von und selbständiger Umgang mit krankheits- oder therapiebedingten Anforderungen und Belastungen: a)in Bezug auf Medikation, Injektionen, Versorgung intravenöser Zugänge, Absaugen und Sauerstoffgabe, Einreibungen sowie Kälte- und Wärmeanwendungen, Messung und Deutung von Körperzuständen, körpernahe Hilfsmittel,b)in Bezug auf Verbandswechsel und Wundversorgung, Versorgung mit Stoma, regelmäßige Einmalkatheterisierung und Nutzung von Abführmethoden, Therapiemaßnahmen in häuslicher Umgebung,c)in Bezug auf zeit- und technikintensive Maßnahmen in häuslicher Umgebung, Arztbesuche, Besuche anderer medizinischer oder therapeutischer Einrichtungen, zeitlich ausgedehnte Besuche medizinischer oder therapeutischer Einrichtungen, Besuch von Einrichtungen zur Frühförderung bei Kindern sowied)in Bezug auf das Einhalten einer Diät oder anderer krankheits- oder therapiebedingter Verhaltensvorschriften;"@de .

dtd:LA-id6111 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Bezug auf Medikation, Injektionen, Versorgung intravenöser Zugänge, Absaugen und Sauerstoffgabe, Einreibungen sowie Kälte- und Wärmeanwendungen, Messung und Deutung von Körperzuständen, körpernahe Hilfsmittel,"@de .

dtd:LA-id6112 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Bezug auf Verbandswechsel und Wundversorgung, Versorgung mit Stoma, regelmäßige Einmalkatheterisierung und Nutzung von Abführmethoden, Therapiemaßnahmen in häuslicher Umgebung,"@de .

dtd:LA-id6113 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Bezug auf zeit- und technikintensive Maßnahmen in häuslicher Umgebung, Arztbesuche, Besuche anderer medizinischer oder therapeutischer Einrichtungen, zeitlich ausgedehnte Besuche medizinischer oder therapeutischer Einrichtungen, Besuch von Einrichtungen zur Frühförderung bei Kindern sowie"@de .

dtd:LA-id6114 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Bezug auf das Einhalten einer Diät oder anderer krankheits- oder therapiebedingter Verhaltensvorschriften;"@de .

dtd:LA-id6115 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gestaltung des Alltagslebens und sozialer Kontakte: Gestaltung des Tagesablaufs und Anpassung an Veränderungen, Ruhen und Schlafen, Sichbeschäftigen, Vornehmen von in die Zukunft gerichteten Planungen, Interaktion mit Personen im direkten Kontakt, Kontaktpflege zu Personen außerhalb des direkten Umfelds."@de .

dtd:LA-id6116 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Punktbereich 0: keine Beeinträchtigungen der Selbständigkeit oder der Fähigkeiten,"@de .

dtd:LA-id6117 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Punktbereich 1: geringe Beeinträchtigungen der Selbständigkeit oder der Fähigkeiten,"@de .

dtd:LA-id6118 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Punktbereich 2: erhebliche Beeinträchtigungen der Selbständigkeit oder der Fähigkeiten,"@de .

dtd:LA-id6119 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Punktbereich 3: schwere Beeinträchtigungen der Selbständigkeit oder der Fähigkeiten und"@de .

dtd:LA-id6120 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Punktbereich 4: schwerste Beeinträchtigungen der Selbständigkeit oder der Fähigkeiten."@de .

dtd:LA-id6121 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mobilität mit 10 Prozent,"@de .

dtd:LA-id6122 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kognitive und kommunikative Fähigkeiten sowie Verhaltensweisen und psychische Problemlagen zusammen mit 15 Prozent,"@de .

dtd:LA-id6123 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Selbstversorgung mit 40 Prozent,"@de .

dtd:LA-id6124 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bewältigung von und selbständiger Umgang mit krankheits- oder therapiebedingten Anforderungen und Belastungen mit 20 Prozent,"@de .

dtd:LA-id6125 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gestaltung des Alltagslebens und sozialer Kontakte mit 15 Prozent."@de .

dtd:LA-id6126 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ab 12,5 bis unter 27 Gesamtpunkten in den Pflegegrad 1: geringe Beeinträchtigungen der Selbständigkeit oder der Fähigkeiten,"@de .

dtd:LA-id6127 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ab 27 bis unter 47,5 Gesamtpunkten in den Pflegegrad 2: erhebliche Beeinträchtigungen der Selbständigkeit oder der Fähigkeiten,"@de .

dtd:LA-id6128 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ab 47,5 bis unter 70 Gesamtpunkten in den Pflegegrad 3: schwere Beeinträchtigungen der Selbständigkeit oder der Fähigkeiten,"@de .

dtd:LA-id6129 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ab 70 bis unter 90 Gesamtpunkten in den Pflegegrad 4: schwerste Beeinträchtigungen der Selbständigkeit oder der Fähigkeiten,"@de .

dtd:LA-id6130 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ab 90 bis 100 Gesamtpunkten in den Pflegegrad 5: schwerste Beeinträchtigungen der Selbständigkeit oder der Fähigkeiten mit besonderen Anforderungen an die pflegerische Versorgung."@de .

dtd:LA-id6131 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ab 12,5 bis unter 27 Gesamtpunkten in den Pflegegrad 2,"@de .

dtd:LA-id6132 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ab 27 bis unter 47,5 Gesamtpunkten in den Pflegegrad 3,"@de .

dtd:LA-id6133 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ab 47,5 bis unter 70 Gesamtpunkten in den Pflegegrad 4,"@de .

dtd:LA-id6134 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ab 70 bis 100 Gesamtpunkten in den Pflegegrad 5."@de .

dtd:LA-id6135 a dtd:LA ;
    dtd:has_Value "allgemeine Verhaltensgrundsätze für alle unter der Verantwortung der Medizinischen Dienste am Begutachtungsverfahren Beteiligten,"@de .

dtd:LA-id6136 a dtd:LA ;
    dtd:has_Value "die Pflicht der Medizinischen Dienste zur individuellen und umfassenden Information des Versicherten über das Begutachtungsverfahren, insbesondere über den Ablauf, die Rechtsgrundlagen und Beschwerdemöglichkeiten,"@de .

dtd:LA-id6137 a dtd:LA ;
    dtd:has_Value "die regelhafte Durchführung von Versichertenbefragungen und"@de .

dtd:LA-id6138 a dtd:LA ;
    dtd:has_Value "ein einheitliches Verfahren zum Umgang mit Beschwerden, die das Verhalten der Mitarbeiter der Medizinischen Dienste oder das Verfahren bei der Begutachtung betreffen."@de .

dtd:LA-id6139 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn nach Absatz 1 unabhängige Gutachterinnen oder Gutachter mit der Prüfung beauftragt werden sollen oder"@de .

dtd:LA-id6140 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn innerhalb von 20 Arbeitstagen ab Antragstellung keine Begutachtung erfolgt ist."@de .

dtd:LA-id6141 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Grund einer eindeutigen Aktenlage das Ergebnis der medizinischen Untersuchung bereits feststeht oder"@de .

dtd:LA-id6142 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei einer Krisensituation von nationaler Tragweite oder, bezogen auf den Aufenthaltsort des Versicherten, von regionaler Tragweite der Antrag auf Pflegeleistungen während der Krisensituation gestellt wird oder ein Untersuchungstermin, der bereits vereinbart war, in den Zeitraum einer Krisensituation fällt."@de .

dtd:LA-id6143 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Bezug auf außerhäusliche Aktivitäten: Verlassen des Bereiches der Wohnung oder der Einrichtung, Fortbewegen außerhalb der Wohnung oder der Einrichtung, Nutzung öffentlicher Verkehrsmittel im Nahverkehr, Mitfahren in einem Kraftfahrzeug, Teilnahme an kulturellen, religiösen oder sportlichen Veranstaltungen, Besuch von Schule, Kindergarten, Arbeitsplatz, einer Werkstatt für behinderte Menschen oder Besuch einer Einrichtung der Tages- oder Nachtpflege oder eines Tagesbetreuungsangebots, Teilnahme an sonstigen Aktivitäten mit anderen Menschen;"@de .

dtd:LA-id6144 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Bezug auf Haushaltsführung: Einkaufen für den täglichen Bedarf, Zubereitung einfacher Mahlzeiten, einfache Aufräum- und Reinigungsarbeiten, aufwändige Aufräum- und Reinigungsarbeiten einschließlich Wäschepflege, Nutzung von Dienstleistungen, Umgang mit finanziellen Angelegenheiten, Umgang mit Behördenangelegenheiten."@de .

dtd:LA-id6145 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Hinweise vorliegen, dass zur Sicherstellung der ambulanten oder stationären Weiterversorgung und Betreuung eine Begutachtung in der Einrichtung erforderlich ist, oder"@de .

dtd:LA-id6146 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Inanspruchnahme von Pflegezeit nach dem Pflegezeitgesetz gegenüber dem Arbeitgeber der pflegenden Person angekündigt wurde oder"@de .

dtd:LA-id6147 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit dem Arbeitgeber der pflegenden Person eine Familienpflegezeit nach § 2 Absatz 1 des Familienpflegezeitgesetzes vereinbart wurde."@de .

dtd:LA-id6148 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Begutachtung des Antragstellers spätestens innerhalb von zehn Arbeitstagen nach Eingang des Antrags bei der zuständigen Pflegekasse durchzuführen und"@de .

dtd:LA-id6149 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Antragsteller vom Medizinischen Dienst oder von der von der Pflegekasse beauftragten Gutachterin oder von dem von der Pflegekasse beauftragten Gutachter unverzüglich schriftlich darüber zu informieren, welche Empfehlung der Medizinische Dienst oder die von der Pflegekasse beauftragte Gutachterin oder der von der Pflegekasse beauftragte Gutachter an die Pflegekasse weiterleitet."@de .

dtd:LA-id6150 a dtd:LA ;
    dtd:has_Value "die Feststellungen, die in der Begutachtung nach § 18a vorzunehmen sind, insbesondere das Ergebnis der Prüfung, ob die Voraussetzungen der Pflegebedürftigkeit erfüllt sind und welcher Pflegegrad vorliegt, sowie"@de .

dtd:LA-id6151 a dtd:LA ;
    dtd:has_DL dtd:DL-id4648 ;
    dtd:has_Value "Feststellungen, ob und in welchem Umfang Maßnahmen zur Vermeidung, Überwindung, Minderung oder Verhinderung einer Verschlimmerung der Pflegebedürftigkeit geeignet, notwendig und zumutbar sind; Empfehlungen auszusprechen sind insbesondere zu a)Maßnahmen der Prävention,b)Maßnahmen der medizinischen Rehabilitation,c)Maßnahmen zur Hilfsmittel- und Pflegehilfsmittelversorgung,d)Maßnahmen zur Heilmittelversorgung,e)anderen therapeutischen Maßnahmen,f)Maßnahmen zur Verbesserung des individuellen oder gemeinsamen Wohnumfelds,g)edukativen Maßnahmen undh)einer Beratung zu Leistungen zur verhaltensbezogenen Primärprävention nach § 20 Absatz 5 des Fünften Buches."@de .

dtd:LA-id6152 a dtd:LA ;
    dtd:has_Value "Maßnahmen der Prävention,"@de .

dtd:LA-id6153 a dtd:LA ;
    dtd:has_Value "Maßnahmen der medizinischen Rehabilitation,"@de .

dtd:LA-id6154 a dtd:LA ;
    dtd:has_Value "Maßnahmen zur Hilfsmittel- und Pflegehilfsmittelversorgung,"@de .

dtd:LA-id6155 a dtd:LA ;
    dtd:has_Value "Maßnahmen zur Heilmittelversorgung,"@de .

dtd:LA-id6156 a dtd:LA ;
    dtd:has_Value "anderen therapeutischen Maßnahmen,"@de .

dtd:LA-id6157 a dtd:LA ;
    dtd:has_Value "Maßnahmen zur Verbesserung des individuellen oder gemeinsamen Wohnumfelds,"@de .

dtd:LA-id6158 a dtd:LA ;
    dtd:has_Value "edukativen Maßnahmen und"@de .

dtd:LA-id6159 a dtd:LA ;
    dtd:has_Value "einer Beratung zu Leistungen zur verhaltensbezogenen Primärprävention nach § 20 Absatz 5 des Fünften Buches."@de .

dtd:LA-id6160 a dtd:LA ;
    dtd:has_Value "die Anzahl der Empfehlungen der Medizinischen Dienste und der beauftragten Gutachterinnen und Gutachter für Leistungen der medizinischen Rehabilitation im Rahmen der Begutachtung zur Feststellung der Pflegebedürftigkeit,"@de .

dtd:LA-id6161 a dtd:LA ;
    dtd:has_Value "die Anzahl der Anträge an den zuständigen Rehabilitationsträger gemäß § 31 Absatz 3 in Verbindung mit § 14 des Neunten Buches,"@de .

dtd:LA-id6162 a dtd:LA ;
    dtd:has_Value "die Anzahl der genehmigten und die Anzahl der abgelehnten Leistungsentscheidungen der zuständigen Rehabilitationsträger einschließlich der Gründe für eine Ablehnung sowie die Anzahl der Widersprüche,"@de .

dtd:LA-id6163 a dtd:LA ;
    dtd:has_Value "die Anzahl der durchgeführten medizinischen Rehabilitationsmaßnahmen,"@de .

dtd:LA-id6164 a dtd:LA ;
    dtd:has_Value "die Gründe, aus denen Versicherte nicht in die Weiterleitung einer Mitteilung über den Rehabilitationsbedarf an den Rehabilitationsträger nach § 31 Absatz 3 Satz 1 einwilligen, soweit diese der Pflegekasse bekannt sind, und inwieweit die zuständige Pflegekasse hier tätig geworden ist, und"@de .

dtd:LA-id6165 a dtd:LA ;
    dtd:has_Value "die Maßnahmen, die die Pflegekassen im jeweiligen Einzelfall regelmäßig durchführen, um ihre Aufgaben nach § 18c Absatz 4 und § 31 Absatz 3 zu erfüllen."@de .

dtd:LA-id6166 a dtd:LA ;
    dtd:has_Value "die Anzahl der bei den zuständigen Pflegekassen eingegangenen Anträge auf Leistungen der Pflegeversicherung sowie jeweils die Fallzahlen, in denen eine Entscheidung nach § 18c Absatz 1 Satz 1 ergangen ist, und die Fallzahlen der abgelehnten Anträge sowie die Anzahl der Arbeitstage ab Eingang des Antrags bei der zuständigen Pflegekasse bis zur schriftlichen Mitteilung der Entscheidung der Pflegekasse nach § 18c Absatz 1 Satz 1,"@de .

dtd:LA-id6167 a dtd:LA ;
    dtd:has_Value "die Anzahl an Fällen, in denen die Fristen nach § 18c Absatz 1 Satz 1 und nach § 18a Absatz 5 und 6 nicht eingehalten wurden, jeweils unter Angabe der Gründe, die für die Nichteinhaltung der Frist ursächlich waren,"@de .

dtd:LA-id6168 a dtd:LA ;
    dtd:has_Value "die Anzahl an Fällen, in denen eine pauschale Zusatzzahlung nach § 18c Absatz 5 Satz 1 geleistet sowie die Anzahl an Fällen, in denen der antragstellenden Person nach § 18 Absatz 3 Satz 1 mindestens drei unabhängige Gutachterinnen oder Gutachter zur Auswahl benannt wurden,"@de .

dtd:LA-id6169 a dtd:LA ;
    dtd:has_Value "die Anzahl an Fällen, in denen der Antragsteller eine Untersuchung verweigert hat,"@de .

dtd:LA-id6170 a dtd:LA ;
    dtd:has_Value "die Anzahl an Fällen, in denen im Gutachten Empfehlungen für Heilmittel abgegeben wurden, und die Anzahl an Fällen, in denen die Pflegekasse eine Mitteilung über empfohlene Heilmittel an den Arzt oder die Ärztin des Antragstellers weitergeleitet hat, jeweils unterteilt nach Art der Heilmittel,"@de .

dtd:LA-id6171 a dtd:LA ;
    dtd:has_Value "die Anzahl an Fällen, in denen Widerspruch gegen die Entscheidung der Pflegekasse nach § 18c Absatz 1 Satz 1 und § 18a Absatz 5 und 6 eingelegt wurde, unter Angabe der Gründe, auf die der Widerspruch jeweils gestützt wurde, sowie die Anzahl an zurückgewiesenen und stattgegebenen Widersprüchen, jeweils unter Angabe der Entscheidungsgründe und der Dauer des Widerspruchsverfahrens (bis zur schriftlichen Mitteilung der Entscheidung der Pflegekasse), und, soweit zum Zeitpunkt der Berichtslegung bekannt, in wie vielen Fällen nach erfolglosem Widerspruchsverfahren Klage erhoben worden ist."@de .

dtd:LA-id6172 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arbeiter, Angestellte und zu ihrer Berufsausbildung Beschäftigte, die gegen Arbeitsentgelt beschäftigt sind; für die Zeit des Bezugs von Kurzarbeitergeld nach dem Dritten Buch bleibt die Versicherungspflicht unberührt,"@de .

dtd:LA-id6173 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen in der Zeit, für die sie Arbeitslosengeld nach dem Dritten Buch beziehen oder nur deshalb nicht beziehen, weil der Anspruch wegen einer Sperrzeit (§ 159 des Dritten Buches) oder wegen einer Urlaubsabgeltung (§ 157 Absatz 2 des Dritten Buches) ruht; dies gilt auch, wenn die Entscheidung, die zum Bezug der Leistung geführt hat, rückwirkend aufgehoben oder die Leistung zurückgefordert oder zurückgezahlt worden ist,"@de .

dtd:LA-id6174 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen in der Zeit, für die sie Bürgergeld nach § 19 Absatz 1 Satz 1 des Zweiten Buches beziehen, auch wenn die Entscheidung, die zum Bezug der Leistung geführt hat, rückwirkend aufgehoben oder die Leistung zurückgefordert oder zurückgezahlt worden ist, es sei denn, dass diese Leistung nur darlehensweise gewährt wird oder nur Leistungen nach § 24 Absatz 3 Satz 1 des Zweiten Buches bezogen werden,"@de .

dtd:LA-id6175 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Landwirte, ihre mitarbeitenden Familienangehörigen und Altenteiler, die nach § 2 des Zweiten Gesetzes über die Krankenversicherung der Landwirte versicherungspflichtig sind,"@de .

dtd:LA-id6176 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "selbständige Künstler und Publizisten nach näherer Bestimmung des Künstlersozialversicherungsgesetzes,"@de .

dtd:LA-id6177 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, die in Einrichtungen der Jugendhilfe, in Berufsbildungswerken oder in ähnlichen Einrichtungen für behinderte Menschen für eine Erwerbstätigkeit befähigt werden sollen,"@de .

dtd:LA-id6178 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Teilnehmer an Leistungen zur Teilhabe am Arbeitsleben sowie an Berufsfindung oder Arbeitserprobung, es sei denn, sie gehören zu dem Personenkreis des § 151 des Vierzehnten Buches,"@de .

dtd:LA-id6179 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "behinderte Menschen, die in anerkannten Werkstätten für behinderte Menschen oder in Blindenwerkstätten im Sinne des § 226 des Neunten Buches oder für diese Einrichtungen in Heimarbeit oder bei einem anderen Leistungsanbieter nach § 60 des Neunten Buches tätig sind,"@de .

dtd:LA-id6180 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Behinderte Menschen, die in Anstalten, Heimen oder gleichartigen Einrichtungen in gewisser Regelmäßigkeit eine Leistung erbringen, die einem Fünftel der Leistung eines voll erwerbsfähigen Beschäftigten in gleichartiger Beschäftigung entspricht; hierzu zählen auch Dienstleistungen für den Träger der Einrichtung,"@de .

dtd:LA-id6181 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Studenten, die an staatlichen oder staatlich anerkannten Hochschulen eingeschrieben sind, soweit sie nach § 5 Abs. 1 Nr. 9 des Fünften Buches der Krankenversicherungspflicht unterliegen,"@de .

dtd:LA-id6182 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, die zu ihrer Berufsausbildung ohne Arbeitsentgelt beschäftigt sind oder die eine Fachschule oder Berufsfachschule besuchen oder eine in Studien- oder Prüfungsordnungen vorgeschriebene berufspraktische Tätigkeit ohne Arbeitsentgelt verrichten (Praktikanten), längstens bis zur Vollendung des 30. Lebensjahres; Auszubildende des Zweiten Bildungsweges, die sich in einem nach dem Bundesausbildungsförderungsgesetz förderungsfähigen Teil eines Ausbildungsabschnittes befinden, sind Praktikanten gleichgestellt,"@de .

dtd:LA-id6183 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, die die Voraussetzungen für den Anspruch auf eine Rente aus der gesetzlichen Rentenversicherung erfüllen und diese Rente beantragt haben, soweit sie nach § 5 Abs. 1 Nr. 11, 11a, 11b oder 12 des Fünften Buches der Krankenversicherungspflicht unterliegen,"@de .

dtd:LA-id6184 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, die, weil sie bisher keinen Anspruch auf Absicherung im Krankheitsfall hatten, nach § 5 Abs. 1 Nr. 13 des Fünften Buches oder nach § 2 Abs. 1 Nr. 7 des Zweiten Gesetzes über die Krankenversicherung der Landwirte der Krankenversicherungspflicht unterliegen."@de .

dtd:LA-id6185 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach dem Bundesversorgungsgesetz oder nach Gesetzen, die eine entsprechende Anwendung des Bundesversorgungsgesetzes vorsehen, bis zu deren Außerkrafttreten einen Anspruch auf Heilbehandlung oder Krankenbehandlung hatten,"@de .

dtd:LA-id6186 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 42 Absatz 2, 3 oder 4 des Vierzehnten Buches leistungsberechtigt sind,"@de .

dtd:LA-id6187 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kriegsschadenrente oder vergleichbare Leistungen nach dem Lastenausgleichsgesetz oder dem Reparationsschädengesetz oder laufende Beihilfe nach dem Flüchtlingshilfegesetz beziehen,"@de .

dtd:LA-id6188 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 145 Absatz 2 Nummer 4 des Vierzehnten Buches ergänzende Hilfe zum Lebensunterhalt nach dem Bundesversorgungsgesetz weiter erhalten oder Leistungen zum Lebensunterhalt nach § 93 des Vierzehnten Buches beziehen,"@de .

dtd:LA-id6189 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "laufende Leistungen zum Unterhalt und Leistungen der Krankenhilfe nach dem Achten Buch beziehen,"@de .

dtd:LA-id6190 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "krankenversorgungsberechtigt nach dem Bundesentschädigungsgesetz sind,"@de .

dtd:LA-id6191 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in das Dienstverhältnis eines Soldaten auf Zeit berufen worden sind,"@de .

dtd:LA-id6192 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Heilfürsorgeberechtigte, die nicht in der sozialen Pflegeversicherung versicherungspflichtig sind,"@de .

dtd:LA-id6193 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mitglieder der Postbeamtenkrankenkasse und"@de .

dtd:LA-id6194 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mitglieder der Krankenversorgung der Bundesbahnbeamten."@de .

dtd:LA-id6195 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Feststellung der Pflegebedürftigkeit sowie für die Zuordnung zu einem Pflegegrad dieselben Maßstäbe wie in der sozialen Pflegeversicherung anzulegen und"@de .

dtd:LA-id6196 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die in der sozialen Pflegeversicherung zurückgelegte Versicherungszeit des Mitglieds und seiner nach § 25 familienversicherten Angehörigen oder Lebenspartner auf die Wartezeit anzurechnen."@de .

dtd:LA-id6197 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihren Wohnsitz oder gewöhnlichen Aufenthalt im Inland haben,"@de .

dtd:LA-id6198 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht nach § 20 Abs. 1 Nr. 1 bis 8 oder 11 oder nach § 20 Abs. 3 versicherungspflichtig sind,"@de .

dtd:LA-id6199 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht nach § 22 von der Versicherungspflicht befreit oder nach § 23 in der privaten Pflegeversicherung pflichtversichert sind,"@de .

dtd:LA-id6200 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht hauptberuflich selbständig erwerbstätig sind und"@de .

dtd:LA-id6201 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "kein Gesamteinkommen haben, das regelmäßig im Monat ein Siebtel der monatlichen Bezugsgröße nach § 18 des Vierten Buches, überschreitet; bei Abfindungen, Entschädigungen oder ähnlichen Leistungen (Entlassungsentschädigungen), die wegen der Beendigung eines Arbeitsverhältnisses in Form nicht monatlich wiederkehrender Leistungen gezahlt werden, wird das zuletzt erzielte monatliche Arbeitsentgelt für die der Auszahlung der Entlassungsentschädigung folgenden Monate bis zu dem Monat berücksichtigt, in dem im Fall der Fortzahlung des Arbeitsentgelts die Höhe der gezahlten Entlassungsentschädigung erreicht worden wäre; bei Renten wird der Zahlbetrag ohne den auf Entgeltpunkte für Kindererziehungszeiten entfallenden Teil berücksichtigt; für Familienangehörige, die eine geringfügige Beschäftigung nach § 8 Absatz 1 Nummer 1 oder § 8a des Vierten Buches in Verbindung mit § 8 Absatz 1 Nummer 1 des Vierten Buches ausüben, ist ein regelmäßiges monatliches Gesamteinkommen bis zur Geringfügigkeitsgrenze zulässig."@de .

dtd:LA-id6202 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bis zur Vollendung des 18. Lebensjahres,"@de .

dtd:LA-id6203 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bis zur Vollendung des 23. Lebensjahres, wenn sie nicht erwerbstätig sind,"@de .

dtd:LA-id6204 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bis zur Vollendung des 25. Lebensjahres, wenn sie sich in Schul- oder Berufsausbildung befinden oder ein freiwilliges soziales Jahr oder ein freiwilliges ökologisches Jahr im Sinne des Jugendfreiwilligendienstegesetzes leisten; wird die Schul- oder Berufsausbildung durch Erfüllung einer gesetzlichen Dienstpflicht des Kindes unterbrochen oder verzögert, besteht die Versicherung auch für einen der Dauer dieses Dienstes entsprechenden Zeitraum über das 25. Lebensjahr hinaus; dies gilt auch bei einer Unterbrechung durch den freiwilligen Wehrdienst nach § 58b des Soldatengesetzes, einen Freiwilligendienst nach dem Bundesfreiwilligendienstgesetz, dem Jugendfreiwilligendienstegesetz oder einen vergleichbaren anerkannten Freiwilligendienst oder durch eine Tätigkeit als Entwicklungshelfer im Sinne des § 1 Absatz 1 des Entwicklungshelfer-Gesetzes für die Dauer von höchstens zwölf Monaten; wird als Berufsausbildung ein Studium an einer staatlichen oder staatlich anerkannten Hochschule abgeschlossen, besteht die Versicherung bis zum Ablauf des Semesters fort, längstens bis zur Vollendung des 25. Lebensjahres; § 186 Absatz 7 Satz 2 und 3 des Fünften Buches gilt entsprechend,"@de .

dtd:LA-id6205 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ohne Altersgrenze, wenn sie wegen körperlicher, geistiger oder seelischer Behinderung (§ 2 Abs. 1 des Neunten Buches) außerstande sind, sich selbst zu unterhalten; Voraussetzung ist, daß die Behinderung (§ 2 Abs. 1 des Neunten Buches) zu einem Zeitpunkt vorlag, in dem das Kind innerhalb der Altersgrenzen nach den Nummern 1, 2 oder 3 familienversichert war oder die Familienversicherung nur wegen einer Vorrangversicherung nach Absatz 1 Satz 1 Nummer 2 ausgeschlossen war."@de .

dtd:LA-id6206 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflegesachleistung (§ 36),"@de .

dtd:LA-id6207 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflegegeld für selbst beschaffte Pflegehilfen (§ 37),"@de .

dtd:LA-id6208 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kombination von Geldleistung und Sachleistung (§ 38),"@de .

dtd:LA-id6209 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zusätzliche Leistungen für Pflegebedürftige in ambulant betreuten Wohngruppen (§ 38a),"@de .

dtd:LA-id6210 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "häusliche Pflege bei Verhinderung der Pflegeperson (§ 39),"@de .

dtd:LA-id6211 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflegehilfsmittel und wohnumfeldverbessernde Maßnahmen (§ 40),"@de .

dtd:LA-id6212 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ergänzende Unterstützung bei Nutzung von digitalen Pflegeanwendungen (§ 39a in Verbindung mit § 40b) und digitale Pflegeanwendungen (§ 40a in Verbindung mit § 40b),"@de .

dtd:LA-id6213 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Tagespflege und Nachtpflege (§ 41),"@de .

dtd:LA-id6214 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kurzzeitpflege (§ 42),"@de .

dtd:LA-id6215 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Versorgung Pflegebedürftiger bei Inanspruchnahme von Vorsorge- oder Rehabilitationsleistungen durch die Pflegeperson (§ 42a),"@de .

dtd:LA-id6216 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vollstationäre Pflege (§ 43),"@de .

dtd:LA-id6217 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pauschalleistung für die Pflege von Menschen mit Behinderungen (§ 43a),"@de .

dtd:LA-id6218 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zusätzliche Betreuung und Aktivierung in stationären Pflegeeinrichtungen (§ 43b),"@de .

dtd:LA-id6219 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leistungen zur sozialen Sicherung der Pflegepersonen (§ 44),"@de .

dtd:LA-id6220 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zusätzliche Leistungen bei Pflegezeit und kurzzeitiger Arbeitsverhinderung (§ 44a),"@de .

dtd:LA-id6221 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflegekurse für Angehörige und ehrenamtliche Pflegepersonen (§ 45),"@de .

dtd:LA-id6222 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Umwandlung des ambulanten Sachleistungsbetrags (§ 45a),"@de .

dtd:LA-id6223 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Entlastungsbetrag (§ 45b),"@de .

dtd:LA-id6224 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leistungen des Persönlichen Budgets nach § 29 des Neunten Buches gemäß § 35a."@de .

dtd:LA-id6225 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflegeberatung gemäß den §§ 7a und 7b,"@de .

dtd:LA-id6226 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beratung in der eigenen Häuslichkeit gemäß § 37 Absatz 3,"@de .

dtd:LA-id6227 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zusätzliche Leistungen für Pflegebedürftige in ambulant betreuten Wohngruppen gemäß § 38a, ohne dass § 38a Absatz 1 Satz 1 Nummer 2 erfüllt sein muss,"@de .

dtd:LA-id6228 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Versorgung mit Pflegehilfsmitteln gemäß § 40,"@de .

dtd:LA-id6229 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "finanzielle Zuschüsse für Maßnahmen zur Verbesserung des individuellen oder gemeinsamen Wohnumfelds gemäß § 40,"@de .

dtd:LA-id6230 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Leistungen zur ergänzenden Unterstützung bei der Nutzung von digitalen Pflegeanwendungen sowie zur Versorgung mit digitalen Pflegeanwendungen gemäß den §§ 39a, 40a und 40b,"@de .

dtd:LA-id6231 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Versorgung Pflegebedürftiger bei Inanspruchnahme von Vorsorge- oder Rehabilitationsleistungen durch die Pflegeperson gemäß § 42a,"@de .

dtd:LA-id6232 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen monatlichen Zuschuss bei vollstationärer Pflege gemäß § 43 Absatz 3,"@de .

dtd:LA-id6233 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zusätzliche Betreuung und Aktivierung in stationären Pflegeeinrichtungen gemäß § 43b,"@de .

dtd:LA-id6234 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zusätzliche Leistungen bei Pflegezeit und kurzzeitiger Arbeitsverhinderung gemäß § 44a,"@de .

dtd:LA-id6235 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflegekurse für Angehörige und ehrenamtliche Pflegepersonen gemäß § 45,"@de .

dtd:LA-id6236 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Entlastungsbetrag gemäß § 45b,"@de .

dtd:LA-id6237 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anschubfinanzierung zur Gründung von ambulant betreuten Wohngruppen gemäß § 45e nach Maßgabe von § 28 Absatz 1b."@de .

dtd:LA-id6238 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "solange sich der Versicherte im Ausland aufhält. Bei vorübergehendem Auslandsaufenthalt von bis zu sechs Wochen im Kalenderjahr ist das Pflegegeld nach § 37 oder anteiliges Pflegegeld nach § 38 weiter zu gewähren. Für die Pflegesachleistung gilt dies nur, soweit die Pflegekraft, die ansonsten die Pflegesachleistung erbringt, den Pflegebedürftigen während des Auslandsaufenthaltes begleitet,"@de .

dtd:LA-id6239 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit Versicherte Entschädigungsleistungen wegen Pflegebedürftigkeit aus der gesetzlichen Unfallversicherung oder aus öffentlichen Kassen auf Grund gesetzlich geregelter Unfallversorgung oder Unfallfürsorge erhalten. Dies gilt auch, wenn vergleichbare Leistungen aus dem Ausland oder von einer zwischenstaatlichen oder überstaatlichen Einrichtung bezogen werden."@de .

dtd:LA-id6240 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Bewältigung psychosozialer Problemlagen oder von Gefährdungen,"@de .

dtd:LA-id6241 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Orientierung, bei der Tagesstrukturierung, bei der Kommunikation, bei der Aufrechterhaltung sozialer Kontakte und bei bedürfnisgerechten Beschäftigungen im Alltag sowie"@de .

dtd:LA-id6242 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Maßnahmen zur kognitiven Aktivierung."@de .

dtd:LA-id6243 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Pflegebedürftige des Pflegegrades 2 Leistungen bis zu einem Gesamtwert von 761 Euro,"@de .


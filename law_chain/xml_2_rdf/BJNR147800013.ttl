dtd:P-id52 a dtd:P ;
    dtd:has_BR dtd:BR-id47 ;
    dtd:has_Value " (+++ Textnachweis ab 11.6.2013 +++)(+++ Zur Anwendung vgl. § 5 +++)Das G wurde als Artikel 6 des G v. 4.6.2013 I 1471 vom Bundestag beschlossen. Es ist gem. Art. 8 dieses G am 11.6.2013 in Kraft getreten."@de ;
    dtd:has_pre dtd:pre-id74 .

dtd:P-id54 a dtd:P ;
    dtd:has_Value "Kommt der eingetragene Eigentümer eines Schiffes seiner Pflicht zur Beseitigung nach Artikel 9 Absatz 2 des Internationalen Übereinkommens von Nairobi von 2007 über die Beseitigung von Wracks (BGBl. 2013 II S. 530, 531) (Wrackbeseitigungsübereinkommen) nicht nach, erfolgt die Beseitigung im Rahmen des Artikels 9 Absatz 6 bis 8 des Wrackbeseitigungsübereinkommens durch die nach § 3 zuständige Behörde nach Maßgabe der §§ 677 bis 687 des Bürgerlichen Gesetzbuches."@de .

dtd:P-id56 a dtd:P ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id75 ;
    dtd:has_Value "(+++ §§ 1 bis 4: Zur Anwendung vgl. § 5 Abs. 1 iVm Bek. v. 12.3.2015 I  321 +++)"@de .

dtd:P-id58 a dtd:P ;
    dtd:has_Value "Auf den Aufwendungsersatz nach den Artikeln 10, 11 und 12 des Wrackbeseitigungsübereinkommens sind die §§ 683 und 670 des Bürgerlichen Gesetzbuches anzuwenden."@de .

dtd:P-id60 a dtd:P ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id76 ;
    dtd:has_Value "(+++ §§ 1 bis 4: Zur Anwendung vgl. § 5 Abs. 1 iVm Bek. v. 12.3.2015 I  321 +++)"@de .

dtd:P-id62 a dtd:P ;
    dtd:has_Value "Zuständige Behörde im Sinne der §§ 1 und 2 ist die für die Maßnahmen nach den Artikeln 6 bis 8 und 9 Absatz 1 und 4 des Wrackbeseitigungsübereinkommens zuständige Schifffahrtspolizeibehörde des Bundes."@de .

dtd:P-id64 a dtd:P ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id77 ;
    dtd:has_Value "(+++ §§ 1 bis 4: Zur Anwendung vgl. § 5 Abs. 1 iVm Bek. v. 12.3.2015 I  321 +++)"@de .

dtd:P-id66 a dtd:P ;
    dtd:has_Value "In Streitigkeiten nach dem Wrackbeseitigungsübereinkommen wegen der Ansprüche nach § 2 ist das Landgericht im ersten Rechtszug zuständig, in dessen Bezirk die nach § 3 zuständige Behörde ihren Sitz hat."@de .

dtd:P-id68 a dtd:P ;
    dtd:has_ABWFORMAT dtd:ABWFORMAT-id78 ;
    dtd:has_Value "(+++ §§ 1 bis 4: Zur Anwendung vgl. § 5 Abs. 1 iVm Bek. v. 12.3.2015 I  321 +++)"@de .

dtd:P-id70 a dtd:P ;
    dtd:has_Value "(1) Die §§ 1 bis 4 sind erst ab dem Tag anzuwenden, an dem das Wrackbeseitigungsübereinkommen für die Bundesrepublik Deutschland in Kraft tritt."@de .

dtd:P-id71 a dtd:P ;
    dtd:has_Value "(2) Das Bundesministerium für Verkehr, Bau und Stadtentwicklung gibt den in Absatz 1 bezeichneten Tag im Bundesgesetzblatt bekannt."@de .

dtd:P-id73 a dtd:P ;
    dtd:has_Value "(+++ § 5 Abs. 2: Zur Anwendung ab dem 14.4.2015 vgl. Bek. v. 12.3.2015 I  321 +++)"@de .


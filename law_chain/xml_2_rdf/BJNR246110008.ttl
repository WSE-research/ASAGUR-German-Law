    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle Rinder des Bestandes für einen Zeitraum von 40 Tagen nicht aus dem Bestand verbracht werden,"@de .

dtd:LA-id269 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu diesem Zeitpunkt tragende Rinder erst nach dem Abkalben aus dem Bestand verbracht werden."@de .

dtd:LA-id270 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Falle der Nummer 1 Rinder unmittelbar zur Schlachtung verbracht werden oder nach § 2 Absatz 2 geimpft sind, sowie"@de .

dtd:LA-id271 a dtd:LA ;
    dtd:has_DL dtd:DL-id185 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Falle der Nummer 2 die tragenden Rinder a)zum Zeitpunkt der Belegung nach § 2 Absatz 2 geimpft waren,b)nach dem 150. Trächtigkeitstag serologisch nach einer in der amtlichen Methodensammlung beschriebenen Methode mit negativem Ergebnis auf BVDV untersucht worden sind."@de .

dtd:LA-id272 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Zeitpunkt der Belegung nach § 2 Absatz 2 geimpft waren,"@de .

dtd:LA-id273 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach dem 150. Trächtigkeitstag serologisch nach einer in der amtlichen Methodensammlung beschriebenen Methode mit negativem Ergebnis auf BVDV untersucht worden sind."@de .

dtd:LA-id274 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Anordnung nach § 2 Absatz 1 oder § 3 Absatz 3 zuwiderhandelt,"@de .

dtd:LA-id275 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Absatz 1 Satz 1 oder Absatz 5 ein Rind nicht oder nicht rechtzeitig untersuchen lässt,"@de .

dtd:LA-id276 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Absatz 1 Satz 2 nicht sicherstellt, dass der dort genannten Einrichtung eine dort genannte Angabe nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig mitgeteilt wird,"@de .

dtd:LA-id277 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Absatz 4 Satz 1 oder § 5 Absatz 2 Satz 1 ein Rind nicht oder nicht rechtzeitig töten lässt,"@de .

dtd:LA-id278 a dtd:LA ;

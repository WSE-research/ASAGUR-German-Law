dtd:LA-id249 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des verwendeten Impfstoffes"@de .

dtd:LA-id250 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nach dem Inkrafttreten dieser Verordnung in seinem Bestand geboren worden sind, bis zur Vollendung des ersten Lebensmonats oder"@de .


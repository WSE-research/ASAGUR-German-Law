dtd:Footnote-id872 a dtd:Footnote ;
    dtd:has_FnZ "*"@de ;
    dtd:has_Group dtd:column ;
    dtd:has_ID "BJNR180910013_01"@de ;
    dtd:has_Pos dtd:normal ;
    dtd:has_Value "Dieses Gesetzes dient der Umsetzung der Richtlinie 2011/16/EU des Rates vom 15. Februar 2011 über die Zusammenarbeit der Verwaltungsbehörden im Bereich der Besteuerung und zur Aufhebung der Richtlinie 77/799/EWG (ABl. L 64 vom 11.3.2011, S. 1)."@de ;
    dtd:has_noindex dtd:noindex-id259 .

dtd:Footnotes-id871 a dtd:Footnotes ;
    dtd:has_Footnote dtd:Footnote-id872 .

dtd:Ident-id703 a dtd:Ident ;
    dtd:has_Class "S0"@de ;

dtd:LA-id324 a dtd:LA ;
    dtd:has_Value "den See- und Hafendienstplan für jedes an Bord beschäftigte Besatzungsmitglied,"@de .

dtd:LA-id325 a dtd:LA ;
    dtd:has_DL dtd:DL-id314 ;
    dtd:has_Value "a)die Höchstarbeitszeiten und die Mindestruhezeiten nach § 48 Absatz 1 des Seearbeitsgesetzes,b)die auf Grund des Seearbeitsgesetzes zulässigen von § 48 Absatz 1 abweichenden Höchstarbeitszeiten und Mindestruhezeiten,"@de .

dtd:LA-id326 a dtd:LA ;
    dtd:has_Value "die Höchstarbeitszeiten und die Mindestruhezeiten nach § 48 Absatz 1 des Seearbeitsgesetzes,"@de .

dtd:LA-id327 a dtd:LA ;
    dtd:has_Value "die auf Grund des Seearbeitsgesetzes zulässigen von § 48 Absatz 1 abweichenden Höchstarbeitszeiten und Mindestruhezeiten,"@de .

dtd:LA-id328 a dtd:LA ;
    dtd:has_Value "die Aufgaben im Wachdienst und jede zu erwartende zusätzliche Arbeit und"@de .


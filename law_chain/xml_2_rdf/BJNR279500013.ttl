    dtd:has_Group dtd:column ;
    dtd:has_ID "f782027_01_BJNR279500013"@de ;
    dtd:has_Pos dtd:normal ;
    dtd:has_Value "Die Verordnung dient der Umsetzung der Richtlinie 1999/63/EG des Rates vom 21. Juni 1999 zu der vom Verband der Reeder in der Europäischen Gemeinschaft (ECSA) und dem Verband der Verkehrsgewerkschaften in der Europäischen Union (FST) getroffenen Vereinbarung über die Regelung der Arbeitszeit von Seeleuten (ABl. L 167 vom 2.7.1999, S. 33), die zuletzt durch die Richtlinie 2009/13/EG (ABl. L 124 vom 20.5.2009, S. 30) geändert worden ist."@de ;
    dtd:has_noindex dtd:noindex-id143 .

dtd:Footnote-id386 a dtd:Footnote ;
    dtd:has_FnZ "1"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f782027_02_BJNR279500013BJNE001000000"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Die in diesem Muster verwendeten Begriffe sind in den Arbeitssprachen des Schiffes sowie in Englisch anzugeben."@de .

dtd:Footnote-id387 a dtd:Footnote ;
    dtd:has_FnZ "2"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f782027_03_BJNR279500013BJNE001000000"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Hier sind die gleichen wie die im Schiffsbesatzungszeugnis für diese dienstliche Stellung/Ränge enthaltenen Begriffe zu verwenden."@de .

dtd:Footnote-id388 a dtd:Footnote ;
    dtd:has_FnZ "3"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f782027_04_BJNR279500013BJNE001000000"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Nichtzutreffendes streichen."@de .

dtd:Footnote-id389 a dtd:Footnote ;
    dtd:has_FnZ "4"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f782027_05_BJNR279500013BJNE001000000"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Für das Wachpersonal können unter der Rubrik „Bemerkungen“ Angaben zu der voraussichtlich für außerplanmäßige Dienste abzuleistenden Stundenzahl eingetragen werden; diese Stunden sind in die Rubrik zur Angabe der täglichen Gesamtarbeitszeit aufzunehmen."@de .

dtd:Footnote-id391 a dtd:Footnote ;
    dtd:has_FnZ "1"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f782027_06_BJNR279500013BJNE001100000"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Die in dieser Mustertabelle enthaltenen Angaben sind in den Arbeitssprachen des Schiffes sowie in Englisch zu machen."@de .

dtd:Footnote-id392 a dtd:Footnote ;
    dtd:has_F dtd:F-id124 ;

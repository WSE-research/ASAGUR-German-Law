    dtd:has_ID "f782027_07_BJNR279500013BJNE001100000"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Mit √ bestätigen."@de .

dtd:Footnote-id393 a dtd:Footnote ;
    dtd:has_FnZ "3"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "f782027_08_BJNR279500013BJNE001100000"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Nichtzutreffendes streichen."@de .

dtd:Footnote-id394 a dtd:Footnote ;
    dtd:has_FnZ "1"@de ;

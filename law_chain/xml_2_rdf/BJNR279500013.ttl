    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Auszufüllen und zu verwenden gemäß den von der Berufsgenossenschaft für Transport und Verkehrswirtschaft vorgegebenen Verfahren."@de .

dtd:Footnote-id395 a dtd:Footnote ;
    dtd:has_FnZ "2"@de ;

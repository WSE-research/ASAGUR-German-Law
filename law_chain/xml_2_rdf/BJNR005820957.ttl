dtd:P-id956 a dtd:P ;
    dtd:has_Value "(1) Die Zulassung erfolgt fÃ¼r den Ort der Niederlassung als Zahnarzt (Vertragszahnarztsitz)."@de .

dtd:P-id957 a dtd:P ;

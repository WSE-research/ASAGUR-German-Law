dtd:LA-id448 a dtd:LA ;
    dtd:has_Value "zielgruppengerechtes Auswählen und Umsetzen der Vertriebs- und Verkaufsaktivitäten eines Unternehmens,"@de .

dtd:LA-id449 a dtd:LA ;
    dtd:has_Value "bedarfsgerechtes Planen, Strukturieren, Durchführen und Nachbereiten operativer Vertriebstätigkeiten, um Kundenbeziehungen nachhaltig zu sichern, Neukunden und Neukundinnen zu gewinnen und die Rückgewinnung von Kunden und Kundinnen zu ermöglichen,"@de .

dtd:LA-id450 a dtd:LA ;
    dtd:has_Value "Entwickeln und Anbieten individueller Lösungen für Kunden und Kundinnen,"@de .

dtd:LA-id451 a dtd:LA ;
    dtd:has_Value "Reflektieren eigener Verhaltensweisen und der Verhaltensweisen der Kunden und Kundinnen, um den Verkaufsprozess zu optimieren."@de .

dtd:LA-id452 a dtd:LA ;
    dtd:has_Value "eine erfolgreich abgelegte Abschlussprüfung in einem anerkannten Ausbildungsberuf mit einer Berufsausbildungsdauer von drei Jahren,"@de .

dtd:LA-id453 a dtd:LA ;
    dtd:has_Value "eine erfolgreich abgelegte Abschlussprüfung in einem anerkannten Ausbildungsberuf mit einer Berufsausbildungsdauer von zwei Jahren und eine auf die Berufsausbildung folgende, mindestens einjährige Berufspraxis,"@de .

dtd:LA-id454 a dtd:LA ;
    dtd:has_Value "den Erwerb von mindestens 60 Leistungspunkten nach dem Europäischen System zur Übertragung und Akkumulierung von Studienleistungen in einem Studium und eine mindestens zweijährige Berufspraxis oder"@de .

dtd:LA-id455 a dtd:LA ;
    dtd:has_Value "eine mindestens vierjährige Berufspraxis."@de .

dtd:LA-id456 a dtd:LA ;
    dtd:has_Value "„Selbstmanagement im Vertrieb sicherstellen“ nach § 4,"@de .

dtd:LA-id457 a dtd:LA ;
    dtd:has_Value "„Operative Tätigkeiten im Vertrieb durchführen“ nach § 5 sowie"@de .

dtd:LA-id458 a dtd:LA ;
    dtd:has_Value "„Kundenorientierte Kommunikation im Vertrieb gestalten“ nach § 6."@de .

dtd:LA-id459 a dtd:LA ;
    dtd:has_Value "Berücksichtigen kultureller Unterschiede im Kontakt mit Kunden und Kundinnen und Reflektieren des eigenen Verhaltens vor diesem Hintergrund,"@de .

dtd:LA-id460 a dtd:LA ;
    dtd:has_Value "Feedback nutzen und Vertriebserfolge evaluieren, um Lern- und Entwicklungsziele im Hinblick auf lebenslanges Lernen abzuleiten und die Qualität der eigenen Vertriebsaktivitäten zu optimieren,"@de .

dtd:LA-id461 a dtd:LA ;
    dtd:has_Value "Reflektieren der Ablauforganisation, Bewerten von Prozessinnovationen und Ableiten von Maßnahmen für die eigenen Vertriebsaktivitäten,"@de .

dtd:LA-id462 a dtd:LA ;
    dtd:has_Value "Einsetzen von Methoden des Selbstmanagements,"@de .

dtd:LA-id463 a dtd:LA ;
    dtd:has_Value "Reflektieren der Merkmale von Vertriebspersönlichkeiten und kundenorientiertes Einsetzen dieser Merkmale."@de .

dtd:LA-id464 a dtd:LA ;
    dtd:has_Value "Gestalten von Vertriebsaktivitäten für eine nachhaltige Beziehung zu Kunden und Kundinnen unter Abwägung von Chancen und Risiken,"@de .

dtd:LA-id465 a dtd:LA ;
    dtd:has_Value "Analysieren der Aufbau- und Ablauforganisation von Kunden und Kundinnen und Identifizieren von Beteiligten an Kaufentscheidungen,"@de .

dtd:LA-id466 a dtd:LA ;
    dtd:has_Value "Ermitteln der Bedürfnisse von Kunden und Kundinnen und Ableiten des Bedarfs,"@de .

dtd:LA-id467 a dtd:LA ;
    dtd:has_Value "Analysieren von Vertriebskennziffern und aus Veränderungen im Kaufverhalten von Kunden und Kundinnen Maßnahmen für die eigene Vertriebstätigkeit ableiten,"@de .

dtd:LA-id468 a dtd:LA ;
    dtd:has_Value "Auswählen und Einsetzen von Maßnahmen zum Kundenbeziehungsmanagement,"@de .


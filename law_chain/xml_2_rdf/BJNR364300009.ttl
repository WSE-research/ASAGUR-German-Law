dtd:Footnote-id891 a dtd:Footnote ;
    dtd:has_FnZ "*"@de ;
    dtd:has_Group dtd:column ;
    dtd:has_ID "F772647_01_BJNR364300009"@de ;
    dtd:has_Pos dtd:normal ;
    dtd:has_Postfix ")"@de ;
    dtd:has_Value "Diese Verordnung dient der weiteren Umsetzung der Artikel 7 und 8 der Richtlinie 2007/64/EG des Europäischen Parlaments und des Rates vom 13. November 2007 über Zahlungsdienste im Binnenmarkt, zur Änderung der Richtlinien 97/7/EG, 2002/65/EG, 2005/60/EG und 2006/48/EG sowie zur Aufhebung der Richtlinie 97/5/EG (ABl. L 319 vom 5.12.2007, S. 1, L 187 vom 18.7.2009, S. 5)."@de .

dtd:Footnote-id893 a dtd:Footnote ;
    dtd:has_FnZ "1"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F804815_01a_BJNR364300009BJNE001102116"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Jeder Betrag, der die Eigenmittel erhöht, hat ein positives Vorzeichen. Jeder Betrag, der die Eigenmittel reduziert, hat ein negatives Vorzeichen."@de ;
    dtd:has_noindex dtd:noindex-id266 .

dtd:Footnote-id894 a dtd:Footnote ;
    dtd:has_FnZ "2"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F804815_02a_BJNR364300009BJNE001102116"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "CRR bezeichnet in dieser Anlage die Verordnung (EU) Nr. 575/2013 des Europäischen Parlaments und des Rates vom 26. Juni 2013 über Aufsichtsanforderungen an Kreditinstitute und Wertpapierfirmen und zur Änderung der Verordnung (EU) Nr. 648/2012 (ABl. L 176 vom 27.6.2013, S. 1)."@de ;
    dtd:has_noindex dtd:noindex-id267 .

dtd:Footnote-id895 a dtd:Footnote ;
    dtd:has_FnZ "3"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F804815_03a_BJNR364300009BJNE001102116"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Bei der Berechnung der Eigenmittel müssen mindestens 75 Prozent des Kernkapitals in Form von hartem Kernkapital nach Artikel 50 CRR berücksichtigt werden."@de ;
    dtd:has_noindex dtd:noindex-id268 .

dtd:Footnote-id896 a dtd:Footnote ;
    dtd:has_FnZ "4"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F804815_04a_BJNR364300009BJNE001102116"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Bei der Berechnung der Eigenmittel darf das Ergänzungskapital höchstens ein Drittel des harten Kernkapitals betragen."@de ;
    dtd:has_noindex dtd:noindex-id269 .

dtd:Footnote-id897 a dtd:Footnote ;
    dtd:has_FnZ "5"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F804815_05a_BJNR364300009BJNE001102116"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Bei Zahlungsinstituten ist die in § 2 Abs. 1 ZIEV vorgegebene Methode B anzuwenden, sofern nicht nach § 6 ZIEV eine andere Methode festgelegt worden ist. Die Anforderungen sind für die jeweils angewendete Methode vollständig zu melden. Die Ziffern 2 bis 5 sind von Unternehmen, die ausschließlich Zahlungsauslösedienste erbringen, nicht anzugeben."@de ;
    dtd:has_noindex dtd:noindex-id270 .

dtd:Footnote-id898 a dtd:Footnote ;
    dtd:has_FnZ "6"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F804815_06a_BJNR364300009BJNE001102116"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das jeweilige Endergebnis für die gerechnete Methode (Zeile 0320, 0360 oder 0430) ist in diese Zeile zu übertragen."@de ;
    dtd:has_noindex dtd:noindex-id271 .

dtd:Footnote-id899 a dtd:Footnote ;
    dtd:has_FnZ "7"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F804815_07a_BJNR364300009BJNE001102116"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Nur auszufüllen von Instituten, die eine Erlaubnis gemäß § 32 Abs. 1 des Kreditwesengesetzes (KWG) haben."@de ;
    dtd:has_noindex dtd:noindex-id272 .

dtd:Footnote-id900 a dtd:Footnote ;
    dtd:has_FnZ "8"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F804815_08a_BJNR364300009BJNE001102116"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "„1“ eintragen, wenn die Eigenmittelanforderungen nach ZIEV kleiner oder gleich den Eigenmittelanforderungen nach der CRR; „2“ eintragen, wenn die Eigenmittelanforderungen nach ZIEV größer den Eigenmittelanforderungen nach der CRR."@de ;
    dtd:has_noindex dtd:noindex-id273 .


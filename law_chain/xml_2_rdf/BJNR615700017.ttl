dtd:LA-id282 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Anerkennung einer Stelle für die Abnahme von Sprachprüfungen sowie Einzelheiten zur Aufsicht gemäß § 125a Absatz 2 der Verordnung über Luftfahrtpersonal und"@de .

dtd:LA-id283 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Verfahren bei Beschwerden bezüglich des Ergebnisses einer Sprachprüfung."@de .


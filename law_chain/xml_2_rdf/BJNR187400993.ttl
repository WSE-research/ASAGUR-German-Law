dtd:Footnote-id524 a dtd:Footnote ;
    dtd:has_FnZ "1"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F828014_01_BJNR187400993BJNE000903311"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Die Maßnahmen dienen der Umsetzung des Deutschlandtakts."@de .

dtd:Footnote-id525 a dtd:Footnote ;
    dtd:has_FnZ "2"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F828014_02_BJNR187400993BJNE000903311"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Bei der baulichen Umsetzung der Abschnitte Veerßen – Salzwedel und Hohenwulsch – Stendal sollen die aktuellen Standards des vorsorgenden Lärmschutzes angewendet werden. Der Abschnitt Salzwedel – Hohenwulsch ist nachträglich mit vergleichbarem vorsorgenden Lärmschutz auszustatten."@de .

dtd:Footnote-id526 a dtd:Footnote ;
    dtd:has_FnZ "3"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F828014_03_BJNR187400993BJNE000903311"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Mit Infrastruktur für Fernverkehrshalt in Aschaffenburg."@de .

dtd:Footnote-id527 a dtd:Footnote ;
    dtd:has_FnZ "4"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F828014_04_BJNR187400993BJNE000903311"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Aus- und Neubau für eine Zielreisezeit im Taktfahrplan Hamburg – Kopenhagen von unter 150 Minuten und Berlin – Kopenhagen von unter 240 Minuten."@de .

dtd:Footnote-id528 a dtd:Footnote ;
    dtd:has_FnZ "5"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F828014_05_BJNR187400993BJNE000903311"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Mit Halt Köln-Mülheim ohne Infrastrukturausbau laut Betriebsprogramm der Aufgabenträger."@de .

dtd:Footnote-id529 a dtd:Footnote ;
    dtd:has_FnZ "6"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F828014_06_BJNR187400993BJNE000903311"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Unter Berücksichtigung des viergleisigen Ausbaus des Bahnhofs Elmshorn."@de .

dtd:Footnote-id530 a dtd:Footnote ;
    dtd:has_FnZ "7"@de ;
    dtd:has_Group dtd:manuell ;
    dtd:has_ID "F828014_07_BJNR187400993BJNE000903311"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Oder mit stadtverträglicher umfahrender Alternative zur Viersener Kurve – Prüfung und Planung kann zu nachrangiger Umsetzung des Teilabschnitts führen."@de .


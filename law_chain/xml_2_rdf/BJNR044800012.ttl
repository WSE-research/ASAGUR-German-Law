dtd:LA-id80 a dtd:LA ;
    dtd:has_Value "Frauen, die von Gewalt betroffen sind,"@de .

dtd:LA-id81 a dtd:LA ;
    dtd:has_Value "Personen aus dem sozialen Umfeld von Frauen, die von Gewalt betroffen sind, und"@de .

dtd:LA-id82 a dtd:LA ;

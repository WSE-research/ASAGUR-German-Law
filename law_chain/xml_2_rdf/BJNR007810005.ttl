    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sprengstoffen, Munition, Zündkapseln, brennbaren Flüssigkeiten, ätzenden oder giftigen Stoffen, Gasen in Behältern sowie sonstigen Stoffen, die allein oder zusammen mit anderen Gegenständen eine Explosion oder einen Brand verursachen können,"@de .

dtd:LA-id751 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gegenständen, die ihrer äußeren Form oder ihrer Kennzeichnung nach den Anschein von Waffen, Munition oder explosionsgefährlichen Stoffen erwecken,"@de .

dtd:LA-id752 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstigen in der Anlage 4-C zu Kapitel 4 des Anhangs der Durchführungsverordnung (EU) 2015/1998 genannten Gegenständen"@de .

dtd:LA-id753 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in aufgegebenem Gepäck die in Anlage 5-B zu Kapitel 5 des Anhangs der Durchführungsverordnung (EU) 2015/1998 genannten Gegenstände;"@de .

dtd:LA-id754 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Fracht und Post die in Ziffer 6.0.2. des Anhangs der Durchführungsverordnung (EU) 2015/1998 genannten Gegenstände;"@de .

dtd:LA-id755 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Bordvorräten und Flughafenlieferungen die in Anlage 1-A zu Kapitel 1 des Anhangs der Durchführungsverordnung (EU) 2015/1998 genannten Gegenstände;"@de .

dtd:LA-id756 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Personen, die keine Fluggäste sind, die in Anlage 1-A zu Kapitel 1 des Anhangs der Durchführungsverordnung (EU) 2015/1998 genannten Gegenstände; dies gilt auch dann, wenn diese Gegenstände sich in sonstigen mitgeführten Gegenständen oder in Fahrzeugen befinden."@de .

dtd:LA-id757 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Identität einer Person feststellen,"@de .

dtd:LA-id758 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gegenstände sicherstellen,"@de .


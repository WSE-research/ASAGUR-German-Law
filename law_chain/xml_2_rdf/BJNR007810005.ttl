dtd:LA-id739 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Luftfahrzeuge, die Gegenstand von Bedrohungen, insbesondere von Bombendrohungen sind, auf eine Sicherheitsposition zu verbringen oder bei einer Verbringung durch den Flugplatzbetreiber nach § 8 Absatz 1 Satz 1 Nummer 7 mitzuwirken;"@de .

dtd:LA-id740 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit erforderlich, an der Überprüfung nach § 7 mitzuwirken."@de .

dtd:LA-id741 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Luftfahrtunternehmen, die eine Genehmigung nach § 20 des Luftverkehrsgesetzes besitzen, auch außerhalb des Geltungsbereichs dieses Gesetzes, wenn und soweit die jeweils örtlich geltenden Vorschriften nicht entgegenstehen;"@de .

dtd:LA-id742 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Luftfahrtunternehmen, die ihren Hauptsitz außerhalb des Geltungsbereichs dieses Gesetzes haben, sofern sie Verkehrsflughäfen in der Bundesrepublik Deutschland benutzen, auch außerhalb des Geltungsbereichs dieses Gesetzes, wenn und soweit die jeweils örtlich geltenden Vorschriften nicht entgegenstehen."@de .

dtd:LA-id743 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name,"@de .

dtd:LA-id744 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nummer des Personalausweises oder des Reisepasses,"@de .


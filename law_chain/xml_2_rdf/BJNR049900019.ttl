dtd:LA-id810 a dtd:LA ;
    dtd:has_Value "zur Krankheitsprävention anleiten und Maßnahmen der Krankheitsprävention organisieren sowie"@de .

dtd:LA-id811 a dtd:LA ;
    dtd:has_Value "die Beendigung von Arbeitsverhältnissen durchführen."@de .

dtd:LA-id812 a dtd:LA ;
    dtd:has_Value "Mitarbeiter und Mitarbeiterinnen anleiten, Leistungen und Verhalten gegebenenfalls unter Hinzuziehung von Leistungsbeurteilungen Dritter feststellen und bewerten,"@de .

dtd:LA-id813 a dtd:LA ;
    dtd:has_Value "Mitarbeiter- und Beurteilungsgespräche führen und Entwicklungsmöglichkeiten aufzeigen,"@de .

dtd:LA-id814 a dtd:LA ;

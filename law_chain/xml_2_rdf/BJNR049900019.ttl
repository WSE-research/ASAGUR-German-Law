dtd:LA-id815 a dtd:LA ;
    dtd:has_Value "Mitarbeiter und Mitarbeiterinnen qualifizieren und bei der Weiterbildung unterstützen,"@de .

dtd:LA-id816 a dtd:LA ;
    dtd:has_Value "soziale Zusammenhänge und Konflikte erkennen,"@de .

dtd:LA-id817 a dtd:LA ;
    dtd:has_Value "Maßnahmen zur Konfliktbewältigung anwenden, Teamarbeit organisieren und unterstützen sowie"@de .

dtd:LA-id818 a dtd:LA ;
    dtd:has_Value "Führungsstile kennen und das eigene Führungsverhalten kritisch beurteilen."@de .


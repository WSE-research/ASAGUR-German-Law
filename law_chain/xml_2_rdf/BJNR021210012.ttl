    dtd:has_FnZ "2"@de ;
    dtd:has_Group dtd:page ;
    dtd:has_ID "F775916_06_BJNR021210012BJNE007501116"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Postfix ")"@de ;
    dtd:has_Value "Dies schließt Vergasung und Pyrolyse unter Verwendung der Bestandteile als Chemikalien ein."@de .

dtd:Footnote-id3210 a dtd:Footnote ;
    dtd:has_FnZ "3"@de ;
    dtd:has_Group dtd:page ;
    dtd:has_ID "F775916_07_BJNR021210012BJNE007501116"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Postfix ")"@de ;
    dtd:has_Value "Dies schließt die Bodenreinigung, die zu einer Verwertung des Bodens und zu einem Recycling anorganischer Baustoffe führt, ein."@de .

dtd:Footnote-id3211 a dtd:Footnote ;
    dtd:has_FnZ "4"@de ;
    dtd:has_Group dtd:page ;
    dtd:has_ID "F775916_08_BJNR021210012BJNE007501116"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Postfix ")"@de ;
    dtd:has_Value "Falls sich kein anderer R-Code für die Einstufung eignet, kann das Verfahren R 12 vorbereitende Verfahren einschließen, die der Verwertung einschließlich der Vorbehandlung vorangehen, zum Beispiel Demontage, Sortieren, Zerkleinern, Verdichten, Pelletieren, Trocknen, Schreddern, Konditionierung, Neuverpacken, Trennung, Vermengen oder Vermischen vor Anwendung eines der in R 1 bis R 11 aufgeführten Verfahren."@de .

dtd:Footnote-id3212 a dtd:Footnote ;
    dtd:has_FnZ "5"@de ;
    dtd:has_Group dtd:page ;
    dtd:has_ID "F775916_09_BJNR021210012BJNE007501116"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Postfix ")"@de ;
    dtd:has_Value "Unter einer zeitweiligen Lagerung ist eine vorläufige Lagerung im Sinne des § 3 Absatz 15 zu verstehen."@de .


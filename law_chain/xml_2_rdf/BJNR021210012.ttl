    dtd:has_Size dtd:normal ;
    dtd:has_Value "legt, soweit erforderlich, weitere Abfallvermeidungsmaßnahmen fest und"@de .

dtd:LA-id2518 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gibt zweckmäßige, spezifische, qualitative oder quantitative Maßstäbe für festgelegte Abfallvermeidungsmaßnahmen vor, anhand derer die bei den Maßnahmen erzielten Fortschritte überwacht und bewertet werden; als Maßstab können Indikatoren oder andere geeignete spezifische qualitative oder quantitative Ziele herangezogen werden."@de .

dtd:LA-id2519 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verhältnismäßigkeit der Maßnahmen entsprechend § 7 Absatz 4,"@de .

dtd:LA-id2520 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "andere Rechtsvorschriften zur Verwendung von Erzeugnissen, zur Produktverantwortung sowie zum Schutz von Mensch und Umwelt und"@de .

dtd:LA-id2521 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Festlegungen des Unionsrechts über den freien Warenverkehr."@de .


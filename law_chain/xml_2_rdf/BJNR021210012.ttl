    dtd:has_Value "Nach EU-Recht und internationalen Übereinkünften verbotenes Verfahren."@de .

dtd:Footnote-id3205 a dtd:Footnote ;
    dtd:has_FnZ "2"@de ;
    dtd:has_Group dtd:page ;
    dtd:has_ID "F775916_03_BJNR021210012BJNE007400000"@de ;

dtd:LA-id454 a dtd:LA ;
    dtd:has_Value "des § 21 Absatz 2 des Elektromagnetische-Verträglichkeit-Gesetzes vom 14. Dezember 2016 (BGBl. I S. 2879)"@de .

dtd:LA-id455 a dtd:LA ;
    dtd:has_DL dtd:DL-id324 ;
    dtd:has_Value "im Hinblick auf Funkanlagen für a)die Anerkennung von Konformitätsbewertungsstellen als notifizierte Stellen undb)die Anerkennung von Konformitätsbewertungsstellen für Drittstaaten nach den in Anlage 1 zu dieser Verordnung aufgeführten Abkommen zwischen der Europäischen Gemeinschaft und den genannten Drittstaaten sowie"@de .

dtd:LA-id456 a dtd:LA ;
    dtd:has_Value "die Anerkennung von Konformitätsbewertungsstellen als notifizierte Stellen und"@de .

dtd:LA-id457 a dtd:LA ;
    dtd:has_Value "die Anerkennung von Konformitätsbewertungsstellen für Drittstaaten nach den in Anlage 1 zu dieser Verordnung aufgeführten Abkommen zwischen der Europäischen Gemeinschaft und den genannten Drittstaaten sowie"@de .

dtd:LA-id458 a dtd:LA ;
    dtd:has_DL dtd:DL-id329 ;
    dtd:has_Value "im Hinblick auf die elektromagnetische Verträglichkeit von Betriebsmitteln für a)die Anerkennung von Konformitätsbewertungsstellen als notifizierte Stellen undb)die Anerkennung von Konformitätsbewertungsstellen für Drittstaaten nach den in Anlage 2 zu dieser Verordnung aufgeführten Abkommen zwischen der Europäischen Gemeinschaft und den genannten Drittstaaten."@de .

dtd:LA-id459 a dtd:LA ;
    dtd:has_Value "die Anerkennung von Konformitätsbewertungsstellen als notifizierte Stellen und"@de .


dtd:LA-id2719 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bescheinigung der Teilnahme an einem Erste-Hilfe-Kurs für Menschen im Umfang von mindestens einem ganzen Tag"@de .

dtd:LA-id2720 a dtd:LA ;

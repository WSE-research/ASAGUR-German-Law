    dtd:has_colname "col2"@de .

dtd:entry-id1664 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Der Hund zeigt das Ertönen des Rauchmelders an und bringt den Menschen zum Ausgang."@de ;
    dtd:has_colname "col3"@de .

dtd:entry-id1665 a dtd:entry ;

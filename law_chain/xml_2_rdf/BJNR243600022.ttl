    dtd:has_colname "col2"@de .

dtd:entry-id1667 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Kann der Mensch in der Öffentlichkeit nicht antworten oder sprechen, überreicht der Hund eine Karte mit Informationen."@de ;
    dtd:has_colname "col3"@de .

dtd:entry-id1668 a dtd:entry ;

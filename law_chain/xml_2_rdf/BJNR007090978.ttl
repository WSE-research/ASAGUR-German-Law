dtd:I-id291 a dtd:I ;
    dtd:has_Value "(7) Bei der Verteilung der Sitze auf die Wahlvorschläge werden nur Wahlvorschläge berücksichtigt, die mindestens 3 Prozent der im Wahlgebiet abgegebenen gültigen Stimmen erhalten haben."@de .

dtd:I-id292 a dtd:I ;
    dtd:has_Value "Nummer 1a"@de .

dtd:LA-id476 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Wahlorgane, das Wahlrecht, die Vorbereitung der Wahl, die Wahlhandlung, die Feststellung des Wahlergebnisses und die Nach- und Wiederholungswahlen sowie die Vorschriften des § 49a des Bundeswahlgesetzes über Ordnungswidrigkeiten und die Vorschrift des § 54 des Bundeswahlgesetzes über Fristen und Termine in der jeweils geltenden Fassung entsprechend."@de .

dtd:LA-id477 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Bundeswahlleiter und der Bundeswahlausschuß für das Wahlgebiet,"@de .

dtd:LA-id478 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Landeswahlleiter und ein Landeswahlausschuß für jedes Land,"@de .

dtd:LA-id479 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Kreiswahlleiter und ein Kreiswahlausschuß für jeden Kreis und für jede kreisfreie Stadt ein Stadtwahlleiter und Stadtwahlausschuß,"@de .

dtd:LA-id480 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Wahlvorsteher und ein Wahlvorstand für jeden Wahlbezirk und"@de .

dtd:LA-id481 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens ein Wahlvorsteher und ein Wahlvorstand für jeden Kreis und für jede kreisfreie Stadt zur Feststellung des Briefwahlergebnisses. Wieviel Briefwahlvorstände zu bilden sind, um das Ergebnis der Briefwahl noch am Wahltage feststellen zu können, bestimmt der Kreiswahlleiter oder der Stadtwahlleiter."@de .

dtd:LA-id482 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das sechzehnte Lebensjahr vollendet haben,"@de .

dtd:LA-id483 a dtd:LA ;
    dtd:has_DL dtd:DL-id314 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seit mindestens drei Monatena)in der Bundesrepublik Deutschland oderb)in den übrigen Mitgliedstaaten der Europäischen Union"@de .

dtd:LA-id484 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Wohnung innehaben oder sich sonst gewöhnlich aufhalten,"@de .

dtd:LA-id485 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Bundesrepublik Deutschland oder"@de .

dtd:LA-id486 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den übrigen Mitgliedstaaten der Europäischen Union"@de .

dtd:LA-id487 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht nach § 6a Abs. 1 vom Wahlrecht ausgeschlossen sind."@de .

dtd:LA-id488 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das sechzehnte Lebensjahr vollendet haben,"@de .

dtd:LA-id489 a dtd:LA ;
    dtd:has_DL dtd:DL-id326 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seit mindestens drei Monatena)in der Bundesrepublik Deutschland oderb)in den übrigen Mitgliedstaaten der Europäischen Union"@de .

dtd:LA-id490 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Wohnung innehaben oder sich sonst gewöhnlich aufhalten,"@de .

dtd:LA-id491 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Bundesrepublik Deutschland oder"@de .

dtd:LA-id492 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den übrigen Mitgliedstaaten der Europäischen Union"@de .

dtd:LA-id493 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht nach § 6a Abs. 2 vom Wahlrecht ausgeschlossen sind."@de .

dtd:LA-id494 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Stimmabgabe in einem beliebigen Wahlbezirk oder"@de .

dtd:LA-id495 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Briefwahl"@de .

dtd:LA-id496 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "er infolge Richterspruchs das Wahlrecht nicht besitzt oder"@de .

dtd:LA-id497 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "er in dem Mitgliedstaat der Europäischen Union, dessen Staatsangehörigkeit er besitzt (Herkunfts-Mitgliedstaat), infolge einer zivil- oder strafrechtlichen Einzelfallentscheidung das Wahlrecht zum Europäischen Parlament nicht besitzt."@de .

dtd:LA-id498 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Deutscher im Sinne des Artikels 116 Abs. 1 des Grundgesetzes ist und"@de .

dtd:LA-id499 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das achtzehnte Lebensjahr vollendet hat."@de .

dtd:LA-id500 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Staatsangehörigkeit eines Mitgliedstaates der Europäischen Union besitzt und"@de .

dtd:LA-id501 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das achtzehnte Lebensjahr vollendet hat."@de .

dtd:LA-id502 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 6a Abs. 1 vom Wahlrecht ausgeschlossen ist"@de .

dtd:LA-id503 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "oder"@de .

dtd:LA-id504 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "infolge Richterspruchs die Wählbarkeit oder die Fähigkeit zur Bekleidung öffentlicher Ämter nicht besitzt."@de .

dtd:LA-id505 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 6a Abs. 2 Nr. 1 in der Bundesrepublik Deutschland vom Wahlrecht ausgeschlossen ist,"@de .

dtd:LA-id506 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach § 6a Abs. 2 Nr. 2 im Herkunfts-Mitgliedstaat vom Wahlrecht ausgeschlossen ist,"@de .

dtd:LA-id507 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "infolge Richterspruchs in der Bundesrepublik Deutschland die Wählbarkeit oder die Fähigkeit zur Bekleidung öffentlicher Ämter nicht besitzt oder"@de .

dtd:LA-id508 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "infolge einer Einzelfallentscheidung im Sinne von Artikel 6 Absatz 1 der Richtlinie 93/109/EG des Rates vom 6. Dezember 1993 über die Einzelheiten der Ausübung des aktiven und passiven Wahlrechts bei den Wahlen zum Europäischen Parlament für Unionsbürger mit Wohnsitz in einem Mitgliedstaat, dessen Staatsangehörigkeit sie nicht besitzen (ABl. L 329 vom 30.12.1993, S. 34), die zuletzt durch die Richtlinie 2013/1/EU (ABl. L 26 vom 26.1.2013, S. 27) geändert worden ist, im Herkunfts-Mitgliedstaat die Wählbarkeit nicht besitzt."@de .

dtd:LA-id509 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zustimmungserklärungen der in den Wahlvorschlag aufgenommenen Bewerber und Ersatzbewerber (§ 9 Abs. 3 Satz 4),"@de .

dtd:LA-id510 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Deutsche die Bescheinigungen der zuständigen Gemeindebehörden über die Wählbarkeit der vorgeschlagenen Bewerber und Ersatzbewerber,"@de .

dtd:LA-id511 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Unionsbürger die Bescheinigungen der zuständigen deutschen Gemeindebehörden, daß sie dort eine Wohnung innehaben oder ihren sonstigen gewöhnlichen Aufenthalt haben und nicht gemäß § 6b Abs. 4 Nr. 1 oder 3 von der Wählbarkeit ausgeschlossen sind,"@de .

dtd:LA-id512 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Unionsbürger die Versicherungen an Eides statt über die Staatsangehörigkeit, das Geburtsdatum und den Geburtsort, die letzte Anschrift im Herkunfts-Mitgliedstaat, die Anschrift in der Bundesrepublik Deutschland, die Gebietskörperschaft oder den Wahlkreis des Herkunfts-Mitgliedstaates, in dem sie zuletzt eingetragen waren, sowie darüber, dass sie sich nicht gleichzeitig in einem anderen Mitgliedstaat der Europäischen Union zur Wahl bewerben und dass sie im Herkunfts-Mitgliedstaat nicht von der Wählbarkeit ausgeschlossen sind (§ 6b Absatz 4 Nummer 2 und 4),"@de .

dtd:LA-id513 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id514 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Ausfertigung der Niederschrift über die Aufstellung des Wahlvorschlages (§ 10 Abs. 6), wobei der Leiter der Versammlung und zwei von dieser bestimmte Teilnehmer gegenüber dem Wahlleiter an Eides statt zu versichern haben, dass die Anforderungen gemäß § 10 Abs. 3 Satz 1 bis 3 beachtet worden sind,"@de .

dtd:LA-id515 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Fällen des § 9 Abs. 5 die erforderlichen gültigen Unterschriften mit dem Nachweis der Wahlberechtigung der Unterzeichner,"@de .

dtd:LA-id516 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die schriftliche Satzung, das Programm, die Namen und Anschriften der Vorstandsmitglieder (§ 9 Abs. 4) sowie der Nachweis, daß die Mitglieder des Vorstandes demokratisch gewählt sind, sofern die Partei oder die sonstige politische Vereinigung nicht im Europäischen Parlament, im Deutschen Bundestag oder in einem Landtag seit deren letzter Wahl auf Grund eigener Wahlvorschläge im Wahlgebiet ununterbrochen mit mindestens fünf Abgeordneten vertreten ist."@de .

dtd:LA-id517 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bezeichnung des Wahlvorschlagsberechtigten nach § 9 Abs. 1 fehlt,"@de .

dtd:LA-id518 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nach § 9 Abs. 4 und 5 erforderlichen gültigen Unterschriften mit dem Nachweis der Wahlberechtigung der Unterzeichner nach Absatz 5 dieser Vorschrift fehlen, es sei denn, der Nachweis kann infolge von Umständen, die der Wahlvorschlagsberechtigte nicht zu vertreten hat, nicht rechtzeitig vorgelegt werden,"@de .

dtd:LA-id519 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nach § 11 Abs. 1 erforderliche Form oder Frist nicht gewahrt ist,"@de .

dtd:LA-id520 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nach § 11 Abs. 2 Nr. 1, 1a, 1b, 1c, 2 und 4 erforderlichen Niederschriften, Versicherungen oder Unterlagen nicht vorgelegt oder abgegeben sind."@de .

dtd:LA-id521 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "verspätet eingereicht sind oder"@de .

dtd:LA-id522 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Anforderungen nicht entsprechen, die durch dieses Gesetz und die dazu erlassene Wahlordnung aufgestellt sind, es sei denn, daß in diesen Vorschriften etwas anderes bestimmt ist."@de .

dtd:LA-id523 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Überschrift \"Wahl der Abgeordneten des Europäischen Parlaments\","@de .

dtd:LA-id524 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Namen der Parteien und, sofern sie eine Kurzbezeichnung verwenden, auch diese, bei sonstigen politischen Vereinigungen deren Namen und, sofern sie ein Kennwort verwenden, auch dieses,"@de .

dtd:LA-id525 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bezeichnung der Wahlvorschläge als Listen für einzelne Länder oder gemeinsame Listen für alle Länder sowie bei Listen für einzelne Länder die Angabe des Landes, für das der Wahlvorschlag aufgestellt ist, und"@de .

dtd:LA-id526 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die ersten zehn Bewerber der zugelassenen Wahlvorschläge mit Vor- und Familiennamen, Beruf oder Stand, Ort der Wohnung (Hauptwohnung) sowie bei Bewerbern für gemeinsame Listen für alle Länder zusätzlich die Abkürzung des Landes, in dem der Ort der Wohnung liegt."@de .

dtd:LA-id527 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ungültigkeit des Erwerbs der Mitgliedschaft,"@de .

dtd:LA-id528 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fehlen der Wählbarkeit eines Unionsbürgers am Wahltag im Herkunfts-Mitgliedstaat infolge einer Einzelfallentscheidung im Sinne von Artikel 6 Absatz 1 der Richtlinie 93/109/EG des Rates vom 6. Dezember 1993 über die Einzelheiten der Ausübung des aktiven und passiven Wahlrechts bei den Wahlen zum Europäischen Parlament für Unionsbürger mit Wohnsitz in einem Mitgliedstaat, dessen Staatsangehörigkeit sie nicht besitzen (ABl. L 329 vom 30.12.1993, S. 34), die zuletzt durch die Richtlinie 2013/1/EU (ABl. L 26 vom 26.1.2013, S. 27) geändert worden ist,"@de .

dtd:LA-id529 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Neufeststellung des Wahlergebnisses,"@de .

dtd:LA-id530 a dtd:LA ;

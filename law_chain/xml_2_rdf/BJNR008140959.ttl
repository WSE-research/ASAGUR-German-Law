    dtd:has_Size dtd:normal ;
    dtd:has_Value "bis zu welchem Höchstbetrag der Inhaber haftet,"@de .

dtd:LA-id1845 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach welcher Frist der Anspruch gegen den Inhaber verjährt oder ausgeschlossen ist,"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id1868 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id1869 a dtd:LA ;
    dtd:has_DL dtd:DL-id1612 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die in Genehmigungen, Erlaubnissen und Zulassungen zur Annahme von weiteren radioaktiven Abfllen oder zu deren Einlagerung zum Zwecke der Endlagerung oder zur Annahme von weiteren Kernbrennstoffen oder sonstigen radioaktiven Stoffen zum Zwecke der Aufbewahrung oder Lagerung enthaltenen Gestattungena)zur Annahme von weiteren radioaktiven Abfllen oder zu deren Einlagerung zum Zwecke der Endlagerung oderb)zur Annahme von weiteren Kernbrennstoffen oder sonstigen radioaktiven Stoffen zum Zwecke der Aufbewahrung oder Lagerung"@de .

dtd:LA-id1870 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "werden mit dem 27. April 2002 unwirksam; im brigen bestehen diese Genehmigungen, Erlaubnisse oder Zulassungen als Genehmigungen nach den Vorschriften dieses Gesetzes fort. Die nach Satz 1 fortbestehenden Genehmigungen knnen nach den Vorschriften dieses Gesetzes gendert oder mit Anordnungen versehen werden."@de .

dtd:LA-id1871 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Annahme von weiteren radioaktiven Abfllen oder zu deren Einlagerung zum Zwecke der Endlagerung oder"@de .

dtd:LA-id1872 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Annahme von weiteren Kernbrennstoffen oder sonstigen radioaktiven Stoffen zum Zwecke der Aufbewahrung oder Lagerung"@de .


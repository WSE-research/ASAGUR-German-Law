dtd:entry-id336 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Dieser Richtwert gilt für nicht relevante Metaboliten von Pestiziden (Pestizid-nrM), wenn keine Ergebnisse zur subchronischen oder chronischen Toxizität aus Tierversuchen vorliegen und der Pestizid-nrM nachweislich nicht gentoxisch ist, aber auch keine Anhaltspunkte für ein besonderes immuntoxisches, neurotoxisches oder keimzellschädigendes Potenzial vorliegen."@de ;
    dtd:has_colname "col3"@de .

dtd:entry-id337 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "B"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id338 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "3 µg/l"@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id339 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Dieser Richtwert gilt für Pestizid-nrM, wenn keine Ergebnisse zur chronischen Toxizität aus Tierversuchen vorliegen und der Pestizid-nrM nachweislich weder gentoxisch, noch keimzellschädigend, immun- oder neurotoxisch ist. Zusätzlich müssen aussagekräftige In-vivo-Daten aus mindestens einer Studie zur subchronisch-oralen Toxizität des Pestizid-nrM vorliegen."@de ;
    dtd:has_colname "col3"@de .

dtd:entry-id340 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "C"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id341 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "10 µg/l"@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id342 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Dieser Richtwert gilt aus trinkwasserhygienischen Gründen und dem Vorsorgeprinzip folgend für alle nicht der Kategorie A oder B zuzuordnenden Pestizid-nrM. Trinkwasserhygienische Gründe sind Substanzeigenschaften wie Persistenz, Mobilität, schwere Entfernbarkeit sowie nicht abschätzbare Restrisiken. Für diese Pestizid-nrM müssen Ergebnisse zur chronischen Toxizität aus Tierversuchen sowie zur Gentoxizität, Neurotoxizität, Immuntoxizität und zur keimzellschädigenden Wirkung vorliegen, die keinen niedrigeren Richtwert als 10 µg/l erforderlich machen."@de ;
    dtd:has_colname "col3"@de .


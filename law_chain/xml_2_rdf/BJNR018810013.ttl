    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kontaktdaten der für die Übermittlung verantwortlichen Person der Vermittlungs- oder Koordinierungsstelle sowie"@de .

dtd:LA-id417 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den folgenden Hinweis: „Enthält personenbezogene Daten. Vor unerlaubter Verbreitung und dem Zugang durch Unbefugte schützen.“"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit einem Organ, das in einen Bestimmungsmitgliedstaat zum Zweck der Übertragung vermittelt worden ist, oder"@de .

dtd:LA-id414 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit einem Spender, dessen Organ in einen Bestimmungsmitgliedstaat zum Zweck der Übertragung vermittelt worden ist,"@de .

dtd:LA-id415 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Datum und Uhrzeit der Übermittlung,"@de .


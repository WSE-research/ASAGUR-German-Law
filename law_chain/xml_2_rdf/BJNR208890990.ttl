dtd:LA-id11454 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nach § 55 werden folgende §§ 56 bis 59 angefügt:"@de .

dtd:LA-id11455 a dtd:LA ;

    dtd:has_Value "für den Verwalter der Besitz einer Genehmigung nach § 13 Abs. 1b,"@de .

dtd:LA-id13027 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Option gemäß Absatz 7."@de .

dtd:LA-id13028 a dtd:LA ;

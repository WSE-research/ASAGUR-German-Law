dtd:LA-id1250 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Biomasse"@de .

dtd:LA-id1251 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Energietrger nach  3 Nummer 21 Buchstabe e des Erneuerbare-Energien-Gesetzes vom 21. Juli 2014 (BGBl. I S. 1066), das zuletzt durch Artikel 1 des Gesetzes vom 21. Juni 2018 (BGBl. I S. 862) gendert worden ist;"@de .

dtd:LA-id1252 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dienstleister"@de .

dtd:LA-id1253 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine natrliche Person, eine juristische Person des privaten oder ffentlichen Rechts oder eine rechtsfhige Personengesellschaft, die von einem Kontoinhaber bevollmchtigt ist, fr ihn Handlungen im Zusammenhang mit der Nutzung des Herkunftsnachweisregisters oder des Regionalnachweisregisters gegenber der Registerverwaltung vorzunehmen;"@de .

dtd:LA-id1254 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grenzkraftwerk"@de .

dtd:LA-id1255 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Anlage, die auf der deutschen Staatsgrenze steht und bei der sich auf beiden Seiten der Staatsgrenze Einrichtungen befinden, die fr die Stromerzeugung in dieser Anlage notwendig sind, wobei die Grenze der deutschen ausschlielichen Wirtschaftszone mit der ausschlielichen Wirtschaftszone eines anderen Staates als deutsche Staatsgrenze gilt;"@de .

dtd:LA-id1256 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Konto"@de .

dtd:LA-id1257 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine dem Kontoinhaber durch die Registerverwaltung zugeordnete Einrichtung innerhalb des Herkunftsnachweisregisters oder des Regionalnachweisregisters, in der die Ausstellung, die bertragung, die Anerkennung und die Entwertung von Herkunftsnachweisen oder die Ausstellung, die bertragung und die Entwertung von Regionalnachweisen erfolgt;"@de .

dtd:LA-id1258 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kontoinhaber"@de .

dtd:LA-id1259 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Hndler, Anlagenbetreiber oder Elektrizittsversorgungsunternehmen, fr den oder fr das die Registerverwaltung ein Konto im Herkunftsnachweisregister oder im Regionalnachweisregister erffnet hat;"@de .

dtd:LA-id1260 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nutzer"@de .

dtd:LA-id1261 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine natrliche Person, die von einem Kontoinhaber oder einem Dienstleister bevollmchtigt ist, im Herkunftsnachweisregister oder im Regionalnachweisregister fr den Vollmachtgeber Handlungen gegenber der Registerverwaltung vorzunehmen;"@de .

dtd:LA-id1262 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Postfach"@de .

dtd:LA-id1263 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine dem Registerteilnehmer und dem Betreiber eines Elektrizittsversorgungsnetzes zugeordnete Einrichtung innerhalb des Herkunftsnachweisregisters oder des Regionalnachweisregisters, die von der Registerverwaltung fr den Empfang von elektronischen Dokumenten und Nachrichten sowie fr die Bekanntgabe von Entscheidungen bereitgestellt wird;"@de .

dtd:LA-id1264 a dtd:LA ;
    dtd:has_DL dtd:DL-id772 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Registerteilnehmer a)im Herkunftsnachweisregister: ein Kontoinhaber, ein registrierter Dienstleister, ein Umweltgutachter und eine Umweltgutachterorganisation, oderb)im Regionalnachweisregister: ein Kontoinhaber und ein registrierter Dienstleister;"@de .

dtd:LA-id1265 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Herkunftsnachweisregister: ein Kontoinhaber, ein registrierter Dienstleister, ein Umweltgutachter und eine Umweltgutachterorganisation, oder"@de .

dtd:LA-id1266 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Regionalnachweisregister: ein Kontoinhaber und ein registrierter Dienstleister;"@de .

dtd:LA-id1267 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Registerverwaltung"@de .

dtd:LA-id1268 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Umweltbundesamt als zustndige Stelle nach  79 Absatz 4 und nach  79a Absatz 4 des Erneuerbare-Energien-Gesetzes;"@de .


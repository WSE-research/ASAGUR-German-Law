    dtd:has_Value "außerhalb des Salzbergbaus bei einer Trockentemperatur von mehr als 28 Grad Celsius oder einer Effektivtemperatur von mehr als 25 Grad Celsius."@de .

dtd:LA-id1002 a dtd:LA ;
    dtd:has_Value "sie bei ihrer Tätigkeit fibrogenen Grubenstäuben ausgesetzt gewesen sind,"@de .

dtd:LA-id1003 a dtd:LA ;
    dtd:has_Value "während ihrer Tätigkeit mindestens eine Nachuntersuchung stattgefunden hat und"@de .

dtd:LA-id1004 a dtd:LA ;
    dtd:has_Value "ihre Beschäftigung im Sinne der Nummer 1 nach dem 31. Dezember 1991 beendet wird."@de .

dtd:LA-id1005 a dtd:LA ;
    dtd:has_Value "die erforderlichen medizinischen Fachkenntnisse besitzen,"@de .

dtd:LA-id1006 a dtd:LA ;
    dtd:has_Value "über die notwendigen Kenntnisse der jeweiligen Arbeitsbedingungen im betroffenen Bergbau verfügen und"@de .

dtd:LA-id1007 a dtd:LA ;
    dtd:has_Value "selbst keine Arbeitgeberfunktion gegenüber den zu untersuchenden beschäftigten Personen ausüben."@de .

dtd:LA-id1008 a dtd:LA ;
    dtd:has_Value "Art und Umfang der Untersuchungen,"@de .

dtd:LA-id1009 a dtd:LA ;
    dtd:has_Value "Kriterien für die Beurteilung,"@de .

dtd:LA-id1010 a dtd:LA ;
    dtd:has_Value "Dokumentation der Ergebnisse."@de .

dtd:LA-id1011 a dtd:LA ;
    dtd:has_Value "vor Durchführung der Untersuchungen die zu untersuchende Person über die Inhalte, den Zweck und eventuelle Risiken der Untersuchung aufklären,"@de .

dtd:LA-id1012 a dtd:LA ;
    dtd:has_Value "das Ergebnis der Untersuchungen den Untersuchten mitteilen und"@de .

dtd:LA-id1013 a dtd:LA ;
    dtd:has_Value "Aufzeichnungen über Ergebnis und Befunde der durchgeführten Untersuchungen führen."@de .

dtd:LA-id1014 a dtd:LA ;
    dtd:has_Value "in einem anderen untertägigen Betrieb als Versatzmaterial verwertet werden müssten,"@de .

dtd:LA-id1015 a dtd:LA ;
    dtd:has_Value "mit einer vergleichbaren Gefährdung für Personen anderweitig verwertet werden müssten oder"@de .

dtd:LA-id1016 a dtd:LA ;
    dtd:has_Value "beseitigt werden müssten."@de .

dtd:LA-id1017 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Eignungsgruppen 1.1 bis 1.3 (Anlage 1) ein persönlicher Staubbelastungswert von 440,"@de .

dtd:LA-id1018 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Eignungsgruppen 2.1 und 2.2 sowie unter 21 Jahren ein persönlicher Staubbelastungswert von 330"@de .

dtd:LA-id1019 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ort, Zeitpunkt und Dauer für repräsentative Erstmessungen sowie repräsentative Wiederholungsmessungen und deren zeitliche Abstände,"@de .

dtd:LA-id1020 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu verwendende Probenahme- und Meßgeräte,"@de .

dtd:LA-id1021 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Form und Inhalt der Meßberichte,"@de .

dtd:LA-id1022 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auswertung von Proben und Messungen."@de .

dtd:LA-id1023 a dtd:LA ;
    dtd:has_DL dtd:DL-id869 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sachgebiete und Dauer der theoretischen und praktischen Unterweisung, insbesonderea)Funktionsweise und Handhabung von Probenahme- und Meßgeräten,b)Durchführung und Dokumentation von Probenahmen und Messungen,c)Auswertung von Proben und Messungen,d)Einstufung von Betriebspunkten, deren Überwachung und Maßnahmen der Arbeitseinsatzlenkung,"@de .

dtd:LA-id1024 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Funktionsweise und Handhabung von Probenahme- und Meßgeräten,"@de .

dtd:LA-id1025 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Durchführung und Dokumentation von Probenahmen und Messungen,"@de .

dtd:LA-id1026 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auswertung von Proben und Messungen,"@de .

dtd:LA-id1027 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einstufung von Betriebspunkten, deren Überwachung und Maßnahmen der Arbeitseinsatzlenkung,"@de .

dtd:LA-id1028 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nachweis der Fachkunde."@de .

dtd:LA-id1029 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Schichtennachweis die vom Arzt festgelegte Eignungsgruppe, die Höhe der in dem jeweiligen Beurteilungszeitraum entstandenen persönlichen Staubbelastung und die Staubbelastungsstufe des Betriebspunktes zu vermerken sowie diese Angaben monatlich auf den neuesten Stand zu bringen,"@de .

dtd:LA-id1030 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufzeichnungen zu führen, die mindestens die in Anlage 9 aufgeführten Angaben enthalten müssen."@de .

dtd:LA-id1031 a dtd:LA ;
    dtd:has_Value "den Grund für die Beantragung der Ausnahme,"@de .

dtd:LA-id1032 a dtd:LA ;
    dtd:has_Value "die betroffenen Tätigkeiten und Verfahren und die dabei zu erwartende Exposition gegenüber Gefahrstoffen,"@de .

dtd:LA-id1033 a dtd:LA ;
    dtd:has_Value "die Zahl der voraussichtlich betroffenen Beschäftigten sowie"@de .

dtd:LA-id1034 a dtd:LA ;
    dtd:has_Value "die geplanten Maßnahmen zur Gewährleistung des Gesundheitsschutzes und der Sicherheit der betroffenen Beschäftigten einschließlich der Maßnahmen zur Verringerung oder Vermeidung einer Exposition der Beschäftigten."@de .

dtd:LA-id1035 a dtd:LA ;

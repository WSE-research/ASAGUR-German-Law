dtd:entry-id2144 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Messunsicherheiten sind zu berechnen und zu dokumentieren. Zusätzlich kann der Vertrauensbereich ausgewiesen werden, wobei dieser mit der Wahrscheinlichkeit 1 − y von 95 Prozent festzulegen ist."@de ;
    dtd:has_colname "col2"@de ;
    dtd:has_rowsep "0"@de .


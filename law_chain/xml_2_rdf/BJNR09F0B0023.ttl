dtd:entry-id2146 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Diese Nachweisgrenze gilt nur für die Verwendung des Prüfwerts von 0,1 Becquerel pro Liter unter Berücksichtigung der Aktivitätskonzentrationen von Blei-210 und Radium-228. Für die Verwendung des Prüfwerts von 0,05 Becquerel pro Liter ohne weitere nuklidspezifische Untersuchungen, wenn ausschließlich natürliche Radionuklide zu berücksichtigen sind, gilt die Nachweisgrenze von 0,025 Becquerel pro Liter."@de ;
    dtd:has_colname "col2"@de ;
    dtd:has_rowsep "0"@de .

dtd:entry-id2147 a dtd:entry ;
    dtd:has_B dtd:B-id879 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Anmerkung 4:"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id2148 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Diese Nachweisgrenze gilt nur für die Erstuntersuchung im Hinblick auf die Richtdosis für ein neues Wasservorkommen. Falls die Erstuntersuchung keinen plausiblen Grund dafür ergibt, dass Radium-228 20 Prozent der abgeleiteten Konzentration überschreitet, kann für regelmäßige Untersuchungen ein Untersuchungsverfahren mit einer Nachweisgrenze von bis zu 0,08 Becquerel pro Liter für Radium-228 angewendet werden."@de ;
    dtd:has_colname "col2"@de .


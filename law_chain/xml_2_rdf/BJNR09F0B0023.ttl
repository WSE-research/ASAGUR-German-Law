dtd:entry-id2142 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Die Nachweisgrenze ist zu berechnen nach der Norm DIN EN ISO 11929-1-3 „Bestimmung der charakteristischen Grenzen (Erkennungsgrenze, Nachweisgrenze und Grenzen des Überdeckungsintervalls) bei Messungen ionisierender Strahlung – Grundlagen und Anwendungen)“ mit Wahrscheinlichkeiten des Fehlers erster bzw. zweiter Art von jeweils fünf Prozent."@de ;
    dtd:has_colname "col2"@de ;
    dtd:has_rowsep "0"@de .


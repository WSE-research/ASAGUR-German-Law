dtd:LA-id2202 a dtd:LA ;
    dtd:has_Value "Arbeitnehmerinnen und Arbeitnehmer die Beschäftigten, die nach dem für die Dienststelle maßgebenden Tarifvertrag oder nach der Dienstordnung Arbeitnehmerinnen und Arbeitnehmer sind, die als übertarifliche Arbeitnehmerinnen und Arbeitnehmer beschäftigt werden oder die sich in einer beruflichen Ausbildung befinden,"@de .

dtd:LA-id2203 a dtd:LA ;
    dtd:has_Value "Arbeitstage die Wochentage Montag bis Freitag mit Ausnahme der gesetzlichen Feiertage,"@de .

dtd:LA-id2204 a dtd:LA ;
    dtd:has_Value "Beamtinnen und Beamte die Beschäftigten, die nach den jeweils für sie geltenden Beamtengesetzen Beamtinnen und Beamte sind,"@de .

dtd:LA-id2205 a dtd:LA ;
    dtd:has_Value "Behörden der Mittelstufe die der obersten Dienstbehörde im Sinne dieses Gesetzes unmittelbar nachgeordneten Behörden, denen andere Dienststellen nachgeordnet sind,"@de .

dtd:LA-id2206 a dtd:LA ;
    dtd:has_Value "Beschäftigte im öffentlichen Dienst vorbehaltlich des Absatzes 2 die Beamtinnen und Beamten sowie die Arbeitnehmerinnen und Arbeitnehmer einschließlich der zu ihrer Berufsausbildung Beschäftigten sowie Richterinnen und Richter, die an eine der in § 1 Absatz 1 genannten Verwaltungen oder zur Wahrnehmung einer nichtrichterlichen Tätigkeit an ein Gericht des Bundes abgeordnet sind,"@de .

dtd:LA-id2207 a dtd:LA ;
    dtd:has_Value "Dienststellen vorbehaltlich des § 6 die einzelnen Behörden, Verwaltungsstellen und Betriebe der in § 1 Absatz 1 genannten Verwaltungen sowie die Gerichte,"@de .

dtd:LA-id2208 a dtd:LA ;
    dtd:has_Value "Personalvertretungen die Personalräte, die Stufenvertretungen und die Gesamtpersonalräte."@de .

dtd:LA-id2209 a dtd:LA ;
    dtd:has_Value "deren Beschäftigung überwiegend durch Beweggründe karitativer oder religiöser Art bestimmt ist oder"@de .

dtd:LA-id2210 a dtd:LA ;
    dtd:has_Value "die überwiegend zu ihrer Heilung, Wiedereingewöhnung oder Erziehung beschäftigt werden."@de .

dtd:LA-id2211 a dtd:LA ;
    dtd:has_Value "für Mitglieder der Personalvertretung und der Jugend- und Auszubildendenvertretung gegenüber den übrigen Mitgliedern der Vertretung,"@de .

dtd:LA-id2212 a dtd:LA ;
    dtd:has_Value "für die in Absatz 1 Satz 1 bezeichneten Personen gegenüber der zuständigen Personalvertretung,"@de .

dtd:LA-id2213 a dtd:LA ;
    dtd:has_Value "gegenüber der vorgesetzten Dienststelle, der bei ihr gebildeten Stufenvertretung und gegenüber dem Gesamtpersonalrat sowie"@de .

dtd:LA-id2214 a dtd:LA ;
    dtd:has_Value "für die Anrufung der Einigungsstelle."@de .

dtd:LA-id2215 a dtd:LA ;
    dtd:has_Value "infolge Richterspruchs das Recht, in öffentlichen Angelegenheiten zu wählen oder zu stimmen, nicht besitzen,"@de .

dtd:LA-id2216 a dtd:LA ;
    dtd:has_Value "am Wahltag seit mehr als zwölf Monaten beurlaubt sind oder"@de .

dtd:LA-id2217 a dtd:LA ;
    dtd:has_Value "Altersteilzeit im Blockmodell ausüben und sich am Wahltag in der Freistellung befinden."@de .

dtd:LA-id2218 a dtd:LA ;
    dtd:has_Value "das 18. Lebensjahr vollendet haben und"@de .

dtd:LA-id2219 a dtd:LA ;
    dtd:has_Value "seit sechs Monaten Beschäftigte im öffentlichen Dienst des Bundes sind."@de .

dtd:LA-id2220 a dtd:LA ;
    dtd:has_Value "Beschäftigte, die infolge Richterspruchs nicht die Fähigkeit besitzen, Rechte aus öffentlichen Wahlen zu erlangen,"@de .

dtd:LA-id2221 a dtd:LA ;
    dtd:has_Value "Beschäftigte, die am Wahltag noch länger als zwölf Monate beurlaubt sind,"@de .

dtd:LA-id2222 a dtd:LA ;
    dtd:has_Value "für die Wahl in eine Stufenvertretung die in § 14 Absatz 3 genannten Personen oder"@de .

dtd:LA-id2223 a dtd:LA ;
    dtd:has_Value "für die Wahl der Personalvertretung ihrer Dienststelle die in § 8 genannten Personen sowie Beschäftigte, die zu selbstständigen Entscheidungen in Personalangelegenheiten der Dienststelle befugt sind."@de .

dtd:LA-id2224 a dtd:LA ;
    dtd:has_Value "5 bis 20 wahlberechtigten Beschäftigten aus einem Mitglied,"@de .

dtd:LA-id2225 a dtd:LA ;
    dtd:has_Value "21 Wahlberechtigten bis 50 Beschäftigten aus drei Mitgliedern,"@de .

dtd:LA-id2226 a dtd:LA ;
    dtd:has_Value "51 bis 150 Beschäftigten aus fünf Mitgliedern,"@de .

dtd:LA-id2227 a dtd:LA ;
    dtd:has_Value "151 bis 300 Beschäftigten aus sieben Mitgliedern,"@de .

dtd:LA-id2228 a dtd:LA ;
    dtd:has_Value "301 bis 600 Beschäftigten aus neun Mitgliedern,"@de .

dtd:LA-id2229 a dtd:LA ;
    dtd:has_Value "601 bis 1 000 Beschäftigten aus elf Mitgliedern."@de .

dtd:LA-id2230 a dtd:LA ;
    dtd:has_Value "bei weniger als 51 Gruppenangehörigen mindestens eine Vertreterin oder einen Vertreter,"@de .

dtd:LA-id2231 a dtd:LA ;
    dtd:has_Value "bei 51 bis 200 Gruppenangehörigen mindestens zwei Vertreterinnen oder Vertreter,"@de .

dtd:LA-id2232 a dtd:LA ;
    dtd:has_Value "bei 201 bis 600 Gruppenangehörigen mindestens drei Vertreterinnen oder Vertreter,"@de .

dtd:LA-id2233 a dtd:LA ;
    dtd:has_Value "bei 601 bis 1 000 Gruppenangehörigen mindestens vier Vertreterinnen oder Vertreter,"@de .

dtd:LA-id2234 a dtd:LA ;
    dtd:has_Value "bei 1 001 bis 3 000 Gruppenangehörigen mindestens fünf Vertreterinnen oder Vertreter,"@de .

dtd:LA-id2235 a dtd:LA ;
    dtd:has_Value "bei mehr als 3 000 Gruppenangehörigen mindestens sechs Vertreterinnen oder Vertreter."@de .

dtd:LA-id2236 a dtd:LA ;
    dtd:has_Value "mit Ablauf von 24 Monaten, vom Tag der Wahl gerechnet, die Zahl der regelmäßig Beschäftigten um die Hälfte, mindestens aber um 50 Personen gestiegen oder gesunken ist,"@de .

dtd:LA-id2237 a dtd:LA ;
    dtd:has_Value "die Gesamtzahl der Mitglieder des Personalrats auch nach Eintreten sämtlicher Ersatzmitglieder um mehr als ein Viertel der vorgeschriebenen Zahl gesunken ist,"@de .

dtd:LA-id2238 a dtd:LA ;
    dtd:has_Value "der Personalrat mit der Mehrheit seiner Mitglieder seinen Rücktritt beschlossen hat,"@de .

dtd:LA-id2239 a dtd:LA ;
    dtd:has_Value "die Personalratswahl mit Erfolg gerichtlich angefochten worden ist,"@de .

dtd:LA-id2240 a dtd:LA ;
    dtd:has_Value "der Personalrat durch gerichtliche Entscheidung aufgelöst ist oder"@de .

dtd:LA-id2241 a dtd:LA ;
    dtd:has_Value "in der Dienststelle kein Personalrat besteht."@de .

dtd:LA-id2242 a dtd:LA ;
    dtd:has_Value "Ablauf der Amtszeit,"@de .

dtd:LA-id2243 a dtd:LA ;
    dtd:has_Value "Niederlegung des Amtes,"@de .

dtd:LA-id2244 a dtd:LA ;
    dtd:has_Value "Beendigung des Dienst- oder Arbeitsverhältnisses,"@de .

dtd:LA-id2245 a dtd:LA ;
    dtd:has_Value "Ausscheiden aus der Dienststelle,"@de .

dtd:LA-id2246 a dtd:LA ;
    dtd:has_Value "Verlust der Wählbarkeit,"@de .

dtd:LA-id2247 a dtd:LA ;
    dtd:has_Value "Eintritt in eine mehr als zwölfmonatige Beurlaubung,"@de .

dtd:LA-id2248 a dtd:LA ;
    dtd:has_Value "Eintritt in die Freistellungsphase der Altersteilzeit im Blockmodell,"@de .

dtd:LA-id2249 a dtd:LA ;
    dtd:has_Value "Ausschluss aus dem Personalrat oder Auflösung des Personalrats auf Grund einer gerichtlichen Entscheidung oder"@de .

dtd:LA-id2250 a dtd:LA ;
    dtd:has_Value "gerichtliche Entscheidung über die Feststellung der Nichtwählbarkeit nach Ablauf der in § 26 bezeichneten Frist, es sei denn, der Mangel liegt nicht mehr vor."@de .

dtd:LA-id2251 a dtd:LA ;
    dtd:has_Value "einem Viertel der Mitglieder des Personalrats,"@de .

dtd:LA-id2252 a dtd:LA ;
    dtd:has_Value "der Mehrheit der Vertreterinnen und Vertreter einer Gruppe,"@de .

dtd:LA-id2253 a dtd:LA ;
    dtd:has_Value "der Leiterin oder dem Leiter der Dienststelle,"@de .

dtd:LA-id2254 a dtd:LA ;
    dtd:has_Value "der Vertrauensperson der schwerbehinderten Menschen in Angelegenheiten, die besonders schwerbehinderte Beschäftigte betreffen, oder"@de .

dtd:LA-id2255 a dtd:LA ;
    dtd:has_Value "der Mehrheit der Mitglieder der Jugend- und Auszubildendenvertretung in Angelegenheiten, die besonders Beschäftigte betreffen, die das 18. Lebensjahr noch nicht vollendet haben oder sich in einer beruflichen Ausbildung befinden."@de .

dtd:LA-id2256 a dtd:LA ;
    dtd:has_Value "vorhandene Einrichtungen genutzt werden, die durch die Dienststelle zur dienstlichen Nutzung freigegeben sind,"@de .

dtd:LA-id2257 a dtd:LA ;
    dtd:has_Value "nicht mindestens ein Viertel der Mitglieder oder die Mehrheit der Vertreterinnen und Vertreter einer Gruppe des Personalrats binnen einer von der oder dem Vorsitzenden zu bestimmenden Frist gegenüber der oder dem Vorsitzenden widerspricht und"@de .

dtd:LA-id2258 a dtd:LA ;

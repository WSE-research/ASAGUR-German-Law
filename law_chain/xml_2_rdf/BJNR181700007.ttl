    dtd:has_Value "bei Abgabe an Betriebe des Einzelhandels Mengen, die der für den jeweiligen Betrieb tagesüblichen Abgabe an Verbraucher entsprechen,"@de .

dtd:LA-id441 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "erlegtem Wild: die Strecke eines Jagdtages,"@de .

dtd:LA-id442 a dtd:LA ;
    dtd:has_Size dtd:normal ;

    dtd:has_Value "entgegen § 8 Absatz 5 einen dort genannten Nachweis nicht, nicht vollständig oder nicht rechtzeitig vorlegt."@de .

dtd:LA-id454 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Eigenschaften und Zusammensetzung des jeweiligen Lebensmittels"@de .

dtd:LA-id455 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Hygienische Anforderungen an die Herstellung und Verarbeitung des jeweiligen Lebensmittels"@de .


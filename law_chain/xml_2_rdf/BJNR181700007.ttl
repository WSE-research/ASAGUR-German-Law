    dtd:has_Value "entgegen § 3 Satz 2 mit einem lebenden Tier umgeht,"@de .

dtd:LA-id447 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 4 Absatz 1 Satz 1 ein leicht verderbliches Lebensmittel herstellt, behandelt oder in den Verkehr bringt,"@de .

dtd:LA-id448 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 5 Absatz 1 Satz 1 in Verbindung mit Anlage 2 Nummer 2 Buchstabe g Umhüllungen oder Verpackungen nicht richtig lagert,"@de .

dtd:LA-id449 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 5 Absatz 1 Satz 1 in Verbindung mit Anlage 2 Nummer 3 Buchstabe c nicht sicherstellt, dass dort genannte Personen nicht mit Primärerzeugnissen umgehen,"@de .

dtd:LA-id450 a dtd:LA ;

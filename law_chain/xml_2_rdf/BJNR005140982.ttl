    dtd:has_Value "eine Meldung nach § 2 nicht richtig, nicht vollständig oder nicht rechtzeitig erstattet."@de .

dtd:LA-id127 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Verordnung festgestellt hat, daß die Energieversorgung im Sinne des § 1 Abs. 1 oder des § 2 Abs. 3 des Energiesicherungsgesetzes 1975 gefährdet oder gestört ist, und"@de .

dtd:LA-id128 a dtd:LA ;

    dtd:has_Size dtd:normal ;
    dtd:has_Value "an Unternehmen und Betriebe, die elektrische Energie erzeugen, weiterleiten oder verteilen, übera)die Erzeugung, den Bezug, die Umwandlung, Umspannung, Weiterleitung, Zuteilung, Abgabe, Verwendung, Einfuhr und Ausfuhr elektrischer Energie,b)die Lagerung, Abgabe und Verwendung von Brennstoffen;"@de .

dtd:LA-id121 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erzeugung, den Bezug, die Umwandlung, Umspannung, Weiterleitung, Zuteilung, Abgabe, Verwendung, Einfuhr und Ausfuhr elektrischer Energie,"@de .

dtd:LA-id122 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Lagerung, Abgabe und Verwendung von Brennstoffen;"@de .

dtd:LA-id123 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an Verbraucher über die Zuteilung, den Bezug und die Verwendung elektrischer Energie sowie den Ausschluß vom Bezug elektrischer Energie und"@de .

dtd:LA-id124 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an Unternehmen der gewerblichen Wirtschaft über die Herstellung, die Instandhaltung, die Abgabe, die Verbringung und die Verwendung von Produktionsmitteln, soweit diese Produktionsmittel der Versorgung mit elektrischer Energie und Erdgas dienen, sowie über Werkleistungen von Unternehmen der gewerblichen Wirtschaft zur Instandhaltung, Instandsetzung, Herstellung und Veränderung von Bauwerken und technischen Anlagen, die der Versorgung mit elektrischer Energie und Erdgas dienen."@de .

dtd:LA-id125 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Verfügung nach § 1 Abs. 1 zuwiderhandelt oder"@de .

dtd:LA-id126 a dtd:LA ;

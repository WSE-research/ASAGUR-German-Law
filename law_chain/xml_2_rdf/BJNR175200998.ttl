dtd:LA-id600 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ist Spende die bei Menschen entnommene Menge an Blut oder Blutbestandteilen, die Wirkstoff oder Arzneimittel ist oder zur Herstellung von Wirkstoffen oder Arzneimitteln und anderen Produkten zur Anwendung bei Menschen bestimmt ist,"@de .

dtd:LA-id601 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ist Spendeeinrichtung eine Einrichtung, die Spenden entnimmt oder deren Tätigkeit auf die Entnahme von Spenden und, soweit diese zur Anwendung bestimmt sind, auf deren Testung, Verarbeitung, Lagerung und das Inverkehrbringen gerichtet ist,"@de .

dtd:LA-id602 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sind Blutprodukte Blutzubereitungen im Sinne von § 4 Abs. 2 des Arzneimittelgesetzes, Sera aus menschlichem Blut im Sinne des § 4 Abs. 3 des Arzneimittelgesetzes und Blutbestandteile, die zur Herstellung von Wirkstoffen oder Arzneimitteln bestimmt sind."@de .

dtd:LA-id603 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine ausreichende personelle, bauliche, räumliche und technische Ausstattung vorhanden ist,"@de .

dtd:LA-id604 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Spendeeinrichtung oder der Träger von Spendeeinrichtungen eine leitende ärztliche Person bestellt hat, die die erforderliche Sachkunde nach dem Stand der medizinischen Wissenschaft besitzt, und"@de .

dtd:LA-id605 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Durchführung der Spendeentnahmen von einem Menschen eine ärztliche Person vorhanden ist; der Einsatz telemedizinischer Verfahren ist zulässig."@de .

dtd:LA-id606 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Risiken, die mit ihm für die Personen verbunden sind, bei denen es durchgeführt werden soll, ärztlich vertretbar sind,"@de .

dtd:LA-id607 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Personen, bei denen es durchgeführt werden soll, ihre schriftliche Einwilligung hierzu erteilt haben, nachdem sie durch eine ärztliche Person über Wesen, Bedeutung und Risiken der Immunisierung sowie die damit verbundene Verarbeitung personenbezogener Daten aufgeklärt worden sind und dies schriftlich oder elektronisch bestätigt haben,"@de .

dtd:LA-id608 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seine Durchführung von einer ärztlichen Person, die nach dem Stand der medizinischen Wissenschaft sachkundig ist, geleitet wird,"@de .

dtd:LA-id609 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein dem Stand der medizinischen Wissenschaft entsprechender Immunisierungsplan vorliegt,"@de .

dtd:LA-id610 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die ärztliche Kontrolle des Gesundheitszustandes der spendenden Personen während der Immunisierungsphase gewährleistet ist,"@de .

dtd:LA-id611 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der zuständigen Behörde die Durchführung des Immunisierungsprogramms angezeigt worden ist und"@de .

dtd:LA-id612 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das zustimmende Votum einer nach Landesrecht gebildeten und für die ärztliche Person nach Satz 1 Nr. 3 zuständigen und unabhängigen Ethik-Kommission vorliegt."@de .

dtd:LA-id613 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Spendeeinrichtungen,"@de .

dtd:LA-id614 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Auswahl und Untersuchung der spendenden Personen,"@de .

dtd:LA-id615 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Aufklärung und Einwilligung der spendenden Personen,"@de .

dtd:LA-id616 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Spendeentnahme,"@de .

dtd:LA-id617 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Spenderimmunisierung und die Vorbehandlung zur Blutstammzellentnahme und"@de .

dtd:LA-id618 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Dokumentation der Spendeentnahme und den Schutz der dokumentierten Daten"@de .

dtd:LA-id619 a dtd:LA ;
    dtd:has_Value "abweichend von Absatz 1 Satz 2 eine Neubewertung der Risiken, die zu einem Ausschluss oder einer Rückstellung führen, vorzunehmen, soweit sie aufgrund der Änderung dieses Gesetzes durch das Gesetz zur Änderung des Fünften Buches Sozialgesetzbuch – Stiftung Unabhängige Patientenberatung Deutschland – und zur Änderung weiterer Gesetze vom 11. Mai 2023 (BGBl. 2023 I Nr. 123) erforderlich ist und"@de .

dtd:LA-id620 a dtd:LA ;
    dtd:has_Value "die Feststellungen der Höchstaltersgrenzen für Erst- und Wiederholungsspender aufzuheben unter Verweis auf eine individuelle Feststellung der Spendetauglichkeit nach ärztlicher Beurteilung."@de .

dtd:LA-id621 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Patientenidentifikationsnummer oder entsprechende eindeutige Angaben zu der zu behandelnden Person, wie Name, Vorname, Geburtsdatum und Adresse,"@de .

dtd:LA-id622 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Chargenbezeichnung,"@de .

dtd:LA-id623 a dtd:LA ;
    dtd:has_DL dtd:DL-id483 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pharmazentralnummer oder-Bezeichnung des Präparates-Name oder Firma des pharmazeutischen Unternehmers-Menge und Stärke,"@de .

dtd:LA-id624 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bezeichnung des Präparates"@de .

dtd:LA-id625 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name oder Firma des pharmazeutischen Unternehmers"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "Menge und Stärke,"@de .

dtd:LA-id627 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Datum und Uhrzeit der Anwendung."@de .

dtd:LA-id628 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anwendung von Blutprodukten, einschließlich der Dokumentation der Indikation zur Anwendung von Blutprodukten und Arzneimitteln zur spezifischen Therapie von Gerinnungsstörungen bei Hämophilie, die Testung auf Infektionsmarker der zu behandelnden Personen anlässlich der Anwendung von Blutprodukten und die Anforderungen an die Rückstellproben,"@de .

dtd:LA-id629 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Qualitätssicherung der Anwendung von Blutprodukten in den Einrichtungen der Krankenversorgung und ihre Überwachung durch die Ärzteschaft,"@de .

dtd:LA-id630 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Qualifikation und die Aufgaben der im engen Zusammenhang mit der Anwendung von Blutprodukten tätigen Personen,"@de .

dtd:LA-id631 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Umgang mit nicht angewendeten Blutprodukten in den Einrichtungen der Krankenversorgung"@de .

dtd:LA-id632 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Rückverfolgungszeitraum für vorangegangene Spenden zum Schutz vor den jeweiligen Übertragungsrisiken muss angemessen sein,"@de .

dtd:LA-id633 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine als infektiös verdächtige Spende muss gesperrt werden, bis durch Wiederholungs- oder Bestätigungstestergebnisse über das weitere Vorgehen entschieden worden ist,"@de .

dtd:LA-id634 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "es muss unverzüglich Klarheit über den Infektionsstatus der spendenden Person und über ihre infektionsverdächtigen Spenden gewonnen werden,"@de .

dtd:LA-id635 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine nachweislich infektiöse Spende muss sicher ausgesondert werden,"@de .

dtd:LA-id636 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die notwendigen Informationsverfahren müssen eingehalten werden, wobei § 16 Abs. 2 Satz 3 entsprechend gilt, und"@de .

dtd:LA-id637 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einleitung des Rückverfolgungsverfahrens ist unverzüglich der zuständigen Behörde anzuzeigen, wenn die Bestätigungstestergebnisse die Infektiosität bestätigen, fraglich sind oder eine Nachtestung nicht möglich ist; § 16 Abs. 2 Satz 3 gilt entsprechend."@de .

dtd:LA-id638 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an die Vertrauensstelle nach § 21a Absatz 2 Satz 1 die personenidentifizierenden Daten nach Maßgabe des nach § 21a Absatz 2 Satz 4 festgelegten Pseudonymisierungsverfahrens und"@de .

dtd:LA-id639 a dtd:LA ;
    dtd:has_DL dtd:DL-id517 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an das Deutsche Hämophilieregister nach § 21a Absatz 1 Satz 1 die pseudonymisierten Daten nach Maßgabe der nach § 21a Absatz 3 Satz 3 getroffenen Festlegungen und des § 2 Absatz 4 Satz 2 Nummer 3 der Transfusionsgesetz-Meldeverordnung, insbesondere a)die Angaben zu Alter, Geschlecht und Wohnort des Patienten,b)die Behandlungsdaten,c)die Angaben zur Krankenkasse,d)die Angaben zum Widerruf der Einwilligung des Patienten oder zum Tod des Patienten"@de .


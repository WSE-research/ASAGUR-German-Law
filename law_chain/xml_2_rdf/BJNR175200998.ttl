    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erhebung, Zusammenführung, Prüfung und Auswertung der Meldungen nach § 21 Absatz 1a,"@de .

dtd:LA-id645 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Festlegung der Einzelheiten zum Datensatz nach Absatz 3 Satz 3 und § 2 Absatz 4 Satz 2 Nummer 3 der Transfusionsgesetz-Meldeverordnung entsprechend dem Stand der medizinischen Wissenschaft und Technik einschließlich der Fortschreibung des Datensatzes,"@de .

dtd:LA-id646 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Festlegung der Einzelheiten des Pseudonymisierungsverfahrens nach Absatz 2 Satz 4,"@de .

dtd:LA-id647 a dtd:LA ;

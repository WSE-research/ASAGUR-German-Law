dtd:LA-id652 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die von der hämophiliebehandelnden ärztlichen Person übermittelten anonymisierten Daten nach § 21 Absatz 1a Satz 1 sowie"@de .

dtd:LA-id653 a dtd:LA ;
    dtd:has_DL dtd:DL-id548 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall der schriftlichen oder elektronischen Einwilligung des behandelten Patienten a)das von der Vertrauensstelle nach Absatz 2 erzeugte Pseudonym undb)die von der hämophiliebehandelnden ärztlichen Person übermittelten Daten nach § 21 Absatz 1a Satz 2 Nummer 2."@de .

dtd:LA-id654 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das von der Vertrauensstelle nach Absatz 2 erzeugte Pseudonym und"@de .

dtd:LA-id655 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die von der hämophiliebehandelnden ärztlichen Person übermittelten Daten nach § 21 Absatz 1a Satz 2 Nummer 2."@de .


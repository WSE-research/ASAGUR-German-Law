dtd:LA-id215 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Prüfung der meisterhaften Verrichtung der wesentlichen Tätigkeiten (Teil I),"@de .

dtd:LA-id216 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Prüfung der erforderlichen fachtheoretischen Kenntnisse (Teil II),"@de .

dtd:LA-id217 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Prüfung der erforderlichen betriebswirtschaftlichen, kaufmännischen und rechtlichen Kenntnisse (Teil III) und"@de .

dtd:LA-id218 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Prüfung der erforderlichen berufs- und arbeitspädagogischen Kenntnisse (Teil IV)."@de .

dtd:LA-id219 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kundenwünsche ermitteln, Kunden beraten, Serviceleistungen anbieten, Auftragsverhandlungen führen und Auftragsziele festlegen, Leistungen kalkulieren und Angebote erstellen, Verträge schließen,"@de .

dtd:LA-id220 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufgaben der technischen, kaufmännischen und personalwirtschaftlichen Betriebsführung wahrnehmen, insbesondere unter Berücksichtigung der Betriebsorganisation, der betrieblichen Aus- und Weiterbildung, des Qualitätsmanagements, der Haftungsvorschriften des Arbeitsschutzrechtes, des Datenschutzes, des Umweltschutzes sowie von Informations- und Kommunikationstechniken,"@de .

dtd:LA-id221 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auftragsabwicklungsprozesse planen, organisieren, durchführen und überwachen,"@de .

dtd:LA-id222 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufträge durchführen, insbesondere unter Berücksichtigung von Fertigungstechniken, Instandhaltungsalternativen, berufsbezogenen rechtlichen Vorschriften und technischen Normen sowie der anerkannten Regeln der Technik, Personal, Material und Geräten sowie Einsatzmöglichkeiten von Auszubildenden,"@de .

dtd:LA-id223 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zahntechnische Arbeits- und Prozessabläufe, insbesondere unter Berücksichtigung der Hygienevorschriften, planen sowie Skizzen und Zeichnungen, auch unter Einsatz rechnergestützter Systeme, erstellen,"@de .

dtd:LA-id224 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Daten der Kieferrelationsbestimmung auch rechnergestützt ermitteln und in mechanische Geräte übertragen,"@de .

dtd:LA-id225 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arten und Eigenschaften zu verarbeitender Werkstoffe, einschließlich der Ur- und Umformverfahren sowie Verfahren zur Oberflächenbehandlung und Stoffeigenschaftsänderung bei der Planung und Fertigung von Zahnersatz, kieferorthopädischen und therapeutischen Geräten, berücksichtigen,"@de .

dtd:LA-id226 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "manuelle, mechanisch gesteuerte sowie programmgesteuerte Be- und Verarbeitungsverfahren beherrschen, auch unter Anwendung von lasergestützten sowie galvanischen Verfahren,"@de .

dtd:LA-id227 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Urformen, Umformen sowie Verbund- und Fügetechniken bei der Herstellung von Zahnersatz beherrschen,"@de .

dtd:LA-id228 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "individuelle Verbindungselemente für prothetische Arbeiten herstellen und einarbeiten; konfektionierte Verbindungselemente einarbeiten,"@de .

dtd:LA-id229 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zahnersatz, Defektprothesen, Epithesen, therapeutische Geräte, Schienen und Mundschutz planen, herstellen, instand setzen und ändern,"@de .

dtd:LA-id230 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zahnrestaurationen planen und herstellen,"@de .

dtd:LA-id231 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Qualitätskontrollen durchführen, Fehler, Mängel und Störungen beseitigen, Ergebnisse bewerten und dokumentieren,"@de .

dtd:LA-id232 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zahntechnische Leistungen abnehmen und dokumentieren sowie Nachkalkulation durchführen und Auftragsabwicklung auswerten."@de .

dtd:LA-id233 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Ober- oder Unterkiefer mit festsitzendem siebengliedrigem Zahnersatz nach vorgegebenem Okklusionskonzept mit mindestens drei Front- und drei Seitenzähnen versorgen und ein Einzelimplantat mit vollkeramischer Restauration sowie einen okklusal adjustierten Aufbissbehelf herstellen,"@de .

dtd:LA-id234 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen kombiniert festsitzenden und herausnehmbaren Zahnersatz für Ober- oder Unterkiefer in Verbindung mit insgesamt vier individuellen und konfektionierten Verbindungselementen herstellen,"@de .

dtd:LA-id235 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "prothetische Versorgung eines unbezahnten Unter- und Oberkiefers nach System herstellen,"@de .


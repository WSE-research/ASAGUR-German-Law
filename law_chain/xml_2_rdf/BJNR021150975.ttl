dtd:LA-id543 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Arzneimittel ausschließlich zur arzneilichen Versorgung der dort vorhandenen oder, im Falle einer Untereinheit der Praxis, von dort behandelten Tiere bestimmt sind und"@de .

dtd:LA-id544 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Betriebsräume ausschließlich der Verfügungsgewalt des Tierarztes unterstehen."@de .

dtd:LA-id545 a dtd:LA ;
    dtd:has_Value "die Tiere oder der Tierbestand in angemessenem Umfang vom Tierarzt untersucht worden sind,"@de .

dtd:LA-id546 a dtd:LA ;
    dtd:has_Value "die Anwendung der Arzneimittel und der Behandlungserfolg vom Tierarzt kontrolliert werden und"@de .

dtd:LA-id547 a dtd:LA ;
    dtd:has_Value "im Falle der Behandlung mit einem Arzneimittel mit antibakterieller Wirkung eine klinische Untersuchung vom Tierarzt durchgeführt wird."@de .

dtd:LA-id548 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Eiern sieben Tage,"@de .

dtd:LA-id549 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Milch sieben Tage,"@de .

dtd:LA-id550 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei essbarem Gewebe von Geflügel und Säugetieren 28 Tage,"@de .

dtd:LA-id551 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei essbarem Gewebe von Fischen die Zahl (Anzahl der Tage), die sich aus der Division von 500 durch die mittlere Wassertemperatur in Grad Celsius ergibt,"@de .

dtd:LA-id552 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei essbarem Gewebe von Einhufern, die der Gewinnung von Lebensmitteln dienen und bei denen Arzneimittel gemäß § 56a Abs. 2a des Arzneimittelgesetzes angewendet wurden, sechs Monate."@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Value "bei Wechsel des Arzneimittels mit antibakterieller Wirkung im Verlauf einer Behandlung,"@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_DL dtd:DL-id425 ;
    dtd:has_Value "bei einer Behandlung mit einem Arzneimittel mit antibakterieller Wirkung, a)die häufiger als einmal in bestimmten Alters- oder Produktionsabschnitten stattfindet, oderb)die die Dauer von sieben Tagen übersteigt, es sei denn, bei der Erteilung der Zulassung wurde ein längerer Zeitraum für die Dauer der Anwendung festgelegt, oder die in diesem Falle den längeren festgelegten Zeitraum übersteigt,"@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_Value "die häufiger als einmal in bestimmten Alters- oder Produktionsabschnitten stattfindet, oder"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Value "die die Dauer von sieben Tagen übersteigt, es sei denn, bei der Erteilung der Zulassung wurde ein längerer Zeitraum für die Dauer der Anwendung festgelegt, oder die in diesem Falle den längeren festgelegten Zeitraum übersteigt,"@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Value "bei kombinierter Verabreichung von Arzneimitteln mit antibakteriellen Wirkstoffen bei einer Indikation, ausgenommen zugelassene Fertigarzneimittel, die eine Kombination von antibakteriellen Wirkstoffen enthalten,"@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Value "bei Abweichung von den Vorgaben der Zulassungsbedingungen von Arzneimitteln mit antibakteriellen Wirkstoffen nach § 56a Absatz 2 Satz 1 Nummer 2 bis 4 des Arzneimittelgesetzes oder"@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Value "bei der Behandlung mit Arzneimitteln, die Cephalosporine der dritten oder vierten Generation oder Fluorchinolone enthalten."@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Value "die Probenahme mit der Gefahr einer zusätzlichen Beeinträchtigung des Gesundheitszustandes des zu behandelnden Tieres verbunden wäre,"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Value "der Erreger nicht mittels zellfreier künstlicher Medien kultiviert werden kann, oder"@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Value "für die Bestimmung der Empfindlichkeit des Erregers keine geeignete Methode verfügbar ist."@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Value "Proben von den zu behandelnden Tieren zu entnehmen oder unter seiner Aufsicht entnehmen zu lassen,"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Value "aus den Proben die die Erkrankung verursachenden bakteriellen Erreger unter Berücksichtigung des Krankheitsbildes zu isolieren oder isolieren zu lassen und"@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Value "die isolierten bakteriellen Erreger auf ihre Empfindlichkeit gegen antibakteriell wirksame Stoffe zu untersuchen oder untersuchen zu lassen."@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anwendungs- oder Abgabedatum, bei der Anwendung oder Abgabe von Arzneimitteln, die antibakteriell wirksame Stoffe enthalten, auch das Untersuchungsdatum,"@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "fortlaufende Belegnummer des Tierarztes im jeweiligen Jahr,"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name des behandelnden Tierarztes und Praxisanschrift,"@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name und Anschrift des Tierhalters,"@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anzahl, Art, es sei denn, es erfolgt eine Angabe nach Satz 3 Nummer 1, Identität und bei verschreibungspflichtigen Arzneimitteln, die nicht ausschließlich zur lokalen Anwendung vorgesehen sind, das geschätzte Gewicht der Tiere,"@de .

dtd:LA-id571 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arzneimittelbezeichnung,"@de .

dtd:LA-id572 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "angewendete oder abgegebene Menge des Arzneimittels und"@de .

dtd:LA-id573 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wartezeit."@de .

dtd:LA-id574 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Diagnose,"@de .

dtd:LA-id575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Chargenbezeichnung,"@de .

dtd:LA-id576 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Dosierung des Arzneimittels pro Tier und Tag sowie Art, Dauer und Zeitpunkt der Anwendung und"@de .

dtd:LA-id577 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit erforderlich, weitere Behandlungsanweisungen an den Tierhalter."@de .

dtd:LA-id578 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Nutzungsart (Mastkälber bis zu einem Alter von acht Monaten, Mastrinder ab einem Alter von acht Monaten, Ferkel bis einschließlich 30 Kilogramm, Mastschweine über 30 Kilogramm, Mastputen oder Masthühner),"@de .

dtd:LA-id579 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für die Berechnung der Therapiehäufigkeit nach § 58c Absatz 1 des Arzneimittelgesetzes notwendige Anzahl der Behandlungstage gemäß § 58b Absatz 1 Nummer 3, gegebenenfalls ergänzt um die Anzahl der Tage, in denen das betroffene Arzneimittel seinen therapeutischen Wirkstoffspiegel gemäß § 58b Absatz 3 des Arzneimittelgesetzes behält und"@de .

dtd:LA-id580 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die dem Betrieb gemäß der Vieh-Verkehrs-Verordnung erteilte Registriernummer."@de .

dtd:LA-id581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anwendungs- oder Abgabedatum, bei der Anwendung oder Abgabe von Arzneimitteln, die antibakteriell wirksame Stoffe enthalten, auch das Untersuchungsdatum,"@de .

dtd:LA-id582 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name und Anschrift des Tierhalters,"@de .

dtd:LA-id583 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anzahl, Art und Identität der Tiere,"@de .

dtd:LA-id584 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Arzneimittelbezeichnung und"@de .

dtd:LA-id585 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "angewendete oder abgegebene Menge des Arzneimittels."@de .

dtd:LA-id586 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Datum der Probenahme,"@de .

dtd:LA-id587 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name und Anschrift des Tierhalters, Identität der beprobten Tiere und Probenmatrix,"@de .

dtd:LA-id588 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bezeichnung des verwendeten Tests,"@de .

dtd:LA-id589 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Datum von Untersuchungsbeginn und -ende,"@de .

dtd:LA-id590 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Befund: quantitatives Ergebnis und qualitative Bewertung der Empfindlichkeitstestung."@de .

dtd:LA-id591 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den Erwerb die geordnete Zusammenstellung der Lieferscheine, Rechnungen oder Warenbegleitscheine, aus denen sich Lieferant, Art und Menge und, soweit vorhanden, die Chargenbezeichnung der Arzneimittel ergeben müssen,"@de .

dtd:LA-id592 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Herstellung die Aufzeichnungen in einem Herstellungsbuch oder auf Karteikarten, aus denen das Datum der Herstellung, die Art und Menge der hergestellten Arzneimittel und die zugrunde liegenden Herstellungsvorschriften hervorgehen,"@de .

dtd:LA-id593 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Prüfung die Aufzeichnungen in einem Prüfungsbuch oder auf Karteikarten oder Prüfungsberichte, wenn die Prüfung nicht in der tierärztlichen Hausapotheke durchgeführt worden ist; die Aufzeichnungen müssen Angaben über Lieferant, Art und Menge der untersuchten Arzneimittel, über das Datum des Erwerbs oder der Herstellung sowie über Ort, Art und Datum der Untersuchung enthalten."@de .

dtd:LA-id594 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Anwendung von Arzneimitteln bei Tieren, die der Gewinnung von Lebensmitteln dienen, die Angaben nach Absatz 2 Satz 1,"@de .

dtd:LA-id595 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Abgabe von Arzneimitteln, die zur Anwendung bei Tieren im Sinne der Nummer 1 bestimmt sind, die Angaben nach Absatz 2 Satz 1 und 2,"@de .

dtd:LA-id596 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Falle der Behandlung von Tieren, für die gemäß § 58a des Arzneimittelgesetzes Mitteilungen über deren Haltung zu machen sind, mit Arzneimitteln, die antibakteriell wirksame Stoffe enthalten, die Angaben nach Absatz 2 Satz 1 bis 3."@de .

dtd:LA-id597 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Der zuständigen Behörde sind Tatsachen bekannt, die darauf schließen lassen, dass Vorschriften über den Verkehr mit Arzneimitteln, einschließlich Verschreibung oder Anwendung von Arzneimitteln, nicht beachtet worden sind, oder"@de .

dtd:LA-id598 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die vorgelegten Unterlagen nach Absatz 5 erlauben nicht den Nachweis über den ordnungsgemäßen Bezug und den Verbleib der Arzneimittel."@de .

dtd:LA-id599 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 3 Abs. 3 einen Betriebsraum zu praxisfremden Zwecken verwendet,"@de .

dtd:LA-id600 a dtd:LA ;

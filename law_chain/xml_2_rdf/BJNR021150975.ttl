    dtd:has_Value "entgegen § 8 Abs. 3 Satz 2 Arzneimittel nicht oder nicht richtig lagert,"@de .

dtd:LA-id602 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 9 Abs. 1 Satz 1 oder Abs. 2 Arzneimittel nicht oder nicht richtig lagert,"@de .

dtd:LA-id603 a dtd:LA ;

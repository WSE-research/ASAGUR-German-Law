dtd:LA-id1000 a dtd:LA ;
    dtd:has_DL dtd:DL-id886 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Kopfzeilea)eine deutlich hervorgehobene Überschrift \"Veröffentlichung von Insiderinformationen nach Artikel 17 der Verordnung (EU) Nr. 596/2014\",b)ein als Betreff erkennbares Schlagwort, das den wesentlichen Inhalt der Veröffentlichung zusammenfasst,"@de .

dtd:LA-id1001 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine deutlich hervorgehobene Überschrift \"Veröffentlichung von Insiderinformationen nach Artikel 17 der Verordnung (EU) Nr. 596/2014\","@de .

dtd:LA-id1002 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein als Betreff erkennbares Schlagwort, das den wesentlichen Inhalt der Veröffentlichung zusammenfasst,"@de .

dtd:LA-id1003 a dtd:LA ;
    dtd:has_DL dtd:DL-id891 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Emittenten oder Teilnehmer am Markt für Emissionszertifikatea)sein Name undb)seine Anschrift,"@de .

dtd:LA-id1004 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sein Name und"@de .

dtd:LA-id1005 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seine Anschrift,"@de .

dtd:LA-id1006 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die internationalen Wertpapierkennnummern der vom Emittenten ausgegebenen Aktien, Options- und Wandelanleihen sowie Genussscheine mit Ausstattungsmerkmalen, die den Aktien vergleichbar sind, soweit sie zum Handel an einem inländischen organisierten Markt zugelassen sind oder für sie eine solche Zulassung beantragt wurde, sowie die Börse und das Handelssegment, für die die Zulassung besteht oder beantragt wurde; hat der Emittent weitere Finanzinstrumente ausgegeben, für die eine Zulassung besteht oder beantragt wurde, genügt die Angabe einer Internetadresse, unter der er die entsprechenden Angaben für diese Finanzinstrumente in einer stets aktuellen und vollständigen Datei bereitzustellen hat, wobei die Hauptseite einen deutlich erkennbaren Hinweis auf eine Seite mit Informationen für Anleger zu enthalten hat, unter der die Datei leicht aufzufinden sein muss,"@de .

dtd:LA-id1007 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zu veröffentlichende Information,"@de .

dtd:LA-id1008 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Datum des Eintritts der der Information zugrunde liegenden Umstände,"@de .

dtd:LA-id1009 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine kurze Erklärung, inwieweit die Information den Emittenten oder Teilnehmer am Markt für Emissionszertifikate unmittelbar betrifft, soweit sich dies nicht schon aus den Angaben zu Nummer 4 ergibt, sowie"@de .

dtd:LA-id1010 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Erklärung, aus welchen Gründen die Information geeignet ist, im Fall ihres öffentlichen Bekanntwerdens den Kurs der Finanzinstrumente oder den Kurs damit verbundener derivativer Finanzinstrumente erheblich zu beeinflussen, soweit sich dies nicht schon aus den Angaben zu Nummer 4 ergibt."@de .

dtd:LA-id1011 a dtd:LA ;
    dtd:has_DL dtd:DL-id905 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Kopfzeilea)eine deutlich hervorgehobene Überschrift \"Aktualisierung einer Veröffentlichung von Insiderinformationen nach Artikel 17 der Verordnung (EU) Nr. 596/2014\",b)ein Schlagwort im Sinn des Absatzes 1 Satz 1 Nr. 1 Buchstabe b,"@de .

dtd:LA-id1012 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine deutlich hervorgehobene Überschrift \"Aktualisierung einer Veröffentlichung von Insiderinformationen nach Artikel 17 der Verordnung (EU) Nr. 596/2014\","@de .

dtd:LA-id1013 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Schlagwort im Sinn des Absatzes 1 Satz 1 Nr. 1 Buchstabe b,"@de .

dtd:LA-id1014 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach den Angaben im Sinn des Absatzes 1 Satz 1 Nr. 2 und 3 die Medien, an die die Information gesandt wurde, sowie den Zeitpunkt dieser Versendung,"@de .

dtd:LA-id1015 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zu veröffentlichende Information über die veränderten Umstände und"@de .

dtd:LA-id1016 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angaben im Sinn des Absatzes 1 Satz 1 Nr. 5 bis 7."@de .

dtd:LA-id1017 a dtd:LA ;
    dtd:has_DL dtd:DL-id919 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Kopfzeilea)eine deutlich hervorgehobene Überschrift \"Berichtigung einer Veröffentlichung von Insiderinformationen nach Artikel 17 der Verordnung (EU) Nr. 596/2014\",b)ein Schlagwort im Sinn des Absatzes 1 Satz 1 Nr. 1 Buchstabe b,"@de .

dtd:LA-id1018 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine deutlich hervorgehobene Überschrift \"Berichtigung einer Veröffentlichung von Insiderinformationen nach Artikel 17 der Verordnung (EU) Nr. 596/2014\","@de .

dtd:LA-id1019 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Schlagwort im Sinn des Absatzes 1 Satz 1 Nr. 1 Buchstabe b,"@de .

dtd:LA-id1020 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach den Angaben im Sinn des Absatzes 1 Satz 1 Nr. 2 und 3 den Inhalt der Veröffentlichung der unwahren Information, die Medien, an die die Information gesandt wurde, sowie den Zeitpunkt dieser Versendung,"@de .

dtd:LA-id1021 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die wahre Information und"@de .

dtd:LA-id1022 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angaben im Sinn des Absatzes 1 Satz 1 Nr. 5 bis 7, bezogen auf die wahre Information."@de .

dtd:LA-id1023 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Ergebnis oder der Gang laufender Verhandlungen über Geschäftsinhalte, die geeignet wären, im Fall ihres öffentlichen Bekanntwerdens den Börsen- oder Marktpreis erheblich zu beeinflussen, von der Veröffentlichung wahrscheinlich beeinträchtigt würden und eine Veröffentlichung die Interessen der Anleger erheblich gefährden würde, oder"@de .

dtd:LA-id1024 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch das Geschäftsführungsorgan des Emittenten abgeschlossene Verträge oder andere getroffene Entscheidungen zusammen mit der Ankündigung bekannt gegeben werden müssten, dass die für die Wirksamkeit der Maßnahme erforderliche Zustimmung eines anderen Organs des Emittenten noch aussteht, und dies die sachgerechte Bewertung der Information durch das Publikum gefährden würde, wenn der Emittent dafür gesorgt hat, dass die endgültige Entscheidung so schnell wie möglich getroffen wird."@de .

dtd:LA-id1025 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle Zeitpunkte, an denen der Fortbestand der Gründe überprüft wurde, sowie"@de .

dtd:LA-id1026 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Vor- und Familiennamen sowie die Geschäftsanschriften und Rufnummern aller Personen, die an der Entscheidung über die Befreiung beteiligt waren."@de .

dtd:LA-id1027 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Wortlaut der vorgesehenen Veröffentlichung,"@de .

dtd:LA-id1028 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der vorgesehene Zeitpunkt der Veröffentlichung und"@de .

dtd:LA-id1029 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Ansprechpartner des Emittenten mit Rufnummer."@de .

dtd:LA-id1030 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Vor- und Familiennamen der Person, der die Insiderinformation offengelegt worden ist,"@de .

dtd:LA-id1031 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihre Geschäftsanschrift oder, falls eine solche nicht besteht, ihre Privatanschrift,"@de .

dtd:LA-id1032 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Zeitpunkt der Informationspreisgabe sowie"@de .


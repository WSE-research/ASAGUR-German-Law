dtd:LA-id2705 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Antragsteller ein Bedürfnis für die beabsichtigte Tätigkeit nicht nachweist,"@de .

dtd:LA-id2706 a dtd:LA ;

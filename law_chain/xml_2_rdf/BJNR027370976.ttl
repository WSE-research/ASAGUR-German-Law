    dtd:has_DL dtd:DL-id1354 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Einheiten und Ausbildungseinrichtungen des Katastrophenschutzes a)der Länder undb)der kommunalen Gebietskörperschaften und"@de .

dtd:LA-id2373 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Länder und"@de .


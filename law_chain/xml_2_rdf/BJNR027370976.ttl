dtd:LA-id2602 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegen die verfassungsmäßige Ordnung gerichtet sind,"@de .

dtd:LA-id2603 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegen den Gedanken der Völkerverständigung, insbesondere gegen das friedliche Zusammenleben der Völker, gerichtet sind oder"@de .

dtd:LA-id2604 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch Anwendung von Gewalt oder darauf gerichtete Vorbereitungshandlungen auswärtige Belange der Bundesrepublik Deutschland gefährden,"@de .

dtd:LA-id2605 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mitglied in einer Vereinigung waren, die solche Bestrebungen verfolgt oder verfolgt hat, oder"@de .

dtd:LA-id2606 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine solche Vereinigung unterstützt haben,"@de .

dtd:LA-id2607 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die innerhalb der letzten fünf Jahre mehr als einmal wegen Gewalttätigkeit mit richterlicher Genehmigung in polizeilichem Präventivgewahrsam waren,"@de .

dtd:LA-id2608 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die wiederholt oder gröblich gegen die Vorschriften eines der in Nummer 1 Buchstabe c genannten Gesetze oder gegen Vorschriften des Arbeitsschutz-, Chemikalien-, Gefahrgut-, Immissionsschutz-, Gewässerschutz- oder Bergrechts verstoßen haben."@de .

dtd:LA-id2609 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die unbeschränkte Auskunft aus dem Bundeszentralregister, die Auskunft aus dem Erziehungsregister und im gewerblichen Bereich auch die Auskunft aus dem Gewerbezentralregister;"@de .

dtd:LA-id2610 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Auskunft aus dem zentralen staatsanwaltschaftlichen Verfahrensregister hinsichtlich der in Absatz 2 Nummer 1 genannten Straftaten;"@de .

dtd:LA-id2611 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Stellungnahme der örtlichen Polizeidienststelle, ob Tatsachen bekannt sind, die Bedenken gegen die Zuverlässigkeit begründen; die örtliche Polizeidienststelle schließt in ihre Stellungnahme das Ergebnis der von ihr vorzunehmenden Prüfung nach Absatz 2 Nummer 4 ein;"@de .

dtd:LA-id2612 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Auskunft der für den Wohnsitz des Betroffenen zuständigen Verfassungsschutzbehörde, ob Tatsachen bekannt sind, die Bedenken gegen die Zuverlässigkeit nach Absatz 2 Nummer 2 und 3 begründen; liegt der Wohnsitz des Betroffenen außerhalb des Geltungsbereichs dieses Gesetzes, ist das Bundesamt für Verfassungsschutz für die Erteilung der Auskunft zuständig;"@de .

dtd:LA-id2613 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Personen aus einem Staat, der nicht Mitgliedstaat der Europäischen Union ist, in der Regel auch die Auskunft der Ausländerbehörde."@de .

dtd:LA-id2614 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "geschäftsunfähig sind,"@de .

dtd:LA-id2615 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "abhängig von Alkohol oder anderen berauschenden Mitteln, psychisch krank oder debil sind oder"@de .

dtd:LA-id2616 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Grund in der Person liegender Umstände mit explosionsgefährlichen Stoffen nicht vorsichtig oder sachgemäß umgehen oder diese nicht sorgfältig aufbewahren können oder dass die konkrete Gefahr einer Fremd- oder Selbstgefährdung besteht."@de .

dtd:LA-id2617 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer die erfolgreiche Teilnahme an einem staatlichen oder staatlich anerkannten Lehrgang für die beabsichtigte Tätigkeit durch ein Zeugnis nachweist oder"@de .

dtd:LA-id2618 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer eine Prüfung vor der zuständigen Behörde bestanden hat."@de .

dtd:LA-id2619 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine mindestens dreijährige praktische Tätigkeit ausgeübt hat oder"@de .

dtd:LA-id2620 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Ausbildung an einer Hochschule, einer Fachhochschule oder einer Technikerschule abgeschlossen und eine mindestens einjährige praktische Tätigkeit ausgeübt hat,"@de .

dtd:LA-id2621 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Anerkennung der in Absatz 1 Nr. 1 bezeichneten Lehrgänge zuverlässiger Antragsteller, die Zulassung der Lehrgangsteilnehmer, die ihnen zu vermittelnden technischen und rechtlichen Kenntnisse und den Nachweis ihrer erfolgreichen Teilnahme,"@de .

dtd:LA-id2622 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die fachlichen Anforderungen an die technischen und rechtlichen Kenntnisse, an die praktischen Fertigkeiten, über die Voraussetzungen für die Prüfung nach Absatz 1 Nr. 2 und über das Prüfungsverfahren einschließlich der Errichtung von Prüfungsausschüssen,"@de .

dtd:LA-id2623 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Verpflichtung des Erlaubnisinhabers, in bestimmten Abständen an einem staatlichen oder staatlich anerkannten Lehrgang oder Wiederholungslehrgang nach Nummer 1 teilzunehmen."@de .

dtd:LA-id2624 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Person einen Wohnsitz, einen ständigen Aufenthaltsort oder eine Niederlassung außerhalb des Geltungsbereiches dieses Gesetzes hat und dort Vorschriften über die besonderen Anforderungen an die Sicherheit der rechtmäßigen Verwendung dieser Stoffe bestehen, die diesem Gesetz vergleichbar sind, und"@de .

dtd:LA-id2625 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die die Stoffe begleitende Person nach den in Nummer 1 bezeichneten Vorschriften zum Verbringen befugt ist."@de .

dtd:LA-id2626 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für das Verbringen innerhalb des Geltungsbereichs dieses Gesetzes die für den Bestimmungsort des Verbringens zuständige Landesbehörde,"@de .

dtd:LA-id2627 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für das Verbringen in den, durch den und aus dem Geltungsbereich dieses Gesetzes die Bundesanstalt."@de .

dtd:LA-id2628 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die an dem Verbringungsvorgang beteiligten und im Geltungsbereich des Gesetzes ansässigen Personen gemäß § 15 Absatz 1 zum Verbringen berechtigt sind und"@de .

dtd:LA-id2629 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den zu verbringenden Explosivstoff eine Baumusterprüfbescheinigung nach § 5b Absatz 2 vorliegt."@de .

dtd:LA-id2630 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "explosionsgefährliche Stoffe, die von dem Inhaber einer Erlaubnis nach § 27 in einer Menge hergestellt, wiedergewonnen, erworben, eingeführt, verbracht, verwendet oder vernichtet werden, für die auf Grund einer Rechtsverordnung nach § 18 keine Genehmigung zur Aufbewahrung nach § 17 erforderlich ist,"@de .

dtd:LA-id2631 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Explosivstoffe und sonstige explosionsgefährliche Stoffe der Stoffgruppe A, die in einer nach § 4 des Bundes-Immissionsschutzgesetzes genehmigungsbedürftigen Anlage zur Bearbeitung oder Verarbeitung hergestellt werden, sofern sie weder vertrieben noch anderen überlassen werden,"@de .

dtd:LA-id2632 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "pyrotechnische Gegenstände."@de .

dtd:LA-id2633 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Explosivstoffe nach § 5a Absatz 1 Nummer 1 und 2,"@de .

dtd:LA-id2634 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Explosivstoffe die einen geringen Gefährlichkeitsgrad haben auf Grund von Merkmalen und Faktoren, wie etwa einer geringen detonierenden Wirkung und dem geringen Sicherheitsrisiko, das von ihnen wegen der geringen potentiellen Auswirkungen eines Missbrauchs ausgeht,"@de .

dtd:LA-id2635 a dtd:LA ;
    dtd:has_DL dtd:DL-id1958 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Explosivstoffe, die unverpackt in Silo- oder Pumpfahrzeugen nach Maßgabe verkehrsrechtlicher Vorschriften befördert und geliefert werden und a)direkt in Sprengbohrlöcher geladen werden oderb)direkt in Silotanks oder Behältnisse und Einrichtungen für die Aufbewahrung oder den Transport in einer der Bergaufsicht unterliegenden Betriebsstätte des Verwenders entladen werden, und"@de .

dtd:LA-id2636 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "direkt in Sprengbohrlöcher geladen werden oder"@de .

dtd:LA-id2637 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "direkt in Silotanks oder Behältnisse und Einrichtungen für die Aufbewahrung oder den Transport in einer der Bergaufsicht unterliegenden Betriebsstätte des Verwenders entladen werden, und"@de .

dtd:LA-id2638 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Explosivstoffe, die am Sprengort hergestellt und danach sofort geladen werden."@de .

dtd:LA-id2639 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Explosivstoffe in den Verkehr bringen oder für eigene Zwecke verwenden, die gemäß den Sicherheitsanforderungen des Anhangs II der Richtlinie 2014/28/EU entworfen und hergestellt wurden,"@de .

dtd:LA-id2640 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "pyrotechnische Gegenstände in den Verkehr bringen, die gemäß den Sicherheitsanforderungen des Anhangs I der Richtlinie 2013/29/EU entworfen und hergestellt wurden."@de .

dtd:LA-id2641 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Explosivstoffe, die er in den Verkehr bringt, ein Konformitätsbewertungsverfahren nach Artikel 20 der Richtlinie 2014/28/EU durchführen lassen und dafür die Unterlagen nach Anhang III der Richtlinie 2014/28/EU erstellen,"@de .

dtd:LA-id2642 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für pyrotechnische Gegenstände, die er in den Verkehr bringt, das Konformitätsbewertungsverfahren nach Artikel 17 der Richtlinie 2013/29/EU durchführen lassen und dafür die Unterlagen nach Anhang II der Richtlinie 2013/29/EU erstellen."@de .

dtd:LA-id2643 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seinen Namen,"@de .

dtd:LA-id2644 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seinen eingetragenen Handelsnamen oder seine eingetragene Handelsmarke,"@de .

dtd:LA-id2645 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Postanschrift einer zentralen Anlaufstelle, an der er kontaktiert werden kann,"@de .

dtd:LA-id2646 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die CE-Kennzeichnung,"@de .

dtd:LA-id2647 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Kennnummer der benannten Stelle, die in der Phase der Fertigungskontrolle tätig war,"@de .

dtd:LA-id2648 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Handelsnamen und Typ des Explosivstoffes oder des pyrotechnischen Gegenstandes."@de .

dtd:LA-id2649 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bereithaltung der EU-Konformitätserklärung und der technischen Unterlagen für die zuständigen Behörden für die Dauer von zehn Jahren ab dem Inverkehrbringen des Explosivstoffes,"@de .

dtd:LA-id2650 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorlage aller erforderlichen Informationen und Unterlagen zum Nachweis der Konformität eines Explosivstoffes auf Verlangen der zuständigen Behörde,"@de .

dtd:LA-id2651 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Aufgabenbereich des Bevollmächtigten die Zusammenarbeit mit der zuständigen Behörde bei allen Maßnahmen, die dazu dienen, Risiken auszuschließen, die mit Explosivstoffen verbunden sind."@de .

dtd:LA-id2652 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Hersteller das Konformitätsbewertungsverfahren nach den §§ 5b und 5c durchgeführt hat,"@de .

dtd:LA-id2653 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Hersteller die technischen Unterlagen erstellt hat,"@de .

dtd:LA-id2654 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die CE-Kennzeichnung an dem Explosivstoff oder dem pyrotechnischen Gegenstand angebracht ist,"@de .

dtd:LA-id2655 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Explosivstoff oder dem pyrotechnischen Gegenstand die Gebrauchsanleitung und die Sicherheitsinformationen in einer für den Verwender verständlichen Weise in einer Amtssprache des Mitgliedstaates der Europäischen Union, in dem die Bereitstellung auf dem Markt erfolgt, oder sofern die Bereitstellung in der Bundesrepublik Deutschland erfolgt, in deutscher Sprache beigefügt sind, und"@de .

dtd:LA-id2656 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Hersteller die Kennzeichnungspflichten des § 16a Absatz 1 und des § 16c Absatz 1 an Explosivstoffe oder die Kennzeichnungspflichten des § 16c Absatz 1 und 3 an pyrotechnische Gegenstände erfüllt hat."@de .

dtd:LA-id2657 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seinen Namen,"@de .

dtd:LA-id2658 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seinen eingetragenen Handelsnamen oder seine eingetragene Handelsmarke und"@de .

dtd:LA-id2659 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Postanschrift einer zentralen Anlaufstelle, an der er kontaktiert werden kann."@de .

dtd:LA-id2660 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem Explosivstoff oder dem pyrotechnischen Gegenstand die Gebrauchsanleitung und Sicherheitsinformationen beigefügt sind, die in deutscher Sprache und in einer für den Verwender und die zuständige Behörde verständlichen Weise abgefasst sind, und"@de .

dtd:LA-id2661 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Hersteller die Anforderungen des § 16c Absatz 1 und der Einführer die Anforderungen des § 16g Absatz 1 erfüllt haben."@de .

dtd:LA-id2662 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Explosivstoff oder einen pyrotechnischen Gegenstand unter ihrem eigenen Namen oder ihrer eigenen Marke in den Verkehr bringen oder"@de .

dtd:LA-id2663 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Explosivstoff oder pyrotechnischen Gegenstand, der bereits auf dem Markt bereitgestellt worden ist, so verändern, dass der Explosivstoff oder der pyrotechnische Gegenstand nicht mehr dem nach § 5b geprüften Baumuster oder dem Explosivstoff oder dem pyrotechnischen Gegenstand, auf den sich die Einzelprüfung bezog, entspricht."@de .

dtd:LA-id2664 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Prüfungen vorzunehmen oder vornehmen zu lassen,"@de .

dtd:LA-id2665 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Verzeichnis der Beschwerden über nichtkonforme pyrotechnische Gegenstände und der Rückrufe pyrotechnischer Gegenstände zu führen und"@de .

dtd:LA-id2666 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Händler über ihre Überwachungsmaßnahmen zu unterrichten."@de .

dtd:LA-id2667 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "innerhalb einer von ihr gesetzten, der Art der Gefahr entsprechenden Frist alle geeigneten, erforderlichen und verhältnismäßigen Maßnahmen zu ergreifen, um die Übereinstimmung des Explosivstoffes oder des pyrotechnischen Gegenstandes mit den Anforderungen dieses Gesetzes herzustellen, oder"@de .

dtd:LA-id2668 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Explosivstoff oder den pyrotechnischen Gegenstand zurückzunehmen oder zurückzurufen."@de .

dtd:LA-id2669 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von denen er einen Explosivstoff oder einen pyrotechnischen Gegenstand erworben hat und"@de .

dtd:LA-id2670 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an die er einen Explosivstoff oder einen pyrotechnischen Gegenstand überlassen hat."@de .

dtd:LA-id2671 a dtd:LA ;
    dtd:has_I dtd:I-id992 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Errichtung und der Betrieb von Lagern, in denen explosionsgefährliche Stoffe zu gewerblichen Zwecken, im Rahmen einer wirtschaftlichen Unternehmung oder eines land- oder forstwirtschaftlichen Betriebes oder bei der Beschäftigung von Arbeitnehmern aufbewahrt werden sollen,"@de .

dtd:LA-id2672 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die wesentliche Änderung der Beschaffenheit oder des Betriebes solcher Lager."@de .

dtd:LA-id2673 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "keine Vorsorge gegen Gefahren für Leben, Gesundheit und Sachgüter Beschäftigter oder Dritter, insbesondere durch die den allgemein anerkannten Regeln der Technik entsprechenden Maßnahmen getroffen sind,"@de .

dtd:LA-id2674 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "andere öffentlich-rechtliche Vorschriften oder Belange des Arbeitsschutzes, der Errichtung, dem Betrieb oder der wesentlichen Änderung des Lagers entgegenstehen."@de .

dtd:LA-id2675 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass bestimmte explosionsgefährliche Stoffe und Gegenstände oder Gruppen von ihnen in bestimmten Räumen ganz oder in begrenzten Mengen unter bestimmten Voraussetzungen ohne Genehmigung nach § 17 Abs. 1 gelagert werden dürfen, sofern dies nach Art, Ausmaß und Dauer der durch diese Lagerung hervorgerufenen Gefahren mit dem Schutz Beschäftigter oder Dritter vereinbar ist,"@de .

dtd:LA-id2676 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "welchen technischen Anforderungen die Bauteile oder Systeme eines Lagers im Sinne des § 17 Abs. 5 Satz 1 entsprechen müssen,"@de .

dtd:LA-id2677 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in welcher Weise das Verfahren der Bauartzulassung nach § 17 Abs. 4 durchzuführen ist, insbesondere, dass der Behörde die erforderlichen Zeichnungen und Beschreibungen über Bauart und Betriebsweise der Bauteile oder Systeme eines Lagers einzureichen und ihr Baumuster zu überlassen sind,"@de .

dtd:LA-id2678 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass die Bauteile oder Systeme nur verwendet werden dürfen, wenn nach näherer Bestimmung nachgewiesen ist, dass die Bauteile oder Systeme der Zulassung entsprechen, insbesondere wenn dem Verwender eine Bescheinigung des Herstellers, des Einführers oder eines Sachverständigen vorliegt."@de .

dtd:LA-id2679 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Erlaubnisinhaber oder der Inhaber eines Betriebes, der nach dem Gesetz oder einer auf Grund des § 4 erlassenen Rechtsverordnung ohne Erlaubnis den Umgang oder den Verkehr mit explosionsgefährlichen Stoffen betreiben darf, im Falle des § 8 Abs. 3 die mit der Gesamtleitung der genannten Tätigkeiten beauftragte Person,"@de .

dtd:LA-id2680 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die mit der Leitung des Betriebes, einer Zweigniederlassung oder einer unselbständigen Zweigstelle beauftragten Personen,"@de .

dtd:LA-id2681 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufsichtspersonen, insbesondere Leiter einer Betriebsabteilung, Sprengberechtigte, Betriebsmeister, fachtechnisches Aufsichtspersonal in der Kampfmittelbeseitigung und Lagerverwalter sowie Personen, die zum Verbringen explosionsgefährlicher Stoffe, zu deren Überlassen an andere oder zum Empfang dieser Stoffe von anderen bestellt sind,"@de .

dtd:LA-id2682 a dtd:LA ;
    dtd:has_DL dtd:DL-id2066 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Betrieben, die der Bergaufsicht unterliegen, neben den in den Nummern 1 und 2 bezeichneten Personena)die zur Beaufsichtigung aller Personen, die explosionsgefährliche Stoffe in Empfang nehmen, überlassen, aufbewahren, verbringen oder verwenden, bestellten Personen,b)die zum Überlassen von explosionsgefährlichen Stoffen an andere oder zum Empfang dieser Stoffe von anderen bestellten Personen,c)die innerhalb der Betriebsstätte die tatsächliche Gewalt über explosionsgefährliche Stoffe bei der Empfangnahme, dem Überlassen, dem Transport, dem Aufbewahren und dem Verwenden ausübenden Personen."@de .

dtd:LA-id2683 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zur Beaufsichtigung aller Personen, die explosionsgefährliche Stoffe in Empfang nehmen, überlassen, aufbewahren, verbringen oder verwenden, bestellten Personen,"@de .

dtd:LA-id2684 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zum Überlassen von explosionsgefährlichen Stoffen an andere oder zum Empfang dieser Stoffe von anderen bestellten Personen,"@de .

dtd:LA-id2685 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die innerhalb der Betriebsstätte die tatsächliche Gewalt über explosionsgefährliche Stoffe bei der Empfangnahme, dem Überlassen, dem Transport, dem Aufbewahren und dem Verwenden ausübenden Personen."@de .

dtd:LA-id2686 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dem vom Auftraggeber bezeichneten Empfänger, einer Person, die einen Befähigungsschein besitzt, oder einer verantwortlichen Person nach § 19 Abs. 1 Nr. 4 Buchstabe b,"@de .

dtd:LA-id2687 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den in § 1 Abs. 4 Nr. 1 bezeichneten Stellen,"@de .

dtd:LA-id2688 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "anderen Verbringern oder Lagerern, die in den Verbringensvorgang eingeschaltet sind."@de .

dtd:LA-id2689 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Reisegewerbe, soweit eine Reisegewerbekarte erforderlich wäre oder die Voraussetzungen des § 55a Abs. 1 Nr. 1 oder 3 der Gewerbeordnung vorliegen,"@de .

dtd:LA-id2690 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Veranstaltungen im Sinne des Titels IV der Gewerbeordnung mit Ausnahme der Entgegennahme von Bestellungen auf Messen und Ausstellungen."@de .

dtd:LA-id2691 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebsanlagen und Betriebseinrichtungen den Anforderungen des Absatzes 1 entsprechend einzurichten und zu unterhalten, insbesondere den erforderlichen Schutz- und Sicherheitsabstand der Betriebsanlagen untereinander und zu betriebsfremden Gebäuden, Anlagen und öffentlichen Verkehrswegen einzuhalten,"@de .

dtd:LA-id2692 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorsorge- und Überwachungsmaßnahmen im Betrieb zu treffen, insbesondere den Arbeitsablauf zu regeln,"@de .

dtd:LA-id2693 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beschäftigten oder Dritten im Betrieb ein den Anforderungen des Absatzes 1 entsprechendes Verhalten vorzuschreiben,"@de .

dtd:LA-id2694 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die erforderlichen Maßnahmen zu treffen, damit explosionsgefährliche Stoffe nicht abhanden kommen oder Beschäftigte oder Dritte diese Stoffe nicht unbefugt an sich nehmen,"@de .

dtd:LA-id2695 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beschäftigten vor Beginn der Beschäftigung über die Unfall- und Gesundheitsgefahren, denen sie bei der Beschäftigung ausgesetzt sind, sowie über die Einrichtungen und Maßnahmen zur Abwendung dieser Gefahren zu belehren; die Belehrungen sind in angemessenen Zeitabständen zu wiederholen."@de .

dtd:LA-id2696 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "welche Maßnahmen zur Erfüllung der sich aus § 24 ergebenden Pflichten zu treffen sind,"@de .

dtd:LA-id2697 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wie sich Beschäftigte und Dritte, soweit es der Arbeitsschutz erfordert, innerhalb oder außerhalb von Betrieben beim Umgang mit explosionsgefährlichen Stoffen oder mit Sprengzubehör zu verhalten haben,"@de .

dtd:LA-id2698 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass explosionsgefährliche Stoffe nur an der Herstellungsstätte oder an dem Ort, an dem sie innerhalb eines Betriebes verwendet werden, oder in besonderen Lagern aufbewahrt werden dürfen, und dass diese Lager insbesondere hinsichtlich des Standortes, der Bauweise, der Einrichtung und des Betriebes bestimmten Sicherheitsanforderungen genügen müssen,"@de .

dtd:LA-id2699 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nach welchen Sicherheitsvorschriften explosionsgefährliche Stoffe außerhalb eines Lagers aufbewahrt werden dürfen,"@de .

dtd:LA-id2700 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass explosionsgefährliche Stoffe bestimmten Lager- und Verträglichkeitsgruppen zuzuordnen sind und dass die Zuordnung der Bundesanstalt, für ausschließlich für militärische Zwecke bestimmte Stoffe der zuständigen Behörde der Bundeswehr übertragen wird,"@de .

dtd:LA-id2701 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass Anzeigen zu erstatten und ihnen bestimmte Unterlagen beizufügen sind."@de .


dtd:LA-id2762 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 15 Abs. 3 Satz 1 explosionsgefährliche Stoffe bei den zuständigen Behörden nicht anmeldet oder auf Verlangen nicht vorführt,"@de .

dtd:LA-id2763 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 15 Abs. 6 Satz 1 und 2 die Verbringungsgenehmigung nicht oder nicht rechtzeitig vorlegt,"@de .


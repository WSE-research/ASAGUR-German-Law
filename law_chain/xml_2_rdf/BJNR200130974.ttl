dtd:LA-id72 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Fläche A (gelb angelegt) 10 m unter der Erdoberfläche,"@de .

dtd:LA-id73 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Flächen B (braun angelegt) von der Erdoberfläche bzw. vom Rande der Flächen C bis zur Schnittlinie mit der Fläche E nach außen im Verhältnis 1 : 7 ansteigend,"@de .

dtd:LA-id74 a dtd:LA ;

    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihre Dauer angemessen ist und insbesondere Kenntnisse und Fertigkeiten vermittelt werden, die dem angestrebten Bildungsziel unter Berücksichtigung der voraussichtlichen Eingliederungserfordernisse und der beruflichen Entwicklungsmöglichkeiten entsprechen,"@de .

dtd:LA-id957 a dtd:LA ;
    dtd:has_Size dtd:normal ;

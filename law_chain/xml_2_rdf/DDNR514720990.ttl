dtd:LA-id573 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Erhaltung, Wiederherstellung und Förderung von Lebensstätten bedrohter Tier- und Pflanzenarten der stark kuppierten, wasserflächenreichen Moränenlandschaft,"@de .

dtd:LA-id574 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aus wissenschaftlichen und erdgeschichtlichen Gründen."@de .

dtd:LA-id575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Totalreservat Nr. 12a"@de .

dtd:LA-id576 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Es handelt sich um naturnahe Buchen- und Eichenbestände auf grund- und stauwasserfreien Moränenstandorten, die auch zahlreiche Sölle enthalten. Unter den Bedingungen eines kontinental beeinflußten Großklimas soll die Entwicklung der Laubwälder zu standortgerechten Waldökosystemen beobachtet werden."@de .

dtd:LA-id577 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Totalreservat Nr. 12b"@de .

dtd:LA-id578 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Es handelt sich um naturnahe Laubwaldbestände auf Moränenhügeln mit kalkhaltigen Hangquellmooren. Die Laubwaldbestände sollen unter den Bedingungen eines kontinental beeinflußten Großklimas hinsichtlich ihrer Entwicklung zu standortgerechten Waldökosystemen untersucht werden."@de .

dtd:LA-id579 a dtd:LA ;
    dtd:has_DL dtd:DL-id309 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "NSG Nr. 13 \"Eulenberge\"-zur Erhaltung und Wiederherstellung von Lebensstätten bedrohter Tier- und Pflanzenarten der kalkreichen Moränenlandschaft mit einem Randverschneidungsrelief zur Ückeraue, insbesondere durch Lebensgemeinschaften der Trockenrasen mit Wiesensteppenpflanzen charakterisiert."@de .

dtd:LA-id580 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Erhaltung und Wiederherstellung von Lebensstätten bedrohter Tier- und Pflanzenarten der kalkreichen Moränenlandschaft mit einem Randverschneidungsrelief zur Ückeraue, insbesondere durch Lebensgemeinschaften der Trockenrasen mit Wiesensteppenpflanzen charakterisiert."@de .

dtd:LA-id581 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Totalreservat Nr. 13"@de .

dtd:LA-id582 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Es handelt sich um die Kiefernbestände der Eulenberge auf Sandrendzinen einschließlich der darin befindlichen Bachläufe. Unter den vielfältigen meso- und großklimatischen Bedingungen soll die Entwicklung in Richtung standortgerechter Pflanzengesellschaften beobachtet werden."@de .


    dtd:has_Value "NSG Nr. 30 \"Fettseemoor\"-zur Erhaltung von Lebensstätten bedrohter Tier- und Pflanzenarten in einem funktionsfähigen mesotrophen Moorkomplex."@de .

dtd:LA-id632 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Erhaltung von Lebensstätten bedrohter Tier- und Pflanzenarten in einem funktionsfähigen mesotrophen Moorkomplex."@de .

dtd:LA-id633 a dtd:LA ;
    dtd:has_DL dtd:DL-id366 ;

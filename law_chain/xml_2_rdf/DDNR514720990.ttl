    dtd:has_Value "außerhalb öffentlicher Straßen und Wege und der besonders dafür gekennzeichneten Wege oder der Feuerschutzstreifen zu reiten,"@de .

dtd:LA-id653 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gewässer mit Fahrzeugen aller Art zu befahren; ausgenommen ist das Fahren mit nichtmotorbetriebenen Wasserfahrzeugen auf dem Werbellinsee, Wolletzsee, Parsteinsee, Oberückersee, Fährsee, Lübbesee und im bisherigen Umfang auf dem Grimnitzsee sowie das Fahren mit motorbetriebenen Wasserfahrzeugen auf dem Oder-Havel-Kanal und dem Finowkanal; das Fahren mit motorbetriebenen Fahrzeugen auf dem Oberückersee ist genehmigungspflichtig; auf dem Kölpinsee, Stiernsee, Lübelowsee, Düstersee, Sabinensee und dem Großen Briesensee ist das Befahren mit nichtmotorbetriebenen Wasserfahrzeugen gestattet,"@de .

dtd:LA-id654 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "außerhalb der gekennzeichneten Stellen zu baden,"@de .

dtd:LA-id655 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht heimische Tierarten in die Gewässer einzusetzen und Fische anzufüttern."@de .

dtd:LA-id656 a dtd:LA ;

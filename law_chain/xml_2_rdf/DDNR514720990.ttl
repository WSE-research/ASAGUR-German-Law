dtd:LA-id590 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "NSG Nr. 17 \"Großer Plötzsee\""@de .

dtd:LA-id591 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Es handelt sich um die Seenfläche des Großen Plötzsees mit einer 100 m breiten Schutzzone entlang des Ufers. Die westliche Grenze des Gebietes ist die östliche Seite des Bahndammes."@de .

dtd:LA-id592 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schutzziel des Großen Plötzsees ist die Erhaltung von Lebensräumen bedrohter Tier- und Pflanzenarten."@de .

dtd:LA-id593 a dtd:LA ;
    dtd:has_DL dtd:DL-id323 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "NSG Nr. 18 \"Fischteiche Blumberger Mühle\"-zur Erhaltung und Wiederherstellung von Lebensräumen bedrohter Tier- und Pflanzenarten, insbesondere auch als Nahrungs- und Rastgebiet bedrohter Wasservögel."@de .

dtd:LA-id594 a dtd:LA ;

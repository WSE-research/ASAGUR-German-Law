    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Betreten der Schutzgebiete durch Personen, die mit Überwachungsaufgaben oder wissenschaftlichen Untersuchungen durch die zuständige Behörde oder durch die Verwaltung des Biosphärenreservats beauftragt sind,"@de .

dtd:LA-id692 a dtd:LA ;
    dtd:has_Size dtd:normal ;

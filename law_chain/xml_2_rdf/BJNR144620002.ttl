    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sechstes Zusatzprotokoll zur Satzung des Weltpostvereins,"@de .

dtd:LA-id77 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Allgemeine Verfahrensordnung des Weltpostvereins,"@de .

dtd:LA-id78 a dtd:LA ;

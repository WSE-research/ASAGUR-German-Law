dtd:LA-id788 a dtd:LA ;
    dtd:has_Value "die von Kreditinstituten oder von Emittenten, deren Aktien bereits zum Handel an einem geregelten Markt zugelassen sind, ausgegeben werden, wenn der Gesamtgegenwert für alle im Europäischen Wirtschaftsraum angebotenen Wertpapiere nicht mehr als 8 Millionen Euro, berechnet über einen Zeitraum von zwölf Monaten, beträgt, oder"@de .

dtd:LA-id789 a dtd:LA ;
    dtd:has_Value "deren Gesamtgegenwert im Europäischen Wirtschaftsraum nicht mehr als 8 Millionen Euro, berechnet über einen Zeitraum von zwölf Monaten, beträgt."@de .

dtd:LA-id790 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Wertpapier-Informationsblatt vollständig alle Angaben, Hinweise und Anlagen enthält, die nach den folgenden Absätzen, auch in Verbindung mit der nach Absatz 9 zu erlassenden Rechtsverordnung, erforderlich sind, und diese Angaben, Hinweise und Anlagen in der vorgeschriebenen Reihenfolge erfolgen und"@de .

dtd:LA-id791 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Feststellungsdatum des letzten Jahresabschlusses des Emittenten und im Falle eines Garantiegebers zusätzlich das Feststellungsdatum des letzten Jahresabschlusses des Garantiegebers zum Zeitpunkt der Gestattung nicht länger als 18 Monate zurückliegt."@de .

dtd:LA-id792 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Art, die genaue Bezeichnung und die internationale Wertpapier-Identifikationsnummer (ISIN) des Wertpapiers,"@de .

dtd:LA-id793 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Funktionsweise des Wertpapiers einschließlich der mit dem Wertpapier verbundenen Rechte,"@de .

dtd:LA-id794 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben zur Identität des Anbieters, des Emittenten einschließlich seiner Geschäftstätigkeit und eines etwaigen Garantiegebers,"@de .

dtd:LA-id795 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die mit dem Wertpapier, dem Emittenten und einem etwaigen Garantiegeber verbundenen Risiken,"@de .

dtd:LA-id796 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den auf Grundlage des letzten aufgestellten Jahresabschlusses berechneten Verschuldungsgrad des Emittenten und eines etwaigen Garantiegebers,"@de .

dtd:LA-id797 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Aussichten für die Kapitalrückzahlung und Erträge unter verschiedenen Marktbedingungen,"@de .

dtd:LA-id798 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die mit dem Wertpapier verbundenen Kosten und Provisionen,"@de .

dtd:LA-id799 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Angebotskonditionen einschließlich des Emissionsvolumens sowie"@de .


    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 4 Absatz 1 kein Wertpapier-Informationsblatt veröffentlicht wurde,"@de .

dtd:LA-id831 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 4 Absatz 2 ein Wertpapier-Informationsblatt veröffentlicht wird oder"@de .


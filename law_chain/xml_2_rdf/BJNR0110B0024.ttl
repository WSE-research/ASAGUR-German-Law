dtd:LA-id1581 a dtd:LA ;
    dtd:has_Value "die Gleichstellung von Soldatinnen und Soldaten der Bundeswehr zu verwirklichen,"@de .

dtd:LA-id1582 a dtd:LA ;
    dtd:has_Value "bestehende Benachteiligungen auf Grund des Geschlechts zu beseitigen und künftige Benachteiligungen zu verhindern,"@de .

dtd:LA-id1583 a dtd:LA ;
    dtd:has_Value "bestehende Unterrepräsentanzen abzubauen und"@de .

dtd:LA-id1584 a dtd:LA ;
    dtd:has_Value "die Vereinbarkeit von Familie, Pflege und Dienst für das militärische Personal zu verbessern."@de .

dtd:LA-id1585 a dtd:LA ;
    dtd:has_Value "bei Verwendungen nach § 56 Absatz 1 des Bundesbesoldungsgesetzes und"@de .

dtd:LA-id1586 a dtd:LA ;
    dtd:has_DL dtd:DL-id1095 ;
    dtd:has_Value "bei der Mitwirkung an der Erfüllung a)einer einsatzgleichen Verpflichtung der Bundesrepublik Deutschland oderb)einer Dauereinsatzaufgabe der Bundesrepublik Deutschland."@de .

dtd:LA-id1587 a dtd:LA ;
    dtd:has_Value "einer einsatzgleichen Verpflichtung der Bundesrepublik Deutschland oder"@de .

dtd:LA-id1588 a dtd:LA ;
    dtd:has_Value "einer Dauereinsatzaufgabe der Bundesrepublik Deutschland."@de .

dtd:LA-id1589 a dtd:LA ;
    dtd:has_Value "Eine unmittelbare Benachteiligung wegen des Geschlechts liegt vor, wenn eine Person wegen des Geschlechts eine weniger günstige Behandlung erfährt als eine Person eines anderen Geschlechts in einer vergleichbaren Situation erfährt, erfahren hat oder erfahren würde."@de .

dtd:LA-id1590 a dtd:LA ;
    dtd:has_Value "Eine mittelbare Benachteiligung liegt vor, wenn dem Anschein nach neutrale Vorschriften, Kriterien oder Verfahren Personen eines Geschlechts in besonderer Weise gegenüber Personen eines anderen Geschlechts benachteiligen können. Eine mittelbare Benachteiligung liegt nicht vor, wenn die betreffenden Vorschriften, Kriterien oder Verfahren durch ein rechtmäßiges Ziel sachlich gerechtfertigt und die Mittel zur Erreichung dieses Ziels angemessen und erforderlich sind."@de .

dtd:LA-id1591 a dtd:LA ;
    dtd:has_Value "der Statusgruppe der Berufssoldatinnen und Berufssoldaten,"@de .

dtd:LA-id1592 a dtd:LA ;
    dtd:has_Value "der Statusgruppe der Soldatinnen auf Zeit und Soldaten auf Zeit,"@de .

dtd:LA-id1593 a dtd:LA ;
    dtd:has_Value "der Statusgruppe der freiwilligen Wehrdienst Leistenden,"@de .

dtd:LA-id1594 a dtd:LA ;
    dtd:has_Value "der jeweiligen Besoldungs- oder Wehrsoldgruppe oder der jeweiligen Laufbahn mit Ausnahme der Laufbahnen des Sanitätsdienstes."@de .

dtd:LA-id1595 a dtd:LA ;
    dtd:has_Value "für die zivilen Vorgesetzten, denen militärisches Personal untersteht, und"@de .

dtd:LA-id1596 a dtd:LA ;
    dtd:has_Value "für die Personen, die mit der Personalbearbeitung des militärischen Personals beauftragt sind."@de .

dtd:LA-id1597 a dtd:LA ;
    dtd:has_Value "über die Maßnahmen der Dienststelle zur Gleichstellung von Soldatinnen und Soldaten sowie"@de .

dtd:LA-id1598 a dtd:LA ;
    dtd:has_Value "über die Maßnahmen zu einer besseren Vereinbarkeit von Familie, Pflege und Dienst."@de .

dtd:LA-id1599 a dtd:LA ;
    dtd:has_Value "ausreichend Bewerbungen von Frauen vorliegen und"@de .

dtd:LA-id1600 a dtd:LA ;
    dtd:has_Value "die Bewerberinnen das erforderliche Anforderungs- und Qualifikationsprofil aufweisen."@de .

dtd:LA-id1601 a dtd:LA ;
    dtd:has_Value "Berufungen in das Dienstverhältnis,"@de .

dtd:LA-id1602 a dtd:LA ;
    dtd:has_Value "Umwandlungen des Dienstverhältnisses,"@de .

dtd:LA-id1603 a dtd:LA ;
    dtd:has_Value "Beförderungen,"@de .

dtd:LA-id1604 a dtd:LA ;
    dtd:has_Value "Wechsel der Laufbahn und"@de .

dtd:LA-id1605 a dtd:LA ;
    dtd:has_Value "förderliche Verwendungsentscheidungen."@de .

dtd:LA-id1606 a dtd:LA ;
    dtd:has_Value "dem Familienstand,"@de .

dtd:LA-id1607 a dtd:LA ;
    dtd:has_Value "einer bestehenden oder geplanten Schwangerschaft und"@de .

dtd:LA-id1608 a dtd:LA ;
    dtd:has_Value "den bestehenden oder geplanten Familien- oder Pflegeaufgaben."@de .

dtd:LA-id1609 a dtd:LA ;
    dtd:has_Value "die Beschäftigung in Teilzeit oder die Absicht, eine Beschäftigung in Teilzeit auszuüben,"@de .


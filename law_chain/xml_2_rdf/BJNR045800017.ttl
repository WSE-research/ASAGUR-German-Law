dtd:LA-id2383 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Lern- und Arbeitstechniken sowie Methoden des selbstgesteuerten Lernens anwenden und beruflich relevante Informationsquellen nutzen"@de .

dtd:LA-id2384 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "berufliche Aufstiegs- und Weiterentwicklungsmöglichkeiten darstellen"@de .

dtd:LA-id2385 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Funktion des Einzelhandels für die Gesamtwirtschaft und die Gesellschaft erläutern"@de .


    dtd:has_Value "Ursachen von Konflikten und Kommunikationsstörungen erkennen und zu deren Lösung beitragen"@de .

dtd:LA-id2392 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Informations- und Kommunikationssysteme einsetzen"@de .

dtd:LA-id2393 a dtd:LA ;

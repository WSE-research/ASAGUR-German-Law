dtd:LA-id286 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in bestimmter Weise über Produkte zu verfügen,"@de .

dtd:LA-id287 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in bestimmter Weise Produkte zu gewinnen, herzustellen, zu verlagern, in Rohrleitungen weiterzuleiten, zu bearbeiten, zu verarbeiten, sonst innerbetrieblich einzusetzen oder auf sie tatsächlich einzuwirken oder"@de .

dtd:LA-id288 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verfügungen und Handlungen im Sinne der Nummern 1 und 2 zu unterlassen."@de .

dtd:LA-id289 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "um die für Zwecke der Verteidigung, insbesondere zur Deckung des Bedarfs der Zivilbevölkerung, der Bundeswehr und der verbündeten Streitkräfte erforderliche Versorgung mit Produkten sicherzustellen und"@de .

dtd:LA-id290 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn eine Gefährdung der Versorgung durch marktgerechte Maßnahmen nicht, nicht rechtzeitig oder nur mit unverhältnismäßigen Mitteln zu beheben oder zu verhindern ist."@de .

dtd:LA-id291 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Verpflichtung nach § 1 Abs. 1 vorliegt,"@de .

dtd:LA-id292 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine allgemeine Genehmigung nach § 5 Abs. 1 oder 3 erlassen ist,"@de .

dtd:LA-id293 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Einzelgenehmigung nach § 5 Abs. 2 erteilt wurde oder"@de .

dtd:LA-id294 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Lieferung gegen Bezugschein nach § 6 erfolgt."@de .

dtd:LA-id295 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur gewerblichen Bearbeitung oder Verarbeitung oder zur Herstellung von anderen Produkten oder Waren bestimmt sind,"@de .

dtd:LA-id296 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an Unternehmer zum Zwecke der gewerblichen Weiterveräußerung abgegeben werden oder"@de .

dtd:LA-id297 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an öffentliche Auftraggeber und die verbündeten Streitkräfte auf Grund von Verträgen zu liefern sind, die zum Zeitpunkt der Anwendbarkeit der Rechtsverordnung nach § 4 Abs. 1 bestehen, und in denen die zu erbringende Leistung nach Art, Umfang und Zeit bestimmt ist."@de .

dtd:LA-id298 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über die Abgabe eine Liste führt, die das Datum der Abgabe, das amtliche Kennzeichen oder das Versicherungskennzeichen des betankten Kraftfahrzeugs, Art und Menge der Abgabe sowie die Unterschrift des Empfängers enthält und"@de .

dtd:LA-id299 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die abgegebene Menge mit dem Namensstempel der Tankstelle in ein zu bestimmendes Kontrollpapier einträgt."@de .


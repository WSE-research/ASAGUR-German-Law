dtd:LA-id436 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflegekinder (Personen, mit denen der Berechtigte durch ein familienähnliches, auf Dauer berechnetes Band verbunden ist, sofern er sie nicht zu Erwerbszwecken in seinen Haushalt aufgenommen hat und das Obhuts- und Pflegeverhältnis zu den Eltern nicht mehr besteht),"@de .

dtd:LA-id437 a dtd:LA ;
    dtd:has_Size dtd:normal ;

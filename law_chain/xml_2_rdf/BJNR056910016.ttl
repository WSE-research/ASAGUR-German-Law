    dtd:has_Size dtd:normal ;
    dtd:has_Value "Außenwerbung: jede Werbung außerhalb geschlossener Räume einschließlich Schaufensterwerbung,"@de .

dtd:LA-id1256 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zollbehörden: die Hauptzollämter und Zollfahndungsämter."@de .

dtd:LA-id1257 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Teer: 10 Milligramm je Zigarette,"@de .

dtd:LA-id1258 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nikotin: 1,0 Milligramm je Zigarette,"@de .

dtd:LA-id1259 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kohlenmonoxid: 10 Milligramm je Zigarette."@de .

dtd:LA-id1260 a dtd:LA ;
    dtd:has_DL dtd:DL-id704 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zigaretten, Tabak zum Selbstdrehen und erhitzte Tabakerzeugnisse, die a)ein charakteristisches Aroma haben oderb)Aromastoffe in ihren Bestandteilen enthalten oder sonstige technische Merkmale aufweisen, mit denen sich der Geruch oder Geschmack oder die Rauchintensität verändern lassen;"@de .

dtd:LA-id1261 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein charakteristisches Aroma haben oder"@de .


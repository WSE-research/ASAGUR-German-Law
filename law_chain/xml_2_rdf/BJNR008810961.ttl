dtd:LA-id8014 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Aufsichtsbehörden gemäß § 9 Absatz 1 Satz 5 Nummer 2, soweit dies zur Erfüllung ihrer aufsichtlichen Aufgaben unter den Voraussetzungen des Absatzes 2 erforderlich ist,"@de .

dtd:LA-id8015 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den für die Leistung der internationalen Rechtshilfe in Strafsachen sowie im Übrigen für die Verfolgung und Ahndung von Straftaten zuständigen Behörden oder Gerichten, soweit dies für die Erfüllung ihrer gesetzlichen Aufgaben erforderlich ist,"@de .

dtd:LA-id8016 a dtd:LA ;

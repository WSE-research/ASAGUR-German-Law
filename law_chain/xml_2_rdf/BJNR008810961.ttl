dtd:LA-id8021 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an das Bundeskriminalamt in seiner Funktion als nationale Stelle nach § 1 Nummer 1 des Europol-Gesetzes zum Zwecke der Weitergabe an Europol, soweit dies zur Erfüllung der Aufgaben von Europol gemäß Artikel 4 der Verordnung (EU) 2016/794 im Rahmen der Zuständigkeit von Europol im Einzelfall erforderlich ist."@de .

dtd:LA-id8022 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Aktenzeichen,"@de .


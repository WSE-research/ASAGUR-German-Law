    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Verfahren zur Vermeidung der Umgehung der zusätzlichen Kapitalanforderungen bei Überschreitung der Gesamtbuch-Großkreditanforderungen,"@de .

dtd:LA-id7607 a dtd:LA ;

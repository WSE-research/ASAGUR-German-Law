dtd:LA-id7647 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ob die Eigenmittelausstattung der Gruppe auf zusammengefasster Basis ihrer Finanzlage und ihrem Risikoprofil angemessen ist,"@de .


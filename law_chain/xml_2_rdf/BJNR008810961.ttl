dtd:LA-id8444 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "um den Aufbau eines zusätzlichen Finanzmittelpuffers für Perioden wirtschaftlichen Abschwungs sicherzustellen,"@de .


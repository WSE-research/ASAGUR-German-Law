dtd:LA-id4818 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "M3: für Messgeräte, die an Einsatzorten verwendet werden, an denen starke bis sehr starke Schwingungen und Erschütterungen auftreten können, zum Beispiel bei Messgeräten, die direkt an Maschinen, Förderbändern oder ähnlichen Einrichtungen angebracht sind."@de .

dtd:LA-id4819 a dtd:LA ;
    dtd:has_DL dtd:DL-id3676 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "In Bezug auf die mechanischen Umgebungsbedingungen hat der Hersteller folgende Einflussgrößen zu berücksichtigen: a)Schwingungen,b)Erschütterungen."@de .


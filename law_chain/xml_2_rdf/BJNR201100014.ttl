    dtd:has_Value ""@de .

dtd:LA-id5260 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value ""@de .

dtd:LA-id5261 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Das Zusatzzeichen zur Bezeichnung des Endes der Eichfrist hat eine der folgenden Formen."@de .

dtd:LA-id5262 a dtd:LA ;
    dtd:has_BR dtd:BR-id1021,
        dtd:BR-id1022 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beispiel: "@de ;
    dtd:has_table dtd:table-id1392 .

dtd:LA-id5263 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value ""@de .

dtd:LA-id5264 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Das Sicherungszeichen besteht aus dem ersten Teil des Eichkennzeichens nach Nummer 1.1; die Hintergrundfarbe ist orange, entsprechend der nachfolgenden Darstellung."@de .

dtd:LA-id5265 a dtd:LA ;
    dtd:has_BR dtd:BR-id1023,
        dtd:BR-id1024 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beispiel: "@de ;
    dtd:has_table dtd:table-id1394 .

dtd:LA-id5266 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Das Entwertungszeichen besteht aus zwei sich tangierenden Halbkreisen in nachstehender Ausführung."@de .


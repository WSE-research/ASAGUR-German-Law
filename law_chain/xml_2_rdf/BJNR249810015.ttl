    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anforderungen an Anteile von Wärme aus erneuerbaren Energien an der erzeugten oder genutzten Wärme und an die Verwendung der in dem innovativen KWK-System erzeugten Wärme,"@de .

dtd:LA-id2375 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anforderungen an die Energieeffizienz, insbesondere an den Brennstoffausnutzungsgrad,"@de .

dtd:LA-id2376 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anforderungen an einen Mindestanteil KWK-Wärme an der erzeugten oder genutzten Wärme,"@de .

dtd:LA-id2377 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anforderungen an die Flexibilität der innovativen KWK-Systeme und der KWK-Anlagen innerhalb innovativer KWK-Systeme, insbesondere Anforderungen, dass KWK-Anlagen innerhalb innovativer KWK-Systeme keine technische Mindesterzeugung aufweisen und die Wärme, die aus dem KWK-Prozess maximal ausgekoppelt werden kann, jederzeit mit einem mit dieser KWK-Anlage verbundenen elektrischen Wärmeerzeuger erzeugt werden kann,"@de .

dtd:LA-id2378 a dtd:LA ;

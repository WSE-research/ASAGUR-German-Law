    dtd:has_Value "die Fähigkeit zur Kommunikation und Teamarbeit,"@de .

dtd:LA-id866 a dtd:LA ;
    dtd:has_Value "die Fähigkeit zum kritischen Überprüfen des eigenen Handelns,"@de .

dtd:LA-id867 a dtd:LA ;
    dtd:has_Value "die Fähigkeit zum selbständigen Handeln,"@de .

dtd:LA-id868 a dtd:LA ;
    dtd:has_Value "die Fähigkeit zum wirtschaftlichen Handeln und"@de .

dtd:LA-id869 a dtd:LA ;
    dtd:has_Value "soziale Kompetenzen."@de .

dtd:LA-id870 a dtd:LA ;
    dtd:has_Value "das Bundesarchiv,"@de .

dtd:LA-id871 a dtd:LA ;
    dtd:has_Value "die oder der Bundesbeauftragte für die Unterlagen des Staatssicherheitsdienstes der ehemaligen Deutschen Demokratischen Republik (Bundesbeauftragter) und"@de .

dtd:LA-id872 a dtd:LA ;
    dtd:has_Value "die Stiftung Preußischer Kulturbesitz."@de .

dtd:LA-id873 a dtd:LA ;
    dtd:has_Value "das Bundesarchiv,"@de .


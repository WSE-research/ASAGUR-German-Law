dtd:LA-id103 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Antrag auf Eröffnung des Insolvenzverfahrens nach § 305 der Insolvenzordnung mit Antrag auf Erteilung der Restschuldbefreiung nach § 287 Abs. 1 der Insolvenzordnung,"@de .

dtd:LA-id104 a dtd:LA ;
    dtd:has_DL dtd:DL-id83 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anlagen zum Antrag auf Eröffnung des Insolvenzverfahrens:a)Personalbogen mit Angaben zur Person des Schuldners,b)Bescheinigung über das Scheitern des außergerichtlichen Einigungsversuchs nach § 305 Abs. 1 Nr. 1 der Insolvenzordnung,c)Abtretungserklärung nach § 287 Abs. 2 der Insolvenzordnung,d)Zusammenfassung des wesentlichen Inhalts des Vermögensverzeichnisses nach § 305 Abs. 1 Nr. 3 der Insolvenzordnung (Vermögensübersicht),e)Verzeichnis des vorhandenen Vermögens und des Einkommens nach § 305 Abs. 1 Nr. 3 der Insolvenzordnung mit Ergänzungsblättern (Vermögensverzeichnis),f)Verzeichnis der Gläubiger und Verzeichnis der gegen den Schuldner gerichteten Forderungen nach § 305 Abs. 1 Nr. 3 der Insolvenzordnung (Gläubiger- und Forderungsverzeichnis),g)Schuldenbereinigungsplan nach § 305 Abs. 1 Nr. 4 der Insolvenzordnung."@de .

dtd:LA-id105 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personalbogen mit Angaben zur Person des Schuldners,"@de .

dtd:LA-id106 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bescheinigung über das Scheitern des außergerichtlichen Einigungsversuchs nach § 305 Abs. 1 Nr. 1 der Insolvenzordnung,"@de .

dtd:LA-id107 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Abtretungserklärung nach § 287 Abs. 2 der Insolvenzordnung,"@de .

dtd:LA-id108 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zusammenfassung des wesentlichen Inhalts des Vermögensverzeichnisses nach § 305 Abs. 1 Nr. 3 der Insolvenzordnung (Vermögensübersicht),"@de .

dtd:LA-id109 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verzeichnis des vorhandenen Vermögens und des Einkommens nach § 305 Abs. 1 Nr. 3 der Insolvenzordnung mit Ergänzungsblättern (Vermögensverzeichnis),"@de .

dtd:LA-id110 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verzeichnis der Gläubiger und Verzeichnis der gegen den Schuldner gerichteten Forderungen nach § 305 Abs. 1 Nr. 3 der Insolvenzordnung (Gläubiger- und Forderungsverzeichnis),"@de .

dtd:LA-id111 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schuldenbereinigungsplan nach § 305 Abs. 1 Nr. 4 der Insolvenzordnung."@de .

dtd:LA-id112 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berichtigungen, die auf einer Änderung von Rechtsvorschriften beruhen;"@de .


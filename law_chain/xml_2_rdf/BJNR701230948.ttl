    dtd:has_Size dtd:normal ;
    dtd:has_Value "Mittelstand, freie Berufe und Existenzgründungen,"@de .

dtd:LA-id268 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Risikokapital,"@de .


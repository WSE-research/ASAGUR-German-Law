dtd:LA-id352 a dtd:LA ;
    dtd:has_B dtd:B-id136 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Schutzzone I die natürliche Entwicklung der Lebensräume zu gewährleisten,"@de .

dtd:LA-id353 a dtd:LA ;
    dtd:has_B dtd:B-id137 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Schutzzone II die gebietstypische Vielfalt an Lebensgemeinschaften und Pflanzen- und Tierarten durch geeignete Pflege- und Nutzungsmaßnahmen zu erhalten und zu entwickeln,"@de .

dtd:LA-id354 a dtd:LA ;
    dtd:has_B dtd:B-id138 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Schutzzone III den natur- und nutzungsbedingten Landschaftscharakter zu erhalten und im Sinne des Schutzzwecks (§ 3) zu gestalten."@de .

dtd:LA-id355 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bestandsregulierungen von Tierarten entsprechend den Zielsetzungen für das Biosphärenreservat in den Schutzzonen I und II nach Maßgabe und in der Schutzzone III im Einvernehmen mit der Reservatsverwaltung vorzunehmen,"@de .

dtd:LA-id356 a dtd:LA ;
    dtd:has_B dtd:B-id139 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in die Schutzzone II den Bau jagdlicher Anlagen dem Schutzzweck (§ 3) unterzuordnen und mit natürlichen Materialien in landschaftsangepaßter Bauweise vorzunehmen,"@de .

dtd:LA-id357 a dtd:LA ;
    dtd:has_B dtd:B-id140 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Schutzzone II Tanne und andere Mischbaumarten in den Buchen- und Fichtenbeständen zu fördern,"@de .

dtd:LA-id358 a dtd:LA ;
    dtd:has_B dtd:B-id141 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Schutzzonen II und III naturnahe Waldbestände durch geeignete waldbauliche Maßnahmen zu entwickeln,"@de .

dtd:LA-id359 a dtd:LA ;
    dtd:has_B dtd:B-id142 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Schutzzonen II und III Biotopenschutz entsprechend § 20c des Bundesnaturschutzgesetzes, erweitert auf Höhlenbäume, Horstbäume von Greifvögeln sowie 100 m - Bereiche um Vermehrungs- und Fortpflanzungsstätten der vom Aussterben bedrohten Großvogelarten durchzuführen,"@de .

dtd:LA-id360 a dtd:LA ;
    dtd:has_B dtd:B-id143 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Schutzzone II durch zweischürige Mahd in Verbindung mit teilweiser extensiver Beweidung das Grünland zu erhalten und kleinflächig die historisch bekannte Berieselung der Weiden wieder einzuführen,"@de .

dtd:LA-id361 a dtd:LA ;
    dtd:has_B dtd:B-id144 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Schutzzone III den ökologischen Landbau schrittweise einzuführen,"@de .

dtd:LA-id362 a dtd:LA ;
    dtd:has_B dtd:B-id145 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Schutzzonen II und III die Fließgewässer zu pflegen,"@de .

dtd:LA-id363 a dtd:LA ;
    dtd:has_B dtd:B-id146 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Schutzzonen II und III innerhalb von zwei Jahren einen Pflege- und Entwicklungsplan zu erstellen,"@de .

dtd:LA-id364 a dtd:LA ;
    dtd:has_B dtd:B-id147 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Schutzzone III zur Förderung des sanften Tourismus in dem Pflege- und Entwicklungsplan geeignete Maßnahmen vorzusehen,"@de .

dtd:LA-id365 a dtd:LA ;
    dtd:has_B dtd:B-id148 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Schutzzone II durch die Bewirtschafter alle Voraussetzungen zu schaffen, daß die mit der Reservatsverwaltung abgestimmten Freilandforschungen und die Nutzung des Biosphärenreservats für Studien- und Demonstrationszwecke gewährleistet ist."@de .

dtd:LA-id366 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grünlandflächen umzubrechen, aufzuforsten oder anderweitig zweckentfremdet zu nutzen,"@de .

dtd:LA-id367 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fließgewässer auszubauen oder zu verunreinigen,"@de .

dtd:LA-id368 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bauliche Anlagen außerhalb im Zusammenhang bebauter Ortsteile oder des Geltungsbereiches rechtskräftiger Bebauungspläne zu errichten oder zu erweitern,"@de .

dtd:LA-id369 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Neuaufschlüsse für Gesteinsabbau anzulegen,"@de .

dtd:LA-id370 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Landschaftscharakter zu verändern,"@de .

dtd:LA-id371 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kahlschläge über 3 ha anzulegen."@de .

dtd:LA-id372 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wege zu verlassen,"@de .

dtd:LA-id373 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflanzen oder Teile von ihnen zu entnehmen oder zu beschädigen,"@de .

dtd:LA-id374 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht jagdbare Tiere und ihre Entwicklungsstadien zu fangen oder zu töten,"@de .

dtd:LA-id375 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Biozide, mineralische Dünger und Gülle anzuwenden und mit Luftfahrzeugen über einen angrenzenden 100 m breiten Streifen um die Schutzzone Agrochemikalien auszubringen,"@de .

dtd:LA-id376 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "außerhalb fester Gebäude zu nächtigen oder zu zelten,"@de .

dtd:LA-id377 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wildfütterungen und Wildäcker anzulegen,"@de .

dtd:LA-id378 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht heimische Tier- und Pflanzenarten und -rassen auszubringen,"@de .

dtd:LA-id379 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hydromeliorative Maßnahmen durchzuführen,"@de .

dtd:LA-id380 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Ruhe der Natur oder den Naturgenuß durch Lärmen oder auf andere Weise zu stören,"@de .

dtd:LA-id381 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit Luftfahrzeugen, Hanggleitern und Gleitschirmen zu starten oder zu landen oder Modellflugzeuge zu betreiben,"@de .

dtd:LA-id382 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Gebiet zu verunreinigen,"@de .

dtd:LA-id383 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Schilder, die nicht den Zielen des § 3 entsprechen, aufzustellen oder anzubringen,"@de .

dtd:LA-id384 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "organisierte Veranstaltungen aller Art durchzuführen,"@de .

dtd:LA-id385 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kahlschläge anzulegen, sofern sie nicht dem Schutzzweck dienen."@de .

dtd:LA-id386 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "jegliche wirtschaftlichen Maßnahmen durchzuführen,"@de .

dtd:LA-id387 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Gebiet in irgendeiner Weise zu beeinträchtigen."@de .

dtd:LA-id388 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "unaufschiebbare Maßnahmen zum Schutz der Bevölkerung und zur Abwehr von Gefahren für Leib und Leben von Menschen und für erhebliche Sachwerte,"@de .

dtd:LA-id389 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen der Reservatsverwaltung, die ausschließlich dem Zweck des § 3 dienen,"@de .

dtd:LA-id390 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Befahren der gesperrten Straßen und Wege mit Kraftfahrzeugen durch Angehörige von staatlichen Verwaltungen oder deren Beauftragte bei zwingend notwendigen Dienstfahrten"@de .

dtd:LA-id391 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sowie durch Sonstige mit Genehmigung der Reservatsverwaltung,"@de .

dtd:LA-id392 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die im Sinne des Bundesnaturschutzgesetzes (§ 8 Abs. 7) ordnungsgemäße landwirtschaftliche Bodennutzung der bisher landwirtschaftlich genutzten Flächen,"@de .

dtd:LA-id393 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die bisherige bestimmungsgemäße Nutzung von baulichen Anlagen einschließlich der dazugehörigen Flächen,"@de .

dtd:LA-id394 a dtd:LA ;
    dtd:has_B dtd:B-id152 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in der Schutzzone III Melkstände, Viehtränken, Viehunterstände, ortsübliche Weidezäune, forstliche Kulturzäune, Wildfutterstellen und Jagdsitze sowie baugenehmigungsfreie Vorhaben im Haus- und Hofbereich,"@de .


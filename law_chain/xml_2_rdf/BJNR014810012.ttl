dtd:LA-id2270 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 26 ein Lebensmittel, ein Futtermittel, Saatgut, Pflanzgut oder ein Kultursubstrat, nicht getrennt hält,"@de .


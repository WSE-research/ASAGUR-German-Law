    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflanzen, Pflanzenerzeugnisse oder sonstige Gegenstände, die Träger bestimmter Schadorganismen sind oder sein können;"@de .

dtd:LA-id1945 a dtd:LA ;
    dtd:has_BR dtd:BR-id760,
        dtd:BR-id761 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Einschleppung:"@de .


dtd:LA-id1926 a dtd:LA ;
    dtd:has_Value "Pflanzen, insbesondere Kulturpflanzen, vor Schadorganismen und nichtparasitären Beeinträchtigungen zu schützen,"@de .

dtd:LA-id1927 a dtd:LA ;
    dtd:has_Value "Pflanzenerzeugnisse vor Schadorganismen zu schützen,"@de .

dtd:LA-id1928 a dtd:LA ;
    dtd:has_Value "Gefahren, die durch die Anwendung von Pflanzenschutzmitteln oder durch andere Maßnahmen des Pflanzenschutzes, insbesondere für die Gesundheit von Mensch und Tier und für den Naturhaushalt, entstehen können, abzuwenden oder ihnen vorzubeugen,"@de .

dtd:LA-id1929 a dtd:LA ;
    dtd:has_Value "Rechtsakte der Europäischen Gemeinschaft oder der Europäischen Union im Anwendungsbereich dieses Gesetzes durchzuführen."@de .

dtd:LA-id1930 a dtd:LA ;
    dtd:has_DL dtd:DL-id1141 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflanzenschutz:a)der Schutz von Pflanzen vor Schadorganismen und nichtparasitären Beeinträchtigungen,b)der Schutz der Pflanzenerzeugnisse vor Schadorganismen (Vorratsschutz)einschließlich der Verwendung und des Schutzes von Tieren, Pflanzen und Mikroorganismen, durch die Schadorganismen bekämpft werden können;"@de .

dtd:LA-id1931 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Schutz von Pflanzen vor Schadorganismen und nichtparasitären Beeinträchtigungen,"@de .

dtd:LA-id1932 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Schutz der Pflanzenerzeugnisse vor Schadorganismen (Vorratsschutz)"@de .

dtd:LA-id1933 a dtd:LA ;
    dtd:has_BR dtd:BR-id748,
        dtd:BR-id749 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "integrierter Pflanzenschutz:"@de .

dtd:LA-id1934 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Kombination von Verfahren, bei denen unter vorrangiger Berücksichtigung biologischer, biotechnischer, pflanzenzüchterischer sowie anbau- und kulturtechnischer Maßnahmen die Anwendung chemischer Pflanzenschutzmittel auf das notwendige Maß beschränkt wird;"@de .

dtd:LA-id1935 a dtd:LA ;
    dtd:has_BR dtd:BR-id750,
        dtd:BR-id751 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflanzen:"@de .

dtd:LA-id1936 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "lebende Pflanzen und lebende Teile von Pflanzen einschließlich der Früchte und Samen;"@de .

dtd:LA-id1937 a dtd:LA ;
    dtd:has_BR dtd:BR-id752,
        dtd:BR-id753 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflanzenerzeugnisse:"@de .

dtd:LA-id1938 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Erzeugnisse pflanzlichen Ursprungs, die nicht oder nur durch einfache Verfahren, wie Trocknen oder Zerkleinern, be- oder verarbeitet worden sind, ausgenommen verarbeitetes Holz;"@de .

dtd:LA-id1939 a dtd:LA ;
    dtd:has_BR dtd:BR-id754,
        dtd:BR-id755 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Pflanzenarten:"@de .


    dtd:has_BR dtd:BR-id756,
        dtd:BR-id757 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Naturhaushalt:"@de .

dtd:LA-id1942 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "seine Bestandteile Boden, Wasser, Luft, Tier- und Pflanzenarten sowie das Wirkungsgefüge zwischen ihnen;"@de .


    dtd:has_Value "Zusammenarbeit mit anderen Berufsgruppen sowie mit Einrichtungen und Diensten des Sozial- und Gesundheitswesens,"@de .

dtd:LA-id231 a dtd:LA ;
    dtd:has_Size dtd:normal ;

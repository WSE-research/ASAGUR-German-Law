dtd:LA-id272 a dtd:LA ;
    dtd:has_Value "Arbeitspläne erstellen, vorgegebene Arbeitspläne, Skizzen und Zeichnungen bewerten und korrigieren; dabei auch Informations- und Kommunikationssysteme anwenden,"@de .

dtd:LA-id273 a dtd:LA ;

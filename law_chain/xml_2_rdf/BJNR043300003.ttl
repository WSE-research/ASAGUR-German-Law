dtd:LA-id715 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in privaten und öffentlichen Unternehmen unterschiedlicher Größe und Branchenzugehörigkeit sowie in verschiedenen Bereichen und Tätigkeitsfeldern eines Unternehmens Sach-, Organisations- und Führungsaufgaben wahrzunehmen und"@de .

dtd:LA-id716 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich auf veränderte Methoden und Systeme, auf sich verändernde Strukturen der Arbeitsorganisation und auf neue Methoden der Organisationsentwicklung, der Personalführung und -entwicklung flexibel einzustellen sowie den technisch-organisatorischen Wandel im Unternehmen mitzugestalten."@de .

dtd:LA-id717 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Planen und Organisieren von Sicherheits- und Ordnungsaufgaben unter Berücksichtigung technischer, wirtschaftlicher, rechtlicher, personeller und sozialer Rahmenbedingungen;"@de .

dtd:LA-id718 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Durchführen von Sicherheitsanalysen und Entwickeln von Sicherheitskonzepten; Übertragen von Sicherheits- und Ordnungsaufgaben auf die Mitarbeiter und Mitarbeiterinnen unter Berücksichtigung ihrer Leistungsfähigkeit, Qualifikation und Eignung;"@de .

dtd:LA-id719 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Führen und Fördern von Mitarbeitern und Mitarbeiterinnen; dazu gehören ihre Einführung in neue Arbeitsbereiche, ihre Anleitung zu selbstständigem und verantwortlichem Handeln, ihre Vorbereitung auf besondere psychologische Anforderungen ihrer Tätigkeit sowie die verantwortliche Ausbildung von Auszubildenden;"@de .

dtd:LA-id720 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Überwachen von Schutz- und Sicherheitseinrichtungen; Sicherstellen der Kontrollen der eingesetzten Technik hinsichtlich ihrer Eignung und Funktion;"@de .

dtd:LA-id721 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Steuern der Arbeitsabläufe; Überwachen der Kosten und der Arbeitsleistung; Koordinieren von Maßnahmen zur Zusammenarbeit mit anderen Betriebseinheiten und Dritten;"@de .

dtd:LA-id722 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sicherstellen qualitätssichernder Maßnahmen; in enger Zusammenarbeit mit den für die Sicherheit zuständigen Fachkräften die Einhaltung der Arbeits-, Sicherheits-, Umwelt- und Gesundheitsvorschriften gewährleisten; sensibilisieren der Mitarbeiter und Mitarbeiterinnen für Belange des Informations- und Datenschutzes."@de .

dtd:LA-id723 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berufs- und arbeitspädagogische Qualifikationen,"@de .

dtd:LA-id724 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundlegende Qualifikationen,"@de .

dtd:LA-id725 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Handlungsspezifische Qualifikationen."@de .

dtd:LA-id726 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Grundlegende Qualifikationen und"@de .

dtd:LA-id727 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Handlungsspezifische Qualifikationen."@de .

dtd:LA-id728 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine mit Erfolg abgelegte Abschlussprüfung in einem dreijährigen anerkannten Ausbildungsberuf, der einem sicherheitsrelevanten Beruf zugeordnet werden kann, oder"@de .

dtd:LA-id729 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine mit Erfolg abgelegte Abschlussprüfung in einem anderen sicherheitsrelevanten anerkannten Ausbildungsberuf und eine mindestens einjährige Berufspraxis oder"@de .

dtd:LA-id730 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine mit Erfolg abgelegte Abschlussprüfung in einem anderen anerkannten Ausbildungsberuf und eine mindestens zweijährige Berufspraxis oder"@de .

dtd:LA-id731 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine mindestens vierjährige Berufspraxis oder"@de .

dtd:LA-id732 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine mit Erfolg abgelegte Prüfung zur Geprüften Werkschutzfachkraft."@de .

dtd:LA-id733 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Ablegen des Prüfungsteils \"Grundlegende Qualifikationen\", das nicht länger als fünf Jahre zurückliegt, und"@de .

dtd:LA-id734 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zu den in Absatz 1 Nummer 1 bis 5 genannten Voraussetzungen ein weiteres Jahr Berufspraxis."@de .

dtd:LA-id735 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rechtsbewusstes Handeln,"@de .

dtd:LA-id736 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebswirtschaftliches Handeln,"@de .

dtd:LA-id737 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zusammenarbeit im Betrieb."@de .

dtd:LA-id738 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen arbeitsrechtlicher Vorschriften und Bestimmungen bei der Gestaltung individueller Arbeitsverhältnisse und bei Fehlverhalten von Mitarbeitern und Mitarbeiterinnen, insbesondere unter Berücksichtigung des Arbeitsvertragsrechts, des Tarifvertragsrechts und betrieblicher Vereinbarungen;"@de .

dtd:LA-id739 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen der Vorschriften des Betriebsverfassungsgesetzes und des Personalvertretungsrechts;"@de .

dtd:LA-id740 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen rechtlicher Bestimmungen hinsichtlich der Sozialversicherungen, der Entgeltfindung sowie der Arbeitsförderung;"@de .

dtd:LA-id741 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen arbeitsschutz- und arbeitssicherheitsrechtlicher Vorschriften und Bestimmungen in Abstimmung mit betrieblichen und außerbetrieblichen Institutionen;"@de .

dtd:LA-id742 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen der Vorschriften des Umweltrechts, insbesondere hinsichtlich des Gewässer- und Bodenschutzes, der Abfallbeseitigung, der Luftreinhaltung und Lärmbekämpfung, des Strahlenschutzes und des Schutzes vor gefährlichen Stoffen;"@de .

dtd:LA-id743 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen einschlägiger wirtschaftsrechtlicher Vorschriften und Bestimmungen sowie des Datenschutzes."@de .

dtd:LA-id744 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen von ökonomischen Handlungsprinzipien unter Einbeziehung volkswirtschaftlicher Zusammenhänge und sozialer Wirkungen;"@de .

dtd:LA-id745 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Berücksichtigen der Grundsätze betrieblicher Aufbau- und Ablauforganisation;"@de .

dtd:LA-id746 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Nutzen und Möglichkeiten der Organisationsentwicklung;"@de .

dtd:LA-id747 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anwenden von Methoden der Entgeltfindung und der kontinuierlichen betrieblichen Verbesserung;"@de .

dtd:LA-id748 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Durchführung von Kostenarten-, Kostenstellen- und Kostenträgerrechnungen sowie von Kalkulationsverfahren."@de .

dtd:LA-id749 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beurteilen und Fördern der beruflichen Entwicklung Einzelner unter Beachtung des bisherigen Berufsweges und unter Berücksichtigung persönlicher und sozialer Gegebenheiten;"@de .

dtd:LA-id750 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beurteilen und Berücksichtigen des Einflusses von Arbeitsorganisation und Arbeitsplatz auf das Sozialverhalten und das Betriebsklima sowie Ergreifen von Maßnahmen zur Verbesserung;"@de .

dtd:LA-id751 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beurteilen von Einflüssen der Gruppenstruktur auf das Gruppenverhalten und die Zusammenarbeit sowie Entwickeln und Umsetzen von Alternativen;"@de .

dtd:LA-id752 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auseinandersetzen mit eigenem und fremdem Führungsverhalten, Umsetzen von Führungsgrundsätzen;"@de .

dtd:LA-id753 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Anwenden von Führungsmethoden und -techniken einschließlich Vereinbarungen entsprechender Handlungsspielräume, um Leistungsbereitschaft und Zusammenarbeit der Mitarbeiter und Mitarbeiterinnen zu fördern;"@de .

dtd:LA-id754 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fördern der Kommunikation und Kooperation durch Anwenden von Methoden zur Lösung betrieblicher Probleme und sozialer Konflikte."@de .


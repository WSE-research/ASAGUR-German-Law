dtd:LA-id2017 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufgaben der Gerichtsverwaltung,"@de .

dtd:LA-id2018 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "andere Aufgaben, die auf Grund eines Gesetzes Gerichten oder Richtern zugewiesen sind,"@de .

dtd:LA-id2019 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufgaben der Forschung und Lehre an einer wissenschaftlichen Hochschule, öffentlichen Unterrichtsanstalt oder amtlichen Unterrichtseinrichtung,"@de .

dtd:LA-id2020 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Prüfungsangelegenheiten,"@de .

dtd:LA-id2021 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Vorsitz in Einigungsstellen im Sinne des § 73 Absatz 2 Satz 1 des Bundespersonalvertretungsgesetzes vom 9. Juni 2021 (BGBl. I S. 1614)."@de .

dtd:LA-id2022 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem ordentlichen Gericht in Zivilsachen,"@de .

dtd:LA-id2023 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Staatsanwaltschaft oder einem Gericht in Strafsachen,"@de .

dtd:LA-id2024 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Verwaltungsbehörde,"@de .

dtd:LA-id2025 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Rechtsanwalt"@de .

dtd:LA-id2026 a dtd:LA ;
    dtd:has_Value "mindestens eines Kindes unter 18 Jahren oder"@de .

dtd:LA-id2027 a dtd:LA ;
    dtd:has_Value "eines laut ärztlichen Gutachtens pflegebedürftigen Ehegatten, Lebenspartners oder in gerader Linie Verwandten."@de .

dtd:LA-id2028 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Deutscher im Sinne des Artikels 116 des Grundgesetzes ist,"@de .

dtd:LA-id2029 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gewähr dafür bietet, dass er jederzeit für die freiheitliche demokratische Grundordnung im Sinne des Grundgesetzes eintritt,"@de .

dtd:LA-id2030 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Befähigung zum Richteramt besitzt (§§ 5 bis 7) und"@de .

dtd:LA-id2031 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "über die erforderliche soziale Kompetenz verfügt."@de .

dtd:LA-id2032 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Beamter des höheren Dienstes,"@de .

dtd:LA-id2033 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im deutschen öffentlichen Dienst oder im Dienst einer zwischenstaatlichen oder überstaatlichen Einrichtung, wenn die Tätigkeit nach Art und Bedeutung der Tätigkeit in einem Amt des höheren Dienstes entsprochen hat,"@de .

dtd:LA-id2034 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als habilitierter Lehrer des Rechts an einer deutschen wissenschaftlichen Hochschule,"@de .

dtd:LA-id2035 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Rechtsanwalt, Notar oder als Assessor bei einem Rechtsanwalt oder Notar,"@de .

dtd:LA-id2036 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in anderen Berufen, wenn die Tätigkeit nach Art und Bedeutung wie die unter den Nummern 1 bis 4 genannten Tätigkeiten geeignet war, Kenntnisse und Erfahrungen für die Ausübung des Richteramts zu vermitteln."@de .

dtd:LA-id2037 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Begründung des Richterverhältnisses,"@de .

dtd:LA-id2038 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Umwandlung des Richterverhältnisses in ein solches anderer Art (§ 8),"@de .

dtd:LA-id2039 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Verleihung eines anderen Amtes mit anderem Endgrundgehalt."@de .

dtd:LA-id2040 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht Deutscher im Sinne des Artikels 116 des Grundgesetzes war oder"@de .

dtd:LA-id2041 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "(weggefallen)"@de .

dtd:LA-id2042 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht die Fähigkeit zur Bekleidung öffentlicher Ämter hatte."@de .

dtd:LA-id2043 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn der Ernannte nicht die Befähigung zum Richteramt besaß,"@de .

dtd:LA-id2044 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn die gesetzlich vorgeschriebene Beteiligung eines Richterwahlausschusses unterblieben war und der Richterwahlausschuß die nachträgliche Bestätigung abgelehnt hat,"@de .

dtd:LA-id2045 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn die Ernennung durch Zwang, arglistige Täuschung oder Bestechung herbeigeführt wurde oder"@de .

dtd:LA-id2046 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn nicht bekannt war, daß der Ernannte ein Verbrechen oder Vergehen begangen hatte, das ihn der Berufung in das Richterverhältnis unwürdig erscheinen läßt, und er deswegen rechtskräftig zu einer Strafe verurteilt war oder wird."@de .

dtd:LA-id2047 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn er die Eigenschaft als Deutscher im Sinne des Artikels 116 des Grundgesetzes verliert,"@de .

dtd:LA-id2048 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn er in ein öffentlich-rechtliches Dienst- oder Amtsverhältnis zu einem anderen Dienstherrn tritt, sofern gesetzlich nicht anderes bestimmt ist, oder"@de .

dtd:LA-id2049 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn er zum Berufssoldaten oder Soldaten auf Zeit ernannt wird."@de .

dtd:LA-id2050 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn er sich weigert, den Richtereid (§ 38) zu leisten,"@de .

dtd:LA-id2051 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn er zur Zeit der Ernennung Mitglied des Bundestages oder eines Landtages war und nicht innerhalb der von der obersten Dienstbehörde gesetzten angemessenen Frist sein Mandat niederlegt,"@de .

dtd:LA-id2052 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn er nach Erreichen der Altersgrenze berufen worden ist,"@de .

dtd:LA-id2053 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn er seine Entlassung schriftlich verlangt,"@de .

dtd:LA-id2054 a dtd:LA ;
    dtd:has_Size dtd:normal ;

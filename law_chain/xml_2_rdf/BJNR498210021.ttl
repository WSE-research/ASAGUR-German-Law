dtd:entry-id7552 a dtd:entry ;
    dtd:has_DL dtd:DL-id9268 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.die Funktionen der Hauptmotoren und Hilfseinrichtungen sowie ihrer Kontrollsysteme zu nutzen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7553 a dtd:entry ;
    dtd:has_DL dtd:DL-id9269 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der Bedienung des Hauptmotors und der Hilfseinrichtungen.2.Kenntnis der Eigenschaften von Brennstoffen und Schmiermitteln.3.Kenntnis der Kontrollsysteme.4.Fähigkeit, verschiedene Systeme verschiedener Antriebsysteme, Hilfsmaschinen und -einrichtungen zu benutzen."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7554 a dtd:entry ;
    dtd:has_DL dtd:DL-id9270 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "2.die Besatzungsmitglieder bei Betrieb und Wartung der Hauptmotoren, Hilfsmaschinen und -einrichtungen zu überwachen und zu beaufsichtigen."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7555 a dtd:entry ;
    dtd:has_DL dtd:DL-id9271 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Fähigkeit, die Besatzung in Bezug auf den Betrieb und die Wartung technischer Einrichtungen zu führen.2.Fähigkeit, die Führung beim Anfahren und Abschalten des Hauptantriebs, der Hilfsmaschinen und -einrichtungen zu übernehmen."@de ;
    dtd:has_colname "col2"@de .


    dtd:has_VJ "1"@de ;
    dtd:has_Value "Zweidimensionale Objekte werden nur im Hintergrund verwendet (in weiter Entfernung), sodass sie kaum in Erscheinung treten. Andernfalls werden 3D-Modelle verwendet."@de ;

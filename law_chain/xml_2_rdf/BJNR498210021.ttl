    dtd:has_Value "1.die Bestandteile von Bränden und Zündarten und -quellen zu unterscheiden;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7367 a dtd:entry ;
    dtd:has_DL dtd:DL-id9124 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der möglichen Brandursachen bei verschiedenen Tätigkeiten sowie der Brandklassen gemäß der europäischen EN-Norm oder einer gleichwertigen Norm.2.Kenntnis der Bestandteile des Verbrennungsprozesses.3.Fähigkeit, die Grundsätze der Brandbekämpfung anzuwenden."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7368 a dtd:entry ;
    dtd:has_DL dtd:DL-id9125 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "2.verschiedene Arten von Feuerlöschern zu benutzen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7369 a dtd:entry ;
    dtd:has_DL dtd:DL-id9126 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der verschiedenen Merkmale und Klassen von Feuerlöschern.2.Fähigkeit, verschiedene Methoden der Brandbekämpfung und Löschgeräte und feste Löschanlagen unter Berücksichtigung z. B. folgender Aspekte einzusetzen: •Gebrauch verschiedener Arten tragbarer Feuerlöscher und•Auswirkungen des Windes beim Annähern an das Feuer."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7370 a dtd:entry ;
    dtd:has_DL dtd:DL-id9127 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "3.gemäß den an Bord geltenden Verfahren und der Organisation der Brandbekämpfung zu handeln;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7371 a dtd:entry ;
    dtd:has_DL dtd:DL-id9128 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der Brandbekämpfungssysteme an Bord.2.Fähigkeit, Brände zu bekämpfen und die entsprechenden Meldungen vorzunehmen."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7372 a dtd:entry ;
    dtd:has_DL dtd:DL-id9129 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "4.Anweisungen zu befolgen betreffend: persönliche Ausrüstung, Methoden, Löschmittel und Verfahren bei Brandbekämpfung und Rettungsarbeiten."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7373 a dtd:entry ;
    dtd:has_DL dtd:DL-id9130 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der Verfahren zur Vermeidung persönlicher Gefährdungen.2.Fähigkeit, gemäß den Notfallverfahren zu handeln."@de ;
    dtd:has_colname "col2"@de .


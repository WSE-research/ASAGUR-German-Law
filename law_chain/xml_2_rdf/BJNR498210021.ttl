dtd:entry-id7230 a dtd:entry ;
    dtd:has_DL dtd:DL-id9012 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.verschiedene Arten von Fahrzeugen zu unterscheiden;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7231 a dtd:entry ;
    dtd:has_DL dtd:DL-id9013 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der häufigsten Arten der in der europäischen Binnenschifffahrt eingesetzten Fahrzeuge, einschließlich Verbände, und ihrer jeweiligen Konstruktion, Abmessungen und Tragfähigkeit.2.Fähigkeit, die Merkmale der häufigsten Arten der in der europäischen Binnenschifffahrt verkehrenden Fahrzeuge, einschließlich Verbände, zu erläutern."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7232 a dtd:entry ;
    dtd:has_DL dtd:DL-id9014 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "2.die Kenntnis der Konstruktion von Fahrzeugen auf Binnenwasserstraßen und ihres Verhaltens im Wasser, insbesondere im Hinblick auf Stabilität und Festigkeit, anzuwenden;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7233 a dtd:entry ;
    dtd:has_DL dtd:DL-id9015 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der Auswirkungen der Fahrzeugbewegungen unter verschiedenen Umständen, die durch Längs- und Querspannungen verursacht werden, und verschiedener Ladebedingungen.2.Fähigkeit, das Verhalten des Fahrzeugs bei verschiedenen Beladungsbedingungen im Hinblick auf die Fahrzeugstabilität und -festigkeit zu erläutern."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7234 a dtd:entry ;
    dtd:has_DL dtd:DL-id9016 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "3.die Kenntnisse über Bauteile des Fahrzeugs anzuwenden und die Bezeichnung und Funktion der Teile zu nennen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7235 a dtd:entry ;
    dtd:has_DL dtd:DL-id9017 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der Bauteile des Fahrzeugs im Hinblick auf die Beförderung verschiedener Arten von Ladung und Fahrgästen, einschließlich der Längs- und Querstruktur und örtlicher Verstärkungen.2.Fähigkeit, die Bauteile des Fahrzeugs zu benennen und ihre Funktionen zu beschreiben."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7236 a dtd:entry ;
    dtd:has_DL dtd:DL-id9018 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "4.die Kenntnisse über die Wasserdichtigkeit des Fahrzeugs anzuwenden;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7237 a dtd:entry ;
    dtd:has_DL dtd:DL-id9019 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnisse über die Wasserdichtigkeit des Fahrzeugs auf Binnenwasserstraßen.2.Fähigkeit zur Überprüfung der Wasserdichtigkeit."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7238 a dtd:entry ;
    dtd:has_DL dtd:DL-id9020 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "5.die Kenntnisse über die für den Fahrzeugbetrieb erforderlichen Dokumente anzuwenden."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7239 a dtd:entry ;
    dtd:has_DL dtd:DL-id9021 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der vorgeschriebenen Fahrzeugdokumente.2.Fähigkeit, deren Bedeutung im Zusammenhang mit (inter)nationalen Anforderungen und Rechtsvorschriften zu erläutern."@de ;
    dtd:has_colname "col2"@de .


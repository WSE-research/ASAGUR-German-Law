dtd:entry-id7409 a dtd:entry ;
    dtd:has_DL dtd:DL-id9157 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Fähigkeit, die Kennzeichnung gefährlicher Güter gemäß ADN zu erklären.2.Fähigkeit, die Verfahren für die Sicherheit der Fahrgastbeförderung zu erklären, einschließlich der Anwendung der Verordnung (EU) Nr. 1177/2010.3.Fähigkeit, mit Fahrgästen effektiv zu kommunizieren."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7410 a dtd:entry ;
    dtd:has_DL dtd:DL-id9158 ;

dtd:LA-id16117 a dtd:LA ;
    dtd:has_B dtd:B-id2770 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sachliche Voraussetzungen"@de .

dtd:LA-id16118 a dtd:LA ;
    dtd:has_DL dtd:DL-id13655 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Die antragstellende Person hat nachzuweisen, dass sie 2.1die in Anhang 2 genannten praktischen Übungen unter realen Bedingungen durchführen kann,2.2ausreichend Atemschutzgeräte für die praktischen Übungen zur Verfügung stellen kann,2.3die eingesetzten Atemschutzgeräte reinigen, desinfizieren, warten und ggf. reparieren kann,2.4über geeignete Räumlichkeiten inkl. Medientechnik zur Durchführung des theoretischen Unterrichts verfügt."@de .

dtd:LA-id16119 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die in Anhang 2 genannten praktischen Übungen unter realen Bedingungen durchführen kann,"@de .


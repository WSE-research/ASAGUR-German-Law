    dtd:has_Value "1.Vorbereitungen für Rettungspläne für verschiedene Arten von Notfällen einzuleiten;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7647 a dtd:entry ;
    dtd:has_DL dtd:DL-id9339 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der verschiedenen Arten möglicher Notfälle, wie Zusammenstöße Feuer, Wassereinbruch, Sinken.2.Fähigkeit, Notfallpläne für das Verhalten in Notsituationen an Bord zu erstellen und Besatzungsmitgliedern spezielle Aufgaben zuzuweisen hierzu gehört auch die Überwachung und Kontrolle."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7648 a dtd:entry ;
    dtd:has_DL dtd:DL-id9340 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "2.Unterweisungen in Methoden zur Brandverhütung, Brandursachenerkennung und Brandbekämpfung entsprechend der verschiedenen Fähigkeiten der Besatzungsmitglieder durchzuführen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7649 a dtd:entry ;
    dtd:has_DL dtd:DL-id9341 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der Brandbekämpfungsmethoden mit besonderem Schwerpunkt auf Taktik und Führung.2.Kenntnis der Auswirkung des Einsatzes von Wasser zum Feuerlöschen auf die Stabilität des Schiffes und Fähigkeit, entsprechende Maßnahmen zu ergreifen.3.Fähigkeit, die Kommunikation und Koordination bei Brandbekämpfungseinsätzen zu übernehmen, einschließlich der Kommunikation mit externen Organisationen, und sich aktiv an den Rettungs- und Brandbekämpfungsmaßnahmen zu beteiligen."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7650 a dtd:entry ;
    dtd:has_DL dtd:DL-id9342 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "3.Unterweisungen im Einsatz von Rettungsmitteln durchzuführen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7651 a dtd:entry ;
    dtd:has_DL dtd:DL-id9343 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der besonderen Eigenschaften und Ausstattungen von Rettungsgeräten.2.Fähigkeit, ein Beiboot zu Wasser zu bringen und wieder an Bord zu nehmen und die Besatzungsmitglieder und das Bordpersonal in die Verwendung eines Beibootes einzuweisen."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7652 a dtd:entry ;
    dtd:has_DL dtd:DL-id9344 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "4.Anweisungen zu Rettungsplänen, Fluchtwegen und internen Kommunikations- und Alarmsystemen zu erteilen."@de ;
    dtd:has_colname "col1"@de .


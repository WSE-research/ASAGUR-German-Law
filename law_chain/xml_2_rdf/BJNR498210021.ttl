dtd:entry-id7416 a dtd:entry ;
    dtd:has_DL dtd:DL-id9162 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.die Maschinen, einschließlich Pumpen, Rohrleitungssystemen, Bilge- und Ballastsystemen, zu bedienen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7417 a dtd:entry ;
    dtd:has_DL dtd:DL-id9163 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der für den sicheren Betrieb der Maschinen und des Bilge- und Ballastsystems zu befolgenden Verfahren sowie der ordnungsgemäßen Abfallentsorgung.2.Fähigkeit, die Maschinen im Maschinenraum entsprechend den Verfahren zu betreiben und zu steuern.3.Fähigkeit, die sichere Funktion, Betriebsweise und Instandhaltung des Bilge- und Ballastsystems zu erklären, einschließlich Meldung von Zwischenfällen im Zusammenhang mit Umpumpvorgängen und Fähigkeit, Tankfüllstände korrekt zu messen und zu melden.4.Fähigkeit, das Abschalten der Maschinen nach dem Einsatz vorzubereiten und durchzuführen.5.Fähigkeit, Bilge-, Ballast- und Ladungspumpensysteme zu bedienen.6.Fähigkeit, die Notwendigkeit einer ordnungsgemäßen und sicheren Sammlung, Lagerung und Abgabe von Abfällen zu erklären.7.Fähigkeit, die hydraulischen und pneumatischen Systeme zu benutzen."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7418 a dtd:entry ;
    dtd:has_DL dtd:DL-id9164 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "2.die Generatoren vorzubereiten, einzuschalten, anzuschließen und zu wechseln und ihre Systeme und den Landanschluss zu überprüfen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7419 a dtd:entry ;
    dtd:has_DL dtd:DL-id9165 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der Kraftanlage.2.Fähigkeit, die Schalttafel zu benutzen.3.Fähigkeit, den Landanschluss zu benutzen."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7420 a dtd:entry ;
    dtd:has_DL dtd:DL-id9166 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "3.die erforderlichen Werkzeuge und Materialien zu verwenden;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7421 a dtd:entry ;
    dtd:has_DL dtd:DL-id9167 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der Eigenschaften und Grenzen der zur Wartung und Instandsetzung von Maschinen und Ausrüstung eingesetzten Prozesse, Materialien und Werkzeuge.2.Fähigkeit, sichere Arbeitsverfahren anzuwenden."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7422 a dtd:entry ;
    dtd:has_DL dtd:DL-id9168 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "4.die täglichen Wartungsarbeiten an den Hauptmotoren, Hilfsmaschinen und Regelungs- und Steuerungsanlagen durchzuführen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7423 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Fähigkeit, Maschinenraum, Hauptmotoren, Hauptmaschinen und Hilfsmaschinen und Regelungs- und Steuerungsanlagen zu warten und instand zu halten."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7424 a dtd:entry ;

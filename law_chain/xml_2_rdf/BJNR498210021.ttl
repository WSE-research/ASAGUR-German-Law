dtd:entry-id7558 a dtd:entry ;
    dtd:has_DL dtd:DL-id9272 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Anweisungen zur Vorbereitung der Antriebsmaschinen und der Hilfsmaschinen und -ausrüstung zu erteilen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7559 a dtd:entry ;
    dtd:has_DL dtd:DL-id9273 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Fähigkeit, die Besatzung bei der Vorbereitung und Bedienung der Antriebsmaschinen und der Hilfsmaschinen und -ausrüstung anzuleiten.2.Fähigkeit, Prüflisten zu erstellen und zu überwachen und Anweisungen zum ordnungsgemäßen Gebrauch solcher Prüflisten zu erteilen.3.Fähigkeit, die Besatzung in die bei der Maschinenüberwachung zu beachtenden Grundsätze einzuweisen."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7560 a dtd:entry ;
    dtd:has_DL dtd:DL-id9274 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "2.Funktionsstörungen und häufige Fehler zu erkennen und Maßnahmen zur Schadensverhütung zu ergreifen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7561 a dtd:entry ;
    dtd:has_DL dtd:DL-id9275 ;
    dtd:has_VJ "1"@de ;

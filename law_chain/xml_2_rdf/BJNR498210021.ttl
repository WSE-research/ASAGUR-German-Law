dtd:entry-id8685 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Das Sichtsystem visualisiert die Dynamik von Fahrzeug und Leine (z. B. Durchhang, Elastizität, Gewicht, Bruchverhalten und Verbindungen zu den Pollerpunkten)."@de ;

dtd:entry-id7411 a dtd:entry ;
    dtd:has_DL dtd:DL-id9159 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der Funktion und des Einsatzes von Ballastsystemen.2.Fähigkeit, den Einsatz des Ballastsystems, z. B. durch die Befüllung oder Entleerung der Ballasttanks, zu erklären."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7412 a dtd:entry ;

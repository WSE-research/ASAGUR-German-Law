dtd:entry-id7508 a dtd:entry ;
    dtd:has_DL dtd:DL-id9234 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.die Grundsätze des Schiffsbaus in der Binnenschifffahrt zu beachten;"@de ;

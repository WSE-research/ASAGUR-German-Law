    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.den Beginn einer Reise vorzubereiten und insbesondere bei eingeschränkten Sichtverhältnissen Navigationsradaranlagen und Wendegeschwindigkeitsanzeiger zu nutzen."@de ;

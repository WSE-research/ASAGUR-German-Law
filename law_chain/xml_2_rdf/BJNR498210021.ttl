    dtd:has_Value "die Seeleute-Befähigungsverordnung vom 8. Mai 2014 (BGBl. I S. 460), die durch Artikel 1 der Verordnung vom 28. Juli 2021 (BGBl. I S. 3236) geändert worden ist, und"@de .

dtd:LA-id14011 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle Vorschriften über die Besatzung und über die Befähigung der Besatzung der Fahrzeuge, die ausschließlich zur Verwendung im Hamburger Hafen bestimmt sind."@de .

dtd:LA-id14012 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Binnenwasserstraße“ eine für die in § 25 Absatz 3 bis 5 genannten Fahrzeuge befahrbare Wasserstraße der Zonen 1 bis 4;"@de .

dtd:LA-id14013 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Fahrzeug“ ein Binnenschiff, einschließlich Fähre, schwimmendes Gerät oder ein Seeschiff;"@de .

dtd:LA-id14014 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Binnenschiff“ ein Schiff, das ausschließlich oder vorwiegend für die Fahrt auf Binnenwasserstraßen bestimmt ist;"@de .

dtd:LA-id14015 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Seeschiff“ ein Schiff, das zur See- oder Küstenfahrt zugelassen und vorwiegend dafür bestimmt ist;"@de .

dtd:LA-id14016 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Motorschiff“ ein zur Güterbeförderung bestimmtes Schiff, das mit eigener Triebkraft allein fahren kann;"@de .

dtd:LA-id14017 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Fähre“ ein Fahrzeug, das dem Übersetzverkehr von einem Ufer zum anderen auf der Wasserstraße dient und von der zuständigen Behörde als Fähre behandelt wird;"@de .

dtd:LA-id14018 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Kahnfähre“ eine zur Beförderung von Personen gebaute, offene Fähre, die durch Muskelkraft fortbewegt wird; auch mit einem – zur Beherrschung besonderer Betriebslagen – Hilfsantrieb ausgestattet;"@de .

dtd:LA-id14019 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Behördenfahrzeug“ ein Fahrzeug, das von einer Behörde im Rahmen ihrer gesetzlichen Aufgaben eingesetzt wird;"@de .

dtd:LA-id14020 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Feuerlöschboot“ ein Fahrzeug, das ausschließlich oder überwiegend zum Feuerlöschen eingesetzt wird;"@de .

dtd:LA-id14021 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schleppboot“ ein eigens zum Schleppen gebautes Schiff;"@de .

dtd:LA-id14022 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schubboot“ ein eigens zur Fortbewegung eines Schubverbandes gebautes Schiff;"@de .

dtd:LA-id14023 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schleppkahn“ ein zur Güterbeförderung bestimmtes und zur Fortbewegung durch Schleppen gebautes Schiff ohne eigene Triebkraft oder mit eigener Triebkraft, die nur erlaubt, kleine Ortsveränderungen vorzunehmen;"@de .

dtd:LA-id14024 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schubleichter“ ein zur Güterbeförderung bestimmtes und zur Fortbewegung durch Schieben gebautes oder eigens eingerichtetes Schiff ohne eigene Triebkraft oder mit eigener Triebkraft, die nur erlaubt, außerhalb des Schubverbandes kleine Ortsveränderungen vorzunehmen;"@de .

dtd:LA-id14025 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Verband“ ein starrer Verband oder ein Schleppverband;"@de .

dtd:LA-id14026 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„starrer Verband“ ein Schubverband oder gekuppelte Fahrzeuge;"@de .

dtd:LA-id14027 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schubverband“ eine starre Verbindung von Fahrzeugen, von denen sich mindestens eines vor dem oder den beiden Fahrzeugen mit Maschinenantrieb befindet, das oder die den Verband fortbewegt oder fortbewegen und als „schiebendes Fahrzeug“ oder „schiebende Fahrzeuge“ bezeichnet werden; als starr gilt auch ein Verband aus einem schiebenden und einem geschobenen Fahrzeug, deren Kupplungen ein gesteuertes Knicken ermöglichen;"@de .

dtd:LA-id14028 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„gekuppelte Fahrzeuge“ eine Zusammenstellung von längsseits starr gekuppelten Fahrzeugen, von denen sich keines vor dem Fahrzeug mit Maschinenantrieb befindet, das die Zusammenstellung fortbewegt;"@de .

dtd:LA-id14029 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schleppverband“ eine Zusammenstellung von einem oder mehreren Fahrzeugen, schwimmenden Anlagen oder Schwimmkörpern, die von einem oder mehreren zum Verband gehörigen Fahrzeugen mit Maschinenantrieb geschleppt wird;"@de .

dtd:LA-id14030 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Großverband“ ein Schubverband, bei dem das Produkt aus Gesamtlänge und Gesamtbreite der geschobenen Fahrzeuge 7 000 Quadratmeter oder mehr beträgt;"@de .

dtd:LA-id14031 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Fahrgastschiff“ ein zur Beförderung von mehr als 12 Fahrgästen gebautes und eingerichtetes Tagesausflugs- oder Kabinenschiff;"@de .

dtd:LA-id14032 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Fahrgastboot“ ein zur Beförderung von Fahrgästen zugelassenes Fahrzeug, das kein Fahrgastschiff ist;"@de .

dtd:LA-id14033 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Tagesausflugsschiff“ ein Fahrgastschiff ohne Kabinen für die Übernachtung von Fahrgästen;"@de .

dtd:LA-id14034 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Kabinenschiff“ ein Fahrgastschiff mit Kabinen für die Übernachtung von Fahrgästen;"@de .

dtd:LA-id14035 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Sportfahrzeug“ ein für Sport- oder Freizeitzwecke bestimmtes Schiff, das kein Fahrgastschiff oder Fahrgastboot ist;"@de .

dtd:LA-id14036 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„schwimmendes Gerät“ eine schwimmende Konstruktion mit auf ihr vorhandenen Arbeitseinrichtungen wie Krane, Bagger, Rammen, Elevatoren;"@de .

dtd:LA-id14037 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„schwimmende Anlage“ eine schwimmende Einrichtung, die in der Regel nicht zur Fortbewegung bestimmt ist, wie eine Badeanstalt, ein Dock, eine Landebrücke oder ein Bootshaus;"@de .

dtd:LA-id14038 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schwimmkörper“ ein Floß sowie andere einzeln oder in Verbindungen fahrtauglich gemachte Gegenstände, soweit es sich nicht um ein Schiff, ein schwimmendes Gerät oder eine schwimmende Anlage handelt;"@de .

dtd:LA-id14039 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Länge“ oder „L“ die größte Länge eines Fahrzeugkörpers in Metern, ohne Ruder und Bugspriet;"@de .

dtd:LA-id14040 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Breite“ oder „B“ die größte Breite eines Fahrzeugkörpers in Metern, gemessen an der Außenseite der Beplattung, ohne Schaufelräder, Scheuerleisten und Ähnliches;"@de .

dtd:LA-id14041 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Tiefgang“ oder „T“ der senkrechte Abstand vom tiefsten Punkt eines Fahrzeugkörpers in Metern, ohne Berücksichtigung des Kiels oder anderer fester Anbauten, bis zur Ebene der größten Einsenkung des Schiffskörpers, in Metern;"@de .

dtd:LA-id14042 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Besatzung“ die Decksmannschaft und das Maschinenpersonal;"@de .

dtd:LA-id14043 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Decksmannschaft“ Personen, die Funktionen auf Einstiegs-, Betriebs- und Führungsebene überwiegend an Deck ausüben;"@de .

dtd:LA-id14044 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Maschinenpersonal“ die Maschinisten und Maschinistinnen im Sinne der Rheinschiffspersonalverordnung und die Maschinenkundigen nach dieser Verordnung;"@de .

dtd:LA-id14045 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Einstiegsebene“ der Verantwortungsbereich, der mit den Funktionen des Decksmannes und der Decksfrau (Decksleute) sowie des Leichtmatrosen und der Leichtmatrosin verbunden ist;"@de .

dtd:LA-id14046 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Betriebsebene“ der Verantwortungsbereich, der mit den Funktionen des Matrosen und der Matrosin, des Bootsmanns und der Bootsfrau (Bootsleute) sowie des Steuermannes und der Steuerfrau (Steuerleute) verbunden ist;"@de .

dtd:LA-id14047 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Führungsebene“ der Verantwortungsbereich, der mit der Funktion des Schiffsführers und der Schiffsführerin (Schiffsführung) verbunden ist;"@de .

dtd:LA-id14048 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Bordpersonal“ alle an Bord eines Fahrgastschiffes Beschäftigten, die nicht zur Besatzung gehören;"@de .

dtd:LA-id14049 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Sicherheitspersonal“ die Sachkundigen für Flüssigerdgas (LNG), die Sachkundigen für die Fahrgastschifffahrt, die Ersthelfer und die Ersthelferinnen sowie die atemschutzgerättragenden Personen;"@de .

dtd:LA-id14050 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Sicherheitspersonal für Fahrgastschiffe“ die Sachkundigen für die Fahrgastschifffahrt, die Ersthelfer und die Ersthelferinnen sowie die atemschutzgerättragenden Personen;"@de .

dtd:LA-id14051 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Sachkundiger für die Fahrgastschifffahrt“ eine an Bord tätige Person, die befähigt ist, in Notsituationen an Bord von Fahrgastschiffen Maßnahmen zu ergreifen;"@de .

dtd:LA-id14052 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Sachkundiger für Flüssigerdgas“ eine Person, die befähigt ist, am Bunkervorgang von Fahrzeugen, die Flüssigerdgas als Brennstoff nutzen, beteiligt zu sein oder der Schiffsführer eines solchen Fahrzeugs zu sein;"@de .

dtd:LA-id14053 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Fährführer“ wer berechtigt ist, eine Fähre zu führen;"@de .

dtd:LA-id14054 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Decksmann 180“ ein Decksmann oder eine Decksfrau mit 180 Tagen nachgewiesener und bestätigter Fahrzeit nach § 27 Absatz 1;"@de .

dtd:LA-id14055 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Radarfahrt“ eine Fahrt bei unsichtigem Wetter mit Radar;"@de .

dtd:LA-id14056 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Befähigungszeugnis“ ein Zeugnis, das dem Inhaber oder der Inhaberin die nötigen Kenntnisse und Fertigkeiten für eine bestimmte Funktion beim Betrieb eines Fahrzeuges bestätigt;"@de .

dtd:LA-id14057 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Unionsbefähigungszeugnis“ das Befähigungszeugnis der Europäischen Union für Funktionen auf der Einstiegsebene und Betriebsebene sowie für Sachkundige nach Artikel 3 Nummer 7 der Richtlinie (EU) 2017/2397 des Europäischen Parlaments und des Rates vom 12. Dezember 2017 über die Anerkennung von Berufsqualifikationen in der Binnenschifffahrt und zur Aufhebung der Richtlinien 91/672/EWG und 96/50/EG des Rates (ABl. L 345 vom 27.12.2017, S. 53);"@de .

dtd:LA-id14058 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Unionspatent“ das Befähigungszeugnis der Europäischen Union für Schiffsführer und Schiffsführerinnen nach Artikel 4 Absatz 1 in Verbindung mit Artikel 11 Absatz 5 der Richtlinie (EU) 2017/2397 ;"@de .

dtd:LA-id14059 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schifferzeugnis“ das Fährschifferzeugnis, das Behördenschifferzeugnis, das Sportschifferzeugnis und das Kleinschifferzeugnis;"@de .

dtd:LA-id14060 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Sprechfunkzeugnis“ ein auf der Grundlage der Anlage 5 der Regionalen Vereinbarung über den Binnenschifffahrtsfunk (Bekanntmachung vom 28. August 2000, BGBl. II S. 1213) erteiltes Sprechfunkzeugnis;"@de .

dtd:LA-id14061 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Rheinpatent“ ein Befähigungszeugnis für Schiffsführer und Schiffsführerinnen nach § 11.02 Satz 1 Buchstabe a der Rheinschiffspersonalverordnung;"@de .

dtd:LA-id14062 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Schifferdienstbuch“ eine persönliche Aufzeichnung der Berufserfahrung eines Besatzungsmitglieds, insbesondere Einzelheiten zu seinen Fahrzeiten und Reisen;"@de .

dtd:LA-id14063 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„aktives Schifferdienstbuch“ ein für Eintragungen offenes Schifferdienstbuch;"@de .

dtd:LA-id14064 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Bordbuch“ eine zum Zwecke der Überwachung geführte Aufzeichnung der von einem Fahrzeug und seiner Besatzung durchgeführten Reisen;"@de .

dtd:LA-id14065 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„aktives Bordbuch“ ein für Eintragungen offenes Bordbuch;"@de .

dtd:LA-id14066 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Prüfling“ eine Person, die eine Prüfung zum Erwerb eines Befähigungszeugnisses ablegt;"@de .

dtd:LA-id14067 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Untersuchungskommission“ die nach § 4 der Binnenschiffsuntersuchungsordnung gebildete Einrichtung;"@de .

dtd:LA-id14068 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„ausstellende Behörde“ diejenige zuständige Behörde, die das Befähigungszeugnis ausstellt;"@de .

dtd:LA-id14069 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Mitgliedsstaat der Zentralkommission für die Rheinschifffahrt“ die Bundesrepublik Deutschland, die Niederlande, die Französische Republik, die Schweizerische Eidgenossenschaft und das Königreich Belgien;"@de .

dtd:LA-id14070 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Flüssigerdgas“ (LNG) Erdgas, das durch Abkühlung auf eine Temperatur von –161 °C verflüssigt wurde;"@de .

dtd:LA-id14071 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„ES-TRIN“ der Europäische Standard der technischen Vorschriften für Binnenschiffe in der jeweils nach § 2 Absatz 1 Nummer 2 der Binnenschiffsuntersuchungsordnung geltenden Fassung;"@de .

dtd:LA-id14072 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„STCW-Übereinkommen“ das Internationale Übereinkommen vom 7. Juli 1978 über Normen für die Ausbildung, die Erteilung von Befähigungszeugnissen und den Wachdienst von Seeleuten (BGBl. 1982 II S. 297, 298) in der jeweils geltenden Fassung;"@de .

dtd:LA-id14073 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Rheinschiffspersonalverordnung“ Anlage 1 zu § 1 Absatz 2 Nummer 1 der Rheinschiffspersonaleinführungsverordnung vom 5. April 2023 (BGBl. 2023 II Nr. 105, Anlageband) in der jeweils geltenden und anzuwendenden Fassung;"@de .

dtd:LA-id14074 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "„Berufsgenossenschaft“ die Berufsgenossenschaft Verkehrswirtschaft Post-Logistik Telekommunikation."@de .

dtd:LA-id14075 a dtd:LA ;
    dtd:has_Value "zur Anpassung an die technische Entwicklung der Binnenschifffahrt oder"@de .

dtd:LA-id14076 a dtd:LA ;
    dtd:has_Value "zu Versuchszwecken."@de .

dtd:LA-id14077 a dtd:LA ;
    dtd:has_NB dtd:NB-id2873 ;
    dtd:has_Value "müssen mit den Vorgaben der Richtlinie (EU) 2017/2397 und den aufgrund dieser Richtlinie erlassenen Rechtsakten der Europäischen Union vereinbar sein,"@de .

dtd:LA-id14078 a dtd:LA ;
    dtd:has_Value "dürfen den Jugendarbeitsschutz, den Arbeitsschutz sowie die Sicherheit und den Gesundheitsschutz von Besatzung nicht gefährden und"@de .

dtd:LA-id14079 a dtd:LA ;
    dtd:has_Value "dürfen die Sicherheit und Leichtigkeit des Schiffsverkehrs nicht gefährden."@de .

dtd:LA-id14080 a dtd:LA ;
    dtd:has_DL dtd:DL-id9578,
        dtd:DL-id9581 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein im Schifferdienstbuch eingetragenes Unionsbefähigungszeugnis, das erteilt worden ista)von einem Land oderb)von einem anderen Mitgliedstaat der Europäischen Union,"@de .

dtd:LA-id14081 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von einem Land oder"@de .

dtd:LA-id14082 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von einem anderen Mitgliedstaat der Europäischen Union,"@de .

dtd:LA-id14083 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein im Schifferdienstbuch eingetragenes Befähigungszeugnis, das von einem Mitgliedstaat der Zentralkommission für die Rheinschifffahrt nach der Rheinschiffspersonalverordnung erteilt worden ist."@de .

dtd:LA-id14084 a dtd:LA ;
    dtd:has_DL dtd:DL-id9590,
        dtd:DL-id9593 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für maschinenkundiges Personal, das erteilt worden ista)von einem Land oderb)von einem anderen Mitgliedstaat der Europäischen Union,"@de .

dtd:LA-id14085 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von einem Land oder"@de .

dtd:LA-id14086 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von einem anderen Mitgliedstaat der Europäischen Union,"@de .

dtd:LA-id14087 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für Maschinisten oder Maschinistinnen, das von einem Mitgliedstaat der Zentralkommission für die Rheinschifffahrt nach der Rheinschiffspersonalverordnung erteilt worden ist."@de .

dtd:LA-id14088 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 78 Absatz 3 Nummer 1 erteilten Unionspatentes oder"@de .

dtd:LA-id14089 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 78 Absatz 3 Nummer 2 bis 5 erteilten Fährschifferzeugnisses, Behördenschifferzeugnisses, Sportschifferzeugnisses oder Kleinschifferzeugnisses für die entsprechende Fahrzeugkategorie."@de .

dtd:LA-id14090 a dtd:LA ;
    dtd:has_DL dtd:DL-id9609 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Unionspatent, das erteilt worden ista)von einem Land oderb)von einem anderen Mitgliedstaat der Europäischen Union, oder"@de .

dtd:LA-id14091 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von einem Land oder"@de .

dtd:LA-id14092 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von einem anderen Mitgliedstaat der Europäischen Union, oder"@de .

dtd:LA-id14093 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Rheinpatent."@de .

dtd:LA-id14094 a dtd:LA ;
    dtd:has_DL dtd:DL-id9621 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Fahrzeug führt, das a)nur mit Muskelkraft oder unter Segel angetrieben wird oderb)mit einer Antriebsmaschine ausgerüstet ist, deren effektive Nutzleistung bei einem Verbrennungsmotor nicht mehr als 11,03 Kilowatt oder bei einem Elektromotor höchstens 7,5 Kilowatt in der Betriebsart S1 (Dauerbetrieb) nach DIN EN 60034-1: Ausgabe Februar 2011 beträgt,"@de .

dtd:LA-id14095 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nur mit Muskelkraft oder unter Segel angetrieben wird oder"@de .

dtd:LA-id14096 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mit einer Antriebsmaschine ausgerüstet ist, deren effektive Nutzleistung bei einem Verbrennungsmotor nicht mehr als 11,03 Kilowatt oder bei einem Elektromotor höchstens 7,5 Kilowatt in der Betriebsart S1 (Dauerbetrieb) nach DIN EN 60034-1: Ausgabe Februar 2011 beträgt,"@de .

dtd:LA-id14097 a dtd:LA ;
    dtd:has_I dtd:I-id2791 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein nicht in Fahrt befindliches schwimmendes Gerät ohne eigenen Antrieb auf den Wasserstraßen der Zonen 3 und 4 sowie außerhalb des Fahrwassers der Wasserstraßen der Anlage 1 führt,"@de .

dtd:LA-id14098 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das 16. Lebensjahr vollendet hat und als Mitglied der Besatzung eines schwimmenden Gerätes ein dazugehöriges Hilfsfahrzeug mit einer Länge von weniger als 20 Metern und einer effektiven Nutzleistung von nicht mehr als 25 Kilowatt führt."@de .

dtd:LA-id14099 a dtd:LA ;
    dtd:has_DL dtd:DL-id9631 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Wasserstraßen der Zonen 1 und 2a)ein Fährschifferzeugnis, wenn es für wenigstens eine Fährstelle dieser Zonen gilt,b)ein Sportschifferzeugnis mit einer besonderen Berechtigung für maritime Wasserstraßen oderc)ein Behördenschifferzeugnis mit einer besonderen Berechtigung für maritime Wasserstraßen,"@de .


dtd:entry-id8812 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Die Winkelgenauigkeit für die horizontale Peilung entspricht der Europäischen Technischen Spezifikation (ETSI) EN  302194. Effekte eines vertikal begrenzten Öffnungswinkels sind erkennbar, z. B. bei der Fahrt durch Brücken."@de ;
    dtd:has_colname "col3"@de .

dtd:entry-id8813 a dtd:entry ;
    dtd:has_DL dtd:DL-id9424 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "„Vertikale“ Konformität: Simulation von Brückendurchfahrten mit Berücksichtigung –der Höhe der Antenne über der Wasseroberfläche bei aktuellem Tiefgang,–des Ausstrahlungswinkels in Abhängigkeit von der Radarkeule und der Trimmung des Fahrzeugs,–der Höhe der Brücke zwischen Unterkante der Brücke und Wasseroberfläche."@de ;
    dtd:has_colname "col4"@de .


    dtd:has_VJ "1"@de ;
    dtd:has_Value "5.die täglichen Wartungsarbeiten an den Maschinen, einschließlich Pumpen, Rohrleitungssystemen, Bilge- und Ballastsystemen, durchzuführen."@de ;
    dtd:has_colname "col1"@de .


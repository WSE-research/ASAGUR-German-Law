dtd:entry-id7376 a dtd:entry ;
    dtd:has_DL dtd:DL-id9131 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.die Umwelt gemäß den einschlägigen Vorschriften zu schützen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7377 a dtd:entry ;
    dtd:has_DL dtd:DL-id9132 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der nationalen und internationalen Umweltschutzvorschriften.2.Fähigkeit, die verfügbaren Dokumentations- und Informationssysteme zu Umweltfragen gemäß den Anweisungen zu nutzen.3.Kenntnis der Folgen eines möglichen Austritts von Ladung und Schadstoffen in die Umwelt.4.Kenntnis gefährlicher Güter und ihrer Klassifizierung in Bezug auf Umweltaspekte."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7378 a dtd:entry ;
    dtd:has_DL dtd:DL-id9133 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "2.Vorsichtsmaßnahmen zur Vermeidung von Umweltverschmutzung zu treffen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7379 a dtd:entry ;
    dtd:has_DL dtd:DL-id9134 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der allgemeinen Vorsichtsmaßnahmen zur Vermeidung von Umweltverschmutzung.2.Fähigkeit, allgemeine Vorsichtsmaßnahmen zu treffen und sichere Bunkerverfahren anzuwenden.3.Fähigkeit, im Falle eines Zusammenstoßes Maßnahmen gemäß den Anweisungen zu ergreifen, z. B. durch das Abdichten von Leckagen."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7380 a dtd:entry ;
    dtd:has_DL dtd:DL-id9135 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "3.Ressourcen effizient einzusetzen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7381 a dtd:entry ;
    dtd:has_DL dtd:DL-id9136 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis des effizienten Kraftstoffverbrauchs.2.Fähigkeit, Materialien wirtschaftlich und energiesparend einzusetzen."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7382 a dtd:entry ;
    dtd:has_DL dtd:DL-id9137 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "4.Abfälle umweltfreundlich zu entsorgen."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7383 a dtd:entry ;
    dtd:has_DL dtd:DL-id9138 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der anwendbaren Abfallvorschriften.2.Fähigkeit zur Durchführung der Sammlung, Abgabe und des Verbrauchs von: •Fahrzeugölen und -fetten,•Ladungsrückständen und•anderen Arten von Abfallprodukten."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7384 a dtd:entry ;
    dtd:has_BR dtd:BR-id2114 ;

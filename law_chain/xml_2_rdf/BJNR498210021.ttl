dtd:entry-id7518 a dtd:entry ;
    dtd:has_DL dtd:DL-id9242 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.die Funktionen der Fahrzeugausrüstung zu verstehen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7519 a dtd:entry ;
    dtd:has_DL dtd:DL-id9243 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der vorgeschriebenen Ausrüstung des Fahrzeugs.2.Fähigkeit, die Funktionen der gesamten Ausrüstung gemäß den anwendbaren Rechtsvorschriften zu nutzen und zu kontrollieren sowie entsprechende Anweisungen zu erteilen und zu beaufsichtigen."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7520 a dtd:entry ;
    dtd:has_DL dtd:DL-id9244 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "2.die speziellen Anforderungen bei der Beförderung von Ladung und Fahrgästen zu beachten."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7521 a dtd:entry ;
    dtd:has_DL dtd:DL-id9245 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der speziellen Anforderungen an die Konstruktion und Ausrüstung von Fahrzeugen für die Beförderung verschiedener Ladungen und Fahrgäste mit verschiedenen Arten von Fahrzeugen gemäß den geltenden Rechtsvorschriften.2.Fähigkeit, entsprechende Anweisungen zu erteilen und zu beaufsichtigen.3.Fähigkeit, Anweisungen zur ordnungsgemäßen Anwendung der Anforderungen des Zeugnisses des Fahrzeugs zu erteilen und zu beaufsichtigen."@de ;
    dtd:has_colname "col2"@de .


dtd:entry-id7242 a dtd:entry ;
    dtd:has_DL dtd:DL-id9022 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Anker zu verwenden und Ankerwinden zu bedienen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7243 a dtd:entry ;
    dtd:has_DL dtd:DL-id9023 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der verschiedenen an Bord von Fahrzeugen eingesetzten Anker und Ankerwinden.2.Fähigkeit, die verschiedenen an Bord von Fahrzeugen eingesetzten Anker und Ankerwinden zu erkennen und zu benennen und ihren speziellen Einsatz zu erläutern.3.Fähigkeit, die verschiedenen Anker und Ankerwinden in verschiedenen Situationen und Bedingungen sicher einzusetzen."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7244 a dtd:entry ;
    dtd:has_DL dtd:DL-id9024 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "2.Deckausrüstung und Hebegeräte zu nutzen;"@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7245 a dtd:entry ;
    dtd:has_DL dtd:DL-id9025 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der auf Deck eines Fahrzeugs verwendeten Ausrüstung wie (Kupplungs-) Winden, Luken, Hebegeräte, Autokrane, Leitungssysteme, Feuerlöschschläuche usw.2.Fähigkeit, die Deckausrüstung und Hebegeräte zu erkennen und zu benennen und ihren speziellen Einsatz zu erläutern.3.Fähigkeit, die Deckausrüstung und Hebegeräte sicher einzusetzen."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7246 a dtd:entry ;
    dtd:has_DL dtd:DL-id9026 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "3.spezielle Ausrüstung für Fahrgastschiffe zu nutzen."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id7247 a dtd:entry ;
    dtd:has_DL dtd:DL-id9027 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "1.Kenntnis der speziellen Konstruktionsanforderungen, Ausrüstung und Geräte für Fahrgastschiffe.2.Fähigkeit, die ausschließlich an Bord von Fahrgastschiffen verwendete Ausrüstung zu erkennen und zu benennen und ihren speziellen Einsatz zu erläutern.3.Fähigkeit, die an Bord von Fahrgastschiffen verwendete Ausrüstung sicher einzusetzen."@de ;
    dtd:has_colname "col2"@de .

dtd:entry-id7248 a dtd:entry ;
    dtd:has_BR dtd:BR-id2011 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Spalte 1 Befähigung"@de ;
    dtd:has_align dtd:center ;
    dtd:has_colname "col1"@de ;
    dtd:has_valign dtd:middle .

dtd:entry-id7249 a dtd:entry ;
    dtd:has_BR dtd:BR-id2012 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Spalte 2 Kenntnisse und Fertigkeiten"@de ;
    dtd:has_align dtd:center ;

dtd:entry-id8691 a dtd:entry ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Anker können geworfen und eingeholt werden. Die Wassertiefe und die Dynamik der Kette werden berücksichtigt."@de ;

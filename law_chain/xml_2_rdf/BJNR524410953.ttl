dtd:LA-id147 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Verlangen des öffentlichen Auftraggebers bei mittelbaren Leistungen zu öffentlichen Aufträgen, soweit der mittelbare Auftragnehmer von diesem Verlangen vor oder bei Abschluß seines Vertrags Kenntnis erhalten hat oder nach Abschluß des Vertrags zustimmt,"@de .

dtd:LA-id148 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei den von deutschen Behörden angeordneten Leistungsauflagen und Leistungsanweisungen mit der Maßgabe, daß die nach dieser Verordnung zulässigen Preise nicht ohne Zustimmung des Auftragnehmers unterschritten werden dürfen."@de .

dtd:LA-id149 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Preise nach den §§ 3 und 4 nicht festgestellt werden können oder"@de .

dtd:LA-id150 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Mangellage vorliegt oder der Wettbewerb auf der Anbieterseite beschränkt ist und hierdurch die Preisbildung nach § 4 nicht nur unerheblich beeinflußt wird."@de .

dtd:LA-id151 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Bundesministerium für Wirtschaft und Energie, wenn die Mangellage oder die Wettbewerbsbeschränkung die Preisbildung in mehr als einem Land beeinflußt oder beeinflussen kann,"@de .

dtd:LA-id152 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für den Sitz des Auftragnehmers zuständige Preisbildungsstelle in allen übrigen Fällen."@de .

dtd:LA-id153 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Selbstkostenfestpreise oder Selbstkostenrichtpreise gemäß § 6,"@de .

dtd:LA-id154 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Selbstkostenerstattungspreise gemäß § 7."@de .


dtd:LA-id702 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sind Kinder Personen, die noch nicht 14 Jahre alt sind,"@de .

dtd:LA-id703 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sind Jugendliche Personen, die 14, aber noch nicht 18 Jahre alt sind,"@de .

dtd:LA-id704 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ist personensorgeberechtigte Person, wem allein oder gemeinsam mit einer anderen Person nach den Vorschriften des Bürgerlichen Gesetzbuchs die Personensorge zusteht,"@de .

dtd:LA-id705 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ist erziehungsbeauftragte Person, jede Person über 18 Jahren, soweit sie auf Dauer oder zeitweise aufgrund einer Vereinbarung mit der personensorgeberechtigten Person Erziehungsaufgaben wahrnimmt oder soweit sie ein Kind oder eine jugendliche Person im Rahmen der Ausbildung oder der Jugendhilfe betreut."@de .

dtd:LA-id706 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zum Verlassen des Ortes anzuhalten,"@de .

dtd:LA-id707 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der erziehungsberechtigten Person im Sinne des § 7 Abs. 1 Nr. 6 des Achten Buches Sozialgesetzbuch zuzuführen oder, wenn keine erziehungsberechtigte Person erreichbar ist, in die Obhut des Jugendamtes zu bringen."@de .

dtd:LA-id708 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bier, Wein, weinähnliche Getränke oder Schaumwein oder Mischungen von Bier, Wein, weinähnlichen Getränken oder Schaumwein mit nichtalkoholischen Getränken an Kinder und Jugendliche unter 16 Jahren,"@de .

dtd:LA-id709 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "andere alkoholische Getränke oder Lebensmittel, die andere alkoholische Getränke in nicht nur geringfügiger Menge enthalten, an Kinder und Jugendliche"@de .

dtd:LA-id710 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an einem für Kinder und Jugendliche unzugänglichen Ort aufgestellt ist oder"@de .

dtd:LA-id711 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in einem gewerblich genutzten Raum aufgestellt und durch technische Vorrichtungen oder durch ständige Aufsicht sichergestellt ist, dass Kinder und Jugendliche alkoholische Getränke nicht entnehmen können."@de .

dtd:LA-id712 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an einem Kindern und Jugendlichen unzugänglichen Ort aufgestellt ist oder"@de .

dtd:LA-id713 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch technische Vorrichtungen oder durch ständige Aufsicht sichergestellt ist, dass Kinder und Jugendliche Tabakwaren und andere nikotinhaltige Erzeugnisse und deren Behältnisse nicht entnehmen können."@de .

dtd:LA-id714 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Schutz vor Medien, die geeignet sind, die Entwicklung von Kindern oder Jugendlichen oder ihre Erziehung zu einer eigenverantwortlichen und gemeinschaftsfähigen Persönlichkeit zu beeinträchtigen (entwicklungsbeeinträchtigende Medien),"@de .

dtd:LA-id715 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Schutz vor Medien, die geeignet sind, die Entwicklung von Kindern oder Jugendlichen oder ihre Erziehung zu einer eigenverantwortlichen und gemeinschaftsfähigen Persönlichkeit zu gefährden (jugendgefährdende Medien),"@de .

dtd:LA-id716 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Schutz der persönlichen Integrität von Kindern und Jugendlichen bei der Mediennutzung und"@de .

dtd:LA-id717 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Förderung von Orientierung für Kinder, Jugendliche, personensorgeberechtigte Personen sowie pädagogische Fachkräfte bei der Mediennutzung und Medienerziehung; die Vorschriften des Achten Buches Sozialgesetzbuch bleiben unberührt."@de .

dtd:LA-id718 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kindern unter sechs Jahren,"@de .

dtd:LA-id719 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kindern ab sechs Jahren, wenn die Vorführung nach 20 Uhr beendet ist,"@de .

dtd:LA-id720 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Jugendlichen unter 16 Jahren, wenn die Vorführung nach 22 Uhr beendet ist,"@de .

dtd:LA-id721 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Jugendlichen ab 16 Jahren, wenn die Vorführung nach 24 Uhr beendet ist."@de .

dtd:LA-id722 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von der obersten Landesbehörde oder einer Organisation der freiwilligen Selbstkontrolle im Rahmen des Verfahrens nach § 14 Absatz 6 mit „Keine Jugendfreigabe“ nach § 14 Absatz 2 gekennzeichnet sind oder"@de .

dtd:LA-id723 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht nach den Vorschriften dieses Gesetzes gekennzeichnet sind."@de .

dtd:LA-id724 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Näheres über Inhalt, Größe, Form, Farbe und Anbringung der Zeichen anordnen und"@de .

dtd:LA-id725 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ausnahmen für die Anbringung auf dem Bildträger oder der Hülle genehmigen."@de .

dtd:LA-id726 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Kind oder einer jugendlichen Person nicht angeboten, überlassen oder sonst zugänglich gemacht werden,"@de .

dtd:LA-id727 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht im Einzelhandel außerhalb von Geschäftsräumen, in Kiosken oder anderen Verkaufsstellen, die Kunden nicht zu betreten pflegen, oder im Versandhandel angeboten oder überlassen werden."@de .

dtd:LA-id728 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Kindern oder Jugendlichen zugänglichen öffentlichen Verkehrsflächen,"@de .

dtd:LA-id729 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "außerhalb von gewerblich oder in sonstiger Weise beruflich oder geschäftlich genutzten Räumen oder"@de .

dtd:LA-id730 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in deren unbeaufsichtigten Zugängen, Vorräumen oder Fluren"@de .

dtd:LA-id731 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Kindern oder Jugendlichen zugänglichen öffentlichen Verkehrsflächen,"@de .

dtd:LA-id732 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "außerhalb von gewerblich oder in sonstiger Weise beruflich oder geschäftlich genutzten Räumen oder"@de .

dtd:LA-id733 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in deren unbeaufsichtigten Zugängen, Vorräumen oder Fluren"@de .

dtd:LA-id734 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "\"Freigegeben ohne Altersbeschränkung\","@de .

dtd:LA-id735 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "\"Freigegeben ab sechs Jahren\","@de .

dtd:LA-id736 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "\"Freigegeben ab zwölf Jahren\","@de .

dtd:LA-id737 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "\"Freigegeben ab sechzehn Jahren\","@de .

dtd:LA-id738 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "\"Keine Jugendfreigabe\"."@de .

dtd:LA-id739 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Rahmen des Verfahrens des § 14 Absatz 6 oder"@de .

dtd:LA-id740 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch eine nach § 19 des Jugendmedienschutz-Staatsvertrages anerkannte Einrichtung der freiwilligen Selbstkontrolle oder durch einen von einer Einrichtung der freiwilligen Selbstkontrolle zertifizierten Jugendschutzbeauftragten nach § 7 des Jugendmedienschutz-Staatsvertrages oder,"@de .

dtd:LA-id741 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn keine Kennzeichnung im Sinne der Nummer 1 oder 2 gegeben ist, durch ein von den obersten Landesbehörden anerkanntes automatisiertes Bewertungssystem einer im Rahmen einer Vereinbarung nach § 14 Absatz 6 tätigen Einrichtung der freiwilligen Selbstkontrolle"@de .

dtd:LA-id742 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Kind oder einer jugendlichen Person angeboten, überlassen oder sonst zugänglich gemacht werden,"@de .

dtd:LA-id743 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an einem Ort, der Kindern oder Jugendlichen zugänglich ist oder von ihnen eingesehen werden kann, ausgestellt, angeschlagen, vorgeführt oder sonst zugänglich gemacht werden,"@de .

dtd:LA-id744 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Einzelhandel außerhalb von Geschäftsräumen, in Kiosken oder anderen Verkaufsstellen, die Kunden nicht zu betreten pflegen, im Versandhandel oder in gewerblichen Leihbüchereien oder Lesezirkeln einer anderen Person angeboten oder überlassen werden,"@de .

dtd:LA-id745 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Wege gewerblicher Vermietung oder vergleichbarer gewerblicher Gewährung des Gebrauchs, ausgenommen in Ladengeschäften, die Kindern und Jugendlichen nicht zugänglich sind und von ihnen nicht eingesehen werden können, einer anderen Person angeboten oder überlassen werden,"@de .

dtd:LA-id746 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Wege des Versandhandels eingeführt werden,"@de .

dtd:LA-id747 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "öffentlich an einem Ort, der Kindern oder Jugendlichen zugänglich ist oder von ihnen eingesehen werden kann, oder durch Verbreiten von Trägermedien oder digitalen Diensten außerhalb des Geschäftsverkehrs mit dem einschlägigen Handel angeboten, angekündigt oder angepriesen werden,"@de .

dtd:LA-id748 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "hergestellt, bezogen, geliefert, vorrätig gehalten oder eingeführt werden, um sie oder aus ihnen gewonnene Stücke im Sinne der Nummern 1 bis 6 zu verwenden oder einer anderen Person eine solche Verwendung zu ermöglichen."@de .

dtd:LA-id749 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen der in § 86, § 130, § 130a, § 131, § 184, § 184a, 184b oder § 184c des Strafgesetzbuches bezeichneten Inhalte haben,"@de .

dtd:LA-id750 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Krieg verherrlichen,"@de .

dtd:LA-id751 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Menschen, die sterben oder schweren körperlichen oder seelischen Leiden ausgesetzt sind oder waren, in einer die Menschenwürde verletzenden Weise darstellen und ein tatsächliches Geschehen wiedergeben, ohne dass ein überwiegendes berechtigtes Interesse gerade an dieser Form der Berichterstattung vorliegt,"@de .

dtd:LA-id752 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "besonders realistische, grausame und reißerische Darstellungen selbstzweckhafter Gewalt beinhalten, die das Geschehen beherrschen,"@de .

dtd:LA-id753 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kinder oder Jugendliche in unnatürlicher, geschlechtsbetonter Körperhaltung darstellen oder"@de .

dtd:LA-id754 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "offensichtlich geeignet sind, die Entwicklung von Kindern oder Jugendlichen oder ihre Erziehung zu einer eigenverantwortlichen und gemeinschaftsfähigen Persönlichkeit schwer zu gefährden."@de .

dtd:LA-id755 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Förderung einer gemeinsamen Verantwortungsübernahme von Staat, Wirtschaft und Zivilgesellschaft zur Koordinierung einer Gesamtstrategie zur Verwirklichung der Schutzziele des § 10a,"@de .

dtd:LA-id756 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Nutzbarmachung und Weiterentwicklung der aus der Gesamtheit der Spruchpraxis der Prüfstelle abzuleitenden Erkenntnisse hinsichtlich durch Medien verursachter sozialethischer Desorientierung von Kindern und Jugendlichen, insbesondere durch Orientierungshilfen für Kinder und Jugendliche, personensorgeberechtigte Personen, Fachkräfte und durch Förderung öffentlicher Diskurse sowie"@de .

dtd:LA-id757 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein regelmäßiger Informationsaustausch mit den im Bereich des Kinder- und Jugendmedienschutzes tätigen Institutionen hinsichtlich der jeweiligen Spruchpraxis."@de .

dtd:LA-id758 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Gewalthandlungen wie Mord- und Metzelszenen selbstzweckhaft und detailliert dargestellt werden oder"@de .

dtd:LA-id759 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Selbstjustiz als einzig bewährtes Mittel zur Durchsetzung der vermeintlichen Gerechtigkeit nahe gelegt wird."@de .

dtd:LA-id760 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "allein wegen seines politischen, sozialen, religiösen oder weltanschaulichen Inhalts,"@de .

dtd:LA-id761 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn es der Kunst oder der Wissenschaft, der Forschung oder der Lehre dient,"@de .

dtd:LA-id762 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn es im öffentlichen Interesse liegt, es sei denn, dass die Art der Darstellung zu beanstanden ist."@de .

dtd:LA-id763 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der oder dem Vorsitzenden,"@de .

dtd:LA-id764 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "je einer oder einem von jeder Landesregierung zu ernennenden Beisitzerin oder Beisitzer und"@de .

dtd:LA-id765 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "weiteren von dem Bundesministerium für Familie, Senioren, Frauen und Jugend zu ernennenden Beisitzerinnen oder Beisitzern."@de .


    dtd:has_Value "CA-Administrator (CAA)"@de .

dtd:LA-id2323 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Key-Manager (KM)"@de .

dtd:LA-id2324 a dtd:LA ;

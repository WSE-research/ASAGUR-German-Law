dtd:LA-id2627 a dtd:LA ;
    dtd:has_Value "in einem anderen Mitgliedstaat der Europäischen Union oder"@de .

dtd:LA-id2628 a dtd:LA ;
    dtd:has_Value "in der Türkei"@de .

dtd:LA-id2629 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "straßenabhängige Bahnen (§ 4 Abs. 1 PBefG),"@de .

dtd:LA-id2630 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "unabhängige Bahnen (§ 4 Abs. 2 PBefG)."@de .

dtd:LA-id2631 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fahrbetrieb (Fahrbedienstete),"@de .

dtd:LA-id2632 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei der Steuerung und Überwachung des Betriebsablaufs,"@de .

dtd:LA-id2633 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Verantwortliche bei der Instandhaltung der Betriebsanlagen und Fahrzeuge,"@de .

dtd:LA-id2634 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Leitende oder Aufsichtführende über Beschäftigte nach den Nummern 1 bis 3."@de .

dtd:LA-id2635 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die bau-, maschinen- und elektrotechnischen Anlagen für den Fahrbetrieb, einschließlich der Hilfsbauwerke,"@de .

dtd:LA-id2636 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die für den Aufenthalt und die Abfertigung der Fahrgäste bestimmten Anlagen,"@de .

dtd:LA-id2637 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Abstellanlagen für Fahrzeuge,"@de .

dtd:LA-id2638 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die an das Gleisnetz angeschlossenen Werkstätten."@de .

dtd:LA-id2639 a dtd:LA ;
    dtd:has_Value "Personenfahrzeuge, die der Beförderung von Personen im Sinne des § 1 Absatz 1 des Personenbeförderungsgesetzes dienen und"@de .

dtd:LA-id2640 a dtd:LA ;
    dtd:has_Value "Betriebsfahrzeuge, die insbesondere für die Ausbildung von Betriebsbediensteten, für die Instandhaltung von Betriebsanlagen oder für Maßnahmen bei Betriebsstörungen und Unfällen eingesetzt werden."@de .

dtd:LA-id2641 a dtd:LA ;
    dtd:has_Value "wenn die Betriebsanlagen, Fahrzeuge oder Teile davon hinsichtlich ihrer Beschaffenheit den für sie unmittelbar geltenden Rechtsakten der Europäischen Gemeinschaft oder der Europäischen Union und den für sie geltenden Rechtsvorschriften, mit denen Richtlinien der Europäischen Gemeinschaft oder der Europäischen Union in deutsches Recht umgesetzt sind, entsprechen, oder"@de .

dtd:LA-id2642 a dtd:LA ;
    dtd:has_Value "soweit solche Rechtsvorschriften nicht bestehen, wenn die Betriebsanlagen, Fahrzeuge oder Teile davon nach den Vorschriften dieser Verordnung, nach den von der Technischen Aufsichtsbehörde und von der Genehmigungsbehörde getroffenen Anordnungen sowie nach den allgemein anerkannten Regeln der Technik gebaut sind und betrieben werden."@de .

dtd:LA-id2643 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die höchsten betrieblich auftretenden Beanspruchungen mechanischer, elektrischer und thermischer Art ohne Betriebsgefährdung aufgenommen werden können,"@de .

dtd:LA-id2644 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gefahrbringende Teile und Einrichtungen nicht unbeabsichtigt berührt werden können,"@de .

dtd:LA-id2645 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Entstehung und Ausbreitung von Bränden durch vorbeugende Maßnahmen erschwert werden und im Brandfall die Möglichkeit zur Selbst- oder Fremdrettung von Personen oder eine Kombination von beidem sowie zur Brandbekämpfung besteht,"@de .

dtd:LA-id2646 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Gleichstrombahnen mit Energieübertragung über Fahrschienen nachteilige Wirkungen der Streustromkorrosion gering sind,"@de .

dtd:LA-id2647 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bauteile und Einrichtungen gegen äußere Einflüsse geschützt sind, soweit es betrieblich erforderlich ist,"@de .

dtd:LA-id2648 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Bestehenbleiben zu hoher Berührungsspannungen durch Schutzmaßnahmen verhindert wird,"@de .

dtd:LA-id2649 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch elektrische Beeinflussungen die Betriebssicherheit nicht beeinträchtigt werden kann,"@de .

dtd:LA-id2650 a dtd:LA ;
    dtd:has_Value "Störungen im Betriebsablauf zügig beseitigt werden können."@de .

dtd:LA-id2651 a dtd:LA ;
    dtd:has_Value "die im Sechsten Abschnitt geregelten betrieblichen Sachverhalte,"@de .

dtd:LA-id2652 a dtd:LA ;
    dtd:has_Value "die mittels nachrichtentechnischer Anlagen nach § 23 geführte sicherheitsrelevante Kommunikation mit Betriebsstellen und"@de .

dtd:LA-id2653 a dtd:LA ;
    dtd:has_Value "die Fahrdaten der in § 33 Absatz 13 genannten Geräte,"@de .

dtd:LA-id2654 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ihm für die Beseitigung von Mängeln eine angemessene Frist setzen,"@de .

dtd:LA-id2655 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei unzureichender Sicherheit die Unterbrechung oder Einstellung von Bauarbeiten anordnen oder die Benutzung bestimmter Betriebsanlagen und Fahrzeuge beschränken oder untersagen."@de .

dtd:LA-id2656 a dtd:LA ;
    dtd:has_Value "keine die Betriebssicherheit einschränkenden Weisungen erhält und"@de .

dtd:LA-id2657 a dtd:LA ;
    dtd:has_Value "in Angelegenheiten, die die Sicherheit des Betriebes berühren, Weisungen gegenüber den Betriebsbediensteten und sonstigen im Betrieb Beschäftigten erteilen kann."@de .

dtd:LA-id2658 a dtd:LA ;
    dtd:has_Value "Planung und Bau von Betriebsanlagen,"@de .

dtd:LA-id2659 a dtd:LA ;
    dtd:has_Value "Beschaffung von Fahrzeugen,"@de .

dtd:LA-id2660 a dtd:LA ;
    dtd:has_Value "Feststellung des Bedarfs an Betriebsbediensteten,"@de .

dtd:LA-id2661 a dtd:LA ;
    dtd:has_Value "Auswahl, Aus- und Fortbildung, Verwendung und Beaufsichtigung der Betriebsbediensteten,"@de .

dtd:LA-id2662 a dtd:LA ;
    dtd:has_Value "Untersuchungen von Dienstverfehlungen der Betriebsbediensteten und den sich daraus ergebenden Maßnahmen,"@de .

dtd:LA-id2663 a dtd:LA ;
    dtd:has_Value "Vereinbarungen über die Übertragung von Aufgaben, die die Verantwortung des Betriebsleiters berühren, auf Personen oder Stellen, die dem Unternehmen nicht angehören."@de .

dtd:LA-id2664 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unfälle, bei denen ein Mensch getötet oder schwer verletzt worden ist oder Betriebsanlagen oder Fahrzeuge erheblich beschädigt worden sind,"@de .

dtd:LA-id2665 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebsvorkommnisse, die öffentliches Aufsehen erregen."@de .

dtd:LA-id2666 a dtd:LA ;
    dtd:has_Value "ihre Befähigung durch erfolgreichen Abschluss der Betriebsleiterprüfung nachgewiesen hat und"@de .

dtd:LA-id2667 a dtd:LA ;
    dtd:has_Value "keine Tatsachen vorliegen, die sie für die Tätigkeit eines Betriebsleiters als unzuverlässig erscheinen lassen."@de .

dtd:LA-id2668 a dtd:LA ;
    dtd:has_Value "die Große Staatsprüfung für den höheren technischen Verwaltungsdienst in einem Fachgebiet bestanden hat, zu dem in erheblichem Umfang Planung, Bau und Betrieb spurgebundener Bahnen gehören und"@de .

dtd:LA-id2669 a dtd:LA ;
    dtd:has_Value "mindestens drei Jahre in Straßenbahnunternehmen in den für den Bau und Betrieb der Straßenbahn wesentlichen Fachbereichen als Ingenieur tätig gewesen ist."@de .

dtd:LA-id2670 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Lebenslauf mit Lichtbild,"@de .

dtd:LA-id2671 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Nachweis, dass ein Führungszeugnis nach § 30 des Bundeszentralregistergesetzes zur Vorlage bei der zuständigen Behörde beantragt ist,"@de .

dtd:LA-id2672 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Zeugnis über die bestandene Betriebsleiterprüfung oder in Fällen nach Absatz 2 das Zeugnis über die bestandene Große Staatsprüfung und Nachweise über die Tätigkeit in Straßenbahnunternehmen."@de .

dtd:LA-id2673 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "mindestens 18 Jahre alt ist,"@de .

dtd:LA-id2674 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "geistig und körperlich tauglich ist und"@de .

dtd:LA-id2675 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nicht durch Tatsachen belastet ist, die ihn für die Tätigkeit als unzuverlässig erscheinen lassen."@de .

dtd:LA-id2676 a dtd:LA ;
    dtd:has_Value "Auszubildende und Absolventen des staatlich anerkannten Ausbildungsberufs der „Fachkraft im Fahrbetrieb“, die das 18. Lebensjahr vollendet haben und die Schienenfahrerlaubnis und seit mindestens einem Jahr die straßenverkehrsrechtliche Fahrerlaubnis der Klasse B besitzen und"@de .

dtd:LA-id2677 a dtd:LA ;
    dtd:has_Value "Zugabfertiger und Fahrbedienstete, die Fahrzeuge ausschließlich in Abstellanlagen und Werkstätten bedienen."@de .

dtd:LA-id2678 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Züge auch unter ungünstigen Betriebsverhältnissen sicher zum Halten gebracht werden können,"@de .

dtd:LA-id2679 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein liegengebliebener Zug von einem anderen fortbewegt werden kann."@de .

dtd:LA-id2680 a dtd:LA ;
    dtd:has_Value "Lichtzeichen mit der Farbfolge Gelb – Rot nach Anlage 1 Bild 2, die mit Halbschranken nach Anlage 1 Bild 3 verbunden sein können und"@de .

dtd:LA-id2681 a dtd:LA ;
    dtd:has_Value "Überwachungssignale Bü 0 und Bü 1 nach Anlage 4 vor dem Bahnübergang oder eine in Zugsicherungsanlagen eingebundene Überwachung der Einrichtungen nach Nummer 1."@de .

dtd:LA-id2682 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fahrzeugführer Aufträge erhalten sollen, die von den Anordnungen der Wechsellichtzeichen des Straßenverkehrs abweichen,"@de .

dtd:LA-id2683 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eingleisige Streckenabschnitte im Zweirichtungsbetrieb befahren werden; dabei müssen die Fahrsignale so geschaltet sein, daß der Abschnitt jeweils nur für eine Richtung freigegeben und die freigegebene Richtung nur bei unbesetztem Abschnitt gewechselt werden kann."@de .

dtd:LA-id2684 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Fahrwege einzustellen und zu sichern,"@de .

dtd:LA-id2685 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Zügen Aufträge über die Fahrweise zu übermitteln,"@de .

dtd:LA-id2686 a dtd:LA ;
    dtd:has_Size dtd:normal ;

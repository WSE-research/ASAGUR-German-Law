dtd:LA-id1401 a dtd:LA ;
    dtd:has_SP dtd:SP-id374 ;
    dtd:has_Value "Gesamtkeimzahl"@de .

dtd:LA-id1402 a dtd:LA ;
    dtd:has_Value "Zur Feststellung der Gesamtkeimzahl sind je Kalendermonat mindestens zwei Proben nach Verfahren zu untersuchen, die in Anlage 3 Abschnitt B festgelegt sind."@de .

dtd:LA-id1403 a dtd:LA ;
    dtd:has_SP dtd:SP-id375 ;
    dtd:has_Value "Hemmstoffe"@de .

dtd:LA-id1404 a dtd:LA ;
    dtd:has_DL dtd:DL-id1023 ;
    dtd:has_Value "Zur Feststellung von Hemmstoffen sind mindestens zu untersuchen: 1.je Kalendermonat hinsichtlich der Hemmstoffgruppen Penicilline, Cephalosporine, Aminoglykoside, Makrolide und Lincosamide, Sulfonamide sowie Tetracyline vier Proben nach Verfahren, die in Anlage 3 Abschnitt C Unterabschnitt II oder III festgelegt sind, und2.je Kalenderjahr hinsichtlich der Hemmstoffgruppe Chinolone zwei Proben nach Verfahren, die in Anlage 3 Abschnitt C Unterabschnitt II Nummer 3 und Unterabschnitt III festgelegt sind."@de .

dtd:LA-id1405 a dtd:LA ;
    dtd:has_Value "je Kalendermonat hinsichtlich der Hemmstoffgruppen Penicilline, Cephalosporine, Aminoglykoside, Makrolide und Lincosamide, Sulfonamide sowie Tetracyline vier Proben nach Verfahren, die in Anlage 3 Abschnitt C Unterabschnitt II oder III festgelegt sind, und"@de .

dtd:LA-id1406 a dtd:LA ;
    dtd:has_Value "je Kalenderjahr hinsichtlich der Hemmstoffgruppe Chinolone zwei Proben nach Verfahren, die in Anlage 3 Abschnitt C Unterabschnitt II Nummer 3 und Unterabschnitt III festgelegt sind."@de .

dtd:LA-id1407 a dtd:LA ;
    dtd:has_SP dtd:SP-id376 ;
    dtd:has_Value "Gehalt an somatischen Zellen"@de .

dtd:LA-id1408 a dtd:LA ;
    dtd:has_Value "Zur Feststellung der somatischen Zellzahl ist je Kalendermonat mindestens eine Probe nach Verfahren zu untersuchen, die in Anlage 3 Abschnitt D Unterabschnitt I festgelegt sind."@de .

dtd:LA-id1409 a dtd:LA ;
    dtd:has_SP dtd:SP-id377 ;
    dtd:has_Value "Gefrierpunkt"@de .

dtd:LA-id1410 a dtd:LA ;
    dtd:has_Value "Zur Feststellung des Gefrierpunktes ist je Kalendermonat mindestens eine Probe nach Verfahren zu untersuchen, die in Anlage 3 Abschnitt A Unterabschnitt I Nummer 4 oder 5 festgelegt sind."@de .


    dtd:has_Value "den Antrag auf Vorlage eines Führungszeugnisses oder eines Europäischen Führungszeugnisses beim Bundesamt für Logistik und Mobilität und"@de .

dtd:LA-id290 a dtd:LA ;
    dtd:has_Size dtd:normal ;

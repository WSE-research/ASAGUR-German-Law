    dtd:has_Pos dtd:exp ;
    dtd:has_Postfix ")"@de ;
    dtd:has_Value "Artikel 6 Nr. 1 der Verordnung vom 28. Juni 2006 (BGBl. I S. 1417)."@de .

dtd:Footnote-id4316 a dtd:Footnote ;
    dtd:has_FnZ "*"@de ;
    dtd:has_Group dtd:column ;
    dtd:has_ID "bjnr286010998bjne001635308_00_BJNR286010998BJNE001663123"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Postfix ")"@de ;
    dtd:has_Value "Eine deutsche Übersetzung der in Kapitel IV der Anlage zum STCW-Übereinkommen genannten Vorschriften der Vollzugsverordnung für den Funkdienst (Radio Regulations) der Internationalen Fernmeldeunion nach dem Stand vom 1. September 2001, soweit sie den mobilen Seefunkdienst und den mobilen Seefunkdienst über Satelliten betreffen, kann beim Bundesministerium für Verkehr und digitale Infrastruktur, Robert-Schuman-Platz 1, 53175 Bonn, bezogen werden."@de .

dtd:Footnote-id4317 a dtd:Footnote ;
    dtd:has_FnZ "3"@de ;
    dtd:has_Group dtd:column ;
    dtd:has_ID "bjnr26010998bjne001635308_03_BJNR286010998BJNE001663123"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Postfix ")"@de ;
    dtd:has_Value "Die in Abschnitt D Nr. 12.1 genannte Richtlinie gilt für Norwegen eingeschränkt, vgl. ABl. EU 2004 Nr. L 88 S. 55."@de .

dtd:Footnote-id4318 a dtd:Footnote ;
    dtd:has_FnZ "4"@de ;
    dtd:has_Group dtd:column ;
    dtd:has_ID "bjnr286010998bjne001635308_04_BJNR286010998BJNE001663123"@de ;
    dtd:has_Pos dtd:exp ;
    dtd:has_Value "Das Bundesministerium für Verkehr-, Bau- und Wohnungswesen hat den Wortlaut der in diesem Abschnitt aufgeführten Regelungen, soweit sie vor dem 1. Januar 2001 in Kraft getreten sind, im Verkehrsblatt 2001 S. 313, Anlagenband B 8126 zusammenfassend veröffentlicht."@de .

dtd:Footnote-id4319 a dtd:Footnote ;
    dtd:has_FnZ "*"@de ;
    dtd:has_Group dtd:column ;
    dtd:has_ID "F805382_01_BJNR286010998BJNE001663123"@de ;
    dtd:has_Pos dtd:normal ;
    dtd:has_Value "Artikel 3 der Richtlinie verweist zusätzlich auf das Protokoll vom 2. April 1993 zu dem Internationalen Übereinkommen von Torremolinos über die Sicherheit von Fischereifahrzeugen von 1997 („Torremolinos-Protokoll“) (VkBl. 1999 S. 142, Anlagenband B 8319)."@de .


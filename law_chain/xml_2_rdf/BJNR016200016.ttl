dtd:entry-id369 a dtd:entry ;
    dtd:has_B dtd:B-id169 ;
    dtd:has_BR dtd:BR-id145 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Musterfoto Qualitativ hochwertige Fotos sind die Grundlage einer einwandfreien Wiedergabe des Bildes und Voraussetzung für die Anwendung der Gesichtsbiometrie in Ankunftsnachweisen. Dieser Foto-Mustertafel sind die Qualitätsmerkmale zu entnehmen, die die Eignung der Fotos für den vorgesehenen Einsatz in Ankunftsnachweisen gewährleisten. Es ist dringend erforderlich, die hier beschriebenen Anforderungen zu beachten, da sonst eine biometrische Erkennung des Asyl- oder Schutzsuchenden sowie die einwandfreie Wiedergabe des Bildes im Dokument nicht gewährleistet sind. Die Person ist grundsätzlich ohne Kopfbedeckung abzubilden. Die Aufnahmeeinrichtung oder die Außenstelle des Bundesamtes kann vom Gebot der fehlenden Kopfbedeckung insbesondere aus religiösen Gründen, von den übrigen Anforderungen aus medizinischen Gründen, die nicht nur vorübergehender Art sind, Ausnahmen zulassen. Auf den Fotos sind keine Uniformteile abzubilden."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id370 a dtd:entry ;
    dtd:has_IMG dtd:IMG-id408 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value ""@de ;
    dtd:has_colname "col2"@de ;
    dtd:has_valign dtd:top .

dtd:entry-id371 a dtd:entry ;
    dtd:has_B dtd:B-id170 ;
    dtd:has_BR dtd:BR-id146 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Format Das Foto muss die Gesichtszüge der Person von der Kinnspitze bis zum oberen Kopfende, sowie die linke und rechte Gesichtshälfte deutlich zeigen. Die Gesichtshöhe muss 70 bis 80 Prozent des Fotos einnehmen. Dies entspricht einer Höhe von 32 bis 36 mm von der Kinnspitze bis zum oberen Kopfende. Dabei ist das obere Kopfende unter Vernachlässigung der Frisur anzunehmen. Wegen des häufig nicht eindeutig zu bestimmenden oberen Kopfendes sind Lichtbilder jedoch erst dann abzulehnen, wenn die Gesichtshöhe 27 mm unterschreitet oder 40 mm überschreitet. Bei volumenreichem Haar sollte darauf geachtet werden, dass der Kopf (einschl.Frisur) möglichst vollständig abgebildet ist, ohne aber die Gesichtsgröße zu verkleinern. Das Gesicht muss zentriert auf dem Foto platziert sein."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id372 a dtd:entry ;
    dtd:has_IMG dtd:IMG-id409 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value ""@de ;
    dtd:has_colname "col2"@de ;
    dtd:has_valign dtd:top .

dtd:entry-id373 a dtd:entry ;
    dtd:has_B dtd:B-id171 ;
    dtd:has_BR dtd:BR-id147 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Schärfe und Kontrast Das Gesicht muss in allen Bereichen scharf abgebildet, kontrastreich und klar sein."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id374 a dtd:entry ;
    dtd:has_IMG dtd:IMG-id410 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value ""@de ;
    dtd:has_colname "col2"@de ;
    dtd:has_valign dtd:top .

dtd:entry-id375 a dtd:entry ;
    dtd:has_B dtd:B-id172 ;
    dtd:has_BR dtd:BR-id148 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Ausleuchtung Das Gesicht muss gleichmäßig ausgeleuchtet werden. Reflexionen oder Schatten im Gesicht sowie rote Augen sind zu vermeiden."@de ;
    dtd:has_colname "col1"@de .

dtd:entry-id376 a dtd:entry ;
    dtd:has_IMG dtd:IMG-id411 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value ""@de ;
    dtd:has_colname "col2"@de ;
    dtd:has_valign dtd:top .

dtd:entry-id377 a dtd:entry ;
    dtd:has_B dtd:B-id173 ;
    dtd:has_BR dtd:BR-id149 ;
    dtd:has_VJ "1"@de ;
    dtd:has_Value "Hintergrund Der Hintergrund muss einfarbig hell sein (idealerweise neutral grau) und einen Kontrast zum Gesicht und zu den Haaren aufweisen. Bei hellen Haaren eignet sich ein mittelgrauer Hintergrund, bei dunklen Haaren ein hellgrauer. Der Hintergrund darf kein Muster aufweisen. Das Foto darf ausschließlich die zu fotografierende Person zeigen (keine weiteren Personen oder Gegenstände im Bild). Auf dem Hintergrund dürfen keine Schatten entstehen."@de ;
    dtd:has_colname "col1"@de .


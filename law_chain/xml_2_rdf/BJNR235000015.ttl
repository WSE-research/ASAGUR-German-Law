dtd:LA-id512 a dtd:LA ;
    dtd:has_Value "ein Krankenhaus oder Teile von akutstationären Versorgungseinrichtungen eines Krankenhauses dauerhaft geschlossen werden, insbesondere wenn ein Standort, eine unselbständige Betriebsstätte oder eine Fachrichtung, mindestens aber eine Abteilung eines Krankenhauses geschlossen wird,"@de .

dtd:LA-id513 a dtd:LA ;
    dtd:has_Value "akutstationäre Versorgungskapazitäten, insbesondere Fachrichtungen mehrerer Krankenhäuser, standortübergreifend konzentriert werden, soweit in den beteiligten Krankenhäusern jeweils mindestens eine Abteilung betroffen ist und das Vorhaben insgesamt zu einem Abbau von Versorgungskapazitäten oder zur Verminderung von Vorhaltungsaufwand führt, oder"@de .

dtd:LA-id514 a dtd:LA ;
    dtd:has_DL dtd:DL-id252 ;
    dtd:has_Value "ein Krankenhaus oder Teile von akutstationären Versorgungseinrichtungen eines Krankenhauses, insbesondere ein Standort, eine unselbständige Betriebsstätte oder eine Fachrichtung, mindestens aber eine Abteilung eines Krankenhauses, umgewandelt werden in a)eine bedarfsnotwendige andere Fachrichtung oderb)eine nicht akutstationäre Versorgungseinrichtung, insbesondere in eine Einrichtung der ambulanten, der sektorenübergreifenden oder der palliativen Versorgung, in eine stationäre Pflegeeinrichtung oder in eine Einrichtung der stationären Rehabilitation."@de .

dtd:LA-id515 a dtd:LA ;
    dtd:has_Value "eine bedarfsnotwendige andere Fachrichtung oder"@de .

dtd:LA-id516 a dtd:LA ;
    dtd:has_Value "eine nicht akutstationäre Versorgungseinrichtung, insbesondere in eine Einrichtung der ambulanten, der sektorenübergreifenden oder der palliativen Versorgung, in eine stationäre Pflegeeinrichtung oder in eine Einrichtung der stationären Rehabilitation."@de .

dtd:LA-id517 a dtd:LA ;
    dtd:has_Value "bei Vorhaben nach § 1 Absatz 1 Nummer 1 die Kosten der Schließung eines Krankenhauses oder von Teilen des Krankenhauses,"@de .

dtd:LA-id518 a dtd:LA ;
    dtd:has_Value "bei Vorhaben nach § 1 Absatz 1 Nummer 2 und 3 die Kosten der Schließung eines Krankenhauses oder von Teilen des Krankenhauses sowie die Kosten für die erforderlichen Baumaßnahmen."@de .

dtd:LA-id519 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Beschreibung des Vorhabens, aus der sich der Träger, der voraussichtliche Beginn und das voraussichtliche Ende des Vorhabens sowie die voraussichtliche Höhe des Investitionsvolumens ergeben,"@de .

dtd:LA-id520 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erklärung, aus der sich die voraussichtliche Höhe der förderungsfähigen Kosten, der Finanzierungsanteil des Landes und gegebenenfalls die Finanzierungsbeiträge Dritter ergeben,"@de .

dtd:LA-id521 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Erklärung zur Verpflichtung, die Voraussetzungen des § 12 Absatz 2 Satz 1 Nummer 3 des Krankenhausfinanzierungsgesetzes einzuhalten,"@de .

dtd:LA-id522 a dtd:LA ;
    dtd:has_DL dtd:DL-id277 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Vorhaben nach § 1 Absatz 1 Nummer 1 die Bestätigung, dass a)die stillgelegte Versorgungsfunktion durch Krankenhäuser in erreichbarer Nähe sichergestellt wird undb)der betroffene Krankenhausträger gegenüber dem antragstellenden Land auf Grund der Schließung nicht zur Rückzahlung von Mitteln für die Investitionsfinanzierung verpflichtet ist,"@de .

dtd:LA-id523 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die stillgelegte Versorgungsfunktion durch Krankenhäuser in erreichbarer Nähe sichergestellt wird und"@de .

dtd:LA-id524 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der betroffene Krankenhausträger gegenüber dem antragstellenden Land auf Grund der Schließung nicht zur Rückzahlung von Mitteln für die Investitionsfinanzierung verpflichtet ist,"@de .

dtd:LA-id525 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Vorhaben nach § 1 Absatz 1 Nummer 3 Buchstabe b die Erklärung, dass die mit der Umwandlung beabsichtigte Nachfolgenutzung in Übereinstimmung mit den maßgeblichen rechtlichen Vorgaben steht,"@de .

dtd:LA-id526 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Berechnung des Barwerts nach § 2 Absatz 3 einschließlich einer Erläuterung der zu Grunde gelegten versicherungsmathematischen Annahmen, wenn ein förderungsfähiges Vorhaben durch Aufnahme eines Darlehens des Krankenhausträgers finanziert werden soll, und"@de .

dtd:LA-id527 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Nachweis, dass mit den Landesverbänden der Krankenkassen und den Ersatzkassen das Einvernehmen über die Förderung des Vorhabens und die Beantragung von Mitteln aus dem Strukturfonds herbeigeführt worden ist."@de .

dtd:LA-id528 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Stand der Umsetzung und den voraussichtlichen Abschluss des Vorhabens,"@de .

dtd:LA-id529 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Zwischenergebnisse über die Prüfung der zweckentsprechenden Verwendung der Mittel oder die begründete Erklärung, dass eine entsprechende Zwischenprüfung nicht erfolgt,"@de .

dtd:LA-id530 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Angaben über die Höhe der ausgezahlten Mittel,"@de .

dtd:LA-id531 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "aussagekräftige Unterlagen, aus denen sich ergibt, dass die Bestimmungen des § 12 Absatz 2 Satz 1 und 2 des Krankenhausfinanzierungsgesetzes, insbesondere die Verpflichtungen nach § 12 Absatz 2 Satz 1 Nummer 3 des Krankenhausfinanzierungsgesetzes eingehalten worden sind."@de .

dtd:LA-id532 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Krankenhaus oder Teile von akutstationären Versorgungseinrichtungen eines Krankenhauses dauerhaft geschlossen werden, insbesondere wenn ein Standort, eine unselbständige Betriebsstätte oder eine Fachrichtung eines Krankenhauses geschlossen wird,"@de .

dtd:LA-id533 a dtd:LA ;
    dtd:has_DL dtd:DL-id304 ;
    dtd:has_Size dtd:normal ;

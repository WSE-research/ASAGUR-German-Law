dtd:LA-id563 a dtd:LA ;
    dtd:has_DL dtd:DL-id381 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei allen Vorhaben nach § 11 Absatz 1 Nummer 2 die Bestätigung, a)dass die Konzentration von akutstationären Versorgungskapazitäten wettbewerbsrechtlich zulässig ist,b)der betroffene Krankenhausträger gegenüber dem antragstellenden Land auf Grund des Vorhabens nicht zur Rückzahlung von Mitteln für die Investitionsfinanzierung verpflichtet ist,"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_Size dtd:normal ;

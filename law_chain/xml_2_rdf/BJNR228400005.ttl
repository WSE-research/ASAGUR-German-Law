dtd:LA-id196 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine gemäß § 2 anerkannte Prüfung an einer Ausbildungsstätte oder vor einer Prüfungsbehörde oder eine Abschlussprüfung an einer staatlichen oder staatlich anerkannten Schule in einer dem Ausbildungsberuf entsprechenden Fachrichtung bestanden haben, oder"@de .

dtd:LA-id197 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Abschlussprüfung an einer deutschen Hochschule in einer dem Ausbildungsberuf entsprechenden Fachrichtung bestanden haben"@de .

dtd:LA-id198 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Meisterprüfungen gemäß der Anlage 1,"@de .


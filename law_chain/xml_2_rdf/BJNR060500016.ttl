dtd:LA-id542 a dtd:LA ;
    dtd:has_Value "Gebäude und Bauten dauerhaft bedienen und"@de .

dtd:LA-id543 a dtd:LA ;
    dtd:has_Value "bestimmt sind zur Personenbeförderung, zur Personen- und Güterbeförderung oder nur zur Güterbeförderung."@de .

dtd:LA-id544 a dtd:LA ;
    dtd:has_Value "Aufzüge, die speziell für militärische Zwecke oder zur Aufrechterhaltung der öffentlichen Ordnung konzipiert und gebaut sind,"@de .

dtd:LA-id545 a dtd:LA ;
    dtd:has_Value "Baustellenaufzüge,"@de .

dtd:LA-id546 a dtd:LA ;
    dtd:has_Value "Fahrtreppen und Fahrsteige,"@de .

dtd:LA-id547 a dtd:LA ;
    dtd:has_Value "Hebezeuge, die in Beförderungsmitteln eingebaut sind,"@de .

dtd:LA-id548 a dtd:LA ;
    dtd:has_Value "Hebezeuge, die mit einer Maschine verbunden sind und ausschließlich für den Zugang zu Arbeitsplätzen, einschließlich Wartungs- und Inspektionspunkten an Maschinen, bestimmt sind,"@de .

dtd:LA-id549 a dtd:LA ;
    dtd:has_Value "Hebezeuge mit einer Fahrgeschwindigkeit von bis zu 0,15 Metern pro Sekunde,"@de .

dtd:LA-id550 a dtd:LA ;
    dtd:has_Value "Hebezeuge, von denen aus Arbeiten durchgeführt werden können,"@de .

dtd:LA-id551 a dtd:LA ;
    dtd:has_Value "Hebezeuge zur Beförderung von Darstellern bei künstlerischen Vorführungen,"@de .

dtd:LA-id552 a dtd:LA ;
    dtd:has_Value "Schachtförderanlagen,"@de .

dtd:LA-id553 a dtd:LA ;
    dtd:has_Value "seilgeführte Einrichtungen einschließlich Seilbahnen,"@de .

dtd:LA-id554 a dtd:LA ;
    dtd:has_Value "Zahnradbahnen."@de .

dtd:LA-id555 a dtd:LA ;
    dtd:has_DL dtd:DL-id433 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Aufzug: a)ein Hebezeug, das zwischen festgelegten Ebenen mittels eines Lastträgers verkehrt, der sich an starren, gegenüber der Horizontalen um mehr als 15 Grad geneigten Führungen entlang bewegt, oderb)eine Hebeeinrichtung, die sich nicht zwingend an starren Führungen entlang, jedoch in einer räumlich vollständig festgelegten Bahn bewegt,"@de .

dtd:LA-id556 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein Hebezeug, das zwischen festgelegten Ebenen mittels eines Lastträgers verkehrt, der sich an starren, gegenüber der Horizontalen um mehr als 15 Grad geneigten Führungen entlang bewegt, oder"@de .

dtd:LA-id557 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Hebeeinrichtung, die sich nicht zwingend an starren Führungen entlang, jedoch in einer räumlich vollständig festgelegten Bahn bewegt,"@de .

dtd:LA-id558 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bereitstellung auf dem Markt: jede entgeltliche oder unentgeltliche Abgabe eines Sicherheitsbauteils für Aufzüge zum Vertrieb oder zur Verwendung auf dem Markt der Europäischen Union im Rahmen einer Geschäftstätigkeit,"@de .

dtd:LA-id559 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bevollmächtigter: jede im Europäischen Wirtschaftsraum ansässige natürliche oder juristische Person, die von einem Montagebetrieb oder einem Hersteller schriftlich beauftragt wurde, in seinem Namen bestimmte Aufgaben wahrzunehmen, um seine Verpflichtungen nach der einschlägigen Gesetzgebung der Europäischen Union zu erfüllen,"@de .

dtd:LA-id560 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "CE-Kennzeichnung: Kennzeichnung, durch die der Montagebetrieb oder der Hersteller erklärt, dass der Aufzug oder das Sicherheitsbauteil für Aufzüge den anwendbaren Anforderungen genügt, die in den Harmonisierungsrechtsvorschriften der Europäischen Union über ihre Anbringung festgelegt sind,"@de .

dtd:LA-id561 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "EU-Konformitätserklärung: eine Erklärung gemäß Artikel 17 der Richtlinie 2014/33/EU,"@de .

dtd:LA-id562 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "harmonisierte Norm: eine harmonisierte Norm im Sinne des Artikels 2 Nummer 1 Buchstabe c der Verordnung (EU) Nr. 1025/2012 des Europäischen Parlaments und des Rates vom 25. Oktober 2012 zur europäischen Normung, zur Änderung der Richtlinien 89/686/EWG und 93/15/EWG des Rates sowie der Richtlinien 94/9/EG, 94/25/EG, 95/16/EG, 97/23/EG, 98/34/EG, 2004/22/EG, 2007/23/EG, 2009/23/EG und 2009/105/EG des Europäischen Parlaments und des Rates und zur Aufhebung des Beschlusses 87/95/EWG des Rates und des Beschlusses Nr. 1673/2006/EG des Europäischen Parlaments und des Rates (ABl. L 316 vom 14.11.2012, S. 12),"@de .

dtd:LA-id563 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Hersteller: jede natürliche oder juristische Person, die ein Sicherheitsbauteil für Aufzüge herstellt oder entwickeln oder herstellen lässt und dieses Sicherheitsbauteil für Aufzüge unter ihrem eigenen Namen oder ihrer eigenen Handelsmarke vermarktet,"@de .

dtd:LA-id564 a dtd:LA ;
    dtd:has_DL dtd:DL-id438 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Inverkehrbringen: a)die erstmalige Bereitstellung eines Sicherheitsbauteils für Aufzüge auf dem Markt oderb)die entgeltliche oder unentgeltliche Abgabe eines Aufzugs zur Verwendung auf dem Markt der Europäischen Union im Rahmen einer Geschäftstätigkeit,"@de .

dtd:LA-id565 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die erstmalige Bereitstellung eines Sicherheitsbauteils für Aufzüge auf dem Markt oder"@de .

dtd:LA-id566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die entgeltliche oder unentgeltliche Abgabe eines Aufzugs zur Verwendung auf dem Markt der Europäischen Union im Rahmen einer Geschäftstätigkeit,"@de .

dtd:LA-id567 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Lastträger: der Teil des Aufzugs, in dem Personen oder Güter oder Personen und Güter zur Aufwärts- oder Abwärtsbeförderung untergebracht sind,"@de .

dtd:LA-id568 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Montagebetrieb: jede natürliche oder juristische Person, die die Verantwortung für den Entwurf, die Herstellung, den Einbau und das Inverkehrbringen eines Aufzugs übernimmt,"@de .

dtd:LA-id569 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Musteraufzug: ein repräsentativer, mit Hilfe objektiver Parameter definierter Aufzug, dessen technische Unterlagen verdeutlichen, wie die von ihm abgeleiteten Aufzüge, die identische Sicherheitsbauteile für Aufzüge verwenden, die in Anhang I der Richtlinie 2014/33/EU festgelegten wesentlichen Gesundheitsschutz- und Sicherheitsanforderungen einhalten,"@de .

dtd:LA-id570 a dtd:LA ;
    dtd:has_DL dtd:DL-id443 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Rückruf: jede Maßnahme, die a)auf die Demontage und unbedenkliche Entsorgung eines Aufzugs abzielt oderb)auf die Rückgabe eines Sicherheitsbauteils für Aufzüge abzielt, das dem Montagebetrieb oder dem Endnutzer bereits bereitgestellt worden ist,"@de .

dtd:LA-id571 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf die Demontage und unbedenkliche Entsorgung eines Aufzugs abzielt oder"@de .

dtd:LA-id572 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf die Rückgabe eines Sicherheitsbauteils für Aufzüge abzielt, das dem Montagebetrieb oder dem Endnutzer bereits bereitgestellt worden ist,"@de .

dtd:LA-id573 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Sicherheitsbauteile für Aufzüge: Bauteile, die in Aufzügen im Sinne dieser Verordnung verwendet werden und in Anhang III der Richtlinie 2014/33/EU aufgeführt sind,"@de .

dtd:LA-id574 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "technische Spezifikation: ein Dokument, in dem die technischen Anforderungen vorgeschrieben sind, denen ein Aufzug oder ein Sicherheitsbauteil für Aufzüge genügen muss,"@de .

dtd:LA-id575 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Wirtschaftsakteure: der Montagebetrieb, der Hersteller, der Bevollmächtigte, der Einführer und der Händler."@de .

dtd:LA-id576 a dtd:LA ;
    dtd:has_Value "sie die Anforderungen dieser Verordnung erfüllen sowie sachgemäß eingebaut, sachgemäß instand gehalten und bestimmungsgemäß betrieben werden,"@de .

dtd:LA-id577 a dtd:LA ;
    dtd:has_Value "die für die Errichtung des Gebäudes oder Bauwerks verantwortliche Person und der Montagebetrieb einander alle notwendigen Informationen zur Verfügung gestellt und die geeigneten Maßnahmen getroffen haben, um den einwandfreien Betrieb und die sichere Benutzung des Aufzugs zu gewährleisten, und"@de .

dtd:LA-id578 a dtd:LA ;
    dtd:has_Value "neben den für die Sicherheit und den Betrieb des Aufzugs erforderlichen Leitungen oder Einrichtungen keine weiteren Leitungen oder Einrichtungen im Aufzugsschacht verlegt oder installiert sind."@de .

dtd:LA-id579 a dtd:LA ;
    dtd:has_Value "nach den wesentlichen Gesundheitsschutz- und Sicherheitsanforderungen nach Anhang I der Richtlinie 2014/33/EU entworfen und hergestellt wurden und"@de .

dtd:LA-id580 a dtd:LA ;
    dtd:has_Value "es ermöglichen, dass die Aufzüge, in die sie eingebaut werden, die wesentlichen Gesundheitsschutz- und Sicherheitsanforderungen nach Anhang I der Richtlinie 2014/33/EU erfüllen."@de .

dtd:LA-id581 a dtd:LA ;
    dtd:has_Value "die Pflicht, die technischen Unterlagen sowie die EU-Konformitätserklärung und gegebenenfalls die Zulassungen des Qualitätssicherungssystems nach § 5 Absatz 3 oder nach § 7 Absatz 3 bereitzuhalten,"@de .

dtd:LA-id582 a dtd:LA ;
    dtd:has_Value "die Pflicht, der Marktüberwachungsbehörde die Informationen und Unterlagen nach § 6 Absatz 5 oder nach § 8 Absatz 5 zur Verfügung zu stellen, und"@de .

dtd:LA-id583 a dtd:LA ;
    dtd:has_Value "die Pflicht, mit der Marktüberwachungsbehörde auf deren Verlangen bei allen Maßnahmen zur Abwendung der Risiken, die mit den Aufzügen oder den Sicherheitsbauteilen für Aufzüge verbunden sind, die zum Aufgabenbereich des Bevollmächtigten gehören, zusammenzuarbeiten."@de .

dtd:LA-id584 a dtd:LA ;
    dtd:has_Value "der Hersteller das Konformitätsbewertungsverfahren nach § 16 durchgeführt hat,"@de .

dtd:LA-id585 a dtd:LA ;
    dtd:has_Value "der Hersteller die technischen Unterlagen erstellt hat,"@de .

dtd:LA-id586 a dtd:LA ;
    dtd:has_Value "das Sicherheitsbauteil für Aufzüge mit der CE-Kennzeichnung versehen ist und ihm die EU-Konformitätserklärung beigefügt ist,"@de .

dtd:LA-id587 a dtd:LA ;
    dtd:has_Value "dem Sicherheitsbauteil für Aufzüge die Betriebsanleitung nach Anhang I Nummer 6.1 der Richtlinie 2014/33/EU in deutscher Sprache beigefügt ist und"@de .

dtd:LA-id588 a dtd:LA ;
    dtd:has_Value "der Hersteller die Pflichten nach § 8 Absatz 1 und 2 erfüllt hat."@de .

dtd:LA-id589 a dtd:LA ;
    dtd:has_Value "das Sicherheitsbauteil für Aufzüge mit der CE-Kennzeichnung versehen ist und ihm die EU-Konformitätserklärung beigefügt ist,"@de .

dtd:LA-id590 a dtd:LA ;
    dtd:has_Value "dem Sicherheitsbauteil für Aufzüge die Betriebsanleitung nach Anhang I Nummer 6.1 der Richtlinie 2014/33/EU in deutscher Sprache beigefügt ist und"@de .

dtd:LA-id591 a dtd:LA ;
    dtd:has_Value "der Hersteller seine Pflichten nach § 8 Absatz 1 und 2 und der Einführer seine Pflichten nach § 11 Absatz 1 erfüllt hat."@de .

dtd:LA-id592 a dtd:LA ;
    dtd:has_Value "ein Sicherheitsbauteil für Aufzüge unter eigenem Namen oder eigener Handelsmarke in den Verkehr bringt oder"@de .

dtd:LA-id593 a dtd:LA ;
    dtd:has_Value "ein bereits in den Verkehr gebrachtes Sicherheitsbauteil für Aufzüge so verändert, dass die Konformität mit den Anforderungen dieser Verordnung beeinträchtigt werden kann."@de .

dtd:LA-id594 a dtd:LA ;
    dtd:has_Value "von denen er ein Sicherheitsbauteil für Aufzüge bezogen hat und"@de .

dtd:LA-id595 a dtd:LA ;
    dtd:has_Value "an die er ein Sicherheitsbauteil für Aufzüge abgegeben hat."@de .

dtd:LA-id596 a dtd:LA ;
    dtd:has_Value "der Aufzug oder das Sicherheitsbauteil für Aufzüge die wesentlichen Gesundheitsschutz- und Sicherheitsanforderungen nach Anhang I der Richtlinie 2014/33/EU nicht erfüllt oder"@de .

dtd:LA-id597 a dtd:LA ;
    dtd:has_Value "die harmonisierten Normen, bei deren Einhaltung gemäß § 4 eine Konformitätsvermutung gilt, mangelhaft sind."@de .

dtd:LA-id598 a dtd:LA ;
    dtd:has_Value "die CE-Kennzeichnung wurde nicht oder unter Verletzung von § 7 des Produktsicherheitsgesetzes angebracht,"@de .

dtd:LA-id599 a dtd:LA ;
    dtd:has_Value "die Kennnummer der notifizierten Stelle wurde nicht oder unter Verletzung von § 7 Absatz 4 des Produktsicherheitsgesetzes angebracht,"@de .

dtd:LA-id600 a dtd:LA ;
    dtd:has_Value "die EU-Konformitätserklärung wurde nicht oder nicht ordnungsgemäß ausgestellt,"@de .

dtd:LA-id601 a dtd:LA ;
    dtd:has_Value "die technischen Unterlagen sind nicht verfügbar oder nicht vollständig,"@de .

dtd:LA-id602 a dtd:LA ;
    dtd:has_Value "die Angaben des Montagebetriebs gemäß § 6 Absatz 2, des Herstellers gemäß § 8 Absatz 2 oder des Einführers gemäß § 11 Absatz 1 fehlen, sind falsch oder unvollständig,"@de .

dtd:LA-id603 a dtd:LA ;
    dtd:has_Value "die Informationen zur Identifikation des Aufzugs gemäß § 6 Absatz 1 oder zur Identifikation des Sicherheitsbauteils für Aufzüge gemäß § 8 Absatz 1 fehlen, sind falsch oder unvollständig oder"@de .

dtd:LA-id604 a dtd:LA ;
    dtd:has_Value "die Betriebsanleitung ist nicht beigefügt oder erfüllt nicht die Anforderungen nach § 6 Absatz 3 und 4 oder nach § 8 Absatz 3 und 4."@de .

dtd:LA-id605 a dtd:LA ;
    dtd:has_Value "trifft die Marktüberwachungsbehörde alle geeigneten Maßnahmen, um die Verwendung des Aufzugs einzuschränken, oder sie untersagt die Verwendung des Aufzugs oder sorgt dafür, dass der Aufzug zurückgerufen wird, oder"@de .

dtd:LA-id606 a dtd:LA ;
    dtd:has_Value "trifft die Marktüberwachungsbehörde alle geeigneten Maßnahmen, um die Bereitstellung des Sicherheitsbauteils für Aufzüge auf dem Markt einzuschränken, oder sie untersagt die Bereitstellung des Sicherheitsbauteils für Aufzüge auf dem Markt oder sorgt dafür, dass das Sicherheitsbauteil für Aufzüge zurückgerufen oder zurückgenommen wird."@de .

dtd:LA-id607 a dtd:LA ;
    dtd:has_Value "entgegen § 5 Absatz 2 Satz 1 oder § 7 Absatz 2 Satz 1 einen Aufzug oder ein Sicherheitsbauteil für Aufzüge in den Verkehr bringt,"@de .

dtd:LA-id608 a dtd:LA ;
    dtd:has_Value "entgegen § 5 Absatz 2 Satz 3 oder § 7 Absatz 2 Satz 3 nicht sicherstellt, dass dem Aufzug oder dem Sicherheitsbauteil für Aufzüge die EU-Konformitätserklärung beigefügt ist,"@de .

dtd:LA-id609 a dtd:LA ;
    dtd:has_Value "entgegen § 6 Absatz 1 oder § 8 Absatz 1 Satz 1 nicht dafür sorgt, dass ein Aufzug oder ein Sicherheitsbauteil für Aufzüge eine dort genannte Nummer oder eine andere Information trägt,"@de .

dtd:LA-id610 a dtd:LA ;
    dtd:has_Value "entgegen § 6 Absatz 2 Satz 1, § 8 Absatz 2 Satz 1 oder § 11 Absatz 1 Satz 1 dort genannte Daten nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig anbringt,"@de .

dtd:LA-id611 a dtd:LA ;
    dtd:has_Value "entgegen § 6 Absatz 3 oder § 8 Absatz 3 nicht dafür sorgt, dass einem Aufzug oder einem Sicherheitsbauteil für Aufzüge eine Betriebsanleitung beigefügt ist,"@de .

dtd:LA-id612 a dtd:LA ;
    dtd:has_Value "entgegen § 8 Absatz 1 Satz 2 nicht dafür sorgt, dass eine dort genannte Information angegeben wird, oder"@de .

dtd:LA-id613 a dtd:LA ;
    dtd:has_Value "entgegen § 10 Absatz 2 Nummer 1, 3 oder Nummer 4 ein Sicherheitsbauteil für Aufzüge in den Verkehr bringt."@de .

dtd:LA-id614 a dtd:LA ;
    dtd:has_Value "entgegen § 5 Absatz 3 oder § 7 Absatz 3, jeweils auch in Verbindung mit § 9 Absatz 3 Nummer 1, oder entgegen § 11 Absatz 2 eine technische Unterlage, die EU-Konformitätserklärung oder eine Zulassung nicht oder nicht mindestens zehn Jahre bereithält,"@de .

dtd:LA-id615 a dtd:LA ;
    dtd:has_Value "entgegen § 6 Absatz 5 Satz 1 oder § 8 Absatz 5 Satz 1, jeweils auch in Verbindung mit § 9 Absatz 3 Nummer 2, entgegen § 11 Absatz 3 Satz 1 oder § 12 Absatz 6 Satz 1 eine Information oder eine Unterlage nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig zur Verfügung stellt oder"@de .

dtd:LA-id616 a dtd:LA ;
    dtd:has_Value "entgegen § 14 Absatz 1 einen Wirtschaftsakteur nicht oder nicht rechtzeitig nennt."@de .


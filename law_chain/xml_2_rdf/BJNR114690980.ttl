    dtd:has_Size dtd:normal ;
    dtd:has_Value "gemeinsame örtliche und überörtliche Pläne in ihrem Aufgabenbereich über soziale Dienste und Einrichtungen, insbesondere deren Bereitstellung und Inanspruchnahme, anstreben."@de .

dtd:LA-id2905 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen Absatz 1 Satz 1 oder"@de .

dtd:LA-id2906 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen Absatz 1 Satz 2 oder Satz 3, jeweils auch in Verbindung mit Absatz 1 Satz 6 oder Absatz 3,"@de .


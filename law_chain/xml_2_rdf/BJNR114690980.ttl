    dtd:has_Value "im Rahmen des § 69 Absatz 4 und 5 und des § 71 Absatz 1 Satz 3,"@de .

dtd:LA-id2865 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Rahmen des § 94 Absatz 2 Satz 2 des Elften Buches."@de .


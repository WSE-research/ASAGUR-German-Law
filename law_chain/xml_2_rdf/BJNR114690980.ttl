    dtd:has_Value "im Übrigen im Rahmen eines öffentlich-rechtlichen oder privatrechtlichen Vertrages der Deutschen Post AG mit den Leistungsträgern, den berufsständischen Versorgungseinrichtungen oder den in § 69 Abs. 2 genannten Stellen."@de .

dtd:LA-id2915 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Anspruch des vorläufig leistenden Leistungsträgers nach § 102,"@de .

dtd:LA-id2916 a dtd:LA ;
    dtd:has_Size dtd:normal ;

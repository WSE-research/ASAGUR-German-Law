    dtd:has_Value "aus tatsächlichen Gründen, besonders weil die zur Vornahme der Amtshandlung erforderlichen Dienstkräfte oder Einrichtungen fehlen, die Amtshandlung nicht selbst vornehmen kann,"@de .

dtd:LA-id2660 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Durchführung ihrer Aufgaben auf die Kenntnis von Tatsachen angewiesen ist, die ihr unbekannt sind und die sie selbst nicht ermitteln kann,"@de .

dtd:LA-id2661 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Durchführung ihrer Aufgaben Urkunden oder sonstige Beweismittel benötigt, die sich im Besitz der ersuchten Behörde befinden,"@de .

dtd:LA-id2662 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Amtshandlung nur mit wesentlich größerem Aufwand vornehmen könnte als die ersuchte Behörde."@de .

dtd:LA-id2663 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie hierzu aus rechtlichen Gründen nicht in der Lage ist,"@de .

dtd:LA-id2664 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch die Hilfeleistung dem Wohl des Bundes oder eines Landes erhebliche Nachteile bereitet würden."@de .

dtd:LA-id2665 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine andere Behörde die Hilfe wesentlich einfacher oder mit wesentlich geringerem Aufwand leisten kann,"@de .

dtd:LA-id2666 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie die Hilfe nur mit unverhältnismäßig großem Aufwand leisten könnte,"@de .

dtd:LA-id2667 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie unter Berücksichtigung der Aufgaben der ersuchenden Behörde durch die Hilfeleistung die Erfüllung ihrer eigenen Aufgaben ernstlich gefährden würde."@de .

dtd:LA-id2668 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "natürliche und juristische Personen,"@de .

dtd:LA-id2669 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vereinigungen, soweit ihnen ein Recht zustehen kann,"@de .

dtd:LA-id2670 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Behörden."@de .

dtd:LA-id2671 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "natürliche Personen, die nach bürgerlichem Recht geschäftsfähig sind,"@de .

dtd:LA-id2672 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "natürliche Personen, die nach bürgerlichem Recht in der Geschäftsfähigkeit beschränkt sind, soweit sie für den Gegenstand des Verfahrens durch Vorschriften des bürgerlichen Rechts als geschäftsfähig oder durch Vorschriften des öffentlichen Rechts als handlungsfähig anerkannt sind,"@de .

dtd:LA-id2673 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "juristische Personen und Vereinigungen (§ 10 Nr. 2) durch ihre gesetzlichen Vertreter oder durch besonders Beauftragte,"@de .

dtd:LA-id2674 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Behörden durch ihre Leiter, deren Vertreter oder Beauftragte."@de .

dtd:LA-id2675 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Antragsteller und Antragsgegner,"@de .

dtd:LA-id2676 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "diejenigen, an die die Behörde den Verwaltungsakt richten will oder gerichtet hat,"@de .

dtd:LA-id2677 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "diejenigen, mit denen die Behörde einen öffentlich-rechtlichen Vertrag schließen will oder geschlossen hat,"@de .

dtd:LA-id2678 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "diejenigen, die nach Absatz 2 von der Behörde zu dem Verfahren hinzugezogen worden sind."@de .

dtd:LA-id2679 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für einen Beteiligten, dessen Person unbekannt ist,"@de .

dtd:LA-id2680 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für einen abwesenden Beteiligten, dessen Aufenthalt unbekannt ist oder der an der Besorgung seiner Angelegenheiten verhindert ist,"@de .

dtd:LA-id2681 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für einen Beteiligten ohne Aufenthalt im Inland, wenn er der Aufforderung der Behörde, einen Vertreter zu bestellen, innerhalb der ihm gesetzten Frist nicht nachgekommen ist,"@de .

dtd:LA-id2682 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für einen Beteiligten, der infolge einer psychischen Krankheit oder körperlichen, geistigen oder seelischen Behinderung nicht in der Lage ist, in dem Verwaltungsverfahren selbst tätig zu werden."@de .

dtd:LA-id2683 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer selbst Beteiligter ist,"@de .

dtd:LA-id2684 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer Angehöriger eines Beteiligten ist,"@de .

dtd:LA-id2685 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer einen Beteiligten kraft Gesetzes oder Vollmacht allgemein oder in diesem Verwaltungsverfahren vertritt oder als Beistand zugezogen ist,"@de .

dtd:LA-id2686 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer Angehöriger einer Person ist, die einen Beteiligten in diesem Verfahren vertritt,"@de .

dtd:LA-id2687 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer bei einem Beteiligten gegen Entgelt beschäftigt ist oder bei ihm als Mitglied des Vorstandes, des Aufsichtsrates oder eines gleichartigen Organs tätig ist; dies gilt nicht für den, dessen Anstellungskörperschaft Beteiligte ist, und nicht für Beschäftigte bei Betriebskrankenkassen,"@de .

dtd:LA-id2688 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wer außerhalb seiner amtlichen Eigenschaft in der Angelegenheit ein Gutachten abgegeben hat oder sonst tätig geworden ist."@de .

dtd:LA-id2689 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Verlobte,"@de .

dtd:LA-id2690 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Ehegatte oder Lebenspartner,"@de .

dtd:LA-id2691 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Verwandte und Verschwägerte gerader Linie,"@de .

dtd:LA-id2692 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geschwister,"@de .

dtd:LA-id2693 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kinder der Geschwister,"@de .

dtd:LA-id2694 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ehegatten oder Lebenspartner der Geschwister und Geschwister der Ehegatten oder Lebenspartner,"@de .

dtd:LA-id2695 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Geschwister der Eltern,"@de .

dtd:LA-id2696 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Personen, die durch ein auf längere Dauer angelegtes Pflegeverhältnis mit häuslicher Gemeinschaft wie Eltern und Kind miteinander verbunden sind (Pflegeeltern und Pflegekinder)."@de .

dtd:LA-id2697 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Fällen der Nummern 2, 3 und 6 die die Beziehung begründende Ehe oder Lebenspartnerschaft nicht mehr besteht,"@de .

dtd:LA-id2698 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Fällen der Nummern 3 bis 7 die Verwandtschaft oder Schwägerschaft durch Annahme als Kind erloschen ist,"@de .

dtd:LA-id2699 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall der Nummer 8 die häusliche Gemeinschaft nicht mehr besteht, sofern die Personen weiterhin wie Eltern und Kind miteinander verbunden sind."@de .

dtd:LA-id2700 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Amts wegen oder auf Antrag tätig werden muss,"@de .

dtd:LA-id2701 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "nur auf Antrag tätig werden darf und ein Antrag nicht vorliegt."@de .

dtd:LA-id2702 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Auskünfte jeder Art, auch elektronisch und als elektronisches Dokument, einholen,"@de .

dtd:LA-id2703 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beteiligte anhören, Zeugen und Sachverständige vernehmen oder die schriftliche oder elektronische Äußerung von Beteiligten, Sachverständigen und Zeugen einholen,"@de .

dtd:LA-id2704 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Urkunden und Akten beiziehen,"@de .

dtd:LA-id2705 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Augenschein einnehmen."@de .

dtd:LA-id2706 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine sofortige Entscheidung wegen Gefahr im Verzug oder im öffentlichen Interesse notwendig erscheint,"@de .

dtd:LA-id2707 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "durch die Anhörung die Einhaltung einer für die Entscheidung maßgeblichen Frist in Frage gestellt würde,"@de .

dtd:LA-id2708 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von den tatsächlichen Angaben eines Beteiligten, die dieser in einem Antrag oder einer Erklärung gemacht hat, nicht zu seinen Ungunsten abgewichen werden soll,"@de .

dtd:LA-id2709 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Allgemeinverfügungen oder gleichartige Verwaltungsakte in größerer Zahl erlassen werden sollen,"@de .

dtd:LA-id2710 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einkommensabhängige Leistungen den geänderten Verhältnissen angepasst werden sollen,"@de .

dtd:LA-id2711 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Maßnahmen in der Verwaltungsvollstreckung getroffen werden sollen oder"@de .

dtd:LA-id2712 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "gegen Ansprüche oder mit Ansprüchen von weniger als 70 Euro aufgerechnet oder verrechnet werden soll; Nummer 5 bleibt unberührt."@de .

dtd:LA-id2713 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die genaue Bezeichnung des Schriftstückes, dessen Abschrift beglaubigt wird,"@de .

dtd:LA-id2714 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Feststellung, dass die beglaubigte Abschrift mit dem vorgelegten Schriftstück übereinstimmt,"@de .

dtd:LA-id2715 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Hinweis, dass die beglaubigte Abschrift nur zur Vorlage bei der angegebenen Behörde erteilt wird, wenn die Urschrift nicht von einer Behörde ausgestellt worden ist,"@de .

dtd:LA-id2716 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Ort und den Tag der Beglaubigung, die Unterschrift des für die Beglaubigung zuständigen Bediensteten und das Dienstsiegel."@de .

dtd:LA-id2717 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ablichtungen, Lichtdrucken und ähnlichen in technischen Verfahren hergestellten Vervielfältigungen,"@de .

dtd:LA-id2718 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf fototechnischem Wege von Schriftstücken hergestellten Negativen, die bei einer Behörde aufbewahrt werden,"@de .

dtd:LA-id2719 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ausdrucken elektronischer Dokumente,"@de .

dtd:LA-id2720 a dtd:LA ;
    dtd:has_DL dtd:DL-id2182 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "elektronischen Dokumenten,a)die zur Abbildung eines Schriftstücks hergestellt wurden,b)die ein anderes technisches Format als das Ausgangsdokument, das verbunden ist mit einer qualifizierten elektronischen Signatur oder einem qualifizierten elektronischen Siegel einer Behörde, erhalten haben."@de .

dtd:LA-id2721 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zur Abbildung eines Schriftstücks hergestellt wurden,"@de .

dtd:LA-id2722 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die ein anderes technisches Format als das Ausgangsdokument, das verbunden ist mit einer qualifizierten elektronischen Signatur oder einem qualifizierten elektronischen Siegel einer Behörde, erhalten haben."@de .

dtd:LA-id2723 a dtd:LA ;
    dtd:has_DL dtd:DL-id2192 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "des Ausdrucks eines elektronischen Dokuments, das mit einer qualifizierten elektronischen Signatur oder einem qualifizierten elektronischen Siegel einer Behörde verbunden ist, die Feststellungen enthalten, a)wen die Signaturprüfung als Inhaber der Signatur oder welche Behörde die Signaturprüfung als Inhaber des Siegels ausweist,b)welchen Zeitpunkt die Signaturprüfung für die Anbringung der Signatur oder des Siegels ausweist undc)welche Zertifikate mit welchen Daten dieser Signatur oder diesem Siegel zugrunde lagen;"@de .

dtd:LA-id2724 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wen die Signaturprüfung als Inhaber der Signatur oder welche Behörde die Signaturprüfung als Inhaber des Siegels ausweist,"@de .

dtd:LA-id2725 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "welchen Zeitpunkt die Signaturprüfung für die Anbringung der Signatur oder des Siegels ausweist und"@de .

dtd:LA-id2726 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "welche Zertifikate mit welchen Daten dieser Signatur oder diesem Siegel zugrunde lagen;"@de .

dtd:LA-id2727 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eines elektronischen Dokuments den Namen des für die Beglaubigung zuständigen Bediensteten und die Bezeichnung der Behörde, die die Beglaubigung vornimmt, enthalten; die Unterschrift des für die Beglaubigung zuständigen Bediensteten und das Dienstsiegel nach Absatz 3 Satz 2 Nummer 4 werden durch eine dauerhaft überprüfbare qualifizierte elektronische Signatur oder durch ein dauerhaft überprüfbares qualifiziertes elektronisches Siegel der Behörde ersetzt."@de .

dtd:LA-id2728 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unterschriften ohne zugehörigen Text,"@de .

dtd:LA-id2729 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unterschriften, die der öffentlichen Beglaubigung (§ 129 des Bürgerlichen Gesetzbuches) bedürfen."@de .

dtd:LA-id2730 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bestätigung, dass die Unterschrift echt ist,"@de .

dtd:LA-id2731 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die genaue Bezeichnung desjenigen, dessen Unterschrift beglaubigt wird, sowie die Angabe, ob sich der für die Beglaubigung zuständige Bedienstete Gewissheit über diese Person verschafft hat und ob die Unterschrift in seiner Gegenwart vollzogen oder anerkannt worden ist,"@de .

dtd:LA-id2732 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Hinweis, dass die Beglaubigung nur zur Vorlage bei der angegebenen Behörde oder Stelle bestimmt ist,"@de .

dtd:LA-id2733 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Ort und den Tag der Beglaubigung, die Unterschrift des für die Beglaubigung zuständigen Bediensteten und das Dienstsiegel."@de .

dtd:LA-id2734 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Bestimmung, nach der eine Vergünstigung oder Belastung zu einem bestimmten Zeitpunkt beginnt, endet oder für einen bestimmten Zeitraum gilt (Befristung),"@de .

dtd:LA-id2735 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Bestimmung, nach der der Eintritt oder der Wegfall einer Vergünstigung oder einer Belastung von dem ungewissen Eintritt eines zukünftigen Ereignisses abhängt (Bedingung),"@de .

dtd:LA-id2736 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Vorbehalt des Widerrufs"@de .

dtd:LA-id2737 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer Bestimmung, durch die dem Begünstigten ein Tun, Dulden oder Unterlassen vorgeschrieben wird (Auflage),"@de .

dtd:LA-id2738 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einem Vorbehalt der nachträglichen Aufnahme, Änderung oder Ergänzung einer Auflage."@de .

dtd:LA-id2739 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit die Behörde einem Antrag entspricht oder einer Erklärung folgt und der Verwaltungsakt nicht in Rechte eines anderen eingreift,"@de .

dtd:LA-id2740 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "soweit demjenigen, für den der Verwaltungsakt bestimmt ist oder der von ihm betroffen wird, die Auffassung der Behörde über die Sach- und Rechtslage bereits bekannt oder auch ohne Begründung für ihn ohne weiteres erkennbar ist,"@de .

dtd:LA-id2741 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn die Behörde gleichartige Verwaltungsakte in größerer Zahl oder Verwaltungsakte mit Hilfe automatischer Einrichtungen erlässt und die Begründung nach den Umständen des Einzelfalles nicht geboten ist,"@de .

dtd:LA-id2742 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn sich dies aus einer Rechtsvorschrift ergibt,"@de .

dtd:LA-id2743 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "wenn eine Allgemeinverfügung öffentlich bekannt gegeben wird."@de .

dtd:LA-id2744 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der schriftlich oder elektronisch erlassen worden ist, die erlassende Behörde aber nicht erkennen lässt,"@de .

dtd:LA-id2745 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der nach einer Rechtsvorschrift nur durch die Aushändigung einer Urkunde erlassen werden kann, aber dieser Form nicht genügt,"@de .

dtd:LA-id2746 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den aus tatsächlichen Gründen niemand ausführen kann,"@de .

dtd:LA-id2747 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der die Begehung einer rechtswidrigen Tat verlangt, die einen Straf- oder Bußgeldtatbestand verwirklicht,"@de .

dtd:LA-id2748 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der gegen die guten Sitten verstößt."@de .

dtd:LA-id2749 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Vorschriften über die örtliche Zuständigkeit nicht eingehalten worden sind,"@de .

dtd:LA-id2750 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine nach § 16 Abs. 1 Satz 1 Nr. 2 bis 6 ausgeschlossene Person mitgewirkt hat,"@de .

dtd:LA-id2751 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein durch Rechtsvorschrift zur Mitwirkung berufener Ausschuss den für den Erlass des Verwaltungsaktes vorgeschriebenen Beschluss nicht gefasst hat oder nicht beschlussfähig war,"@de .

dtd:LA-id2752 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nach einer Rechtsvorschrift erforderliche Mitwirkung einer anderen Behörde unterblieben ist."@de .

dtd:LA-id2753 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der für den Erlass des Verwaltungsaktes erforderliche Antrag nachträglich gestellt wird,"@de .

dtd:LA-id2754 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die erforderliche Begründung nachträglich gegeben wird,"@de .

dtd:LA-id2755 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die erforderliche Anhörung eines Beteiligten nachgeholt wird,"@de .

dtd:LA-id2756 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Beschluss eines Ausschusses, dessen Mitwirkung für den Erlass des Verwaltungsaktes erforderlich ist, nachträglich gefasst wird,"@de .

dtd:LA-id2757 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die erforderliche Mitwirkung einer anderen Behörde nachgeholt wird,"@de .

dtd:LA-id2758 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die erforderliche Hinzuziehung eines Beteiligten nachgeholt wird."@de .

dtd:LA-id2759 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "er den Verwaltungsakt durch arglistige Täuschung, Drohung oder Bestechung erwirkt hat,"@de .

dtd:LA-id2760 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Verwaltungsakt auf Angaben beruht, die der Begünstigte vorsätzlich oder grob fahrlässig in wesentlicher Beziehung unrichtig oder unvollständig gemacht hat, oder"@de .

dtd:LA-id2761 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "er die Rechtswidrigkeit des Verwaltungsaktes kannte oder infolge grober Fahrlässigkeit nicht kannte; grobe Fahrlässigkeit liegt vor, wenn der Begünstigte die erforderliche Sorgfalt in besonders schwerem Maße verletzt hat."@de .

dtd:LA-id2762 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Voraussetzungen des Absatzes 2 Satz 3 Nr. 2 oder 3 gegeben sind oder"@de .

dtd:LA-id2763 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Verwaltungsakt mit einem zulässigen Vorbehalt des Widerrufs erlassen wurde."@de .

dtd:LA-id2764 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Widerruf durch Rechtsvorschrift zugelassen oder im Verwaltungsakt vorbehalten ist,"@de .

dtd:LA-id2765 a dtd:LA ;
    dtd:has_Size dtd:normal ;

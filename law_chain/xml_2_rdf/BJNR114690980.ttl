    dtd:has_Value "der Anspruch des nachrangig verpflichteten Leistungsträgers nach § 104,"@de .

dtd:LA-id2918 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der Anspruch des unzuständigen Leistungsträgers nach § 105."@de .

dtd:LA-id2919 a dtd:LA ;

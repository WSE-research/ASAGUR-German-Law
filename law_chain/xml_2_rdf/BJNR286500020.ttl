    dtd:has_Size dtd:normal ;
    dtd:has_Value "Basismittel in Höhe von 2 Milliarden Euro, davon a)1 Milliarde Euro im Jahr 2020 undb)1 Milliarde Euro im Jahr 2021,"@de .

dtd:LA-id246 a dtd:LA ;

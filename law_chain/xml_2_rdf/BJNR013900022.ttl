dtd:LA-id2532 a dtd:LA ;
    dtd:has_Value "ein Differenzwert im Fall der Öko-Regelungen des Jahres 2027 nicht berücksichtigt wird."@de .

dtd:LA-id2533 a dtd:LA ;

dtd:LA-id102 a dtd:LA ;
    dtd:has_Value "als Ombudsperson für die Anliegen der Opfer der SED-Diktatur und der kommunistischen Herrschaft in der Sowjetischen Besatzungszone in Deutschland und in der ehemaligen Deutschen Demokratischen Republik sowie deren bis einschließlich im zweiten Grad verwandten Angehörigen in Politik und Öffentlichkeit zu wirken und zur Würdigung der Opfer des Kommunismus in Deutschland beizutragen,"@de .

dtd:LA-id103 a dtd:LA ;
    dtd:has_Value "den Deutschen Bundestag und seine Ausschüsse, die Bundesregierung sowie andere öffentliche Einrichtungen in Fragen, die die Angelegenheiten der Opfer der SED-Diktatur und der kommunistischen Herrschaft in der Sowjetischen Besatzungszone in Deutschland und in der ehemaligen Deutschen Demokratischen Republik betreffen, zu beraten,"@de .

dtd:LA-id104 a dtd:LA ;
    dtd:has_Value "den Prozess der gesellschaftlichen Verständigung über die unterschiedlichen biografischen Erfahrungen in der Zeit der deutschen Teilung zu befördern,"@de .

dtd:LA-id105 a dtd:LA ;
    dtd:has_Value "den Deutschen Bundestag in dem Anliegen zu unterstützen, die Aufmerksamkeit für die Belange der Opfer kommunistischer Gewaltherrschaft im europäischen und internationalen Rahmen zu stärken und daran mitzuwirken, die Vermittlung von Erfahrungen in Deutschland im Umgang mit den Opfern diktatorischer kommunistischer Gewalt im internationalen Kontext zu leisten, und"@de .

dtd:LA-id106 a dtd:LA ;
    dtd:has_Value "alle Institutionen des Bundes in Fragen von Opferinteressen beim Umgang mit den Unterlagen des Ministeriums für Staatssicherheit der ehemaligen Deutschen Demokratischen Republik sowie mit den Archivbeständen, die Bezug zur Geschichte der ehemaligen Deutschen Demokratischen Republik und zu der Zeit der deutschen Teilung haben, zu beraten."@de .

dtd:LA-id107 a dtd:LA ;
    dtd:has_Value "durch Abberufung aufgrund eines Beschlusses des Deutschen Bundestages oder"@de .

dtd:LA-id108 a dtd:LA ;
    dtd:has_Value "durch Entlassung auf Verlangen der oder des Opferbeauftragten."@de .

dtd:LA-id109 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für den Monat Juni 2023 eine einmalige Sonderzahlung in Höhe von 1 240 Euro sowie"@de .

dtd:LA-id110 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Monate Juli 2023 bis Februar 2024 eine monatliche Sonderzahlung in Höhe von jeweils 220 Euro."@de .


dtd:P-id723 a dtd:P ;
    dtd:has_Value " Diese Rechtsverordnung dient der Umsetzung der Richtlinie 1999/105/EG des Rates vom 22. Dezember 1999 über den Verkehr mit forstlichem Vermehrungsgut (ABl. EG 2000 Nr. L 11 S. 17, 2001 Nr. L 121 S. 48). "@de ;
    dtd:has_noindex dtd:noindex-id80 .

dtd:P-id725 a dtd:P ;
    dtd:has_BR dtd:BR-id63 ;
    dtd:has_Value " (+++ Textnachweis ab: 1.1.2003 +++)(+++ Amtlicher Hinweis des Normgebers auf EG-Recht:     Umsetzung der       EGRL 105/99 (CELEX Nr: 399L0105) +++)"@de ;
    dtd:has_pre dtd:pre-id751 .

dtd:P-id727 a dtd:P ;
    dtd:has_Value "Auf Grund des § 4 Abs. 7 Satz 1 und des § 6 Abs. 3 des Forstvermehrungsgutgesetzes vom 22. Mai 2002 (BGBl. I S. 1658) verordnet das Bundesministerium für Verbraucherschutz, Ernährung und Landwirtschaft:"@de .

dtd:P-id729 a dtd:P ;
    dtd:has_DL dtd:DL-id302 ;
    dtd:has_Value "(1) Für die Zulassung von 1.Erntebeständen unter der Kategorie \"Ausgewählt\", 2.Samenplantagen unter der Kategorie \"Qualifiziert\" und 3.Erntebeständen, Samenplantagen, Familieneltern, Klonen und Klonmischungen unter der Kategorie \"Geprüft\" gelten die in der Anlage 1 dieser Verordnung festgelegten Mindestanforderungen."@de .

dtd:P-id730 a dtd:P ;
    dtd:has_Value "(2) Für die Zulassung von Erntebeständen und Saatgutquellen unter der Kategorie \"Quellengesichert\" gelten die in der Anlage 2 dieser Verordnung festgelegten Mindestanforderungen."@de .

dtd:P-id731 a dtd:P ;
    dtd:has_Value "(3) Samenplantagen, die zum Zeitpunkt des Inkrafttretens dieser Verordnung als Ausgangsmaterial zur Gewinnung von \"Ausgewähltem Vermehrungsgut\" nach dem Gesetz über forstliches Saat- und Pflanzgut in der Fassung der Bekanntmachung vom 26. Juli 1979 (BGBl. I S. 1242), zuletzt geändert durch Artikel 201 der Verordnung vom 29. Oktober 2001 (BGBl. I S. 2785), zugelassen waren, können ohne weitere Überprüfung unter der Kategorie \"Qualifiziert\" registriert werden."@de .

dtd:P-id733 a dtd:P ;
    dtd:has_Value "Im Register über zugelassenes Ausgangsmaterial nach § 6 Abs. 1 des Forstvermehrungsgutgesetzes sind die in Anlage 3 dieser Verordnung festgelegten Angaben zu machen."@de .

dtd:P-id735 a dtd:P ;
    dtd:has_Value "Diese Verordnung tritt am 1. Januar 2003 in Kraft."@de .

dtd:P-id737 a dtd:P ;
    dtd:has_Value "Der Bundesrat hat zugestimmt."@de .

dtd:P-id739 a dtd:P ;
    dtd:has_Value "(Fundstelle des Originaltextes: BGBl. I 2002, 4722 - 4726)"@de ;
    dtd:has_kommentar dtd:kommentar-id752 .


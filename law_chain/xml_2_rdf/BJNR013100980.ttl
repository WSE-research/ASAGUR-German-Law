dtd:LA-id4555 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zur Sicherung der Rohstoffversorgung das Aufsuchen, Gewinnen und Aufbereiten von Bodenschätzen unter Berücksichtigung ihrer Standortgebundenheit und des Lagerstättenschutzes bei sparsamem und schonendem Umgang mit Grund und Boden zu ordnen und zu fördern,"@de .

dtd:LA-id4556 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Sicherheit der Betriebe und der Beschäftigten des Bergbaus zu gewährleisten sowie"@de .

dtd:LA-id4557 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vorsorge gegen Gefahren, die sich aus bergbaulicher Tätigkeit für Leben, Gesundheit und Sachgüter Dritter ergeben, zu verstärken und den Ausgleich unvermeidbarer Schäden zu verbessern."@de .

dtd:LA-id4558 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Aufsuchen, Gewinnen und Aufbereiten von bergfreien und grundeigenen Bodenschätzen einschließlich des Verladens, Beförderns, Abladens, Lagerns und Ablagerns von Bodenschätzen, Nebengestein und sonstigen Massen, soweit es im unmittelbaren betrieblichen Zusammenhang mit dem Aufsuchen, Gewinnen oder Aufbereiten steht und sich nicht aus Absatz 4 etwas anderes ergibt,"@de .

dtd:LA-id4559 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Wiedernutzbarmachen der Oberfläche während und nach der Aufsuchung, Gewinnung und Aufbereitung von bergfreien und grundeigenen Bodenschätzen,"@de .

dtd:LA-id4560 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Betriebsanlagen und Betriebseinrichtungen (Einrichtungen), die überwiegend einer der in den Nummern 1 oder 2 bezeichneten Tätigkeiten dienen oder zu dienen bestimmt sind."@de .

dtd:LA-id4561 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Untersuchen des Untergrundes auf seine Eignung zur Errichtung von Untergrundspeichern,"@de .

dtd:LA-id4562 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Errichten und Betreiben von Untergrundspeichern sowie der Einrichtungen, die überwiegend dem Betrieb eines Untergrundspeichers dienen oder zu dienen bestimmt sind,"@de .

dtd:LA-id4563 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sonstige Tätigkeiten und Einrichtungen,"@de .

dtd:LA-id4564 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Schienenverkehr der Eisenbahnen des öffentlichen Verkehrs,"@de .

dtd:LA-id4565 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Kraftfahrzeugverkehr auf öffentlichen Wegen oder Plätzen"@de .

dtd:LA-id4566 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Schiffsverkehr seewärts der Begrenzung des Küstenmeeres und auf Binnen- und Seewasserstraßen und in den Seehäfen,"@de .

dtd:LA-id4567 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in Luftfahrzeugen und"@de .

dtd:LA-id4568 a dtd:LA ;

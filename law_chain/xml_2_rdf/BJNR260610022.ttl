dtd:LA-id491 a dtd:LA ;
    dtd:has_Value "bei im Eigentum befindlichen oder kontrollierten Kraftfahrzeugen, Flugzeugen und Schiffen die amtlichen Kennzeichnungen oder Registrierungsmerkmale,"@de .


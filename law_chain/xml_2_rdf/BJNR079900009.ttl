dtd:LA-id1043 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Kopien der Unterlagen nach Absatz 2a Satz 1 Nummer 2 und 3,"@de .

dtd:LA-id1044 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unterlagen über die Entlohnung des Kraftfahrers oder der Kraftfahrerin einschließlich der Zahlungsbelege,"@de .

dtd:LA-id1045 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "den Arbeitsvertrag oder gleichwertige Unterlagen im Sinne des Artikels 3 Absatz 1 der Richtlinie 91/533/EWG des Rates vom 14. Oktober 1991 über die Pflicht des Arbeitgebers zur Unterrichtung des Arbeitnehmers über die für seinen Arbeitsvertrag oder sein Arbeitsverhältnis geltenden Bedingungen (ABl. L 288 vom 18.10.1991, S. 32) und"@de .

dtd:LA-id1046 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Unterlagen über die Zeiterfassung, die sich auf die Arbeit des Kraftfahrers oder der Kraftfahrerin beziehen, insbesondere die Aufzeichnungen des Fahrtenschreibers."@de .

dtd:LA-id1047 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 8 Abs. 1 Satz 1 oder Abs. 3 eine Arbeitsbedingung, deren Einhaltung nach § 16 von den Behörden der Zollverwaltung geprüft wird, nicht oder nicht rechtzeitig gewährt oder einen Beitrag nicht oder nicht rechtzeitig leistet,"@de .

dtd:LA-id1048 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 17 Satz 1 in Verbindung mit § 5 Abs. 1 Satz 1 Nummer 1 oder 3 des Schwarzarbeitsbekämpfungsgesetzes eine Prüfung nicht duldet oder bei einer Prüfung nicht mitwirkt,"@de .

dtd:LA-id1049 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 17 Satz 1 in Verbindung mit § 5 Abs. 1 Satz 1 Nummer 2 des Schwarzarbeitsbekämpfungsgesetzes das Betreten eines Grundstücks oder Geschäftsraums nicht duldet,"@de .

dtd:LA-id1050 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 17 Satz 1 in Verbindung mit § 5 Absatz 5 Satz 1 des Schwarzarbeitsbekämpfungsgesetzes Daten nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig übermittelt,"@de .

dtd:LA-id1051 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 18 Absatz 1 Satz 1, Absatz 2 Satz 1 oder Absatz 3 Satz 1 eine Anmeldung nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig vorlegt oder nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig zuleitet,"@de .

dtd:LA-id1052 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 18 Absatz 1 Satz 3, auch in Verbindung mit Absatz 2 Satz 2 oder Absatz 3 Satz 2, eine Änderungsmeldung nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig macht,"@de .

dtd:LA-id1053 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 19 Absatz 1 Satz 1, auch in Verbindung mit Satz 2, eine Aufzeichnung nicht, nicht richtig, nicht vollständig oder nicht rechtzeitig erstellt oder nicht oder nicht mindestens zwei Jahre aufbewahrt,"@de .

dtd:LA-id1054 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 19 Absatz 2 eine Unterlage nicht, nicht richtig, nicht vollständig oder nicht in der vorgeschriebenen Weise bereithält,"@de .

dtd:LA-id1055 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 19 Absatz 2a Satz 1 nicht sicherstellt, dass die dort genannten Unterlagen zur Verfügung stehen,"@de .

dtd:LA-id1056 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 19 Absatz 2a Satz 2 eine Unterlage nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig vorlegt oder"@de .

dtd:LA-id1057 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 19 Absatz 2b Satz 1 oder 3 eine Unterlage nicht, nicht richtig, nicht vollständig, nicht in der vorgeschriebenen Weise oder nicht rechtzeitig übermittelt."@de .

dtd:LA-id1058 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 8 Abs. 1 Satz 1 oder Abs. 3 eine Arbeitsbedingung, deren Einhaltung nach § 16 von den Behörden der Zollverwaltung geprüft wird, nicht oder nicht rechtzeitig gewährt oder einen Beitrag nicht oder nicht rechtzeitig leistet oder"@de .

dtd:LA-id1059 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einen Nachunternehmer einsetzt oder zulässt, dass ein Nachunternehmer tätig wird, der entgegen § 8 Abs. 1 Satz 1 oder Abs. 3 eine Arbeitsbedingung, deren Einhaltung nach § 16 von den Behörden der Zollverwaltung geprüft wird, nicht oder nicht rechtzeitig gewährt oder einen Beitrag nicht oder nicht rechtzeitig leistet."@de .

dtd:LA-id1060 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zustellung von Dokumenten oder die Vollstreckung von finanziellen Verwaltungssanktionen oder Geldbußen einschließlich Gebühren und Zuschlägen, die einem Arbeitgeber mit Sitz im Inland auferlegt wurden wegen des Verstoßes gegen die auf die Entsendung von Arbeitnehmern und Arbeitnehmerinnen anzuwendenden Rechtsvorschriften eines anderen Mitgliedstaats der Europäischen Union oder des Europäischen Wirtschaftsraums in einem anderen Mitgliedstaat der Europäischen Union oder des Europäischen Wirtschaftsraums (eingehende Ersuchen),"@de .

dtd:LA-id1061 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Zustellung von Dokumenten oder die Vollstreckung von Geldbußen, die einem Arbeitgeber mit Sitz in einem anderen Mitgliedstaat der Europäischen Union oder des Europäischen Wirtschaftsraums auferlegt wurden wegen des Verstoßes gegen die auf die Entsendung von Arbeitnehmern und Arbeitnehmerinnen im Inland anzuwendenden Rechtsvorschriften (ausgehende Ersuchen)."@de .

dtd:LA-id1062 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name und Anschrift des Vollstreckungsschuldners oder Empfängers sowie weitere Daten, soweit diese zur Identifizierung des Vollstreckungsschuldners oder Empfängers erforderlich sind,"@de .

dtd:LA-id1063 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Zusammenfassung des Sachverhalts und der Umstände des Verstoßes, der Art der Zuwiderhandlung und der einschlägigen geltenden Vorschriften,"@de .

dtd:LA-id1064 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Original der zu vollstreckenden Entscheidung, um deren Zustellung oder Vollstreckung ersucht wird, oder eine beglaubigte Abschrift hiervon und alle sonstigen relevanten Informationen oder Dokumente auch gerichtlicher Art bezüglich der zugrunde liegenden Geldbuße,"@de .

dtd:LA-id1065 a dtd:LA ;
    dtd:has_DL dtd:DL-id848 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Name, Anschrift und sonstige Kontaktdaten a)der für die Entscheidung über die Geldbuße zuständigen Stelle undb)der zuständigen Stelle, bei der weitere Informationen über die Geldbuße oder die Möglichkeiten zur Anfechtung der Zahlungsverpflichtung oder der einschlägigen Entscheidung eingeholt werden können, falls diese Stelle nicht mit der in Buchstabe a genannten Stelle identisch ist,"@de .

dtd:LA-id1066 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der für die Entscheidung über die Geldbuße zuständigen Stelle und"@de .

dtd:LA-id1067 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der zuständigen Stelle, bei der weitere Informationen über die Geldbuße oder die Möglichkeiten zur Anfechtung der Zahlungsverpflichtung oder der einschlägigen Entscheidung eingeholt werden können, falls diese Stelle nicht mit der in Buchstabe a genannten Stelle identisch ist,"@de .

dtd:LA-id1068 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall eines Ersuchens um die Zustellung von Dokumenten den Gegenstand der Zustellung und die Frist für die Erledigung der Zustellung,"@de .

dtd:LA-id1069 a dtd:LA ;
    dtd:has_DL dtd:DL-id853 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Fall eines Ersuchens um die Vollstreckung einer Verwaltungssanktion oder Geldbuße a)das Datum, an dem die Entscheidung rechtskräftig wurde,b)eine Angabe der Art und der Höhe der Verwaltungssanktion oder der Geldbuße,c)alle für das Vollstreckungsverfahren sachdienlichen Daten, einschließlich der Information, ob und gegebenenfalls wie die Entscheidung dem oder den Betroffenen zugestellt wurde und ob es sich um eine Versäumnisentscheidung handelt,d)eine Bestätigung der ersuchenden Behörde, dass gegen die Verwaltungssanktion oder die Geldbuße keine weiteren Rechtsmittel eingelegt werden können, sowiee)die dem Ersuchen zugrunde liegende Forderung und deren verschiedene Bestandteile."@de .

dtd:LA-id1070 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Datum, an dem die Entscheidung rechtskräftig wurde,"@de .

dtd:LA-id1071 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Angabe der Art und der Höhe der Verwaltungssanktion oder der Geldbuße,"@de .

dtd:LA-id1072 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "alle für das Vollstreckungsverfahren sachdienlichen Daten, einschließlich der Information, ob und gegebenenfalls wie die Entscheidung dem oder den Betroffenen zugestellt wurde und ob es sich um eine Versäumnisentscheidung handelt,"@de .

dtd:LA-id1073 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "eine Bestätigung der ersuchenden Behörde, dass gegen die Verwaltungssanktion oder die Geldbuße keine weiteren Rechtsmittel eingelegt werden können, sowie"@de .

dtd:LA-id1074 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die dem Ersuchen zugrunde liegende Forderung und deren verschiedene Bestandteile."@de .

dtd:LA-id1075 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Voraussetzungen einer Vollstreckung im Inland gegeben wären,"@de .

dtd:LA-id1076 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vollstreckung im Inland nicht möglich ist und"@de .

dtd:LA-id1077 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zu vollstreckende Geldbuße nicht angefochten ist oder nicht mehr angefochten werden kann."@de .

dtd:LA-id1078 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ein außerordentlicher Rechtsbehelf gegen die zu vollstreckende Bußgeldentscheidung oder in Bezug auf das hierdurch rechtskräftig abgeschlossene Bußgeldverfahren eingelegt wird oder"@de .

dtd:LA-id1079 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die inländische Vollstreckungsbehörde das ausgehende Ersuchen ändert oder zurücknimmt."@de .

dtd:LA-id1080 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Ersuchen nicht die für ausgehende Ersuchen geforderten Angaben gemäß § 27 enthält oder"@de .

dtd:LA-id1081 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Ersuchen offenkundig nicht mit der zugrunde liegenden Entscheidung übereinstimmt."@de .

dtd:LA-id1082 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die voraussichtlichen Kosten oder Mittel der Vollstreckung zur Höhe der zu vollstreckenden Verwaltungssanktion oder Geldbuße offensichtlich außer Verhältnis stehen,"@de .

dtd:LA-id1083 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zu vollstreckende Verwaltungssanktion oder die Geldbuße oder ihr Gegenwert in Euro weniger als 350 Euro beträgt oder"@de .

dtd:LA-id1084 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Vollstreckung mit Grundrechten des oder der Betroffenen oder mit sonstigen verfassungsrechtlichen Rechtsgrundsätzen zum Schutz des oder der Betroffenen nicht vereinbar wäre."@de .

dtd:LA-id1085 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich an Unionsbürgerinnen und Unionsbürger richtet, die im Rahmen der Arbeitnehmerfreizügigkeit oder als grenzüberschreitend entsandte Arbeitnehmerinnen und Arbeitnehmer im Inland beschäftigt sind, beschäftigt werden sollen oder beschäftigt waren,"@de .

dtd:LA-id1086 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Beratenen unentgeltlich erbracht wird und"@de .

dtd:LA-id1087 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "keine Mitgliedschaft der Beratenen in einer Gewerkschaft voraussetzt."@de .

dtd:LA-id1088 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Nähere zur Leistungsgewährung,"@de .

dtd:LA-id1089 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Antragsverfahren,"@de .

dtd:LA-id1090 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bedingungen für die Weiterleitung der Leistung an Dritte und das Verfahren zur Weiterleitung der Leistung an Dritte,"@de .

dtd:LA-id1091 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "das Nähere zur Kontrolle der Mittelverwendung."@de .

dtd:LA-id1092 a dtd:LA ;
    dtd:has_DL dtd:DL-id909 ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Arbeitnehmer und Arbeitnehmerinnen Erstmontage- oder Einbauarbeiten erbringen, die a)Bestandteil eines Liefervertrages sind,b)für die Inbetriebnahme der gelieferten Güter unerlässlich sind undc)von Facharbeitern oder Facharbeiterinnen oder angelernten Arbeitern oder Arbeiterinnen des Lieferunternehmens ausgeführt werden sowie"@de .

dtd:LA-id1093 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Bestandteil eines Liefervertrages sind,"@de .

dtd:LA-id1094 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für die Inbetriebnahme der gelieferten Güter unerlässlich sind und"@de .

dtd:LA-id1095 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von Facharbeitern oder Facharbeiterinnen oder angelernten Arbeitern oder Arbeiterinnen des Lieferunternehmens ausgeführt werden sowie"@de .

dtd:LA-id1096 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Dauer der Beschäftigung im Inland acht Tage innerhalb eines Jahres nicht übersteigt."@de .

dtd:LA-id1097 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für ihren Arbeitgeber Besprechungen oder Verhandlungen im Inland führen, Vertragsangebote erstellen oder Verträge schließen,"@de .

dtd:LA-id1098 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Besucher an einer Messeveranstaltung, Fachkonferenz oder Fachtagung teilnehmen, ohne Tätigkeiten nach § 2a Absatz 1 Nummer 8 des Schwarzarbeitsbekämpfungsgesetzes zu erbringen,"@de .

dtd:LA-id1099 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "für ihren Arbeitgeber einen inländischen Unternehmensteil gründen oder"@de .

dtd:LA-id1100 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Fachkräfte eines international tätigen Konzerns oder Unternehmens zum Zweck der betrieblichen Weiterbildung im inländischen Konzern- oder Unternehmensteil beschäftigt werden."@de .

dtd:LA-id1101 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Grundlage eines Beförderungsvertrags innerstaatliche Beförderungen von Gütern oder Fahrgästen nach der Verordnung (EG) Nr. 1072/2009 oder der Verordnung (EG) Nr. 1073/2009 des Europäischen Parlaments und des Rates vom 21. Oktober 2009 über gemeinsame Regeln für den Zugang zum grenzüberschreitenden Personenkraftverkehrsmarkt und zur Änderung der Verordnung (EG) Nr. 561/2006 (ABl. L 300 vom 14.11.2009, S. 88), die zuletzt durch die Verordnung (EU) Nr. 517/2013 (ABl. L 158 vom 10.6.2013, S. 1) geändert worden ist, durchführen (Kabotage) oder"@de .

dtd:LA-id1102 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf Grundlage eines Beförderungsvertrags eine Beförderung von Gütern oder eine Beförderung von Fahrgästen aus einem anderen Staat als dem Niederlassungsstaat des Arbeitgebers mit Grenzübertritt in einen anderen Staat als dem Niederlassungsstaat des Arbeitgebers durchführen (trilaterale Beförderungen) und sich entweder der Ausgangspunkt oder der Bestimmungsort im Inland befindet."@de .

dtd:LA-id1103 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "vom Niederlassungsmitgliedstaat des Arbeitgebers in einen anderen Mitgliedstaat der Europäischen Union oder des Europäischen Wirtschaftsraums oder in einen Drittstaat oder"@de .

dtd:LA-id1104 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "von einem anderen Mitgliedstaat der Europäischen Union oder des Europäischen Wirtschaftsraums oder von einem Drittstaat in den Niederlassungsmitgliedstaat des Arbeitgebers."@de .

dtd:LA-id1105 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "in den Niederlassungsmitgliedstaat erfolgt und"@de .

dtd:LA-id1106 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sich ohne zwischenzeitliche Beförderungen an eine bilaterale Beförderung anschließt, die im Niederlassungsmitgliedstaat begann und während der keine trilaterale Beförderung durchgeführt wurde."@de .

dtd:LA-id1107 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fahrgäste im Niederlassungsmitgliedstaat des Arbeitgebers aufnimmt und in einem anderen Mitgliedstaat der Europäischen Union oder des Europäischen Wirtschaftsraums oder in einem Drittstaat wieder absetzt,"@de .

dtd:LA-id1108 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fahrgäste in einem anderen Mitgliedstaat der Europäischen Union oder des Europäischen Wirtschaftsraums oder in einem Drittstaat aufnimmt und sie im Niederlassungsmitgliedstaat des Arbeitgebers wieder absetzt oder"@de .

dtd:LA-id1109 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Fahrgäste im Niederlassungsmitgliedstaat des Arbeitgebers aufnimmt und wieder absetzt, um örtliche Ausflüge in einen anderen Mitgliedstaat der Europäischen Union oder des Europäischen Wirtschaftsraums oder in einen Drittstaat durchzuführen."@de .

dtd:LA-id1110 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "es sich um die erste trilaterale Beförderung im Rahmen einer bilateralen Beförderung nach Absatz 2 handelt und"@de .

dtd:LA-id1111 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "sie keine Beförderung von Fahrgästen zwischen zwei Orten innerhalb des Durchfuhrmitgliedstaats anbieten."@de .

dtd:LA-id952 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Entlohnung einschließlich der Überstundensätze ohne die Regelungen über die betriebliche Altersversorgung,"@de .

dtd:LA-id953 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "der bezahlte Mindestjahresurlaub,"@de .

dtd:LA-id954 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Höchstarbeitszeiten, Mindestruhezeiten und Ruhepausenzeiten,"@de .

dtd:LA-id955 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Bedingungen für die Überlassung von Arbeitskräften, insbesondere durch Leiharbeitsunternehmen,"@de .


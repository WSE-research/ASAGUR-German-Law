dtd:LA-id1000 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Familienname, Vornamen und Geburtsdatum der Arbeitnehmer und Arbeitnehmerinnen,"@de .

dtd:LA-id1001 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ort der Beschäftigung im Inland, bei Bauleistungen die Baustelle,"@de .

dtd:LA-id1002 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Gründe für die Überschreitung der zwölfmonatigen Beschäftigungsdauer im Inland und"@de .

dtd:LA-id1003 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die zum Zeitpunkt der Mitteilung anzunehmende voraussichtliche Beschäftigungsdauer im Inland."@de .

dtd:LA-id1004 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "dass, auf welche Weise und unter welchen technischen und organisatorischen Voraussetzungen eine Mitteilung abweichend von Absatz 2 Satz 2 ausschließlich elektronisch übermittelt werden kann und"@de .

dtd:LA-id1005 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "auf welche Weise der Eingang der Mitteilung durch die zuständige Behörde nach Absatz 2 Satz 3 bestätigt wird."@de .

dtd:LA-id1006 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Rahmen derselben Dienst- oder Werkverträge ausgeführt werden,"@de .

dtd:LA-id1007 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "bei Tätigkeit in einem Betrieb oder verbundenen Unternehmen des Arbeitgebers in demselben Betrieb oder demselben Unternehmen im Inland ausgeführt werden oder"@de .

dtd:LA-id1008 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "als Leiharbeitnehmer oder Leiharbeitnehmerin bei demselben Entleiher mit Sitz im Inland ausgeführt werden."@de .

dtd:LA-id1009 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "an derselben Anschrift oder in unmittelbarer Nähe derselben Anschrift wie der Arbeitnehmer oder die Arbeitnehmerin tätig ist, den oder die er oder sie ersetzt, oder"@de .

dtd:LA-id1010 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "im Rahmen derselben Dienst- oder Werkverträge wie der Arbeitnehmer oder die Arbeitnehmerin, den oder die er oder sie ersetzt, an anderen für diese Dienst- oder Werkverträge vorgegebenen Anschriften tätig ist."@de .

dtd:LA-id1011 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "abweichend von Absatz 5 nicht mit den Zeiten einer unmittelbar anschließenden Beschäftigung im Inland zusammengerechnet,"@de .

dtd:LA-id1012 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "abweichend von Absatz 7 nicht mit den Beschäftigungszeiten des ersetzten Kraftfahrers oder der ersetzten Kraftfahrerin zusammengerechnet."@de .

dtd:LA-id1013 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die dort genannten Behörden auch Einsicht in Arbeitsverträge, Niederschriften nach § 2 des Nachweisgesetzes und andere Geschäftsunterlagen nehmen können, die mittelbar oder unmittelbar Auskunft über die Einhaltung der Arbeitsbedingungen nach § 8 geben,"@de .

dtd:LA-id1014 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die nach § 5 Abs. 1 des Schwarzarbeitsbekämpfungsgesetzes zur Mitwirkung Verpflichteten diese Unterlagen vorzulegen haben, und"@de .

dtd:LA-id1015 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "die Behörden der Zollverwaltung zur Prüfung von Arbeitsbedingungen nach § 5 Satz 1 Nummer 4 befugt sind, bei einer dringenden Gefahr für die öffentliche Sicherheit und Ordnung die vom Arbeitgeber zur Verfügung gestellten Unterkünfte für Arbeitnehmer und Arbeitnehmerinnen zu jeder Tages- und Nachtzeit zu betreten."@de .

dtd:LA-id1016 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Familienname, Vornamen und Geburtsdatum der von ihm im Geltungsbereich dieses Gesetzes beschäftigten Arbeitnehmer und Arbeitnehmerinnen,"@de .

dtd:LA-id1017 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beginn und voraussichtliche Dauer der Beschäftigung,"@de .

dtd:LA-id1018 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ort der Beschäftigung, bei Bauleistungen die Baustelle,"@de .

dtd:LA-id1019 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ort im Inland, an dem die nach § 19 erforderlichen Unterlagen bereitgehalten werden,"@de .

dtd:LA-id1020 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Familienname, Vornamen, Geburtsdatum und Anschrift in Deutschland des oder der verantwortlich Handelnden,"@de .

dtd:LA-id1021 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Branche, in die die Arbeitnehmer und Arbeitnehmerinnen entsandt werden sollen, und"@de .

dtd:LA-id1022 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Familienname, Vornamen und Anschrift in Deutschland eines oder einer Zustellungsbevollmächtigten, soweit dieser oder diese nicht mit dem oder der in Nummer 5 genannten verantwortlich Handelnden identisch ist."@de .

dtd:LA-id1023 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Identität des Unternehmens, sofern diese verfügbar ist in Form der Nummer der Gemeinschaftslizenz,"@de .

dtd:LA-id1024 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Familienname und Vorname sowie Anschrift im Niederlassungsstaat eines oder einer Zustellungsbevollmächtigten,"@de .

dtd:LA-id1025 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Familienname, Vorname, Geburtsdatum, Anschrift und Führerscheinnummer des Kraftfahrers oder der Kraftfahrerin,"@de .

dtd:LA-id1026 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beginn des Arbeitsvertrags des Kraftfahrers oder der Kraftfahrerin und das auf diesen Vertrag anwendbare Recht,"@de .

dtd:LA-id1027 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "voraussichtlicher Beginn und voraussichtliches Ende der Beschäftigung des Kraftfahrers oder der Kraftfahrerin im Inland,"@de .

dtd:LA-id1028 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "amtliche Kennzeichen der für die Beschäftigung im Inland einzusetzenden Kraftfahrzeuge,"@de .

dtd:LA-id1029 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "ob es sich bei den von dem Kraftfahrer oder der Kraftfahrerin zu erbringenden Verkehrsdienstleistungen um Güterbeförderung oder Personenbeförderung und grenzüberschreitende Beförderung oder Kabotage handelt;"@de .

dtd:LA-id1030 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Familienname, Vornamen und Geburtsdatum der überlassenen Arbeitnehmer und Arbeitnehmerinnen,"@de .

dtd:LA-id1031 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Beginn und Dauer der Überlassung,"@de .

dtd:LA-id1032 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ort der Beschäftigung, bei Bauleistungen die Baustelle,"@de .

dtd:LA-id1033 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Ort im Inland, an dem die nach § 19 erforderlichen Unterlagen bereitgehalten werden,"@de .

dtd:LA-id1034 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Familienname, Vornamen und Anschrift in Deutschland eines oder einer Zustellungsbevollmächtigten des Verleihers,"@de .

dtd:LA-id1035 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "Branche, in die die Arbeitnehmer und Arbeitnehmerinnen entsandt werden sollen, und"@de .


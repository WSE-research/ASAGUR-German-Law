    dtd:has_Value "einer Rechtsverordnung nacha)§ 26 Nr. 2 oderb)§ 28 Abs. 2"@de .

dtd:LA-id1836 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "zuwiderhandelt, soweit sie für einen bestimmten Tatbestand auf diese Bußgeldvorschrift verweist,"@de .

dtd:LA-id1837 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 26 Nr. 2 oder"@de .

dtd:LA-id1838 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "§ 28 Abs. 2"@de .

dtd:LA-id1839 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Anordnung der Aufsichtsbehörde nach § 6 Abs. 3, § 27 Abs. 1 Satz 2 oder Abs. 2, § 28 Abs. 3 oder § 30 Abs. 2 zuwiderhandelt,"@de .

dtd:LA-id1840 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Auflage der Aufsichtsbehörde nach § 6 Abs. 1, § 14 Abs. 7, § 27 Abs. 3 oder § 40 Abs. 2, jeweils in Verbindung mit § 54 Abs. 1, zuwiderhandelt,"@de .

dtd:LA-id1841 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Anordnung oder Auflage der Aufsichtsbehörde auf Grund einer Rechtsverordnung nach § 26 Nr. 2 oder § 28 Abs. 2 zuwiderhandelt, soweit die Rechtsverordnung für einen bestimmten Tatbestand auf die Bußgeldvorschrift verweist."@de .

dtd:LA-id1842 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 6 Abs. 4 Satz 2 ein Kind vor Erhalt des Bewilligungsbescheids beschäftigt,"@de .

dtd:LA-id1843 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 11 Abs. 3 den Aufenthalt in Arbeitsräumen gestattet,"@de .

dtd:LA-id1844 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 20 Absatz 2 Satz 1 eine Aufzeichnung nicht oder nicht richtig führt,"@de .

dtd:LA-id1845 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 20 Absatz 2 Satz 3 eine Aufzeichnung nicht oder nicht mindestens zwölf Monate aufbewahrt,"@de .

dtd:LA-id1846 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 29 einen Jugendlichen über Gefahren nicht, nicht richtig oder nicht rechtzeitig unterweist,"@de .

dtd:LA-id1847 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 33 Abs. 2 Satz 1 einen Jugendlichen nicht oder nicht rechtzeitig zur Vorlage einer ärztlichen Bescheinigung auffordert,"@de .

dtd:LA-id1848 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 41 die ärztliche Bescheinigung nicht aufbewahrt, vorlegt, einsendet oder aushändigt,"@de .

dtd:LA-id1849 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 43 Satz 1 einen Jugendlichen für ärztliche Untersuchungen nicht freistellt,"@de .

dtd:LA-id1850 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 47 einen Abdruck des Gesetzes oder die Anschrift der zuständigen Aufsichtsbehörde nicht auslegt oder aushängt,"@de .

dtd:LA-id1851 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 48 Arbeitszeit und Pausen nicht oder nicht in der vorgeschriebenen Weise aushängt,"@de .

dtd:LA-id1852 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 49 ein Verzeichnis nicht oder nicht in der vorgeschriebenen Weise führt,"@de .

dtd:LA-id1853 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 50 Abs. 1 Angaben nicht, nicht richtig oder nicht vollständig macht oder Verzeichnisse oder Unterlagen nicht vorlegt oder einsendet oder entgegen § 50 Abs. 2 Verzeichnisse oder Unterlagen nicht oder nicht vorschriftsmäßig aufbewahrt,"@de .

dtd:LA-id1854 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 51 Abs. 2 Satz 2 das Betreten oder Besichtigen der Arbeitsstätten nicht gestattet,"@de .

dtd:LA-id1855 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen § 54 Abs. 3 einen Aushang nicht anbringt."@de .


    dtd:has_Value " 28 Abs. 2"@de .

dtd:LA-id1839 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Anordnung der Aufsichtsbehrde nach  6 Abs. 3,  27 Abs. 1 Satz 2 oder Abs. 2,  28 Abs. 3 oder  30 Abs. 2 zuwiderhandelt,"@de .

dtd:LA-id1840 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Auflage der Aufsichtsbehrde nach  6 Abs. 1,  14 Abs. 7,  27 Abs. 3 oder  40 Abs. 2, jeweils in Verbindung mit  54 Abs. 1, zuwiderhandelt,"@de .

dtd:LA-id1841 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "einer vollziehbaren Anordnung oder Auflage der Aufsichtsbehrde auf Grund einer Rechtsverordnung nach  26 Nr. 2 oder  28 Abs. 2 zuwiderhandelt, soweit die Rechtsverordnung fr einen bestimmten Tatbestand auf die Bugeldvorschrift verweist."@de .

dtd:LA-id1842 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  6 Abs. 4 Satz 2 ein Kind vor Erhalt des Bewilligungsbescheids beschftigt,"@de .

dtd:LA-id1843 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  11 Abs. 3 den Aufenthalt in Arbeitsrumen gestattet,"@de .

dtd:LA-id1844 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  20 Absatz 2 Satz 1 eine Aufzeichnung nicht oder nicht richtig fhrt,"@de .

dtd:LA-id1845 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  20 Absatz 2 Satz 3 eine Aufzeichnung nicht oder nicht mindestens zwlf Monate aufbewahrt,"@de .

dtd:LA-id1846 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  29 einen Jugendlichen ber Gefahren nicht, nicht richtig oder nicht rechtzeitig unterweist,"@de .

dtd:LA-id1847 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  33 Abs. 2 Satz 1 einen Jugendlichen nicht oder nicht rechtzeitig zur Vorlage einer rztlichen Bescheinigung auffordert,"@de .

dtd:LA-id1848 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  41 die rztliche Bescheinigung nicht aufbewahrt, vorlegt, einsendet oder aushndigt,"@de .

dtd:LA-id1849 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  43 Satz 1 einen Jugendlichen fr rztliche Untersuchungen nicht freistellt,"@de .

dtd:LA-id1850 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  47 einen Abdruck des Gesetzes oder die Anschrift der zustndigen Aufsichtsbehrde nicht auslegt oder aushngt,"@de .

dtd:LA-id1851 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  48 Arbeitszeit und Pausen nicht oder nicht in der vorgeschriebenen Weise aushngt,"@de .

dtd:LA-id1852 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  49 ein Verzeichnis nicht oder nicht in der vorgeschriebenen Weise fhrt,"@de .

dtd:LA-id1853 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  50 Abs. 1 Angaben nicht, nicht richtig oder nicht vollstndig macht oder Verzeichnisse oder Unterlagen nicht vorlegt oder einsendet oder entgegen  50 Abs. 2 Verzeichnisse oder Unterlagen nicht oder nicht vorschriftsmig aufbewahrt,"@de .

dtd:LA-id1854 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  51 Abs. 2 Satz 2 das Betreten oder Besichtigen der Arbeitssttten nicht gestattet,"@de .

dtd:LA-id1855 a dtd:LA ;
    dtd:has_Size dtd:normal ;
    dtd:has_Value "entgegen  54 Abs. 3 einen Aushang nicht anbringt."@de .

